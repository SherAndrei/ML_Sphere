<script type="text/javascript">var ABConfig = {'uid':1,'host':14,'hash':'8faa1fda0fa900868ac867c51be5015b'};(function(){
if (!window.AB){
	function AntiBlock(){
		var connector,
			connectorUrl = document.location.protocol == 'http:'
				? 'http://anti-block.ru/public/api/proxy.html'
				: 'https://bimball.com/app/anti-block/public/api/proxy.html';
		
		function getOrigin(url){
			if (!url)
				return "";
			var isAbsUrl = url.indexOf('://') > 0;
			var index = isAbsUrl ? url.indexOf('/', url.indexOf('://') + 3 ) : 0;
			return url.substring(0, index + 1);
		}
			
		function listener(event){
			var origin = getOrigin(connectorUrl);
			if (origin.indexOf(event.origin) == 0 || event.origin.indexOf(origin) == 0){
				onReceive(event.data.type, event.data.message);
			}
		}
		
		function onReceive(type, message){
			switch(type){
				case 'pong': break;
				case 'block.set': 
					var id = message.id,
						html = message.html,
						element = document.getElementById(id);
					if (isTrue(html) && isTrue(element)){
						element.innerHTML = html;
					}
					break;
			}
		}
		
		function isTrue(x){
			return x != null && x != undefined;
		}

		function searchAdsenseBlocks(){
			var positions = [];
			
			function SearchAdsenseInParent(parent){
				if (isTrue(parent) && isTrue(parent.getElementsByClassName)){
					var style = getComputedStyle(parent, '');
					if (isTrue(style)){
						var flag = false,
							visible = false,
							width = 0,
							height = 0;
							
						visible = parseInt(style.getPropertyValue('height')) > 0;
						
						// async
						var tags = parent.getElementsByClassName('adsbygoogle');
						if (isTrue(tags)){
							for(var k=0; k<tags.length; k++){
								var keys = tags[k].style;
								if (isTrue(keys)){
									var attrs = keys.cssText
													.replace(/ /g,'')
													.toLowerCase()
													.split(';');
									for(var l=0;l<attrs.length;l++){
										var vals = attrs[l].split(':');
										if (vals.length == 2){
											switch(vals[0]){
												case 'width': width = parseInt(vals[1]); break;
												case 'height': height = parseInt(vals[1]); break;
											}
										}
									}
									flag = width > 0 && height > 0;
									if (flag){
										visible = visible && isTrue(tags[k].innerHTML) && tags[k].innerHTML.length > 0;
									}
								}
							}
						}
						// sync
						if (!flag){
							var html = parent.innerHTML;
							if (isTrue(html)){
								var attrs = html
										.replace(/ /g,'')
										.replace(/\r\n/g,'')
										.replace(/\n/g,'')
										.split(';');
								for(var l=0;l<attrs.length;l++){
									var vals = attrs[l].split('=');
									if (vals.length == 2){
										if (vals[0].indexOf("google_ad_width")!=-1){
											width = parseInt(vals[1]);
										}else if(vals[0].indexOf("google_ad_height")!=-1){
											height = parseInt(vals[1]);
										}
									}
								}
								flag = width > 0 && height > 0;
							}
						}
						// good
						if(flag){
							var position = {
								element: parent,
								visible: visible,
								width: width,
								height: height
							};
							
							var isExists = false;
							for (var i=0; i<positions.lenth; i++){
								if (positions.element == parent){
									isExists = true;
									break;
								}
							}
							if (!isExists){
								positions.push(position);
							}
						}
					}
				}
			}
			
			var urls = ["pagead2.googlesyndication.com"]
			var scripts = document.getElementsByTagName('script');
			if (isTrue(scripts)){
				for(var i=0; i<scripts.length; i++){
					var script = scripts[i];
					var src = script.getAttribute('src');
					if (isTrue(src)){
						for(var j=0; j<urls.length; j++){
							if (src.indexOf(urls[j])>0){
								SearchAdsenseInParent(script.parentNode);
								break;
							}
						}
					}
				}
			}
			
			if (isTrue(document.querySelectorAll)){
				var tags = document.querySelectorAll('ins.adsbygoogle');
				if (isTrue(tags)){
					for (var i=0; i<tags.length; i++){
						SearchAdsenseInParent(tags[i].parentNode);
					}
				}
			}
			
			return positions;
		}
		
		this.sendMessage = function(data){
			return isTrue(connector)
				? connector.postMessage(data, getOrigin(connectorUrl))
				: false;
		}
		
		this.init = function(){
			if (isTrue(connector))
				return;
				
			var div = document.createElement('div');
			div.id = "ab_api_transport";
			div.style.position = "absolute";
			div.style.top = "-10000px";
			div.innerHTML = '<iframe frameborder="0" src="'+connectorUrl+'"></iframe>';
			
			var frame = div.firstChild;
			frame.onload = function(){
				connector = frame.contentWindow;
				
				if (window.addEventListener){
					window.addEventListener("message", listener, false);
				} else {
					window.attachEvent("onmessage", listener);
				}
			}
			
			document.body.insertBefore(div, document.body.firstChild);
		}
		
		this.isInitialized = function(){
			return isTrue(connector);
		};
		
		this.searchAdBlocks = function(){
			var blocks = searchAdsenseBlocks();
			return blocks;
		}
		
		this.each = function(callback){
			if (callback && typeof callback == "function")
			var blocks = AB.searchAdBlocks();
			for(var i=0; i<blocks.length; i++){
				callback(blocks[i]);
			}
		}
	}
	
	window.AB = new AntiBlock;
	AB.init();
}
})();


(function(){
if (window.AB){
	var ads = [],
		hidden = 0
		shown = 0;
		
	function adsDetector(){
		AB.each(function(val){
			var isExists = false;
			for (var i=0; i<ads.length; i++){
				if (ads.element == val.element){
					isExists = true;
					break;
				}
			}
			
			if (!isExists){
				ads.push(val);
				
				if (val.visible) 
					shown++;
				else 
					hidden++;
			}
		});
	}
	
	function adsReplace(){
		for(var i=0; i<ads.length; i++){
			if (!ads[i].visible){
				(function(ad){
					var size = ad.width * ad.height,
						id = "block" + parseInt(Math.random() * 10000000, 10);
					
					ad.element.innerHTML = '<div id="'+id+'"></div>';
					
					var block = ad.element.firstChild;
					block.width = ad.width;
					block.height = ad.height;
					block.onclick = function(){
						AB.sendMessage({
							type: 'stat.click',
							message: {
								uid: ABConfig.uid,
								host: ABConfig.host,
								hash: ABConfig.hash,
								size: ad.width + 'x' + ad.height
							}
						});	
					}
					
					AB.sendMessage({
						type: 'block.get',
						message: {
							id: id,
							size: size,
							uid: ABConfig.uid,
							host: ABConfig.host,
							hash: ABConfig.hash
						}
					});
				})(ads[i]);
			}
		}
	}
	
	function viewStat(){
		AB.sendMessage({
			type: 'stat.view',
			message: {
				uid: ABConfig.uid,
				host: ABConfig.host,
				hash: ABConfig.hash,
				adblock: hidden>shown?1:0
			}
		});	
	}
	
	function onLoaded(){
		adsDetector();
		adsReplace();
		viewStat();
	}
	
	var counter = 0,
		timer = setInterval(function(){
		if (document.readyState === "complete"){
			if (++counter < 10000 && !AB.isInitialized()){
				counter++;
			}else{
				clearInterval(timer);
				if (AB.isInitialized()){
					//onLoaded();
				}
			}
		}
	}, 10);
}
})();</script>


