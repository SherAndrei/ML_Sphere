});

function ajaxLoadMore(area_id) {
	uri = window.location.search.substr(1);
	$.ajax({
		url: "/general/ajax.php?cl=m_deal_offer&method=type_page&category=134&type=1&template=default_v4&from_"+area_id+"="+nextPage+"&area_id="+area_id+'&'+uri,
		context: document.body,
		beforeSend: function() {
			$('div#loadMoreLoader').show();
		},
		success: handleLoadMore
	});
}

function handleLoadMore(responseText) {
	var btnObj = $('span#loadMoreDealsText');
	newElements = responseText.trim().replace(/\n/g, '');
	var newDiv = $('<div></div>');
	newDiv.append($(newElements));
	newDivLength = newDiv.find('.action_item_v4').length;
	if( newDivLength < nextPageCount) {
		btnObj.hide();
		$('#loadMoreDealsBtn').attr('style', '');
	}
	try {
		$(newElements).insertBefore($('#clearId')).animate();
		$('#currentCount').html(parseInt($('#currentCount').html()) + newDivLength);
		availableCount = totalDosCount - parseInt($('#currentCount').html());
		//console.log(availableCount);

		btnObj.html('Загрузить еще '+ Math.min(availableCount, nextPageCount) + ' ' + numeral("акция", "акции", "акций"));
		//console.log(btnObj);
		if(availableCount == 0) {
			btnObj.hide();
			$('#loadMoreDealsBtn').attr('style', '');
		}
		totalResurection();
		nextPage += 1;
		$('div#loadMoreLoader').hide();
	} catch(e) {
		btnObj.hide();
		$('#loadMoreDealsBtn').attr('style', '');
	}
}

function totalResurection() {
	$('div.more_info_top').each(function(){
		$(this).css('bottom', -1 * +$(this).outerHeight()).attr('data-margin', $(this).outerHeight());
	});
	$('div.catalog_container .action_item').bind({
		mouseenter: function(){
			$('div.more_info', this).stop(true, true).animate({'bottom': 0}, 300);
			$('div.more_info_top', this).stop(true, true).animate({'bottom': 0}, 300);
		},
		mouseleave: function(){
			$('div.more_info', this).stop(true, true).css('bottom', '-105px');
			$('div.more_info_top', this).stop(true, true).css('bottom', -1 * +$('div.more_info_top', this).attr('data-margin'));
			$(this).removeClass('active');
			$('span.arr_white_top', this).css({backgroundPosition: '-18px 0px'});
			$('div.more_metro').hide();
		}
	});
	$('div.choosed_metro').on('click', function(){
		if( $(this).hasClass('active') ){
			$(this).removeClass('active');
			$('span.arr_white_top', this).css({backgroundPosition: '-18px 0px'});
			$(this).parent().parent().find('div.more_metro').stop(true, true).animate({'height': 'toggle'}, 300);
		}else{
			$(this).addClass('active');
			$('span.arr_white_top', this).css({backgroundPosition: '-18px -19px'});
			$(this).parent().parent().find('div.more_metro').stop(true, true).animate({'height': 'toggle'}, 300);
		};
		if( $(this).parent().parent().find('div.more_metro ul li').length != 0 )
			return false;
	});
	$(".ai_likes_icon").on('click', function(){
		object=$(this);
		$.ajax({
			type: "GET",
			url: "/general/ajax.php",
			data: { deal_id: $(this).attr("data-deal-id"), location: "charset", cl: "c_likes", method: "register" }
		}).done(function( msg ) {
					if(msg !=''){
						object.removeClass('ai_default');
						object.next().html(msg);
					}
				});
	});
}

function applyVars(page) {
    var data = getUrlVars(),
        form = $('<form></form>'),
        uri = window.location.href.slice(0, window.location.href.indexOf('?'));

    document.getElementById('biglion_shadow').style.display = 'block';
    if(page) {
        form.append('<input type="hidden" name="page" value="'+page+'">');
    }
    for (var name in data) {
        if (data[name] != undefined && !$.isNumeric(name) && name !== 'page') {
            form.append('<input type="hidden" name="'+name+'" value="'+data[name]+'">');
        }
    }
    form.attr({'action': uri, 'method': 'GET'}).appendTo('body').submit();
    return false;
}

//Функция разбора GET параметров строки
function getUrlVars() {
  vars = [];
  var hashes = window.location.href.slice(window.location.href.indexOf('?') + 1).split('&');
  for(var i = 0; i < hashes.length; i++) {
    hash = hashes[i].split('=');
    if(!$.isNumeric(hash[0])) {
        vars.push(hash[0]);
        vars[hash[0]] = hash[1];
    }
  }
  return vars;
}
