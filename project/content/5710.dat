var nextPage = 2;
var totalDosCount = 223;
var nextPageCount = 60;
$(document).ready(function(){
	$('a#loadMoreDealsBtn').mouseup(function(e){
		e.preventDefault();
		$('div#loadMoreLoader').show();
		ajaxLoadMore(4, this);
	});
	$('a#loadMoreDealsBtn').mousedown(function(e){
