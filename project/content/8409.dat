jQuery(function($) {
  //подвешиваем на событие клик по кнопке

  $('#go').on('click',function(e) {

    $('#form_chat').hide("fast");
    $('.chater2').hide("fast");
    $('.form_comp').show();    
  });

  $('.go').on('click',function(e) {

    $('#form_chat').hide("fast");
    $('.chater2').hide("fast");
    $('.form_comp').show();    
    $('#chatbox').hide();    

    //отключение ее
    $(e.currentTarget).attr('disabled', true);
    // показываем процесс закгрузки
    setTimeout(function() {
        $('.loader').toggleClass('hide');
    }, 1000);

    // подвешиваем паузу на 5 секунд
    setTimeout(function() {
      // скрываем процесс загрузки
      $('.loader').toggleClass('hide');
        $('#chatbox').show();          
      // делаем кнопку активной
      $(e.currentTarget).attr('disabled', false);
      // добавляем текст о том что загрузка успешно закончилась
//      $(e.currentTarget).after('<div>Загрузка завершена</div>');
      $('#form_chat').show("slow");
    }, 5000)
  });
});

</script>
