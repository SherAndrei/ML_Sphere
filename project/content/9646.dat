        $(function () {
            /*меню юзера*/
            var $window = $(window);
            $window.resize(function resize(){
                if ($window.width() < 1025) {
                    $('.js-ava__link').off('click');
                    $('.js-ava__link,.js-overlay-user').on('click', function () {
                        $('.user-widget-block,.js-overlay-user').toggleClass('user-widget-block_open');
