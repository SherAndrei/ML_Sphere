						if ($(event.target).closest('#curSortBusiness').length == 0 && $(event.target).attr('id') != 'curSortBusiness') {
							$('#curSortBusiness').removeClass('curParent');
							$('#sortBusinessList').hide();
						}
					});
					
					$('#curCatBusiness').click(function(event) {
						$(this).addClass('curParent');
						$('#catBusinessList').toggle();
					});
					$(document).click(function (event) {
						if ($(event.target).closest('#curCatBusiness').length == 0 && $(event.target).attr('id') != 'curCatBusiness') {
							$('#curCatBusiness').removeClass('curParent');
							$('#catBusinessList').hide();
						}
					});
					
					$(".cost_money, .numericSep i").each(function(){
					$(this).text($(this).text().replace(/\B(?=(\d{3})+(?!\d))/g, " "));
				});
					});
				</script>
<script type="text/javascript">$(document).ready(function(){
$(window).scroll(function () {if ($(this).scrollTop() > 0) {$('#scroller').fadeIn();} else {$('#scroller').fadeOut();}});
$('#scroller').click(function () {$('body,html').animate({scrollTop: 0}, 400); return false;});
});</script>				
