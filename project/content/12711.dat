<script type="text/javascript">
/* <![CDATA[ */
/* POPUP WINDOW */
$('#popup_window a.close_btn').click(function(){
	$('#popup_window').hide();
	return false;
});

function popup_show(winId)
{
	$('#popup_window div.content>div').hide();
	$('#popup_window #' + winId).show();

	popup_window_w = $('#popup_window').width();
	popup_window_h = $('#popup_window').height();
	window_w = $(window).width();
	window_h = $(window).height();
	new_left = (window_w - popup_window_w)/2;
	if (new_left < 0) new_left = 10;
	var new_top = (window_h - popup_window_h)/2;
	if (new_top < 0) new_top = 10;
	$('#popup_window').css('left', new_left + 'px');
	$('#popup_window').css('top', new_top + 'px');


	$('#popup_window').show();
}
$('#add_user form').submit(function(){
	var error = false;
	$('#add_user input[type!="submit"], #add_user textarea').each(function(){
		if ( $(this).val() == '' ){			
			error = true;
			return false;
		}
		});
	if ( error )
	{
		alert('Заполните все поля!');
		return false;
	}
	email = $('#add_user input[name="email"]').val();
	reg = /^([A-Za-z0-9_\-\.])+\@([A-Za-z0-9_\-\.])+\.([A-Za-z]{2,6})$/;
	if (reg.test(email) == false)
	{
		alert('Неправильный email!');
		return false;
	}	
	});
$('#edit_user form').submit(function(){
	var error = false;
	$('#edit_user input[type!="submit"], #edit_user textarea').each(function(){
		if ( $(this).val() == '' && $(this).attr('name') != 'pass' ){alert($(this).attr('name'));
			error = true;
			return false;
		}
		});
	if ( error )
	{
		alert('Заполните все поля!');
		return false;
	}
	email = $('#edit_user input[name="email"]').val();
	reg = /^([A-Za-z0-9_\-\.])+\@([A-Za-z0-9_\-\.])+\.([A-Za-z]{2,6})$/;
	if (reg.test(email) == false)
	{
		alert('Неправильный email!');
		return false;
	}	
	});



$('.content_selector a').click(function(){
	need_div = '.selectable_content .' + $(this).attr('rel');
	$('.selectable_content>div').hide();
	$(need_div).show();

	$('.content_selector a.active').removeClass('active');
	$(this).addClass('active');

	return false;
});



var consult_hidden = true;
function hideConsult()
{
	$('#consult .content').animate({'width': '0px'}, 'slow');
	$('#consult').removeClass('opened');
	consult_hidden = true;
	
	$(document).unbind('click');
}

$('#consult .link a').click(function(){
	if ( consult_hidden )
	{
		$('#consult .content').animate({'width': '300px'}, 'slow');
		$('#consult').addClass('opened');
		consult_hidden = false;

		$(document).bind('click', function(e){
				if ($(e.target).parents().filter('#consult:visible').length != 1)
				{
					hideConsult();
				}
			});
	}
	else
	{
		hideConsult();
	}
});

var textarea_clicked = false;
$('#consult .content textarea').click(function(){
	if ( !textarea_clicked )
	{
		$(this).val('');
		textarea_clicked = true;
	}
});

$('#consult form').submit(function(){
	$('#consult input[type=submit]').attr('disabled', 'disabled');
	data = new Object();
	data.mess = $('#consult textarea').val();
	data.name = $('#consult input[name=name]').val();
	data.phone = $('#consult input[name=phone]').val();
	if ( data.mess == '' || data.name == '' || data.phone == '' )
	{
		alert('Пожалуйста, заполните все поля!');
		$('#consult input[type=submit]').removeAttr('disabled');
	}
	else
	{
		$('#consult .content div').html('<div style="text-align: center; margin-top: 60px;"><img src="/themes/dontworry/images/ajax-loader.gif" alt="Пожалуйста, подождите..." />');
		$.ajax({
			url : '',
			dataType : 'json',
			data: data,
			type : 'POST',
			success : function (data){
					if ( data.result == true )
					{
						$('#consult .content div').html('<p style="margin: 10px; 10px;"><br />Спасибо, Ваше сообщение отправлено нашим юристам!');
						setTimeout(function(){hideConsult();}, 3000);
					}
					else
					{
						if ( data.err_mess != '' ) alert(data.err_mess);
					}
				},
			complete: function(jqXHR, textStatus){
				$('#consult input[type=submit]').removeAttr('disabled');
			}
			});
	}
	return false;
	});

$(document).ready(function(){
	$("#consult input[name=phone]").mask("8 (999) 999-99-99");
	
	center_h = $('#main_content .static_content').height();
	right_h = $('#right').height();//alert(center_h + ', ' + right_h);
	if (right_h > center_h) $('#main_content').css('min-height', (right_h)+'px');
});
