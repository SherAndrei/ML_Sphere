	__PM.draw({
		logo: {
			image: '/img/logotype-help.png',
			image_x2: '/img/logotype-help@2x.png',
			width: 200,
			height: 36,
			marginRight: 0,
			marginLeft: 0,
			title: 'Помощь@Mail.Ru',
			href: '/',
			attrs: {
				name: 'clbXXXXX'
			}
		},
		toolbar: {
			items: [
			
				{
					type: 'spacer',
					width: 50
				},
				{
					type: 'search',
					placeholder: 'Поиск в помощи',
					reinforced: false,
					autofocus: false,
					buttonName: 'search',
					inputName: 'q',
					searchUrl: '/my-help/search',
					method: 'get',
					width: 300,
					autocomplete: 'off',
					suggests: {
						searchInternet: false
					},
					flexible: true,
					flex: 1,
					buttonAttrs: {},
				}
			
			],
		}
	}, function(menuItems){
		menuItems.toolbar // главное меню
		menuItems.submenu // второй уровень меню
	});
	
	
	// suggest  
	__PM.getItems(function(hat){
		var search = hat.toolbar.items[0].itemsByName.search;
		search.input.on('input', function(e){
			if (e.target.value.length > 4){
				jQuery.getJSON("/api/suggest",{search: e.target.value},function(data){
				// где data имеет вид описанный ниже
					search.suggests.list(data);
				});
			}
		});
		search.suggests.on('select', function(e, item){
			if(item.data && item.data.link){
				document.location = item.data.link;
			}
		});
		search.form.on('submit', function(e){
			//e.preventDefault();
		});
	});
	// /suggest  

</script>

 


		
		
