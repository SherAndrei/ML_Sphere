sup {top: -0.5em;}
sub {bottom: -0.25em;}
table {border-collapse: collapse; border-spacing: 0;}
.pressed {background-color: rgba(0, 0, 0, 0.7);}
.hidden {display: none !important; visibility: hidden;}
.invisible {visibility: hidden;}
/* Clearfix helper. Used to contain floats: h5bp.com/q */
.clearfix:before, .clearfix:after {display: table; content: "";}
.clearfix:after {clear: both;}
/* For IE 6/7 only */
.clearfix {*zoom: 1;}
.cluster{position: relative; top:-44px; left:-25px;}
.aligncenter {text-align:center;}

.mobile .mainlogo {display:none;}
.mobile .header-menu_btn:active, .mobile .header-menu_btn.active{background-position: 0 -36px;}
.mobile .header-menu {display:block;}
.mobile .header-menu_top li:first-child {border-top: none;}
.mobile .header-menu_top li {border-top: 1px solid #DCE2F0; padding: 5px 0px;}
.mobile .header-menu_top li a {display:block; padding:0; color:#214496; text-decoration:none; text-transform:uppercase; font-weight:bold; font-size:12px; line-height:14px;}
.mobile .header-menu_bottom {padding: 7px 20px;}
.mobile .header-menu_bottom li:first-child {border-top: none;}
.mobile .header-menu_bottom li {border-top: 1px solid #DCE2F0;}
.mobile .header-menu_bottom li a {display:block; padding:10px 0; color:#214496; text-decoration:none; font-size:.75em; line-height:20px;}
/* Content */
.section-segment_gradient {background-color: #eceeee; 
background-image: -moz-linear-gradient(top, #fff, #eceeee); background-image: -ms-linear-gradient(top, #fff, #eceeee); background-image: -o-linear-gradient(top, #fff, #eceeee); background-image: -webkit-linear-gradient(top, #fff, #eceeee); background-image: linear-gradient(top, #fff, #eceeee); background-image: -webkit-gradient(linear, center top, center bottom, from(#fff), to(#fff));}
.section-segment_breadcrumbs {float: none; clear: both; margin: 8px 2.5% 0; width: 95%; color:#6C9908;}
.section-segment_breadcrumbs a {color:#6C9908;}
.section-segment_title{margin: 0 24px 0 -10px; padding: 12px 8px; text-transform: uppercase; font-size: 0.9167em; line-height: 1.45em;
 border-radius: 0px 20px 20px 0px; -webkit-border-radius: 0px 20px 20px 0px; -o-border-radius: 0px 20px 20px 0px; -ms-border-radius: 0px 20px 20px 0px; -moz-border-radius: 0px 20px 20px 0px;}
.segment-item_title {margin: 0; padding:10px 0 3px; color: #d42152; text-transform: uppercase; font-size: 1.3em; line-height: 1.45em;}
.segment-item_title_i {margin: 0; padding:3px 0 3px; color: #006F82; text-transform: uppercase; font-size: 1.3em; line-height: 1.45em;}

.segment-item_descr {padding: 0 0 9px; color: #59595a; font-size: 0.833em; line-height: 1.28em; text-align:justify;}
.segment-item_text {padding: 1em 0; color: #59595a; font-size: 1.15em; line-height: 1.2em; text-align:left;}
.segment-item_date {display:inline-block; color:#ccc; font-size:0.9em; margin-right:0px;}
.segment-item_author {display:inline-block; color:#ccc; font-size:0.9em; margin-right:0px;}

.dividerline {clear: both; margin:15px 10px 12px; border-top: 1px solid #e5e5e5;}
/*-------------- section slider widget --------------*/
.section-widget{position:relative; margin:0 -2.5%; padding: 0 0 35px 0; background-color: #eceeee;}
.section-widget_gradient {background-color: #eceeee; 
background-image: -moz-linear-gradient(top, #fff, #eceeee); background-image: -ms-linear-gradient(top, #fff, #eceeee); background-image: -o-linear-gradient(top, #fff, #eceeee); background-image: -webkit-linear-gradient(top, #fff, #eceeee); background-image: linear-gradient(top, #fff, #eceeee); background-image: -webkit-gradient(linear, center top, center bottom, from(#fff), to(#eceeee));}
.section-widget__title{margin: 0 24px 0 0px; padding: 12px 8px; color:#222; text-transform: uppercase; font-size: 0.9167em; line-height: 1.45em;
 border-radius: 0px 20px 20px 0px; -webkit-border-radius: 0px 20px 20px 0px; -o-border-radius: 0px 20px 20px 0px; -ms-border-radius: 0px 20px 20px 0px; -moz-border-radius: 0px 20px 20px 0px;}
.section-widget__slider{/* position: relative; */ /* width: 100%; */}
.section-slide__link-wrap{display: block; text-decoration: none;}
.section-widget__item__title{padding:0px 0px 0; color:#006F82;  text-transform: uppercase; font-weight:700; font-size:1.3em; line-height: 1.3em;}
.section-widget__item__text{padding:9px 10px 0; color:#222; font-size:0.833em; line-height:1.25em; min-height:64px;}
.section-widget__item__image{display: block; padding: 0px 0 0px 0; width: 100%; height: auto;}
a.section-widget__wrapper{text-decoration: none;}
/* Footer */
.footer {position: relative; overflow: hidden; min-width: 320px; width: 100%; border-top: 1px solid #E7E7E6; background-color: #F2F2F2;}
.footer, .footer a {color: #999999;}
.footer-inner{margin: 0 auto; min-width: 320px; max-width: 480px; width: 100%;}
.footer-inner .footer-inner_box:first-child {border-top:none;}
.footer-inner_box {margin: 0 2.5%; padding: 33px 16px 15px; border-top: 1px solid #E5E5E5;}
.footer-inner_box + .footer-inner_box{padding-top: 15px; padding-bottom: 22px;}
.copyright {margin: 24px 0 10px; color: #999999; font-size: .75em;}
.device-switcher{margin: 0 auto; min-width: 320px; max-width: 480px; width: 100%; text-align: center; border-top: 1px solid #E5E5E5; padding: 14px 0 20px;}
.device-switcher a{display:inline-block; padding-bottom:1px; border-bottom:1px solid #aeaeae; font:normal 14px/16px 'Open Sans',arial, sans-serif; color: #b5b5b5;}
.device-switcher a:hover{text-decoration: none; border-bottom-color: #bebebe;}
.device-switcher a:active{outline:1px solid #bebebe; border-bottom-width: 0px;}
.pageination_wrap .page_link {display: inline-block; margin: 0px 3px;}
.perelinkovka {text-align: left;}
.perelinkovka li {padding: 2px 0px 7px;}
.perelinkovka li a {font-size: 17px; line-height:18px; color: #000; text-align: left;}
.item_wrap{padding:0px 0px 0; color:#265FA9;  text-transform: uppercase; font-weight:200; font-size:1.2em; line-height: 1.3em;}
</style>

