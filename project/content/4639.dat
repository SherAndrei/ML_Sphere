        <input id="Form_TypeId" name="Form.TypeId" type="hidden" value="1" />
        <input id="Form_CurrentBookmarkFolderId" name="Form.CurrentBookmarkFolderId" type="hidden" value="-1" />
        <IDXSKIP><p><label for="Form_BookmarkFolderId">Поместить в:</label></p></IDXSKIP>

        <IDXSKIP><div class="bookmarks-add-on_list">
            <div class="bookmarks-add-on_item">
                <label for="BookmarkFolder-1" class="js-folder-title bookmarks-add-on_item__active">
