		      // Автоматически геокодируем полученный результат.
		      autoReverseGeocode: true
		  }).then(function (result) {
		  var geolocation = result.geoObjects.get(0).properties.get('metaDataProperty').GeocoderMetaData.text;
		  geolocation = geolocation.split(',')[1].split(',')[0];
		  var numbertel = ''; //для РФ.

		  if (geolocation.search('Моск') > -1) {
		    numbertel = '8 (499) 350-80-26'; //для МСК
		  }
		  if (geolocation.search('Ленинг') > -1 || geolocation.search('Санкт') > -1 ) {
		    numbertel = '8 (812) 627-15-62'; //для СПБ
		  }
		  if(!numbertel == ''){
		  var bar = '<div id="bar__nt" class="container__bar"><div class="box__bar"><div class="left__bar"><span class="title__bar">Бесплатная косультация </span></div><div class="right__bar"><a href="tel:'+numbertel+'" class="tel__bar" id="number_phone"></a><a href="tel:'+numbertel+'" class="btn__bar">Задать вопрос онлайн</a></div></div></div>';
		  
		  jQuery("body").prepend(bar);
		  jQuery(window).scroll(function() {
		   var scrollPercent = 100 * jQuery(window).scrollTop() / (jQuery(document).height() - jQuery(window).height());
		   if (scrollPercent > 20) {
		    jQuery(".container__bar").css({"position":"fixed"});
		   }
		   else {
		      jQuery(".container__bar").css({"position":"inherit"});
		   }
		});
		  
		  var style = '<style>@import url(https://fonts.googleapis.com/css?family=Ubuntu);.container__bar{top:0;left:0;z-index:99999;width:100%;background:#383f44;text-align:center;font-family:Ubuntu,Helvetica;font-weight:700}.box__bar{width:1080px;margin:0 auto;display:flex;justify-content:center}.box__bar .left__bar,.box__bar .right__bar{margin:0}.left__bar{text-align:left;padding:6px 0;line-height:16px}.geo__bar{color:#fff;font-size:14px;font-weight:500}.tel__bar a,.title__bar{color:#fff}.right__bar a:hover,.right__bar span a:hover{text-decoration:none;color:#444}.title__bar{text-transform:uppercase;font-weight:600;display:flex;font-size:14px}.right__bar{text-align:right;padding:5px 0;display:flex;align-items:center}.right__bar span a{text-decoration:none}.right__bar a:hover{height:auto}#number_phone:hover:after,.btn__bar,.tel__bar{color:#fff}.tel__bar{margin:0 50px;font-size:24px;font-weight:500}#number_phone:after{content:"'+numbertel+'"}.btn__bar{background:#e04262;padding:5px 6px;border-radius:5px;font-size:15px;font-weight:500}@media screen and (max-width:320px){.left__bar,.right__bar{text-align:center}.btn__bar{display:none}.box__bar{width:100%;display:block}.title__bar{display:block;font-size:14px;font-weight:900}.right__bar{display:block;padding:0}.tel__bar{font-size:16px;font-weight:700}.geo__bar{font-size:15px;font-weight:500}}@media screen and (max-width:375px){.left__bar,.right__bar{text-align:center}.btn__bar{display:none}.box__bar{width:100%;display:block}.title__bar{display:block;font-size:14px;font-weight:900}.right__bar{display:block;padding:0}.tel__bar{font-size:16px;font-weight:700}.geo__bar{font-size:15px;font-weight:500}}@media screen and (max-width:750px){.left__bar,.right__bar{text-align:center}.btn__bar{display:none}.box__bar{height:60px;box-shadow:0 0 6px #000;width:100%;display:block;background:#383f44}.title__bar{display:block;font-size:14px;font-weight:900}.right__bar{display:block;padding:0}.tel__bar{font-size:16px;font-weight:700}#number_phone:after,.geo__bar{font-size:15px;font-weight:500}#number_phone:after{content:"'+numbertel+'";background:#e04262;color:#fff;padding:5px 11px;border-radius:5px;margin:31px;box-shadow:1px 1px 5px #292929,0 0 1px #000}}@media screen and (max-width:960px){.box__bar{width:100%}.box__bar .right__bar{position:relative;z-index:1000}.geo__bar{font-weight:500;font-size:13px}.tel__bar,.title__bar{font-weight:700}.tel__bar{margin:0 16px;font-size:16px}}</style>'; 
		  jQuery("head").append(style);
		  }
		  });
		}   
		});
		</script>	
</body>

</html>

