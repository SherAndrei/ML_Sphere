<div data-log-click="{&quot;groupActionType&quot;:&quot;TEXT&quot;,&quot;mtBlockType&quot;:&quot;text&quot;,&quot;target&quot;:&quot;text&quot;}" data-scroll-to="2" class="media-block media-text __full" style="margin: 16px auto; font-size: 14px; line-height: 1.5; color: #333333; word-wrap: break-word; max-width: 548px; font-family: Arial, Helvetica, sans-serif; background-color: #ffffff;" data-mce-style="margin: 16px auto; font-size: 14px; line-height: 1.5; color: #333333; word-wrap: break-word; max-width: 548px; font-family: Arial, Helvetica, sans-serif; background-color: #ffffff;">
