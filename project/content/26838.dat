	<script>var PUBLIC_URL = '';</script>
	<script>

									$(function() {
									    $('#upload').fileupload({
									        url: '/block/commentform?imgupload',
									        dataType: 'json',
									        done: function (e, data) {
									        	$('#progress').fadeOut('slow');
									        	if (data.result.error !== undefined) {
									        		alert(data.result.error);
									        	} else {
									        	    var div = $('<div><img /></div>');
									        		var img = div.find('img');
									        		img.attr('src', data.result.src);
									        		img.attr('id', data.result.id);
									        		img.css('display', 'none');
									        		img.css('cursor', 'pointer');
									        		img.on('click', function(){
									        			action('/block/commentform?imgdelete='+this.id)
									        		});
									        		div.appendTo('#images');
									        		$('#images img:hidden').fadeIn('slow');
									        	}
									        },
									        progressall: function (e, data) {
									            var progress = parseInt(data.loaded / data.total * 100, 10);
									            $('#progress').show().css( 'width', progress + '%' );
									        }
									    });
									});
								

	function showReplay( ID ) {
		if ($(".comments .replay-form").is(":visible")) {
			if ($(".comments .replay-form").closest("div.item").prop("id") != "c"+ID) {
				$(".comments .replay-form").remove();
			} else {
				$(".comments .replay-form").slideUp();
				return false;
			}
		}
		$(".comments .replay-form").remove();
		$(".comments div#c"+ID+"").after($("#replay-source").html());
		$(".comments .replay-form #replayID").val(ID);
		$(".comments .replay-form #captcha_r").prop('src',PUBLIC_URL+'/captcha?namespace=commentreplay&amp;rand='+Math.random());
		$('.comments .replay-form').slideDown();
		$("html, body").animate({
	        scrollTop: $(".comments .replay-form").offset().top
	    }, 1000);
		return false;
	}
	
	function reloadReplay( ID, commentID ) {
		if ($('#c'+ID).closest('.replay').length)
			ID = $('#c'+ID).closest('.replay').prev().attr("cID");
		action(PUBLIC_URL + "/block/comments?replayID=" + ID);
		$('.comments .replay-form').remove();
		$("html, body").animate({
	        scrollTop: $(".comments #c"+commentID).offset().top
	    }, 1000);

		return false;
	}

</script>

	
