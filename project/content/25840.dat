<div class=" c-user-card c-user-card--h c-user-card--nano"  itemscope itemtype="http://schema.org/Person">
    <div class="c-user-card__pic-container">
        <div class="c-user-card__pic u-vignette-tiny"><a href="/users/grafkin/"><img src="https://a.d-cd.net/b41ed8u/100.jpg" width="25" height="25" itemprop="image" alt=""></a></div>
    </div>
    <div class="c-user-card__body">
        <div class="c-user-card__username">
            <a class="c-link c-link--color00 c-username c-username--wrap" href="/users/grafkin/"  data-ihc-type="u" data-ihc-id="4899916394579245254" data-action="ihc.show" data-npd=""  itemprop="url" ><span  itemprop="name" >Grafkin</span></a>
