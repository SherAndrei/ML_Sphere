            <h1>Анекдоты</h1>
            
            <div class="content-after-title">
                
            </div>
            
            

            <div class="content-top-bar">
                <!-- custom_baner: not found trainings for output --><div style="text-align:center;"><a href="http://www.syntone-spb.ru/trainings/index.php?training_id=918&param=4"><img src="/files/images/customb/bannjer_rup_v_bibliotjeku.gif" /></a></div><!-- custom_baner: empty events -->
            </div>
