  <div class="_post_content">
    
    <div class="post_header">
      <a class="post_image" href="/id51195933">
        <img src="https://pp.vk.me/c614727/v614727933/16ab1/IzywQJ9_aiE.jpg" data-post-id="51195933_note9643265" width="50" height="50" class="post_img" />
