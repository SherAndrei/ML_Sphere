function Over(name1, name2){
    clearTimeout(_time[name1]);  
    name2.style.backgroundImage="url(http://www.budka-omsk.ru/images/menu_fonA.gif)";
    
     if(name1!="m_8"){   document.getElementById("m_8").style.visibility="hidden"; }  if(name1!="m_394"){   document.getElementById("m_394").style.visibility="hidden"; }  if(name1!="m_15"){   document.getElementById("m_15").style.visibility="hidden"; }  if(name1!="m_378"){   document.getElementById("m_378").style.visibility="hidden"; }  if(name1!="m_20"){   document.getElementById("m_20").style.visibility="hidden"; }  if(name1!="m_25"){   document.getElementById("m_25").style.visibility="hidden"; }  if(name1!="m_24"){   document.getElementById("m_24").style.visibility="hidden"; }  if(name1!="m_390"){   document.getElementById("m_390").style.visibility="hidden"; }  if(name1!="m_392"){   document.getElementById("m_392").style.visibility="hidden"; }  if(name1!="m_239"){   document.getElementById("m_239").style.visibility="hidden"; }     

    positionMenu=getAbsolutePosition(name2);

    
    document.getElementById(name1).style.width=name2.offsetWidth+"px";
