<script>
  limitURLSize();

  $(function(){
    $('.karma-popover').popover({
      html: true,
      placement: "bottom",
      content: function(){
        var link = $(this);
        if (!link.attr('data-load')){ // prevent double load
          link.attr('data-load', true);
          return "Loading!!!"
        }
        link.removeAttr('data-load');

        return details_in_popup('/forum/new/post/showKarmaVotes?id='+link.attr('data-id'));
      }
    });

    function details_in_popup(link){
      var div_id =  "tmp-id-" + $.now();
      $.ajax({
        url: link,
        success: function(response){
//          log('loaded...');
          $('#'+div_id).html(response);
        }
      });
      return '<div id="'+ div_id +'">загружаем последние голоса...<br/></div>';
    }
  })
</script>


  
