				e.preventDefault();
				$('#navbar').show(500);
				$('.wrapper, footer, body, .mobile_search').addClass('righted');
				$('.navbar-toggle').fadeOut(500);
			});
			$('.mobile_close').click(function(e){
				e.preventDefault();
				$('#navbar').hide(500);
				$('.wrapper, footer, body, .mobile_search').removeClass('righted');
				$('.navbar-toggle').fadeIn(500);
			});
			
