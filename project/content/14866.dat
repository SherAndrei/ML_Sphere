(function($){
	window.tModalForm = {
		overlayBox:'#tModalForm .overlay', formBox:'#tModalForm .form-box',
		show: function(id, flds) {
			var cs = $(window).scrollTop();
			var wh = window.innerHeight;
			var hb = $('#'+id).outerHeight();
			var po = (parseInt(wh) - parseInt(hb)) / 2;
			if( po > 0 ) {
				eo = parseInt(cs) + parseInt(po);
			} else {
				eo = parseInt(cs) + 10;
			}
			$(tModalForm.overlayBox).fadeIn('fast',function(){
				$('#'+id).animate({'top':eo}, 500);
			});
			if( flds ) {
				$.each(flds, function(i,v) {
					if(i && v) $('#'+i).val(v);
				});
			}
			return false;
		},
		closeAll:function() {
			$(tModalForm.overlayBox).fadeOut('fast');
			$(tModalForm.formBox).each(function() {
				$(this).animate({top:-2000}, 500);
			});
			return false;
		}
	};
})(jQuery);
</script>

