<script>
function get_cookie(name) {
    var results = document.cookie.match('(^|;) ?' + name + '=([^;]*)(;|$)');
    return results ? unescape(results[2]) : null;
}

function set_cookie(name, value) {
    document.cookie = name + '=' + escape(value) + '; expires=' + (new Date('2100-01-01').toGMTString());
}

$(function() {
    var w = $('#webpush'),
        p = w.children('.push-popup'),
        c = 'social_subscribe_showed',
        ua = navigator.userAgent;

    p = p.eq(Math.floor(p.length * Math.random()));
    p.siblings().remove();

    if (!get_cookie(c)
        && (('Notification' in window) ||  ua.search(/Safari/) > 0)
        && (ua.search(/Chrome|Safari|Firefox/) > 0)
        && !(ua.search(/Edge|OPR|opera/) > 0)) {

        setTimeout(function() {
            set_cookie(c, 1);
            ga('send', 'event', 'webpush', 'show', p.data('label'));
            w.removeClass('hidden');

            p.on('click', 'a', function() {
                ga('send', 'event', 'webpush', 'click', p.data('label'));
            });

            w.add(p.children('.push-popup__close')).on('click', function(e) {
                if (e.target === e.currentTarget) {
                    w.remove();
                }
            });
        }, 3000);
    }
});
</script></noindex>    </body>
