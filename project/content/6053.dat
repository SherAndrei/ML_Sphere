        <script type="text/javascript">//<![CDATA[
            $(function() {
                var wnd = $(window),
                    up  = $('<div/>', {id: 'arrow-scroll-top', class:'arrow-scroll-top'})
                        .appendTo(document.body)
                        .click(function(e) {
                            e.preventDefault();
                            $([document.documentElement, document.body]).animate({scrollTop: 0}, 'fast');
                        });

                function checkScrollY() {
                    wnd.scrollTop() > wnd.height() ? up.show() : up.hide();

                    if($('body').width() < 1510){
                        $('.ears_l, .ears_r').addClass('hide');
                    }else{
                        $('.ears_l, .ears_r').removeClass('hide');
                    }
                }

                checkScrollY();
                wnd.bind('scroll resize', checkScrollY);
            });
