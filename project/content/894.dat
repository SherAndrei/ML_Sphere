 var myOptions = {
 scrollwheel: true, // изменение масштаба колесиком
 zoom: 1,
 center: latlng,
 mapTypeId: google.maps.MapTypeId.ROADMAP , //HYBRID
 zoomControlOptions: {
 style: google.maps.ZoomControlStyle.SMALL,
 position: google.maps.ControlPosition.LEFT_TOP
 },
 panControlOptions: {
