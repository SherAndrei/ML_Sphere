 }
 };
 var contentString ='Hello';
 infowindow = new google.maps.InfoWindow({
 maxWidth: 450,
 content: contentString
 });
 
 map = new google.maps.Map(document.getElementById("mapBank"),
 myOptions);

 var label = new Array();

 label[0] = new google.maps.MarkerImage(
 'https://belarusbank.by/templates/default/public/i/map_icon2.png' /* Инфокиоск */
 );
 label[1] = new google.maps.MarkerImage(
 'https://belarusbank.by/templates/default/public/i/map_icon3.png' /* Банкомат */
 );
 label[2] = new google.maps.MarkerImage(
 'https://belarusbank.by/templates/default/public/i/map_icon.png' /* Филиал */
 );
 label[3] = new google.maps.MarkerImage(
 'https://belarusbank.by/templates/default/public/i/map_icon1.png' /* ОТС */
 ); 

 

 for (var i = 0; i < locations.length; i++) {

 var marker = new google.maps.Marker({
 position: new google.maps.LatLng(locations[i][2], locations[i][3]),
 map: map,
 animation: google.maps.Animation.DROP,
 icon: label[locations[i][5]]

 });
 
 gmarkers.push(marker);

 
 google.maps.event.addListener(marker, 'click', (function(marker, i) {
 return function() {
 closeBoxMapContent();
 infowindow.setContent(locations[i][1]);
 infowindow.open(map, marker);
 }
 })(marker, i));

 }
 
 google.maps.event.addListener(map, 'click', function() {
 infowindow.close();
 }); 

 //var markerCluster = new MarkerClusterer(map, gmarkers);

 if (locations.length == 1) {
 map.setCenter(new google.maps.LatLng(locations[0][2], locations[0][3]));
 map.setZoom(locations[0][4]);
 }
 else if ( locations.length > 1 ) {
 
 
 var GLOBE_WIDTH = 256; // a constant in Google's map projection
 
 var west = 1000;
 var east = 0;
 
 var north = 0;
 var south = 1000;
 
 for (var i = 0; i < locations.length; i++) {
 west = Math.min(west, locations[i][3]);
 east = Math.max(east, locations[i][3]);
 
 north = Math.max(north, locations[i][2]);
 south = Math.min(south, locations[i][2]);
 }
 var angle = east - west;
 var angle2 = north - south;
 var zoomfactor;
 var delta = 0;
 var horizontal = false;
 
 if(angle2 > angle) {
 angle = angle2;
 delta = 3;
 }
 
 if (angle < 0) {
 angle += 360;
 }
 
 zoomfactor = Math.floor(Math.log(960 * 360 / angle / GLOBE_WIDTH) / Math.LN2) - 1 - delta;
 var sw = new google.maps.LatLng(south, west);
 var ne = new google.maps.LatLng(north, east);
 var bounds = new google.maps.LatLngBounds(sw, ne);
 map.setCenter(bounds.getCenter());
 map.setZoom(zoomfactor);
 }
 };

