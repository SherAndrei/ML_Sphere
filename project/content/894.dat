var declOfNum = function(number, titles) 
{ 
 cases = [2, 0, 1, 1, 1, 2]; 
 return titles[ (number%100>4 && number%100<20)? 2 : cases[(number%10<5)?number%10:5] ]; 
} 

var searchCredit = function(step) {
 
 var query = {};

 switch(step) {
 case 1:

 var vid = $("#credit_vid :selected").attr("value");

 query = { 'vid': vid }

 break;

 case 2:

 var vid = $("#credit_vid :selected").attr("value");
 var vid_dop = $("#credit_vid_"+vid+" select :selected").attr("value");

 //var index_dop = $("#credit_vid_"+vid+" div.jqTransformSelectWrapper ul li a.selected").attr('index');
 //var vid_dop = $('#credit_vid_'+vid+' select.jqTransformHidden option:eq('+index_dop+')').attr('value');

 query = { 'vid': vid, 'vid_dop': vid_dop }

 break;

 case 3:
 //var index = $("#credits_step1 div.jqTransformSelectWrapper ul li a.selected").attr('index');
 //var vid = $('#credits_step1 select.jqTransformHidden option:eq('+index+')').attr('value');

 //var index_dop = $("#credit_vid_"+vid+" div.jqTransformSelectWrapper ul li a.selected").attr('index');
 //var vid_dop = $('#credit_vid_'+vid+' select.jqTransformHidden option:eq('+index_dop+')').attr('value');

 var vid = $("#credit_vid :selected").attr("value");
 var vid_dop = $("#credit_vid_"+vid+" select :selected").attr("value");

 if( vid == '2') {
 //var index_tparam = $("#credit_step3_"+vid+"_"+vid_dop+" div.jqTransformSelectWrapper ul li a.selected").attr('index');
 //var tparam = $('#credit_step3_'+vid+'_'+vid_dop+' select.jqTransformHidden option:eq('+index_tparam+')').attr('value');
 var tparam = $('#credit_step3_'+vid+'_'+vid_dop+' select :selected').attr('value');
 } else {
 //var index_tparam = $("#credit_step3_"+vid+" div.jqTransformSelectWrapper ul li a.selected").attr('index');
 //var tparam = $('#credit_step3_'+vid+' select.jqTransformHidden option:eq('+index_tparam+')').attr('value');
 var tparam = $('#credit_step3_'+vid+' select :selected').attr('value');
 }

 query = { 'vid': vid, 'vid_dop': vid_dop, 'tparam': tparam }
 break;
 }

 $.get("/ajax/credit.php", query, function(data) {
 $("#credit_finedblock_step"+step+" span:not('a span')").html(declOfNum(data, ['НАЙДЕН', 'НАЙДЕНО', 'НАЙДЕНО'])+" —");
 $("#credit_fined_step"+step).html('<span>'+data+' </span> '+declOfNum(data, ['кредит', 'кредита', 'кредитов']));
 }, "html");

 return false;
}

var searchDeposit = function(step) {
 
 var query = {};

 switch(step) {
 case 1:
 query = { currency: $("#deposit_params_currency").val(), summ: $("#deposit_params_summ").val() }
 break;

 case 2:
 //var index = $("#deposit_step2 div.jqTransformSelectWrapper ul li a.selected").attr('index');
 //var term = $('#deposit_step2 select.jqTransformHidden option:eq('+index+')').attr('value').split("_");
 var term = $("#deposit_params_term :selected").attr('value').split("_");
 query = { currency: $("#deposit_params_currency").val(), summ: $("#deposit_params_summ").val(), term: term[0], term_unit: term[1] }

 break;

 case 3:
 var options = [];
 $("#deposit_options :checked").each(function() {
 options.push($(this).attr('id'));
 });
 var term = $("#deposit_params_term :selected").attr('value').split("_");
 var _options = options.join(',');
 /*var options = [];
 $("#deposit_options .jqTransformChecked").each(function() {
 options.push($(this).parent().find('input').attr('id'));
 });
 var index = $("#deposit_step2 div.jqTransformSelectWrapper ul li a.selected").attr('index');
 var term = $('#deposit_step2 select.jqTransformHidden option:eq('+index+')').attr('value').split("_");
 var _options = options.join(',');*/
 query = { currency: $("#deposit_params_currency").val(), summ: $("#deposit_params_summ").val(), term: term[0], term_unit: term[1], options: _options }
 break;
 }

 $.get("/ajax/deposit.php", query, function(data) {
 $("#deposit_finedblock_step"+step+" span:not('a span')").html(declOfNum(data, ['НАЙДЕН', 'НАЙДЕНО', 'НАЙДЕНО'])+" —");
 $("#deposit_fined_step"+step).html('<span>'+data+' </span> '+declOfNum(data, ['вклад', 'вклада', 'вкладов']));
 //if( $("#deposit_finedblock_step"+step).css('visibility') == "hidden" ) $("#deposit_finedblock_step"+step).css('visibility', 'visible');
 }, "html");

 return false;
}

function formatMoney(number) {
 return number.toString().replace(/\B(?=(\d{3})+(?!\d))/g, " ")
}




