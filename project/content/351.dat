	$(document).ready(function() {
		// Функция скролла окна
		$(window).scroll(function() {
			// Проверяем видимость
			if($('.elastic_box').css('display') == 'block') {
				// Проверяем существует ли элемент прокрутки
				if($('#elasticid_layer').length > 0) {
					// Минимальное верхнее положение слоя прокрутки
					var gsa_minLayerY = -400;
					// Максимальное верхнее положение слоя прокрутки
					var gsa_maxLayerY = 0;
					
					// Положение окна элемента сверху
					var gsa_elementWindowTop = $('#elasticid_outer').offset().top;
					// Высота окна элемента прокрутки
					var gsa_elementWindowHeight = 600;
					
					// Текущая прокрутка окна сверху
					var gsa_windowTop = $(window).scrollTop();
					// Текущая высота окна
					var gsa_windowHeight = $(window).height();
					
					// Считаем границы прокрутки
					// Верхняя граница
					var gsa_borderTop = (gsa_elementWindowTop - gsa_windowHeight) + 100;
					
					// Проверяем верхнюю границу
					if(gsa_borderTop < 0) {
						// Начало окна
						gsa_borderTop = 0;
					}
					
					// Нижняя граница
					var gsa_borderBottom = (gsa_elementWindowTop + gsa_elementWindowHeight) - 100;
					
					// Проверяем текщуий отступ
					if(gsa_windowTop >= gsa_borderBottom || gsa_windowTop <= gsa_borderTop) {
						// Ничего не делаем
						return false;
					}
					
					// Получаем скролл сверху относительно элемента
					var gsa_scrollLabelTop = gsa_windowTop - gsa_borderTop;
					// Получаем высоту области скролла элемента
					var gsa_scrollLabelHeight = gsa_borderBottom - gsa_borderTop;
					
					// Получаем высоту скролла элемента
					var gsa_scrollElementHeight = gsa_maxLayerY - gsa_minLayerY;
					
					// Рассчитываем новый отступ элемента
					var gsa_scrollElementTop = (gsa_scrollElementHeight * gsa_scrollLabelTop) / gsa_scrollLabelHeight;
					// Приводим к целому
					gsa_scrollElementTop = parseInt(gsa_scrollElementTop, 10);
					
					// Получаем координату
					var gsa_coorElementY = 0 - gsa_scrollElementTop;
					
					// Проверяем значение координаты
					// Нижний предел
					if(gsa_coorElementY < gsa_minLayerY) {
						// По умолчанию
						gsa_coorElementY = gsa_minLayerY;
					}
					
					// Верхний предел
					if(gsa_coorElementY > gsa_maxLayerY) {
						// По умолчанию
						gsa_coorElementY = gsa_maxLayerY;
					}
					
					// Изменяем положение элемента
					$('#elasticid_layer').css('top', gsa_coorElementY + 'px');
				}
			}
		});
	});
