<script type="text/javascript">

$(document).ready(function(){
	// проверка длины поля
	$.fn.extend({
		checklength: function(minval, maxval){
			var min = minval || 0;
			var max = maxval || 0;
			var val = $(this).val().length;
			if(val == 0) return false;
			if(min == 0 && max == 0 && val != 0) return true;
			if((val >= min) && (val <= max))  return true;
			return false;
		}
	});
	
	// число символов в поле "Текст"
	$('#comment').keyup(function(){
		var symbols_count = 0;
		var cnt = 5000 - $(this).val().length;
		$('#comcnt_symbols').html("<font color='#8F8F8F'>Осталось символов: "+ cnt + "</font>");
	});
	
	// число символов в поле "Название"
	$('#title').keyup(function(){
		var symbols_count = 0;
		var cnt = 60 - $(this).val().length;
		$('#titlecnt_symbols').html("<font color='#8F8F8F'>Осталось символов: "+ cnt + "</font>");
	});
});

function typechange(val){
	if(val == "advise"){ $("#title_row").show();	}
	else{	$("#title_row").hide();		}
}

function element_count(el){
	var el_count = 0;
	for(e in el){ el_count++; }
	return el_count;
}

function resetform(){
	$('#titlecnt_symbols').html("<font color='#8F8F8F'>Осталось символов: 60</font>");
	$('#comcnt_symbols').html("<font color='#8F8F8F'>Осталось символов: 5000</font>");
}

function validateform(){
	var regexs = {	'name': /[^a-zA-Zа-яА-Я\-\s]+/,	
			'title': /[^a-zA-Zа-яА-Я0-9_\-\s]+/ 								
	};
	
	var err_message = {	'type': 'Выберите тип',
				'name': 'Поле <b>Имя</b> может содержать от 2 до 50 знаков',
				'title': 'Поле <b>Название</b> может содержать от 3 до 60 знаков',
				'comment': 'Поле <b>Текст</b> может содержать от 20 до 5000 знаков'
	};
	
	var err_array = [];
	$('.form_error').remove();
	
	if($('#type').val() == 0) {	err_array['type'] = err_message['type'];	}
	if(!($('#name').checklength(2, 50))){ err_array['name'] = err_message['name'];	}
	if($('#type').val() == "advise"){
		if(!($('#title').checklength(3, 60))) {	err_array['title'] = err_message['title'];	}
	}
	if(!$('#comment').checklength(20, 5000)) {	err_array['comment'] = err_message['comment'];	}
	
	if(element_count(err_array) == 0) {	return true;	}
	else{
		for(var key in err_array) { $('#'+ key).after('<span class="form_error"><br><font color=red>'+ err_array[key] +'</font></span>'); }
	}
	return false;
}
</script>

