                <script type="text/javascript">
function SplitPrice(price) 
{
	if (price > 10000)
	{
		var res = new Array();
		var arr = price.toString().split("").reverse();
		for(var i=0; i < arr.length; i++){
			res.push(arr[i]);
			if ((i+1)%3 == 0)
				res.push(" ");
		}
		price = res.reverse().join("");
	}
	return price;
}
</script>
