  <div class="ui_actions_menu_icons"  tabindex="0" aria-label="Действия" role="button" onclick="uiActionsMenu.keyToggle(this, event);"><span class="blind_label">Действия</span></div>
  <div class="ui_actions_menu _ui_menu" ><a class="ui_actions_menu_item" onclick="wall.markAsSpam(this, '-64994882_477208', '4dcd9ffe6f9024aed1');" tabindex="0" role="link">Пожаловаться</a></div>
