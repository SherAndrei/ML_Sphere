	var catchingRenderingErrorEnabled = true;

	(function () {
		var whitelistRaw = [
	"^https?:\/\/.*\.tu-?tu\.ru\/.*$",
	"^https?:\/\/.*\.tutu\.travel\/.*$",
	"^https?:\/\/.*\.tutu\.pro\/.*$"
],
			whitelist = whitelistRaw.map(function (item) {
				return new RegExp(item);
			});
		var PATH_COOKIE_NAME = "render_error_page_path";
		var ERROR_PAGE_PATH = "/mistake/";
		var RELOAD_DELAY = 5000;

		function getXmlHttp(){
			try {
				return new ActiveXObject("Msxml2.XMLHTTP");
			} catch (e) {
				try {
					return new ActiveXObject("Microsoft.XMLHTTP");
				} catch (ee) {
				}
			}
			if (typeof XMLHttpRequest!='undefined') {
				return new XMLHttpRequest();
			}
		}

		function logError (message, url, lineNumber, charNumber, error) {
			var errorString = getErrorString (message, url, lineNumber, charNumber, error);

			if (catchingRenderingErrorEnabled) {
				onRenderError(errorString);
			} else {
				sendLogError(errorString);
			}
		}

		function getErrorString (message, url, lineNumber, charNumber, error) {
			var data = {
					'url': encodeURIComponent(window.location.href)
				},
				result = [],
				i;

			if (error && error instanceof Error) {
				data.message = error.message;
				data.name = error.name;
				data.stack = error.stack;
			} else {
				if (!isUrlUseful(url) || !(s(lineNumber) && s(charNumber))) {
					return;
				}
				data.message = message;
				data.name = 'window.onerror';
				data.stack = s(url) + ':' + s(lineNumber) + ':' + s(charNumber);
			}
			data.message = messageToString(data.message);

			for (i in data) {
				if (data.hasOwnProperty(i)) {
					result.push(i + '=' + data[i]);
				}
			}

			return result.join('&');
		}

		function sendLogError (data) {
			var xmlhttp;
			if (!data) {
				return;
			}
			xmlhttp = getXmlHttp();
			xmlhttp.open("POST", getLogUrl(), true);
			xmlhttp.setRequestHeader('Content-Type', 'application/x-www-form-urlencoded');
			xmlhttp.send(data);
		}

		function sendLogErrorLeave(data) {
			if (!data) {
				return;
			}

			var img = new Image(1, 1);
			var url = getLogUrl() + "&type=onRender" + "&" + data;
			img.src = url;
		}

		function onRenderError (message) {
			if (getCookie(PATH_COOKIE_NAME) == decodeURIComponent(window.location.href)) {
				setTimeout(
					function () {
						if (catchingRenderingErrorEnabled) {
							sendLogErrorLeave("phase=repeated_render&" + message);
							setCookie(PATH_COOKIE_NAME, "", 0);
							window.location.href = ERROR_PAGE_PATH;
						} else {
							sendLogError(message);
						}
					},
					RELOAD_DELAY
				);
			} else {
				setTimeout(
					function () {
						if (catchingRenderingErrorEnabled) {
							sendLogErrorLeave("phase=primary_render&" + message);
							setCookie(PATH_COOKIE_NAME, decodeURIComponent(window.location.href), 60);
							window.location.reload();
						} else {
							sendLogError(message);
						}
					},
					RELOAD_DELAY
				);
			}
		}

		function getLogUrl () {
			return '/ajax_usage_log/?log=TrainWizardError\\TrainScheduleWithoutDateError' + '&r=' + Math.random();
		}

		function isUrlUseful (url) {
			return whitelist.some(function (regExp) {
				return regExp.test(s(url));
			})
		}

		function s (str) {
			return str ? str : '';
		}

		function messageToString (message) {
			if (!message) {return '';}
			if (typeof message !== 'object') {return message;}

			var i, v, result = [];

			for (i in message) {
				if (!message.hasOwnProperty(i)) {continue;}
				v = message[i];
				if (typeof v === 'function') {continue;}
				result.push(i + ':' + v);
			}
			return result.join(',');
		}

		function setCookie (name, value, expireSeconds) {
			var date = new Date()
			date.setTime(date.getTime() + (expireSeconds * 1000));
			document.cookie = name + '=' + value + ';expires=' + date + ';path=/';
		}

		function getCookie (name) {
			var matches = document.cookie.match(new RegExp(
				"(?:^|; )" + name.replace(/([\.$?*|{}\(\)\[\]\\\/\+^])/g, '\\$1') + "=([^;]*)"
			));
			return matches ? decodeURIComponent(matches[1]) : undefined;
		}

		window.onerror = logError;
	})();

