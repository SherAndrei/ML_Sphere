function toggle_op()
{
  if($("#cnt_and").val()){  
    $("#cnt_and").val("");
    $("#cnt_op").text("ИЛИ");
  }else{
    $("#cnt_and").val("1");
    $("#cnt_op").text("И");
  }
}
function cp()
{
  var qu = "";
  var kw_text = "";
  var names = "";
  var cnt = $("#cnt_sel li:has(:checked)");
  if( cnt.length==2){

    names = cnt.eq(0).text();
    names += " <span id='cnt_op' class='btn btn-default btn-xs' onclick='toggle_op()'>";
    names += $("#cnt_and").val() ? "И" : "ИЛИ";
    names += "</span> ";
    names += cnt.eq(1).text() + qu + "; ";
  }else{
    cnt.each(function(){
      names += $(this).text() + qu + "; ";
    });
  }
  $("#fo_sel li:has(:checked)").each(function(){
    names += $(this).text() + qu + "; ";
  });

  $("#otr_sel li:has(:checked)").each(function(){
    names += $(this).text() + qu + "; ";
  });
  names += kw_text;

  $("ul:not(#cnt_sel,#otr_sel,#fo_sel) li:has(:checked)").each(function(){
    names += $(this).text() + "; ";
  });


  var pers = $("#pers_name").val();
  if( pers != ""  && pers != "- Нет -"){
    names += pers + "; ";
  }
  if($("#dpbeg").val()!=""){
    names += " c " + $("#dpbeg").val();
  }
  if($("#dpend").val()!=""){
    names += " по " + $("#dpend").val() + "; ";
  }

  if($("#cat_a").prop("checked")){
    names += " главное; ";
  }

  if( names != "" ){
    var txt = "Вы выбрали: " + names  
    + '<button type="button" class="btn btn-default btn-xs" title="Сохранить выборку в Личные списки" onclick="save_prv_list()"><span title="Сохранить выборку в Личные списки" class="glyphicon glyphicon-floppy-disk"></span></button>'
    + '<button type="button" class="btn btn-default btn-xs" title="Сбросить выбранное" onclick="clean_prv_list()"><span title="Сбросить выбранное" class="glyphicon glyphicon-remove"></span></button>'
    ;
    $("#selinfo").html(txt);
  }else{
    $("#selinfo").html("");
  }
}

