    function popupLoaded(cb, eb) {
        return (function (cb) {
        if (window.__isFbPopupLoaded) {
            cb && cb ();
            return;
        };
        var interval = setInterval(function () {
            console.log($('.fb-page iframe').length)
            if ($('.fb-page iframe').length) {
                $('.fb-page iframe').on('load', function(){
                    console.log('experiment: fb loaded');
                    window.__isFbPopupLoaded = true;
                    cb && cb();
                });

                clearInterval(interval)
            }

        },100);
        })(cb);
    }

    $(document).ready(function(){
        var y0 = 0;
        document.addEventListener('mousemove', function (e) {

            if (e.pageY < y0 && e.pageY <= 5) {

                console.log('experiment: leave window')
                if (!window.__showPayload) showPayload();
                window.__showPayload = true;

            }
            y0 = e.pageY;
        });

        var bb_popup = getCookie('bb_popup');
        !bb_popup && popupLoaded();

        function showPayload(test) {
            bb_popup = getCookie('bb_popup');
            console.warn($('#enter').length, bb_popup)
            if (test || ($('#enter').length && !bb_popup)) {
            // if (window.__popupOnceClick) {
                console.log('experiment: start loading')
                popupLoaded(function () {
                    $('.fb-popup').show();
                    $('#AdFox1200x200').hide();
                    FB && FB.Event.subscribe('edge.create', function(response) {
                        setCookie('bb_popup', 1, 30);
                        console.log('experiment: cookie updated')
                    });
                });
                setCookie('bb_popup', 1);
                return true;
            } else {
                return false
            }

            //для тестирования
        };
        window.showPayload = showPayload

        //скрытие окна
        $('.fb-popup__inner').click(function (event) {
            var x = event.clientX, y = event.clientY,
            elementMouseIsOver = document.elementFromPoint(x, y);
            if (elementMouseIsOver === this) {
                $('.fb-popup').hide();
                $('#AdFox1200x200').show()
            }

        })

    });


    function setCookie(cname, cvalue, exdays) {
        var d = new Date();
        d.setTime(d.getTime() + (exdays*24*60*60*1000));
        var expires = "expires="+ d.toUTCString();
        document.cookie = cname + "=" + cvalue + ";" + expires + ";path=/";
    }
    function getCookie(cname) {
        var name = cname + "=";
        var decodedCookie = decodeURIComponent(document.cookie);
        var ca = decodedCookie.split(';');
        for(var i = 0; i <ca.length; i++) {
            var c = ca[i];
            while (c.charAt(0) == ' ') {
                c = c.substring(1);
            }
            if (c.indexOf(name) == 0) {
                return c.substring(name.length, c.length);
            }
        }
        return false;
    }

