	function toggle(scrollTop){
		$panel.toggleClass('hide', scrollTop < hiddenBefore);
	}
	
	function update(){
		var scrollTop = $window.scrollTop();
		toggle(scrollTop);
		
		var pos = (scrollTop > offset ? 'fixed' : 'absolute');
		if (position == pos) return;
		position = pos;
		
