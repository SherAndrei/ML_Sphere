  //[dan1] Activity
  function forAction(actionName, method) {
    if(!method) method = 'pay';
    $.post(dle_root + "engine/ajax/action.php", {action : actionName, hash : dle_login_hash, method : method}, function(points){
      if(points) {
        var container, col;
        container = $("#user_points");
        col = container.css('background-color');
        $("#user_points").animate({backgroundColor: '#007F0E'}, 1000, function() {
          container.html(points).animate({backgroundColor: col}, 1000);
        });
      }
    });
  }
  function doRate( rate, id ) {
    ShowLoading('');
    $.get(dle_root + "engine/ajax/rating.php", { go_rate: rate, news_id: id, skin: dle_skin }, function(data){
      HideLoading('');
      if(data.substring(0, 5) == 'voted') {
        $("#ratig-layer-" + id).html(data.substring(5));
        forAction('vote_rating'); //[dan1] Activity
      } else {
        $("#ratig-layer-" + id).html(data);
      }
    });
  }
  jQuery.fn.extend({
