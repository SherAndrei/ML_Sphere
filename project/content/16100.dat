<script>
	$('#perezvoni [name=phone]').keypress(function(){
		var e=$('#perezvoni'),a=e.val();
		if(!a)return;
		switch(a){
			case '+8':e.val('+7');
			case '495':case '+495':e.val('+7495');
			case '499':case '+499':e.val('+7499');
		}
		if(a.charAt(0)!=='+')e.val('+'+a.replace('+',''));
		e.val(e.val().replace(/[^\d\+ ()]/g,''));
	});
	$('body').click(function(event){
		if(!$(event.target).closest('#perezvoni,.perezvoniWidget_show_popup,[onclick],[href]').length)$('#perezvoni').hide();
	});
</script>


