                global.authPopup = global.authPopup || {};

                require(['ui.popup'], function(Popup) {
                    var originBackUrl;
                    var $backUrlInputField;

                    popup = Popup({
                        content: popupHtml,
                        onAfterShow: function(popupObj) {
                            if (!Modernizr.touch) {
                                popupObj.$popup.find('input').not('[type="hidden"]').first().focus();
                            }
                        },
                        onAfterClose: function() {
                            if (originBackUrl) {
                                $backUrlInputField.val(originBackUrl);
                                originBackUrl = false;
