<script type="text/javascript">
	$('.countryWrap .contryList .cElem').bind('click', function () {
		var cElem = $(this),
			attr = cElem.attr('data-value'),
			name = cElem.text(),
			parent = cElem.parent(),
			phone = cElem.attr('data-phone');

		if(cElem.hasClass('this'))return false;

		parent.find('.cElem').removeClass('this');
		cElem.addClass('this');
		
		$('.countryWrap .contryList').fadeOut(100);
		$('.countryWrap .cCountry').removeClass('show');

		$('.countryWrap .cCountry span').text(name);
		$('.changeNumber').text(phone);
		return false;
	});
	$('.countryWrap .cCountry').bind('click', function () {
		var cElem = $(this);

		if(cElem.hasClass('show'))return false;

		cElem.addClass('show');
		$('.countryWrap .contryList').fadeIn(100);
		return false;
	});
