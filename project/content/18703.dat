	$(document).ready(function () {
		
		$(".subcategory ul").wrap("<div id='menu'></div>");
		$(".subcategory #menu ul").after("<div class='block png'></div>");
		//Set the height of the block
		$('#menu .block').height($('#menu li').height());

		//go to the default selected item
		topval = $('.widget #menu li.current-menu-parent').addClass('active');
		topval = $('.widget #menu li.current-menu-item').addClass('active');
		topval = $('#menu li.active').position()['top'];
		
		$('#menu .block').stop().animate({top: topval}, {easing: '', duration:500});

		$('#menu li').hover(
			
			function() {
				
				//get the top position
				topval = $(this).position()['top'];
				
				//animate the block
				//you can add easing to it
				$('#menu .block').stop().animate({top: topval}, {easing: '', duration:500});
				
				//add the hover effect to menu item
				$(this).addClass('hover');	
			},
			
			function() {		
				//remove the hover effect
				$(this).removeClass('hover');	
				topval = $('#menu li.active').position()['top'];
		$('#menu .block').stop().animate({top: topval}, {easing: '', duration:500});
			}
		);
		
	
	});
	
	</script>
	
