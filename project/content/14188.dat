<script>
    $(function() {
        var navigation = $('#navigation');

        var scrollTo = function(href, duration) {
            var tab = tabs.find('a[href$="#' + href + '"]').click();
            var anchor = tab.length ? tab : barCard.find('a[name="' + href + '"]').click();
            if (anchor.length) {
                $('body,html').animate({
                    scrollTop: anchor.offset().top - navigation.outerHeight()
                }, duration || 500);
            }
            return false;
        };
        var processTabs = function(tab, content, event) {
            if (event.originalEvent !== undefined) {
                var data = {};
                var name = tab.attr('name') ? tab.attr('name') : tab.text();
                data['tab-' + name] = '1935';
                runEvent('click', data);
            }
            if (tab[0].hasAttribute('name')) {
                processNewPage.call(tab, event, content);
            }
        };
        var processNewPage = function(event, content) {
            var self = $(this);
            var tabHref = self.attr('href');
            if (tabHref && tabHref.indexOf('#') < 0 && (!content || content.is(':empty') || self.hasClass('forceLoad'))) {
                var loader = $('<img src="/data/base/img/ajax-loader.gif" class="center-block">');
                content ? content.html(loader) : self.html(loader);
                $.post(tabHref, {
                    barId: 1935                }, function(json) {
                    if (json.ret == 1) {
                        if (content) {
                            content.html(json.result);
                            if (content.hasClass('tab-section')) {
                                content.tabs(processTabs, false);
                            }
                            if (content.has('.tab-section')) {
                                content.find('.tab-section').tabs(processTabs, false);
                            }
                        } else {
                            if (json.gallery) {
                                self.parent().nextAll('.gallery-of-option').html(json.gallery);
                            }
                            self.replaceWith(json.result);
                        }
                    }
                    loader.remove();
                })
            }
            return false;
        };
        var barCard = $('.bar-card');
        barCard.find('.option-info > a').on('click', processNewPage);
        barCard.find('.link-of-option-alone').click();
        var tabs = $('.tab-section', barCard).tabs(processTabs, false);

        navigation.on('click', 'ul a:not(.noProcess)', function() {
            var href = this.href.split('#')[1];
            var data = {};
            data['navTop-' + href] = '1935';
            runEvent('click', data);
            var ret = scrollTo(href);
            return href == 'showchat' ? true : ret;
        });

        var url = location.pathname.split('/');
        var hash = url.length === 6 ? url[url.length - 1] : location.hash ? location.hash.split('#')[1] : '';

        if (hash) {
            setTimeout(function() {scrollTo(hash)}, 1000);
        }

        if (hash == 'bron' || hash == 'svob') {
            setTimeout(function() {
                triggerFirstBronButton(hash == 'svob');
                var directParams = {};
                directParams['direct-' + hash] = '1935';
                runEvent('direct', directParams);
            }, 500);
        }
        if (hash == 'map') {
            setTimeout(function() {
                $('.address-on-map:first').click();
                runEvent('direct', {address: '1935'});
            }, 500);
        }
        if (hash == 'photo') {
            runEvent('direct', {photo: '1935'});
        }

        var gallery = $('#gallery').owlCarousel({
            singleItem: true,
            navigation: true,
            pagination: false,
            lazyLoad: true,
            lazyFollow: false,
            navigationText: false
        }).on('contextmenu', false);
        gallery.find('a').fancybox({
            padding: 0,
            afterShow: function() {
                                    this.inner.on('contextmenu', false);
                            }
        }).on('contextmenu', false);
        
        var bronForm = $('#bron-form-fixed');
        var mainColumn = $('#main-column');
        $(window).on('load', function() {
            if ($(this).width() > 991 && mainColumn.height() > bronForm.height()) {
                mainColumn.next().height(mainColumn.height());
            }
        });

        navigation.stickit({
            screenMinWidth: 768,
            onStick: function() {
                var h1 = gallery.prevAll('.h1');
                h1.find('div:first-child').removeClass('pull-left m-r-5').prependTo(h1.find('h1'));
                h1.addClass('text-center').removeClass('overlay').prependTo(navigation).append($('#phone-where-h1'));
                if (mainColumn.height() > bronForm.height()) {
                    bronForm.stickit({
                        zIndex: 9,
                        screenMinWidth: 992,
                        top: navigation.height()
                    });
                }
            },
            onUnstick: function() {
                if (mainColumn.height() > bronForm.height()) {
                    bronForm.stickit('destroy');
                }
                $('#phone-where-h1').insertAfter(navigation.find('.h1').removeClass('text-center').addClass('overlay').prependTo(gallery.parent()));
            }
        });

        $('.bronForm', barCard).processBronForm({
            fromBarCard: true,
            onSuccess: function(index) {
                var params = {};
                params['fromBarCardOpened' + (index + 1)] = '1935';
                runEvent('bron-submit', params);
            }
        });

        processPhoneForm('#form-resolve');

