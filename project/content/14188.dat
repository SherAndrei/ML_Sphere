<script>
    $(function() {
        var formRating = document.getElementById('form-rating');
        var opinionList = document.getElementById('opinion-list');
        var initListRating = function() {
            $('select', opinionList).barrating({
                theme: 'bars-1to10',
                readonly: true
            });
            $('.like-dislike a', opinionList).on('click', function() {
                var img = $('img', this)[0];
                $.get(this.href, function(json) {
                    if (json.ret == 1) {
                        img.src = $(img).attr('src').split('.')[0] + '-active.png';
                        $(img).next().remove().end().after(json.count);
                    } else if (json.msg) {
                        alert(json.msg);
                    }
                });
                return false;
            });
        };

        $(formRating).easyValidate(function(json, button) {
            if (json.ret == 1) {
                $('#opinion-list').prepend(json.opinion);
                this.prev().andSelf().slideUp(400);
                runEvent('opinion-submit', {opinionBarId: '1935'});
                initListRating();
            } else {
                runEvent('opinion-click', {opinionBarId: '1935'});
                if (json.needAuth) {
                    $.fancybox(json.msg);
                } else if (json.msg) {
                    button.parent().prev().empty().append('<div class="color-red">' + json.msg + '</div>');
                }
            }
        }, true);

        $('select[name^="rating["]', formRating).barrating({
            theme: 'bars-1to10'
        });

        initListRating();
    });
</script>
