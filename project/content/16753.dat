    <script>
        $(function() {
            var fixed = false;
            var no_fix = false;
            function fix_pit(act) {
                if (typeof(act) != 'undefined' && act == 'close') {
                    no_fix = true;
                }
                var show_height = 400;
                var pre_pos = $("#pre_pit").offset();
                var scroll = $(window).scrollTop();
                if (!no_fix && scroll > show_height && !fixed && scroll < (pre_pos.top - $(window).height())) {
                    fixed = true;
                    $("#pit").css({
                        'position': 'fixed',
                        'bottom': '0px',
                        'display': 'none'
                    });
                    $("#pit").fadeIn(500);
                    $("#pit a.close").fadeIn(500);
                } else if (no_fix || scroll <= show_height || scroll >= (pre_pos.top - $(window).height())) {
                    fixed = false;
                    $("#pit").css({
                        'position': 'static',
                        'bottom': 'auto',
                        'display': 'block'
                    });
                    $("#pit a.close").hide();
                }
            }
            fix_pit();
            $(window).scroll(fix_pit);
            $("#pit a.close").click(function() {
                fix_pit('close');
                $.ajax({
                    type: 'post',
                    data: 'act=pit_close'
                });
                return false;
            });
        });
    </script>

