            </div>
            <div class="arrow">→</div>
            <div class="select">
                <select id="rateValute_2" onchange="ex_rate();">
                </select>
            </div>
        </div>

        <div class="first-area">
            <input type="text" id="ex_first_in" value="100" onkeyup="ex_rate();">
            <span id="ex_first_in_label"></span>
        </div>

        <div class="result-area">
            <span id="ex_result_out">0.00</span>
            <span class="label">USD</span>
        </div>
    </div>
