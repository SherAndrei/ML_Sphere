<script type="text/javascript">

// search input and afisha auto complete handlers
$.manageAjax.create('search_request', {queue: 'clear', maxRequests: 1, abortOld: true});
var search_input_curr_value = $("input.search-input").val();
var ac_search_page_input = $(".s-ac-search-page-input");
var searchForm = $("#search");
var search_autocomplete_pad = $("#s-afisha-search-autocomplete");
var search_block = $('.panel.s-form');
var site_search_focus = true;

var search_no_data_msg = $('body.m-narrow-hat .search-no-data');
var search_btn_close = $('body.m-narrow-hat .topline_icon_close');
var topline_search = $('body.m-narrow-hat .topline_search');
var topline_search_input = $('body.m-narrow-hat .topline_input_search');

var search_input = $("input.search-input");
var search_button = $(".s-search-button");
var ajax_results_visible = false;
var home_geography_search = [], geography_id_value;
search_input.bind($.browser.opera ? "keypress" : "keydown", captioned_input_change);

ac_search_page_input.bind("focus", function(){ site_search_focus = false; });
search_input.bind("focus", function(){ site_search_focus = true; });

if (!$('body').hasClass('m-narrow-hat')) {
	ac_search_page_input.bind($.browser.opera ? "keypress" : "keydown", captioned_input_change);
}

search_input
	.bind("click", function (e) { e.stopPropagation() })
	.bind("focus", { text: search_input_curr_value }, captioned_input_focus)
	.bind("blur", { text: search_input_curr_value }, captioned_input_blur);

var KEY = {
	UP: 38,
	DOWN: 40,
	DEL: 46,
	RETURN: 13,
	ESC: 27,
	BACKSPACE: 8
};

$(".s-search-button").bind('click', function (e) {
	e.stopPropagation();
	search_string = searchForm.find('.search-input').val();
	cur_uri = location.href;

	if(( search_string != '')&&(search_string != 'Новый поиск по сайту')){
		if (cur_uri.search('/People/') != -1) {
		    location.href = '/Search/People/?Search_str=' + encodeURIComponent(search_string);
		}
		else {
		    location.href = '/Search/?Search_str=' + encodeURIComponent(search_string);
		}
		return false;
	}
	return false;
});

function arrow_key_pressed(pressed_key){
	var ajax_items = $("#ajax_items a");
	var alredy_selected_ac_item = false;
	var ajax_items_length = ajax_items.length;
	for(i=0; i<ajax_items_length; i++){
			if ($(ajax_items.get(i)).hasClass("selected")) {
			if (i == 0 && pressed_key == "up"){
				$(ajax_items.get(i)).removeClass("selected");
				$(ajax_items.get(ajax_items_length - 1)).addClass("selected");
				alredy_selected_ac_item = true;
				break;
			}
			else if (i < (ajax_items_length - 1) && pressed_key == "down"){
				$(ajax_items.get(i)).removeClass("selected");
				$(ajax_items.get(i+1)).addClass("selected");
				alredy_selected_ac_item = true;
				break;
			}
			else if (i < (ajax_items_length) && pressed_key == "up"){
				$(ajax_items.get(i)).removeClass("selected");
				$(ajax_items.get(i-1)).addClass("selected");
				alredy_selected_ac_item = true;
				break;
			}

			else if (i == (ajax_items_length - 1) && pressed_key == "down"){
				$(ajax_items.get(i)).removeClass("selected");
				break;
			}
		}
	}
	if (!alredy_selected_ac_item && pressed_key == "down") $(ajax_items.get(0)).addClass("selected");
	else if (!alredy_selected_ac_item && pressed_key == "up") $(ajax_items.get(ajax_items.length - 1)).addClass("selected");
}

function captioned_input_change(e){

	//не понял, зачем здесь это
	//$(this).focus();

	switch(e.keyCode){
		case KEY.DOWN:
			arrow_key_pressed("down");
			break;
		case KEY.UP:
			arrow_key_pressed("up");
			break;
		case KEY.RETURN:

			search_autocomplete_pad.hide();
			var ajax_items = $("#ajax_items a");
			var finde_location = false;
			for(i=0; i<ajax_items.length; i++){
				if ($(ajax_items.get(i)).hasClass("selected")){
					location.href = $(ajax_items.get(i)).attr("href");
					finde_location = true;
					break;
				}
			}
			if (!finde_location && $(this).parents(".s-form").size() > 0) {
				var parent = $(this).parents(".s-form").get(0);
				setTimeout(function () {
					validate_onsubmit(parent);
				}, 100);
			}
			break;
		default:
			break;
	}

	if(e.keyCode != KEY.DOWN && e.keyCode != KEY.UP && e.keyCode != KEY.RETURN){
		ajaxRequestProceed();
	}
}

function ajaxRequestProceed() {
	$.manageAjax.abort('search_request');
	var cur_uri = location.href;
	var autocomplete_response = "";
	setTimeout(function () {
		var search_string = (site_search_focus) ? search_input.val() : ac_search_page_input.val();
		if (search_string.length >= 4) {
			var ajax_url = "/ajaxpage.aspx?component=AutocompleteSiteSearchWidget&search=" + escape(search_string) + "&count=10";
			if (home_geography_search.length > 0 && home_geography_search.get(0).checked) ajax_url += geography_id_value;
			if (site_search_focus) {
				$("#ac-search-input-loader").show();
				ajax_url += "&max=60";
			}
			else {
				ac_search_page_input.css("background", "#fff url('../dsn/map_preloader.gif') 445px center no-repeat");
				ajax_url += "&max=135";
			}
			$.manageAjax.add('search_request', {
				success: function (responseText) {
					if (responseText.length > 0) {
						var people_search = "";
						if (cur_uri.search('/People/') != -1) people_search = "/People/";
						search_autocomplete_pad.html(responseText + "<li class='search-link'><a href='/Search/" + people_search + "?Search_str=" + escape(search_string) + "' >Все результаты</a></li></ul>");
						//search_autocomplete_pad.html(responseText);
						//Затенение (в новой шапке)

						if (site_search_focus) {
							if (afhWFader) {
								search_autocomplete_pad
									.css({
										"padding-left": search_input.offset().left
									})
									.css("top", search_input.height())
									.css("left", 0)
									.removeClass("m-ac-search-page").fadeIn(globalFadeTime);
							}
							else {
								search_autocomplete_pad
									.css("top", search_input.offset().top - $('.b-picnic-tru').offset().top)
									.css("left", search_input.offset().left)
									.removeClass("m-ac-search-page").fadeIn(globalFadeTime);
							}
							$("#ac-search-input-loader").hide();
						}
						else {
							search_autocomplete_pad
								.css("top", search_block.offset().top + search_block.outerHeight() - search_input.offset().top)
								.css("left", ac_search_page_input.offset().left - 10)
								.css({
									"padding-left": "0px"
								})
								.addClass("m-ac-search-page").show();
							ac_search_page_input.css("background", "#fff");
						}
						var highlite_text = $("#ajax_items a");
						highlite_text
							.bind("mouseenter", function () {
								$("#ajax_items a").removeClass("selected");
								$(this).addClass("selected");
							})
							.bind("mouseleave", function () { $(this).removeClass("selected") });

						highlite_text.each(function () {
							var search_obj = $(".class-obj", this);
							if (search_obj.length > 0) {
								var text_str = search_obj.html();
								var search_case_string = text_str.substr(text_str.toLowerCase().indexOf(search_string.toLowerCase()), search_string.length);
								search_obj.html(text_str.replace(search_case_string, ("<strong>" + search_case_string + "</strong>")));
							}
						});

						search_no_data_msg.hide();
					}
					else {
						search_autocomplete_pad.fadeOut(globalFadeTime);

                        search_no_data_msg.show();

						if (site_search_focus)
							$("#ac-search-input-loader").hide();
						else
							ac_search_page_input.css("background", "#fff");

					}
				},
				url: ajax_url
			});
		}
		else {
			search_autocomplete_pad.fadeOut(globalFadeTime);
		}
	}, 10);
}

//маркирую новый дизайн поиска. w-fader - с затемнением всей страницы, присуще только новому дизайну.
var afhWFader = false;
var mobileBody = $('body.m-narrow-hat');

if (search_input.closest('.w-fader').length > 0 || search_input.parents('.topline').length > 0) {
	afhWFader = true;
	$('.b-afh__sub').append(search_autocomplete_pad);
	var afhParent = $('.b-afh');

	search_autocomplete_pad
		.removeClass('b-afisha-search-autocomplete')
		.addClass('b-afh-autocomplete')
		.bind('click', function () {
			afhFaderON = false;
		});

	search_input.bind("focus", function () {
		ajaxRequestProceed();
		mobileBody.addClass('search-result');
		search_autocomplete_pad.hide();
	});
}

search_btn_close.on('click', function() {
    mobileBody.removeClass('search-result');
    search_autocomplete_pad.hide();
});

$('body.m-narrow-hat .topline_search .search-input').on('input', function() {
    var str = $(this).val();
    if (str.length === 0) {
        search_no_data_msg.hide();
        topline_search_input.val('');
    }
});

search_btn_close.on('click', function() {
    search_no_data_msg.hide();
    topline_search_input.val('');
});
</script>
