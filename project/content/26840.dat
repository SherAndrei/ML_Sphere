<script type="text/javascript">
$(document).ready(function() {
	$('#content .content-box.comments textarea[name=content]').live('keydown', function(e) {
		if (e.ctrlKey && e.keyCode == 13) {
			comments_submit();
		};
	});
});

function comments_form_place(comment_id) {
	
	var form_ob = $('#content .content-box.comments .form');
	var parent_container = $('#content .content-box.comments .parent-form');
	var parent_form_title = $(parent_container).find('.title').text();
	
	$('#comment-send-form .errors').remove();
	
	$('#content .content-box.comments .actions .item.ans.disabled').removeClass('disabled');
	
	if (!comment_id) {
		$(parent_container).append($(form_ob).remove().clone(true));
		$.scrollTo($(parent_container), 300);
		$(parent_container).find('textarea').focus();
		
		$('#comment-send-form input[name=parent_id]').val(0);
		
		$(parent_container).find('.title').text(parent_form_title);
		
		return true;
	}
	
	$('#response-comment-' + comment_id + ' .actions .item.ans').addClass('disabled');
	$('#response-comment-' + comment_id).append($(form_ob).remove().clone(true));
	$('#comment-send-form input[name=parent_id]').val(comment_id);
	$('#comment-send-form textarea').focus();
	$.scrollTo($('#response-comment-' + comment_id), 300);
	
	$(parent_container).find('.title').html('<a href="#" class="ajax" onclick="comments_form_place(0); return false;">' + parent_form_title + '</a>');
	
	return true;
	
}

function comments_submit() {

	forms_overlayer('comment-send-form');
	$('#comment-send-form .loading-circ').show();
	
	$.ajax({
		type     : 'post',
		url      : '/responses/',
		data     : 'do=comments_send&' + $('#comment-send-form').serialize(),
		dataType : 'json',
		success  : function(data, ans) {
			if (data.status == 'fail') {
				$('#comment-send-form .errors').remove();
				$('#comment-send-form').prepend('<ul class="errors"></ul>');
				for (i = 0; i < data.errors.length; i++) {
					$('#comment-send-form .errors').append('<li>' + data.errors[i] + '</li>');
				}
				forms_overlayer('comment-send-form');
			} else if (data.status == 'success') {
				if (data.comments_premod_all) {
					alert('Комментарий отправлен на модерацию');
				}
				location.reload();
			}
		},
		error    : function() {
			forms_overlayer('comment-send-form');
		},
		complete : function() {
			$('#comment-send-form .loading-circ').hide();
		}
	});

	return false;

}

function comments_quote_selected() {

	var text = '';
	
	if (window.getSelection) {
		text = window.getSelection();
	} else if (document.getSelection) {
		text = document.getSelection();
	} else if (document.selection) {
		text = document.selection.createRange().text;
	}
	
	if (text == '') {
		return false;
	}
	
	$('#comment-send-form textarea').val($('#comment-send-form textarea').val() + '[quote]' + text + '[/quote]');
	
	$('#comment-send-form textarea').focus();
	
	return true;
	
}

function comments_contact_by_nick(login) {

	$('#comment-send-form textarea').val($('#comment-send-form textarea').val() + '[b]' + login + '[/b], ');
	
	$('#comment-send-form textarea').focus();
	
	return true;

}

function comments_edit(comment_id) {
	
	forms_overlayer('comment-edit-form-' + comment_id);
	
	$.ajax({
		type     : 'post',
		url      : '/responses/',
		data     : 'do=comments_edit&unicode=&comment_id=' + comment_id + '&' + $('#comment-edit-form-' + comment_id).serialize(),
		dataType : 'json',
		success  : function(data, ans) {
			if (data.status == 'fail') {
				forms_overlayer('comment-edit-form-' + comment_id);
				$('#comment-edit-form-' + comment_id + ' .errors').remove();
				$('#comment-edit-form-' + comment_id).prepend('<ul class="errors"></ul>');
				for (i = 0; i < data.errors.length; i++) {
					$('#comment-edit-form-' + comment_id + ' .errors').append('<li>' + data.errors[i] + '</li>');
				}
			} else if (data.status == 'success') {
				window.location.reload();
			}
		},
		error    : function() {
			alert('Ошибка редактирования');
			forms_overlayer('comment-edit-form-' + comment_id);
		}
	});

	return false;
	
}

function comments_edit_timeout(comment_id, add_time_diff) {

	if (window.comments_seconds_passed === undefined) {
		window.comments_seconds_passed = new Array();
	}
	
	if (window.comments_seconds_passed[comment_id] === undefined) {
		window.comments_seconds_passed[comment_id] = add_time_diff + 1;
	} else {
		window.comments_seconds_passed[comment_id]++;
	}
	
	var comment_edit_time = 300;
	var seconds_left = comment_edit_time - comments_seconds_passed[comment_id];
	
	if (seconds_left < 0) {
		seconds_left = 0;
	}
	
	if (seconds_left > 0) {
		$('#response-comment-' + comment_id + ' .actions .item.edit .edit-timeout').html(' (' + numeral(seconds_left).format('00:00:00') + ')');
	} else {
		$('#response-comment-' + comment_id + ' .actions .item.edit').fadeOut(500, function() {
			$(this).remove();
		});
		clearInterval(eval('comment_interval_' + comment_id));
	}
	
}

</script>

