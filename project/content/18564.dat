				l.parentNode.style.display = '';
			}
			if (vBmenu.menus[id])
			{
				vBmenu.menus[id]._set_menu_position = vBmenu.menus[id].set_menu_position;
				vBmenu.menus[id].set_menu_position = function(obj)
				{
					vBmenu.menus[id]._set_menu_position(obj);
					this.toppx = this.pos['top'] - this.menuobj.offsetHeight;
					this.menuobj.style.top  = this.toppx + 'px';
				}
			}
		}
	}
	
	moveblock('forumrules');
//-->
</script>

