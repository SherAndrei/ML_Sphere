			// nothing to collapse!
			if (img)
			{
				// hide the clicky image if there is one
				img.style.display = 'none';
			}
			return false;
		}

		if ((obj.style.display == 'none' && forcestate == 'open') || (obj.style.display != 'none' && forcestate == 'closed'))
		{
			return _toggle_collapse(objid, forcestate);
		}

		if (obj.firstwrapping !== 1)
		{
			jQuery(obj)
				.children('tr')
				.children('td, th')
				.wrapInner(
					function(index)
					{
						if (this.collapsewrapped == 1)
						{
							return "";
						}
						else
						{
							this.collapsewrapped = 1;
							return "<div class=\"collapsewrap_" + objid + "\"></div>";
						}
					}
				);
		}

		var speed = forcestate ? 0 : 'fast';

		if ((obj.style.display == 'none' && !forcestate) || 'open' == forcestate)
		{
			if (obj.firstwrapping !== 1)
			{
				jQuery('.collapsewrap_' + objid)
					.slideUp(0)
					.parent().animate({ paddingTop: "toggle", paddingBottom: "toggle" }, 0)
			}

			obj.style.display = '';
			jQuery('.collapsewrap_' + objid)
				.parent().animate({ paddingTop: "toggle", paddingBottom: "toggle" }, speed)
				.end().animate({ height: "toggle", opacity: "toggle" }, speed);

			if (!forcestate)
			{
				save_collapsed(objid, false);
			}

			if (img)
			{
				img_re = new RegExp("_collapsed\\.gif$");
				img.src = img.src.replace(img_re, '.gif');
			}
			if (cel)
			{
				cel_re = new RegExp("^(thead|tcat)(_collapsed)$");
				cel.className = cel.className.replace(cel_re, '$1');
			}
		}
