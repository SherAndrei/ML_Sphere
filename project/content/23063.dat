        //console.log(close);
        setTimeout(function(){
            close.style.display = 'block';
            closeButtonPosition();
        }, delay);
                
                //Позиционируем кнопку закрыть
                function closeButtonPosition(){
                    var bW = 728;
                    var cW = document.documentElement.clientWidth;
                    var dX = (cW-bW)/2 - 16;
                    close.style.right = dX + 'px';
                    console.log(cW);
                }
                
                window.onresize = closeButtonPosition;
    </script>
