		if(!pFrom){
			alert('Выберите клинику!');
			return false;
			}
		pFrom = pFromCity+", " + pFrom;
		var pTo = document.getElementById('address_to').value;
		if(!pTo || pTo=='Введите свой адрес'){
			alert('Укажите Ваш адрес!');
			return false;
			}
		pTo = pFromCity+", " + pTo;
		var router = new YMaps.Router([pFrom,pTo], [], {viewAutoApply: true});

		YMaps.Events.observe(router, router.Events.Success, function() {
			map.addOverlay(router);
			document.getElementById('print').innerHTML='<a href="http://maps.yandex.ru/print/?rt='+encodeURIComponent(pTo)+'~'+encodeURIComponent(pFrom)+'&l=map" target="_blank"><img src="/i/print_route.jpg"></a>';
		});

}
