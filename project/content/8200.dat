            $('.show_chapter_sidebar').click(function() {
                $('.right_content').hide();
                $('.hide_chapter_sidebar').show();
                $('.sidebar').addClass('show_sidebar');
                $(this).hide();
                return false;
            });
            $('.hide_chapter_sidebar').click(function() {
                $('.sidebar').removeClass('show_sidebar');
                $(this).hide();
                $('.show_chapter_sidebar').show();
                $('.right_content').show();
                return false;
            });
          
        });

