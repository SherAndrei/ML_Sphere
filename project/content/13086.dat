<script language="javascript" type="text/javascript">

    var can_hide=true;

    function is_bottom()
    {
        return $(document).height() - $(window).scrollTop() - 20 < document.body.clientHeight;
    }

    function is_top()
    {
        return $(window).scrollTop() + $(document).height() == $(document).height();
    }

    $(document).ready(function(){


        {
            //check login
            var logged_in=false;
            $.post("/login_check.php",
                    function(data){
                        if(data=="yes")
                        {
                            logged_in=true;
                            //load settings

			    font_size=null;
			    font_size=$.cookie('sm_font');

			    if(font_size==null)
			    {

                                $.post("/get_settings.php",
                                    function(data){
                                        var font_size="aaa";
                                        try
                                        {
                                            font_size = $(data).find( "font_size" );
                                            font_size=font_size.text();
                                        }catch (e) {};
                                        //alert(font_size);

                                        if(!isNaN(font_size))
                                        {
                                            setFontSize(font_size);
					    $.cookie('sm_font', font_size, { expires: 30, path:'/'});
                                            ScrollToUnreaded();
                                        }
                                        else
                                        {
                                            ScrollToUnreaded();
                                        }
                                    });
                            }
			    else
			    {
				setFontSize(font_size);
				$.cookie('sm_font', font_size, { expires: 30, path:'/'});
				ScrollToUnreaded();
			    }
                        }
                        else
                        {
			    font_size=null;
			    font_size=$.cookie('sm_font');
			    if(font_size!=null) setFontSize(font_size);

                            ScrollToUnreaded();
                            $("#book_readed").addClass("set_book_readed_disabled");
                        }
                    });
            //ScrollToUnreaded(); //before set_font_size
        }

        function ScrollToUnreaded()
        {
	    var lnk=$('.last_text_block');
	    if(lnk.text()=='') return;

            $('html, body').animate({
                scrollTop: lnk.offset().top
            });
        }

        if( (!is_bottom()) && (!is_top()) ) $("#description").hide();

        $(window).scroll(function(){
            if(can_hide) $("#description").hide();

            /*if(is_bottom())
            {
                $("#description").show();
            }*/
	    if(logged_in)
            {
                if(is_bottom())
                    $( "#book_readed" ).css({'font-weight': 'bold','color': 'green'});
                else
                    $( "#book_readed" ).css({'font-weight': 'normal','color': 'blue'});
            }

            if(is_top())
            {
                $("#description").show();
            }
        });

        $( "#fixed-div" ).hover(
                function(){
                    $("#description").show();
                    can_hide=false;
                },
                function(){
                    if( (!is_bottom()) && (!is_top()))
                        $("#description").hide();
                    can_hide=true;
                }
        ); 

        $("#book_readed").click(function(e)
        {
            e.preventDefault();
            if(!logged_in)
	    {
		 window.location.href = "/poslednie-obnovleniya/";
		 return;
	    }

            var href="/set_book_readed.php";//$(this).attr('href');
            if(is_font_size_changed)
            {
                var fnt_size=getFontSize();
            $.post(href, { font_size: fnt_size, id: 22203 },
                    function(data){
                        if($.trim(data)!="ok")
                            console.log(data);
                        window.location.href = "/obnovleniya/";//"/?page_id=6";
                    });
            }
            else
                $.post(href, {id: 22203},
                        function(data){
                            if($.trim(data)!="ok")
                                console.log(data);
                            window.location.href = "/obnovleniya/";//"/?page_id=6";
                        });
        });

        var is_font_size_changed=false;

                                $(".increaseFont").click(function(e){
                e.preventDefault();
                var fontSize = getFontSize();
                var newFontSize = fontSize + 1;
                setFontSize(newFontSize);
                is_font_size_changed=true;
		$.cookie('sm_font', newFontSize, { expires: 30, path:'/'});
                return false;
            });
            $(".decreaseFont").click(function(e){
                e.preventDefault();
                var fontSize = getFontSize();
                var newFontSize = fontSize - 1;
                setFontSize(newFontSize);
                is_font_size_changed=true;
		$.cookie('sm_font', newFontSize, { expires: 30, path:'/'});
                return false;
            });

        function getFontSize() {
            var currentSize = $(".txt").css("font-size");
            var currentSizeNumber = parseFloat(currentSize, 12);
            return currentSizeNumber;
        }
        function setFontSize(size) {
            $(".txt").css("font-size", size+"px");
        }
            });
</script>

