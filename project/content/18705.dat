						var tags=string_to_tags(req.responseText);
						cache_add(inputString, tags);
						suggestion_show(tags);
					}
				};
				req.send(null);
			}
		}
	} // lookup

	//преобразовывает полученную от сервера строку в массив extra_tags - есть ли еще теги, tags- теги, каждая ячейка массива - массив 0-номер тега, 1 - размер шрифта, 2 - тег
	function string_to_tags(s)
	{
		var tags=new Array();
		if(s!='empty')
		{
			var tags_str=s.split(','); //в каждой ячейке тройка номер-размер шрифта-тег разделенные пробелом
			for(i=0; i<tags_str.length; i++)
				tags.push(tags_str[i].split(' '));
		}
		return tags;
	}

