			if(found)
				k=i;
		}
		if(found)
			return suggestionscacheoutput[k];
		else
			return false;
	}

	function cache_add(inputString, outputString)
	{
		suggestionscacheinput.push(inputString);
		suggestionscacheoutput.push(outputString);
		if(suggestionscacheinput.length>suggestionscachelength) //нужно удалить первый элемент
		{
			suggestionscacheinput.shift();
			suggestionscacheoutput.shift();
		}
	}

	function lookup_timeout(inputString)
	{
		clearTimeout(lookup_timeout_id);
		lookup_timeout_id=setTimeout(function (){lookup(inputString);}, 500);
	}

	// вызывается при изменении поля ввода
	function lookup(inputString)
	{
		if(inputString.length == 0)
		{
			suggestion_show(empty_input_tags);
		} else
		{
			inputString=inputString.toUpperCase();
			inputString=inputString.replace(/[^А-ЯІЇЄA-Z]/g, '');
			var cached=cache_search(inputString);
			if(cached)
				suggestion_show(cached);
