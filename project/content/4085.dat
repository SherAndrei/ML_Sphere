        });
        $("#survey-no").click(function(){
            ga.track_event_at("catalog_survey", 'button_no_click', page_place);
        });
        $("#negative_survey").click(function(e){
            var selection = $("#survey_negative_form input:checked");
            var response = $("#negative_survey_description").val();
            var support_link = $('#support_link');
            var support_href = selection.data('support_href');
            var action = "negative_response";
            if (!!selection.val()) {
                action = action + "-" + selection.val();
            }
            if (!!response) {
                action = action + '/' + response;
            }
            if (!!support_href) {
                support_link.attr("href", support_href);
                support_link.text(selection.data("support_text"));
            }
            ga.track_event_at("catalog_survey", action, page_place);
            thanks();
            return false;
        });
        $("#positive_survey").click(function(e){
            var response = $("#positive_survey_description").val();
            var action = "positive_response";
            if (!!response) {
                action = action + '/' + response;
            }
            ga.track_event_at("catalog_survey", action, page_place);
            thanks();
            return false;
        });
