<script type="text/javascript">
function resize_image()
{
	toggle_img_size = function(e)
	{
		var img = this;
		iwidth = img.width;
		img.width = img.iwidth;
		img.iwidth = iwidth;
	}

	resize = function(e)
	{
		var img = this;
		img.style.maxWidth = 'none';
		if (img.width > 600)
		{
			img.title = img.src.substr(img.src.lastIndexOf('/', img.src.length - 1) + 1) + ' (' + img.width + ' x ' + img.height + ')';

			img.iwidth = img.width;
			img.width = 600;

			if (img.parentNode.tagName == 'A')
			{
				img.title = img.parentNode.href;
			}
			else
			{
				img.style.cursor = 'move';
				img.onclick = toggle_img_size;
			}
		}
	}

	var tds = fetch_tags(fetch_object('posts'), 'td');
	for (var i = 0; i < tds.length; i++)
	{
		if (tds[i].id.indexOf('td_post') != 0) continue;
		var imgs = fetch_tags(tds[i], 'img');
		for (var ii = 0; ii < imgs.length; ii++)
		{
			if (imgs[ii].className == 'inlineimg' || imgs[ii].className == 'thumbnail') continue;
			var stored_src = imgs[ii].src;
			imgs[ii].src = '';
			imgs[ii].onload = resize;
			imgs[ii].src = stored_src;
		}
	}
}
resize_image();

