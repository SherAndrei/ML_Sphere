  <div class="post_full_like">
    <a href="#" class="post_like _like_wrap " onmouseover="Wall.likesShow(this, '-43096175_61697')" onclick="return Wall.likeIt(this, '-43096175_61697', '19d1d26e9418aa13ad', event);">
