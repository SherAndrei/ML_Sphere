    linkProcess = function () {
        var urlPattern = new RegExp("(http|ftp|https)://[\\w-]+(\\.[\\w-]+)+([\\w-.,@?^=%&:/~+#\(\)-]*[\\w@?^=%&;/~+#\(\)-])?", 'gim');
        var elements = document.querySelectorAll('[data-role="comment-text-wrapper"]');
        for (var i = 0; i < elements.length; i++) {
            var el = elements[i];
            var rawText = el.innerText || el.textContent;
            var text = rawText.replace(/([^>])\n/g, '$1<br/>');
            var html = text.replace(urlPattern, function(url){
                var title = url.length>30 ? url.substr(0,30)+'...' : url;
                if (/\.(jpg|jpeg|png|gif)/i.test(url)) {
                    if (url.indexOf('http://') == 0) {
                        title = '<img src="https://hkcinema.ru/proxy?url='+url+'" style="max-width:150px;max-height:150px;display:block;margin: 5px 10px 10px 0"/>';
                    } else {
                        title = '<img src="'+url+'" style="max-width:150px;max-height:150px;display:block;margin: 5px 10px 10px 0"/>';
                    }
                }
                return '<a href="'+url+'" target=_blank>'+title+'</a>';
            });
            el.innerHTML = html;
        }
    };
    linkProcess();
</script>    </div>
