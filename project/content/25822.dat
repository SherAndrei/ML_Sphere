		submitHandler: function(form) {

			var $form = $(form), $button = $form.find("input[type=submit]"), $panel = $(".submit-panel", $form), $agr = $("input[name=user_agreement]", $form.parents(".iform-auth"));

			if ($agr.length && !$agr.prop("checked")) {
