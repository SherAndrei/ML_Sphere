					return false;
				$agr.click();
			}

			$button.attr("disabled", true).addClass("btn-disabled").loadingTxt({txt: "Идет регистрация"});

			$panel.addClass('lab-loader');

			JsHttpRequest.query("/ajax.php", {
				cl_name: "cabinet",
				me_name: "registration",
				firstname: form.firstname.value,
				email: form.email.value,
				sex: $form.find("[name=sex]:checked").val()
			}, function(result, errors) {
				if (result && result.errors != undefined) {
					$panel.removeClass('lab-loader');
					$form.formvalidate("errors", result.errors);
					$button.removeAttr("disabled").removeClass("btn-disabled").val("Зарегистрироваться").loadingTxt("destroy");
				}
				else {
					if (form.actionurl)
						window.location = form.actionurl.value;
					else
						window.location = window.location;
				}
			}, true);

			return false;
		}

	});

