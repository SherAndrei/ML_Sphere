=message maxlength=200000 onkeypress="if(isEnterPressed(event)){this.form.action='http://miusha.beon.ru/p/a' + 'dd_comment.cgi';SendFormAddTinyComment('13');return false}" onkeyup="resizeTinyCommentsTextArea('13');" onFocus="resizeTinyCommentsTextArea('13', 0)" onblur="resizeTinyCommentsTextArea('13', 1)" onPaste="resizeTinyCommentsTextAreaDelayed('13')" placeholder="Написать комментарий&hellip;"></textarea><script type="text/javascript">resizeTinyCommentsTextAreaDelayed('13')</script></form></td></table></td></tr></table><table class=datebg><td>пятница, 9 июня 2017 г.</td></table><div style="clear:right;"></div>
