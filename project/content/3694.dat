1.Time = [09] +4 = 0861h + 4 = 0865h<br />
2.Conquest = [09] +5 = 0861h + 5 = 0866h<br />
3.Domination = [09] +6 = 0861h + 6 = 0867h<br />
4.Cultural = [09] +7 = 0861h + 7 = 0868h<br />
5.Religious = [09] +8 = 0861h + 8 = 0869h<br />
6.Space Race = [09] +9 = 0861h + 9 = 086Ah<br />
7.Diplomatic = [09] +Ah = 0861h + Ah = 086Bh<br />
8.Scientific = [09] +Bh = 0861h + Bh = 086Ch</div></div><hr />


