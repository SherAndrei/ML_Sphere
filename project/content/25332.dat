&#x201c;&#x418; &#x43c;&#x44b; &#x434;&#x43e;&#x43b;&#x436;&#x43d;&#x44b; &#x43d;&#x430;&#x437;&#x44b;&#x432;&#x430;&#x442;&#x44c; &#x441;&#x435;&#x431;&#x44f; &#x433;&#x440;&#x435;&#x448;&#x43d;&#x438;&#x43a;&#x430;&#x43c;&#x438;, &#x2013; &#x434;&#x430;, &#x432;&#x441;&#x435; &#x43c;&#x44b;, &#x443;&#x432;&#x44b;, &#x44f;&#x432;&#x43b;&#x44f;&#x435;&#x43c;&#x441;&#x44f; &#x438;&#x43c;&#x438;. &#x41d;&#x43e; &#x43d;&#x435; &#x43f;&#x43e;&#x440;&#x43e;&#x447;&#x43d;&#x44b;&#x43c;&#x438;. &#x41f;&#x43e;&#x440;&#x43e;&#x447;&#x43d;&#x44b;&#x439; &#x447;&#x435;&#x43b;&#x43e;&#x432;&#x435;&#x43a; &#x432;&#x441;&#x435;&#x433;&#x434;&#x430; &#x434;&#x43e;&#x432;&#x43e;&#x43b;&#x435;&#x43d; &#x441;&#x43e;&#x431;&#x43e;&#x439;, &#x43e;&#x43d; &#x43d;&#x435; &#x437;&#x43d;&#x430;&#x435;&#x442;, &#x447;&#x442;&#x43e; &#x442;&#x430;&#x43a;&#x43e;&#x435; &#x441;&#x43c;&#x438;&#x440;&#x435;&#x43d;&#x438;&#x435;. &#x418;&#x438;&#x441;&#x443;&#x441; &#x433;&#x43e;&#x432;&#x43e;&#x440;&#x438;&#x43b; &#x442;&#x430;&#x43a;&#x438;&#x43c; &#x43f;&#x43e;&#x440;&#x43e;&#x447;&#x43d;&#x44b;&#x43c;: &#x2018;&#x41a;&#x440;&#x430;&#x441;&#x43e;&#x442;&#x430; &#x432;&#x430;&#x448;&#x430; &#x2013; &#x44d;&#x442;&#x43e; &#x43a;&#x440;&#x430;&#x441;&#x43e;&#x442;&#x430; &#x443;&#x431;&#x435;&#x43b;&#x435;&#x43d;&#x43d;&#x44b;&#x445; &#x433;&#x440;&#x43e;&#x431;&#x43e;&#x432;&#x2019;, &#x43a;&#x43e;&#x442;&#x43e;&#x440;&#x44b;&#x435; &#x43a;&#x430;&#x436;&#x443;&#x442;&#x441;&#x44f; &#x43f;&#x440;&#x435;&#x43a;&#x440;&#x430;&#x441;&#x43d;&#x44b;&#x43c;&#x438; &#x441;&#x43d;&#x430;&#x440;&#x443;&#x436;&#x438;, &#x43d;&#x43e; &#x432;&#x43d;&#x443;&#x442;&#x440;&#x438; &#x43f;&#x43e;&#x43b;&#x43d;&#x44b; &#x43c;&#x435;&#x440;&#x442;&#x432;&#x44b;&#x445; &#x43a;&#x43e;&#x441;&#x442;&#x435;&#x439; &#x438; &#x433;&#x43d;&#x438;&#x43b;&#x438;. &#x418; &#x447;&#x435;&#x43b;&#x43e;&#x432;&#x435;&#x43a;, &#x43a;&#x43e;&#x442;&#x43e;&#x440;&#x44b;&#x439; &#x445;&#x432;&#x430;&#x441;&#x442;&#x430;&#x435;&#x4