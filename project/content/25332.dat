&#x201c;&#x420;&#x430;&#x437;&#x43d;&#x438;&#x446;&#x430; &#x432; &#x442;&#x43e;&#x43c;, &#x447;&#x442;&#x43e; &#x442;&#x43e;&#x442;, &#x43a;&#x442;&#x43e; &#x433;&#x440;&#x435;&#x448;&#x438;&#x442; &#x438; &#x43a;&#x430;&#x435;&#x442;&#x441;&#x44f;, &#x2013; &#x442;&#x43e;&#x442; &#x43f;&#x440;&#x43e;&#x441;&#x438;&#x442; &#x43f;&#x440;&#x43e;&#x449;&#x435;&#x43d;&#x438;&#x44f;, &#x447;&#x443;&#x432;&#x441;&#x442;&#x432;&#x443;&#x435;&#x442; &#x441;&#x435;&#x431;&#x44f; &#x441;&#x43b;&#x430;&#x431;&#x44b;&#x43c;, &#x447;&#x443;&#x432;&#x441;&#x442;&#x432;&#x443;&#x435;&#x442; &#x441;&#x435;&#x431;&#x44f; &#x447;&#x430;&#x434;&#x43e;&#x43c; &#x411;&#x43e;&#x436;&#x44c;&#x438;&#x43c; &#x438; &#x43f;&#x440;&#x43e;&#x441;&#x438;&#x442; &#x441;&#x43f;&#x430;&#x441;&#x435;&#x43d;&#x438;&#x44f; &#x443; &#x418;&#x438;&#x441;&#x443;&#x441;&#x430;. &#x41d;&#x43e; &#x447;&#x442;&#x43e; &#x448;&#x43e;&#x43a;&#x438;&#x440;&#x443;&#x435;&#x442; &#x432; &#x442;&#x43e;&#x43c;, &#x434;&#x440;&#x443;&#x433;&#x43e;&#x43c;, &#x43a;&#x43e;&#x442;&#x43e;&#x440;&#x44b;&#x439; &#x441;&#x43e;&#x431;&#x43b;&#x430;&#x437;&#x43d;&#x44f;&#x435;&#x442;? &#x422;&#x43e;, &#x447;&#x442;&#x43e; &#x43e;&#x43d; &#x43d;&#x435; &#x43a;&#x430;&#x435;&#x442;&#x441;&#x44f;. &#x41e;&#x43d; &#x43f;&#x440;&#x43e;&#x434;&#x43e;&#x43b;&#x436;&#x430;&#x435;&#x442; &#x433;&#x440;&#x435;&#x448;&#x438;&#x442;&#x44c;, &#x43d;&#x43e; &#x43f;&#x440;&#x438;&#x442;&#x432;&#x43e;&#x440;&#x44f;&#x435;&#x442;&#x441;&#x44f; &#x434;&#x43e;&#x431;&#x440;&#x44b;&#x43c; &#x445;&#x440;&#x438;&#x441;&#x442;&#x438;&#x430;&#x43d;&#x438;&#x43d;&#x43e;&#x43c;: &#x43e;&#x43d; &#x432;&#x435;&#x434;&#x435;&#x442; &#x434;&#x432;&#x43e;&#x439;&#x43d;&#x443;&#x44e; &#x436;&#x438;&#x437;&#x43d;&#x44c;. &#x418; &#x44d;&#x442;&#x430; &#x434;&#x432;&#x43e;&#x439;&#x43d;&#x430;&#x44f; &#x436;&#x438;&#x437;&#x43d;&#x44c; &#x445;&#x440;&#x438;&#x441;&#x442;&#x438;&#x430;&#x43d;&#x438;&#x43d;&#x430; &#x43d;&#x435;&#x441;&#x435;&#x442; &#x43e;&#x433;&#x440;&#x43e;&#x43c;&#x43d;&#x43e;&#x435; &#x437;&#x43b;&#x43e;. &#x2018;&#x41d;&#x43e; &#x43a;&#x430;&#x43a; &#x436;&#x435;, &#x44f; &#x432;&#x435;&#x434;&#x44c; &#x431;&#x43b;&#x430;&#x433;&#x43e;&#x442;&#x432;&#x43e;&#x440;&#x438;&#x442;&#x435;&#x43b;&#x44c; &#x426;&#x435;&#x440;&#x43a;&#x432;&#x438;! &#x42f; &#x432;&#x44b;&#x43d;&#x438;&#x43c;&#x430;&#x44e; &#x438;&#x437; &#x441;&#x432;&#x43e;&#x435;&#x433;&#x43e; &#x43a;&#x430;&#x440;&#x43c;&#x430;&#x43d;&#x430; &#x438; &#x43e;&#x442;&#x434;&#x430;&#x44e; &#x426;&#x435;&#x440;&#x43a;&#x432;&#x438;!&#x2019;. &#x41e;&#x434;&#x43d;&#x43e;&#x439; &#x440;&#x443;&#x43a;&#x43e;&#x439; &#x43e;&#x43d; &#x43e;&#x442;&#x434;&#x430;&#x435;&#x442;, &#x430; &#x434;&#x440;&#x443;&#x433;&#x43e;&#x439; &#x43a;&#x440;&#x430;&#x434;&#x435;&#x442;: &#x443; &#x433;&#x43e;&#x441;&#x443;&#x434;&#x430;&#x440;&#x441;&#x442;&#x432;&#x430;, &#x443; &#x431;&#x435;&#x434;&#x43d;&#x44b;&#x445; &#x2026; &#x43a;&#x440;&#x430;&#x434;&#x435;&#x442;. &#x41e;&#x43d; &#x447;&#x435;&#x43b;&#x43e;&#x432;&#x435;&#x43a; &#x43d;&#x435;&#x43f;&#x440;&#x430;&#x432;&#x435;&#x434;&#x43d;&#x44b;&#x439;, &#x432;&#x435;&#x434;&#x443;&#x449;&#x438;&#x439; &#x434;&#x432;&#x43e;&#x439;&#x43d;&#x443;&#x44e; &#x436;&#x438;&#x437;&#x43d;&#x44c;. &#x418; &#x442;&#x430;&#x43a;&#x43e;&#x439; &#x447;&#x435;&#x43b;&#x43e;&#x432;&#x435;&#x43a; &#x437;&#x430;&#x441;&#x43b;&#x443;&#x436;&#x438;&#x432;&#x430;&#x435;&#x442; &#x442;&#x43e;&#x433;&#x43e;, &#x2013; &#x433;&#x43e;&#x432;&#x43e;&#x440;&#x438;&#x442; &#x418;&#x438;&#x441;&#x443;&#x441;, &#x430; &#x43d;&#x435; &#x44f;, &#x2013; &#x447;&#x442;&#x43e;&#x431;&#x44b; &#x435;&#x43c;&#x443; &#x43d;&#x430;&#x432;&#x435;&#x441;&#x438;&#x43b;&#x438; &#x43d;&#x430; &#x448;&#x435;&#x44e; &#x43c;&#x435;&#x43b;&#x44c;&#x43d;&#x438;&#x447;&#x43d;&#x44b;&#x439; &#x436;&#x435;&#x440;&#x43d;&#x43e;&#x432; &#x438; &#x431;&#x440;&#x43e;&#x441;&#x438;&#x43b;&#x438; &#x432; &#x43c;&#x43e;&#x440;&#x435;. &#x417;&#x434;&#x435;&#x441;