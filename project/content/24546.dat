<script type="text/javascript">
favorite_old='';
favorite_count=0;


function film_favorite(film_id){

if(favorite_old=='1')favorite='';
else favorite='1';

ajaxGETRequest('/films/ajax_favorite.php', 'favorite=' + favorite + '&film_id='+ film_id , function(result){

if(result=='ok'){

if(favorite=='1'){className='mylovelg';favorite_count=favorite_count+1;}
else {className='notmylovelg';favorite_count=favorite_count-1;}

document.getElementById("favorite_count_" + film_id).innerHTML=favorite_count;
document.getElementById("url_favorite_" + film_id).className=className + ' icon32fav';

favorite_old=favorite;

}
else alert('Ошибка: '.result);

});

}
</script>


<script type="text/javascript">

var watch_type_old='';
var count_film_watched=8;
var count_film_watch=0;
var count_film_want=0;
var count_film_dont_want=0;


function user_watch_film(film_id,watch_type){

if(watch_type_old==watch_type)watch_type='';

if(watch_type=='look')document.getElementById("url_user_watch_film_look").className='seemark icon32fav';
else if(watch_type=='looking')document.getElementById("url_user_watch_film_looking").className='watchnowmark icon32fav';
else if(watch_type=='want')document.getElementById("url_user_watch_film_want").className='wannaseemark active icon32fav';
else if(watch_type=='dont_want')document.getElementById("url_user_watch_film_dont_want").className='dontwannaseemark active icon32fav';

ajaxGETRequest('/films/ajax_watch_film.php', 'film_id='+ film_id +'&film_table=films&watch_type=' + watch_type, function(result){

if(result=='ok'){

if(watch_type=='look' || watch_type=='looking')document.getElementById('rating_div').style.display="block";

if(watch_type_old=='look')document.getElementById("url_user_watch_film_look").className='seeunmark icon32fav';
else if(watch_type_old=='looking')document.getElementById("url_user_watch_film_looking").className='notwatchnowmark icon32fav';
else if(watch_type_old=='want')document.getElementById("url_user_watch_film_want").className='wannaseemark passive icon32fav';
else if(watch_type_old=='dont_want')document.getElementById("url_user_watch_film_dont_want").className='dontwannaseemark passive icon32fav';

if(watch_type=='look' || watch_type_old=='look'){

if(watch_type=='look')count_film_watched=count_film_watched+1;
else count_film_watched=count_film_watched-1;

document.getElementById("watched_count_" + film_id).innerHTML=count_film_watched;
}

if(watch_type=='looking' || watch_type_old=='looking'){

if(watch_type=='looking')count_film_watch=count_film_watch+1;
else count_film_watch=count_film_watch-1;

document.getElementById("watch_count_" + film_id).innerHTML=count_film_watch;
}

if(watch_type=='want' || watch_type_old=='want'){

if(watch_type=='want')count_film_want=count_film_want+1;
else count_film_want=count_film_want-1;

document.getElementById("want_count_" + film_id).innerHTML=count_film_want;
}

if(watch_type=='dont_want' || watch_type_old=='dont_want'){

if(watch_type=='dont_want')count_film_dont_want=count_film_dont_want+1;
else count_film_dont_want=count_film_dont_want-1;

document.getElementById("dont_want_count_" + film_id).innerHTML=count_film_dont_want;
}

if(subscription_old=='' && watch_type=='want')film_subscription2(film_id);

watch_type_old=watch_type;

}
else if(result=='repeat'){}
else if(result=='no_auth'){alert('Необходимо авторизоваться!');}
else alert('Ошибка: '.result);

});

}

var subscription_old='';
var subscription='';

function film_subscription2(film_id){

if(subscription_old=='1')subscription='';
else subscription='1';

$.get('/films/ajax_subscription.php?subscription=' + subscription + '&film_id=' + film_id, function(data){

if(subscription_old=='1')className='subunmark';
else className='submark';

document.getElementById("url_film_subscription").className=className + ' icon32fav';

subscription_old=subscription;

});
}



</script>






