<script type="text/javascript">
function CheckMail() {
	valueLogin = document.getElementById('USER_LOGIN').value;
        value = document.getElementById('USER_EMAIL').value;
	if(value != "")
	{
	        reg = /[a-z0-9!#$%&'*+/=?^_`{|}~-]+(?:.[a-z0-9!#$%&'*+/=?^_`{|}~-]+)*@(?:[a-z0-9](?:[a-z0-9-]*[a-z0-9])?.)+[a-z0-9](?:[a-z0-9-]*[a-z0-9])?/;
	        if (!value.match(reg)) {
	                var error_text = document.getElementById('dop_text');
	                error_text.innerHTML = "Введен неверный e-mail. Повторите попытку.";
	                document.getElementById('USER_EMAIL').value=""; return false;
	        }
	        else
	                document.getElementById('bform').submit();
	}
	if(valueLogin != "")
	{
        	document.getElementById('bform').submit();
	}
}
</script>


