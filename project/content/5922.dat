 $('a[href="#step2"]').tab('show');
 }
 function toStep3() {
 $('a[href="#step3"]').tab('show');
 }
 function submitFormReview() {
 if($('#review-form-step1').valid()) {
 var data = $('#review-form-step1').serialize();
 data+= '&' + $('#review-form-step2').serialize();
 data+= '&' + $('#review-form-step3').serialize();
 data+= '&YII_CSRF_TOKEN=36a0a3d84d2df10cff5d1922371d3c555959196d';
 var lang = window.lang;
 url = '/?r=site/reviewFrom&lang='+lang;
 $.post(url, data, function(response){
 if (response.success) {
 $('#review_form_info').show().text(response.message);
 $('#review_form').hide();
 setTimeout(function(){
 $('input:not([disabled]):not([readonly]), textarea:not([disabled]):not([readonly])', '#review_form').each(function(){
 $(this).val('');
 })
 $('#review_form select:not([disabled]):not([readonly])').each(function(){
 $('option:selected', this).removeAttr('selected');
 $(this).trigger('chosen:updated');
 })
 $('#review_form_info').hide();
 $('#review_form').show();
 toStep1();
 }, 4000)
 } else {
 $.each(response.errors, function(i,v) {
 _errorId = 'Review_'+i+'-error';
 _field = $('#Review_'+i);
 _field.parents('.form-group').addClass('has-error');
 if (_field.next().attr('id') == _errorId) {
 _field.next().html(v);
 } else {
 _field.after('<span id="'+_errorId+'" class="help-block">'+v+'</span>');
 }
 $('#'+_errorId).show();
 })
 }
 }, 'json');
 } else {
 $('#Review-submit').attr('disabled','disabled');
 $('a[href="#step1"]').tab('show');
 return false;
 }
 }
