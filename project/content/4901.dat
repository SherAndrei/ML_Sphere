                        ng-class="{'is-opened': $root['show_all_smiles_' + (comment.id?comment.id + '_':'') + 'show'] == true }">
                        <span class="b-smiles__show-all"
                        ng-click="$root['show_all_smiles_' + (comment.id?comment.id + '_':'') + 'show'] = !$root['show_all_smiles_' + (comment.id?comment.id + '_':'') + 'show']"></span>
