                        ng-class="{'is-opened': $root['smiles_form_' + (comment.id?comment.id + '_':'') + 'show'] == true}"
                        ng-click="$root['smiles_form_' + (comment.id?comment.id + '_':'') + 'show'] = !$root['smiles_form_' + (comment.id?comment.id + '_':'') + 'show']"
