/textarea><textarea class="artc_tr" style="height:35px" id=message11 spellcheck=true rows=1 cols=50 name=message maxlength=200000 onkeypress="if(isEnterPressed(event)){this.form.action='http://errorfatal.beon.ru/p/a' + 'dd_comment.cgi';SendFormAddTinyComment('11');return false}" onkeyup="resizeTinyCommentsTextArea('11');" onFocus="resizeTinyCommentsTextArea('11', 0)" onblur="resizeTinyCommentsTextArea('11', 1)" onPaste="resizeTinyCommentsTextAreaDelayed('11')" placeholder="Написать комментарий&hellip;"></textarea><script type="text/javascript">resizeTinyCommentsTextAreaDelayed('11')</script></form></td></table></td></tr></table>
