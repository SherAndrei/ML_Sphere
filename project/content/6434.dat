				$(".contentTabs .tab1").toggle();
				$(".contentTabs .tab2").toggle();

				if ($(".contentTabs .tab1").css('display') == 'none'){
					$.cookie("activetab", 2);
					activetab = 2;
				} else {
					$.cookie("activetab", 1);
					activetab = 1;
				}
				
				if (activetab == 1) updateActivity();
				else updateSiteActivity();
				
				var $jsp1 = $("#scroll1").data( 'jsp' ); 
				if( $jsp1 ) $jsp1.reinitialise();
				var $jsp2 = $("#scroll2").data( 'jsp' ); 
