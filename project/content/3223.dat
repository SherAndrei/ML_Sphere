	<p dir="ltr" style="text-align:center;margin-top:9pt"><span style="font-size:10pt" xml:lang="en-US" lang="en-US">40</span></p>
	<p dir="ltr" style="text-align:center;margin-top:9pt"><span style="font-size:10pt" xml:lang="en-US" lang="en-US">20</span></p>
	<p dir="ltr" style="text-align:center;margin-top:9pt"><span style="font-size:10pt" xml:lang="en-US" lang="en-US">10</span></p>
