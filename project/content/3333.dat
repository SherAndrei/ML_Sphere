 <script type="text/javascript">
        if (typeof ymaps !== 'undefined')
	ymaps.ready(init_maps);
	
	function init_maps(){
                    myMap = new ymaps.Map("map", {
                center: [55.798550, 49.106323],
                zoom: 9
            });      
            
	myMap.controls.add('zoomControl', { left: 5, top: 5 })
        myMap.container.fitToViewport();
        
        var url = "/api/items"; // the script where you handle the form input.
        var coords_set = [];
        $.ajax({
            type: "GET",
            url: url,
            data: { ts_type: "57", ts_typeof: "", region_id: "5246", limit: 2000, with_cords: 0 },
            dataType: 'json',
            success: function (data)
            {   
                if(data.results)
                $.each(data.results, function(key,item){
                        if(item['coord_x']>0){
			var _elem = [];
			_elem.coords = [item['coord_x'],item['coord_y']];
			_elem.hint = item['name'];
			_elem.baloon = '<div class="map-ballons">'+ item['image'] + '<b>' + item['name'] + '</b><br/><br/>' + item['costs'] + '<div class="clear"></div>' + item['opts'] + '<br/>' + item['phone'] + ' (' + item['user_name'] + ')' + '<div class="item_num">№' + item['id'] + '</div></div>';
			coords_set.push(_elem);
                    }
                });
        for(i = 0; i < coords_set.length; i++){
	        var placemark = new ymaps.Placemark([coords_set[i].coords[0], coords_set[i].coords[1]], {hintContent:coords_set[i].hint, balloonContent:coords_set[i].baloon});
	        
	        placemark.events.add('balloonopen', function (e) {
		        	$('#demo-btn').removeClass('active').click();
	        });
	        
	        myMap.geoObjects.add(placemark);
            }
            }
        })
        	}
 </script>
