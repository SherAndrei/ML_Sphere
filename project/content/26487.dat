<script>
    var gAppCapsule = $(".wrapper");
    var gStateNormal = "app-state-normal";
    var gStateExtended = "app-state-extended";
    var gStateKey = "app-state";
    var gTabKey = "tab-button";

    function setAutoActivePanelTab() {
        var activeTabButton = $('[data-ui].active');
        var target = $(activeTabButton).data('target');
        $('.panel-tab').hide();
        $('[data-container='+target+']').show();

        $.cookie(gTabKey, target, {path: "/"});
    }

    $(function(){
        var curState =  $.cookie(gStateKey);
        var curTab = $.cookie(gTabKey);

        if(curState == gStateNormal) gAppCapsule.removeClass(gStateExtended).addClass(curState);
        else if(curState == gStateExtended) gAppCapsule.removeClass(gStateNormal).addClass(curState);

        $('.panel-tab').hide();

        $('.panel .menu li').removeClass("active");

        // если пользователь уже открывал вкладки
        if(curTab) {
            // показать выделенную вкладку
            $('[data-container='+curTab+']').show();
            $('.panel .menu li[data-target=' + curTab + ']').addClass('active');
        } else {
            // показать первую вкладку
            var firstTab = $('.panel .menu li').first();
            firstTab.addClass("active");
            $('[data-container='+firstTab.data('target')+']').show();
        }


        // wrap & unwrap panel
       $('.lr-horizontal,.logged-in .avatar,.app-state-normal .panel .menu li[data-target], ' +
           '.app-state-extended .panel .menu li.active[data-target]').live("click",function(){
           
           if ($(window).width()>1280) {
               
           gAppCapsule.toggleClass(gStateNormal).toggleClass(gStateExtended);

           if( gAppCapsule.hasClass(gStateNormal) ) $.cookie(gStateKey, gStateNormal, {path: "/"});
           else $.cookie(gStateKey, gStateExtended, {path: "/"});

           return false;
           }
       });

       // toggle active panel tab
       $('.panel .menu li[data-target]').live("click",function(){
           $(this).siblings("li").removeClass("active");
           $(this).addClass("active");

           // show active tab panel
           setAutoActivePanelTab();

           return false;
       });

      $('[data-ui=dropdown]').live("click",function(e){
          e.stopPropagation();
          var mine = $(this).find('[data-dropdown='+$(this).data('target')+']');
          var others = $('[data-dropdown]').not(mine).removeClass("dd-opened");
          mine.toggleClass("dd-opened");
      });

    $(document).click(function(event) {
        if ( $(event.target).data('data-ui')!="dropdown") {
            $('[data-dropdown]').removeClass("dd-opened");
        }
    });

