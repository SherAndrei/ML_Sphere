				}).show();
				$("html").css("overflow","hidden");
				$("#layout-popup").css("top",$(window).scrollTop() + $(window).height()/2-220 +"px");
				$("#popup").show();
			}else{
				$("#overlay").css("top",$(window).scrollTop()).show();
				$("#popup").show();
			}    
		}else{
			$("#overlay").fadeIn(200,function(){
				$("#popup").fadeIn(200);
			});
		}
		ga('send', 'event', 'WOWALERT', 'ShowAlert_articles', 'Show alert newsbox');
		return false;
	};

	Popup.init = function() {
		setTimeout(this.takeOutPopup, this.time);
	}

	jQuery(function($){

		Popup.init();

		$("#overlay .close").click(function(e){
			if(e.target == this){
