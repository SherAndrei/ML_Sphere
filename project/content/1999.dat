			var bannerId = '7453';
			var locUrlBanner = 'http://banners.turizm.ru/cgi-bin/bindex?click=1&bid=' + bannerId + '&url=';
			
			$('#tw_wnen_banner').datepicker({
				minDate : new Date()
			});
			$('#tw_wnen_banner').datepicker('setDate', '+14d');
			
			if($('body').data('cityname') == 'Москва'){
				toSugg.setCity('Санкт-Петербург');
			}else if($('body').data('cityname') !== ''){
				toSugg.setCity($('body').data('cityname'));
			}else if($('body').data('cityname') === ''){
				toSugg.setCity('Берлин');
			}
			
			fromSugg.setCity('Москва');
			
			$('#tw_banner_form').on('click', function(){
			
				if(fromSugg.isEmpty() || toSugg.isEmpty()){
					alert('Заполните поля с направлениями');
					return false
				}else if($('#tw_wnen_banner').val() == ''){
					alert('Выберите дату вылета');
					return false
				}
				
				var choicesDate = $('#tw_wnen_banner').datepicker('getDate');
				var monthDate = choicesDate.getMonth() + 1 + '';
				monthDate = monthDate.length == 1 ? '0' + monthDate : monthDate;
				
				var dayDate = choicesDate.getDate() + '';
				dayDate = dayDate.length == 1 ? '0' + dayDate : dayDate;							
				
				var twUrlString = 'http://avia.turizm.ru/#/r/' + dayDate + monthDate + fromSugg.getValueCode() + toSugg.getValueCode();
				var encodeString = $.base64.encode(twUrlString);
				var fullString = locUrlBanner + encodeString;

				window.location.assign(fullString);
				
				return false
				
			})
			
			
		})();
		
	})
</script>
