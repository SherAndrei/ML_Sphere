<script type="text/javascript">
function bquote() {
	var quote_name = "";
	function getSelectedText(){
		if(window.getSelection){
			return window.getSelection().toString();
		}
		else if(document.getSelection){
			return document.getSelection();
		}
		else if(document.selection){
			return document.selection.createRange().text;
		}
	}
	$('body').append('<div id="bquote"><a href="#">цитировать выделенный текст</a></div>');
	$('div.comtxt').each(function() {
		var mouseX = 0;
		var mouseY = 0;
		$(this).mousemove(function(e) {
			mouseX = e.pageX;
			mouseY = e.pageY;
		});
		$(this).mouseup(function() {
			if (getSelectedText()) {
				$('#bquote').css({top: mouseY - 38, left: mouseX - 85}).fadeIn('800');
				quote_name = $.trim($(this).parent().prev().children(1).text().replace('+','').replace('+','').replace('+',''));
			}
		});
	})
	$('body').click(function() {
		 if (!getSelectedText()) { $('#bquote').fadeOut('800'); }
	});
	$('#bquote').click(function() {
		if (getSelectedText()) {
			var br = '';
			if ( $('#message').val() != '' ) br = '\r\n';
			$('#message').val($('#message').val() + br + '[quote="'+ quote_name +'"]' + getSelectedText() + '[/quote]' + '\r\n').focus();
		}
		$(this).fadeOut('800');
		return false;
	});
}
$(document).ready(function(){
	owrw();

	if ($('#message').length) bquote();
	$('.arrows a').click(function(){
		current_post_id = $(this).parent().parent().parent().attr('post_id');
		$(this).parent().parent().html('&nbsp;').addClass('ratingload');
		$.post('/forum/topics/104584/',{ rate_value: ( ($(this).attr('href')=='#/+1') ? 1 : -1 ), rate_post: current_post_id },function(data){
			data = data.split('|'); if (!data[1]) {
				$('.arrows').hide('fast');
				return false;
			}
			rating = data[0]*1; new_class = (rating==0) ? 'gray' : ( (rating>0) ? 'green' : 'red' );

			$('td[post_id='+ data[1] +']').children('.arrows').hide('fast');
			$('td[post_id='+ data[1] +']').children('.rating').removeClass('ratingred').removeClass('ratinggreen').removeClass('ratinggray').addClass('rating' + new_class);
			$('td[post_id='+ data[1] +']').children('.rating').text(rating)

		});
		return false;
	})
	$('.drdown select').change(function(){
		if ( $(this).val() == 0 ) return false;
		current_val = $(this).val();
		current_post_id = $(this).parent().parent().parent().attr('post_id');
		$(this).parent().parent().html('&nbsp;').addClass('ratingload');
		$.post('/forum/topics/104584/',{ rate_value: current_val, rate_post: current_post_id },function(data){
				data = data.split('|'); if (!data[1]) {
				$('.arrows').hide('fast');
				return false;
			}
			rating = data[0]*1; new_class = (rating==0) ? 'gray' : ( (rating>0) ? 'green' : 'red' );

			$('td[post_id='+ data[1] +']').children('.drdown').hide('fast');
			$('td[post_id='+ data[1] +']').children('.rating').removeClass('ratingred').removeClass('ratinggreen').removeClass('ratinggray').addClass('rating' + new_class);
			$('td[post_id='+ data[1] +']').children('.rating').text(rating)

		});
		return false;
	})
})

function owrw() {
	var dwth = $(window).width();
	$('div.comtxt').each(function(){
//		$('#input2').parent().html( $('#input2').parent().html() + ' ' + dwth );
		$(this).width( dwth - 400 );
	})
	$('.bbcquote img').each(function(){
//		$('#input2').parent().html( $('#input2').parent().html() + ' ' + dwth );
		$(this).css('max-width', (dwth - 500) + 'px' );
	})
}
$( window ).resize(function() {
	owrw();
});

