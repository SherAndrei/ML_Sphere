<script>
    var publicationCode = 109;
    var model = "Article";
    var modelId = "382342";

    $(document).ready(function () {
        $(".DeliveryOfsysWidget .submit").click(function () {
            var email = $(".DeliveryOfsysWidget .emailInput").val();
            if (validateEmail(email)) {
                $(".DeliveryOfsysWidget .submit").attr("disabled", "disabled");
                $(".DeliveryOfsysWidget .submit").text("Отправка данных...");
                getSubscribesApi();
            }
        });
    });

    // берем данные о последних подписчиках с Api за последние 2 часа, так как в прокси подписки приходят в течении часа
    function getSubscribesApi() {
        var url = "/delivery/deliveryApi/subscribes";
        var email = $(".DeliveryOfsysWidget .emailInput").val();
        $.ajax({
            cache: false,
            async: false,
            type: "GET",
            data: {email: email, publicationCode: publicationCode, period: 2, confirmed: 1, userInteger1: 1},
            url: url,
            success: function (dataApi) {
                if (typeof dataApi == "object" && ("count" in dataApi)) {
                    if (dataApi.count == 0) {
                        getDeliveryTypeApi(dataApi);
                    } else {
                        subscribedToAll();
                    }
                } else {
                    console.log("Ошибка! Неправильные данные от api");
                    console.log(dataApi);
                }
            },
            error: function (data) {
                alert("Произошла ошибка сервиса. Попробуйте попозже!");
                console.log("Ошибка! Код ответа от api: " + data.status + ". URL: " + url);
                console.log(data);
            }
        });
    }

    // берем данные о типах подписок с Api
    function getDeliveryTypeApi() {
        var url = "/delivery/deliveryApi/types";
        $.ajax({
            cache: false,
            async: false,
            type: "GET",
            data: {publicationCode: publicationCode, enabled: 1},
            url: url,
            success: function (dataApi) {
                if (typeof dataApi == "object") {
                    if (dataApi == []) {
                                                console.log("У издания c PublicationCode " + publicationCode + " нет подписок!");
                    }
                    getOfsys(dataApi);
                    return;
                }
                console.log("Ошибка! Непрвильные данные от api");
            },
            error: function (data) {
                alert("Произошла ошибка сервиса. Попробуйте подписаться позднее!");
                console.log("Ошибка! Код ответа от api: " + data.status + ". URL: " + url);
                console.log(data);
            }
        });
    }

    // Берем данные с Ofsys
    function getOfsys(dataApi) {
        var email = $(".DeliveryOfsysWidget .emailInput").val();
        var url = "//ofsys.tool.vmcl.ru/api/v1/user-info";
        $.ajax({
            cache: false,
            type: "GET",
            data: {email: email, publicationCode: publicationCode, userInteger1: 1},
            url: url,
            success: function (dataOfsys) {
                if (typeof dataOfsys == 'object' && ("Records" in dataOfsys)) {
                    comparison(dataApi, dataOfsys);
                }
            },
            error: function (data) {
                alert("Произошла ошибка сервиса. Попробуйте попозже!");
                console.log("Ошибка! Код ответа от ofsys: " + data.status + ". URL: " + url);
                console.log(data);
            }
        });
    }

    // Сравниваем данные Api и Ofsys, и находим неподписанные Типы
    function comparison(dataApi, dataOfsys) {
        var dataApi = processingApi(dataApi);
        var dataOfsys = processingOfsys(dataOfsys);

        // Начинаем сравнивать два массива
        var diffDeliveryType = []; // сюда пишем типы разницу двух массивов
        for (var i = 0; i < dataApi.length; i++) {
            var issetType = false;
            for (var ii = 0; ii < dataOfsys.length; ii++) {
                if (dataApi[i] == dataOfsys[ii]) {
                    issetType = true;
                    break;
                }
            }
            if (!issetType) diffDeliveryType.push(dataApi[i]);
        }

        if (diffDeliveryType.length == 0) {
            // пользователь подписан на все
            subscribedToAll();
            return;
        }
        console.log("Пользователь не подписан на: ");
        console.log(diffDeliveryType);
        var deliveryTypes = addSuffixPublicationCode(diffDeliveryType);
        subscription(deliveryTypes);
    }

    // добавляем в суфикс рассылки publicationCode
    function addSuffixPublicationCode(diffDeliveryType) {
        var types = [];
        for (var i = 0; i < diffDeliveryType.length; i++) {
            types[i] = diffDeliveryType[i] + "_" + publicationCode; // если да, то следующий
            console.log(types[i]);
        }
        return types;
    }

    // пользователь подписан на все
    function subscribedToAll() {
        $('.paywall').find('.close').click();
                window.location.href = "/delivery/delivery/subscribedToAll?referer=" + window.location.href;
    }

    // обработка данных, пришедших с Апи
    function processingApi(dataApi) {
        var result = [];
        for (var prop in dataApi) {
            result[prop] = dataApi[prop].subscriptionType;
        }
        result.sort();
        console.log("Подписки с Api:");
        console.log(result);
        return result;
    }

    // Обработка данных, пришедших с Офсис
    function processingOfsys(dataOfsys) {
        var result = [];
        for (var prop in dataOfsys.Records) {
            result[prop] = dataOfsys.Records[prop]["SubscriptionType"];
        }
        result = uniqueOfsys(result);
        result.sort();
        //var result = ["Adv", "Event", "Market", "News", "Project"];
        console.log("Подписки с Ofsys:");
        console.log(result);
        return result;
    }

    // Оставляем только уникальные значения в массиве
    function uniqueOfsys(arr) {
        var result = [];
        nextInput:
            for (var i = 0; i < arr.length; i++) {
                var str = arr[i]; // для каждого элемента
                for (var j = 0; j < result.length; j++) { // ищем, был ли он уже?
                    if (result[j] == str) continue nextInput; // если да, то следующий
                }
                result.push(str);
            }
        return result;
    }

    // Начинаем подписывать
    function subscription(deliveryTypes) {
        var purchase = $('.DeliveryOfsysWidget input[name="purchase"]').val(); // Канал подписки
        var subscribeEmail = $('.DeliveryOfsysWidget input[name="email"]').val(); //  емейл подписчика
        var source = $('.DeliveryOfsysWidget input[name="source"]').val();
        var deliveryTypes = deliveryTypes; // Типы рассылок для подписки
        var url = "/delivery/subscribe"; // экшн
        var referer = location.href;

        $.ajax({
            url: url,
            type: 'POST',
            data: {
                'subscribeTheme': deliveryTypes,
                'subscriberEmail': '' + subscribeEmail + '',
                'purchase': '' + purchase + '',
                'source': '' + source + '',
                'referer': '' + referer + ''
            },
            dataType: 'json',
            success: function (json) {
                // GA событие при успешной подписке
                var gaEventOnSubscribe = $(this).data('ga-event-name');
                if (gaEventOnSubscribe != '' && gaEventOnSubscribe != null) {
                    _gaq.push(['_trackEvent', gaEventOnSubscribe, 'Submit', 'Submit']);
                }
                $('.paywall').find('.close').click();
                // при успешной подписке редиректим
                                window.location.href = "/delivery/delivery/afterSubscribe?model=" + model + "&modelId=" + modelId + "&email=" + subscribeEmail + "&referer=" + referer;
            },
            error: function (data) {
                var deliveryServerResponse = JSON.parse(data.responseText);
                console.log(deliveryServerResponse.message);
            }
        });
    }
</script>
