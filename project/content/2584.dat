	if($('#nk-auth-form').length == 0) return false;
	var actionURI = $('#nk-auth-form').attr('action');
	if(!actionURI) return false;
	var params = new Object;
	var inputs = $('#nk-auth-form').find('input');
	for(var i=0;i<inputs.length;i++) {
		var iName = $(inputs[i]).attr('name');
		if($(inputs[i]).attr('type') == 'checkbox') {
			if($(inputs[i]).is(":checked")) {
				params[iName] = $(inputs[i]).val();
			} else {
				params[iName] = '';
			}
		} else {
			params[iName] = $(inputs[i]).val();	
		}
	}
