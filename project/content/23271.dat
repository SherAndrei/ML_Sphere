  // Extension interaction
  document.addEventListener('extension-query', function(event){
    try
    {
      var node = event.target;
      if (!node || node.nodeType != Node.TEXT_NODE)
        return;

      var json = $.parseJSON(node.nodeValue);
      node.parentNode.removeChild(node);

      switch(json.msg)
      {
        case 'setBrowserExtension':
          setBrowserExtension(json.params);
          break;
      }
    }
    catch(err){}
  }, false);

  function setBrowserExtension(params)
  {
    if(!params || !params.id || !params.version)
      return;

    var ext = params.id + ' ' + params.version;

    if(params.userjs)
      ext += ' (userjs)';

    if(!params.enable || params.enable == '0')
      ext += ' - disabled';

    setTimeout(function(){
      if(window.ga)
        window.ga('set', 'dimension2', ext);
    }, 1000);

    if(appendExtensionInfoText)
      appendExtensionInfoText(ext);
  }
  // /Extension interaction
</script>

