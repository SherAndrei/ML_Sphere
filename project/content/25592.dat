function setFilter(o, fname) {

    $(o).attr(fname+'_filter', 1);

}



function canFilter(el, fname) {

    if ( $(el).attr(fname+'_filter') == 1 || !otherFilters(fname) ) {

        return true;

    }

    return false;

}



function otherFilters(name) {

    for (property in Filters) {

        if (property != name) return true;

    }

    return false;

}



// Развернуть/свернуть

function toggleLetters(input) {

