                    $(this).css('display', 'block');

                } else {

                    $(this).css('display', 'none');

                }

                break;

            default:

                $(this).css('display', 'block');

        }



        if (fields.genre != 'all') {

            if ( $(this).attr('genre') != fields.genre ) $(this).css('display', 'none');

