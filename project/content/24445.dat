function smallerImage(field, notes) {
	window.onload = function() {

		if (notes) {
			$(field + ' img').jQueryNotes(jquerysetting);
		}

		var mfield = field.substring(1);
		var images = document.getElementById(mfield);
		if (!images) images = document.getElementsByClassName(mfield)[0];

		images = images.getElementsByTagName('img');

		for (var i = 0; i < images.length; i++) {
			var img = images[i];
			img.removeAttribute('style');
			var width = img.naturalWidth;
			
			if (width == 0) {
				img.onload = function(e) {
					width = this.width;
				}
			}

			if (width > 700) {

				var src = img.src;
				img.removeAttribute('style');
				img.className = 'small';

				img.title = 'Нажмите для увеличения';

				var wrapper = document.createElement('a');
				wrapper.href = src;
				wrapper.rel = 'lightbox';
				wrapper.appendChild(img.cloneNode(true));
				img.parentNode.replaceChild(wrapper, img);
			}
            else if (img.src.indexOf('/_thumbs/') > -1) {


                var wrapper = document.createElement('a');
                wrapper.href = img.src.replace('/_thumbs/', '/images/');
