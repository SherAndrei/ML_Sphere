					analytics="{name:'dropdown/selection', data:{option: option.id}}"
					class="search-filters__advanced-dropdown__option"
					ng-if="!option.needAuth || simsStoreDropDownVM.authenticated"
					ng-bind-html="::option.text"
					ng-click="simsStoreDropDownVM.setOption(option)"
					ng-repeat="option in simsStoreDropDownVM.content track by option.label"
					sims-store-audio
					source="dropdown-select"
					>
				</li>
			</ul>
		</div>
	</div>
</div></script>


