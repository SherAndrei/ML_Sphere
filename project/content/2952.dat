<script type="text/ng-template" id="template-checkout-modal">
	<div sims-store-checkout></div></script>

<script type="text/ng-template" id="template-simple-carousel">
	<carousel
	class="store-carousel"
	ng-if="simsSimpleCarouselVM.slides"
	>
	<slide
		active="slide.active"
		ng-repeat="slide in simsSimpleCarouselVM.slides"
		>
		<div
			class="store-carousel__slide slide__image"
			ng-if="slide.type == 'image'"
			style="background-image: url('{{::slide.paths.full}}')"
			>
		</div>
		<div
			class="slide__youtube"
			ng-if="slide.type == 'youtube'"
			>
			<div
				analytics="{name:'carousel/video-play'}"
				class="store-carousel__slide youtube-cover"
				ng-click="simsSimpleCarouselVM.playVideo()"
				style="background-image: url('/media/cache/full/{{::slide.image | encodeURI }}')"
				>
				<div
					class="slide__youtube__overlay"
					ng-class="{'slide__youtube__overlay--ready': simsSimpleCarouselVM.videoReady || slide.videoPlayer.currentState === 'ended'}"
					></div>
			</div>
			<div
				class="youtube-container"
				ng-class="{'youtube-container--playing': slide.showVideo}"
				>
				<youtube-video
					player-vars="{controls: 0,
						showinfo: 0,
						rel: 0,
						autoplay: 0,
						modestbranding: 1,
						wmode: 'transparent'}"
					player="slide.videoPlayer"
					video-id="slide.youTubeId"
					>
				</youtube-video>
			</div>
		</div>
	</slide>
</carousel>
</script>

