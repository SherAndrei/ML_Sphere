  var s = document.createElement("script"); s.type = "text/javascript"; s.src = url; s.async = true; s.onload = function () {if (cb) cb();}
  document.body.appendChild(s);
}
function loadJQuery() {
  		loadScript("//ajax.googleapis.com/ajax/libs/jquery/2.1.1/jquery.min.js", function () {
        loadScript("http://inv4you.com/tema/inv4you/js/dop.js", false);
        loadScript("http://inv4you.com/tema/inv4you/js/vk.js", function(){vk.init();});
        loadScript("http://inv4you.com/base/modules/dlemovie/interface/functions.js", false);        
        loadScript("http://yastatic.net/share/share.js", false);

		
		
		
		$(".vkbox").hover(function(){$(".vkbox").stop(true, false).animate({right:"0"},"medium");},function(){$(".vkbox").stop(true, false).animate({right:"-250"},"medium");},500);
        $(".subbox").hover(function(){$(".subbox").stop(true, false).animate({right:"0"},"medium");},function(){$(".subbox").stop(true, false).animate({right:"-250"},"medium");},500);
  });

}
    if (window.addEventListener) window.addEventListener("load", loadJQuery, false);
    else if (window.attachEvent) window.attachEvent("onload", loadJQuery);
    else window.onload = loadJQuery;
</script>


