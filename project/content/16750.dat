      $('.b-icon').click(function(){
        ctype = $(this).attr('data-type');
        $bbimg = $(this).parent().parent().parent().find('.bbimg');
        pimg = $bbimg.attr('src');
        purl = bpurl + '?image=' + ($('.usertext img').index($bbimg) + 1);
        console.log(purl);
        //$("meta[property='og:image']").attr('content', pimg);

        switch (ctype) {
          case 'vk':
