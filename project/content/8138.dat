<script>


function drop_comment(id)
{
  var comment = $('#comment_' + id);
  $.ajax({url: '/comments/drop/' + id,dataType:'json'}).done( function (data){
    if(data.status == 'ok')
    {
      $('<div id="deleted_' + id + '" style="display:none"></div>').insertAfter(comment);
      deleted = $('#deleted_' +id);
      deleted.html(comment.html());
      comment.html('<p class="i-comment-removed">Комментарий удален (<a href="#" onclick="undo_comment(' + id + ',\'' + data.token + '\');return false;">восстановить</a>)</p><div class="clear"></div>');
    }
    else
    {
      alert('Ошибка удаления комментария');
    }

  });

}

function undo_comment(id, token)
{
  $.ajax({url: '/comments/undo/' + id + '/' + token,dataType:'json'}).done(function (data){
    if(data.status == 'ok')
    {
      $('#comment_' + id).html($('#deleted_' + id).html());
      $('#deleted_' + id).remove();
    }
    else if(data.status == 'err')
    {
      alert(data.message);
    }
  });
}

function bootstrap(id)
{
  var comment = $('#comment_' + id);
  $('#preloader_' + id).css('display', 'block');
  $('#bootstrap_link_' + id).attr('onclick', 'return false;');
   $.ajax({url: '/comments/bootstrap/' + id }).done( function (data){
    $(data).insertAfter(comment);
    $('#bootstrap_' + id).remove();
    return false;
   });


}

function getSelText()
{
    if (window.getSelection) {
        return window.getSelection();
    } else if (document.getSelection) {
        return document.getSelection();
    } else if (document.selection) {
        return document.selection.createRange().text;
    } else return false;
}


function doquote(form_name){
  if(form_name == 'form_static'){
    sel = getSelText();
    if(sel || sel != ''){
      var form = $('#static_comment_form');
      data = '[QUOTE]' + sel + '[/QUOTE]'
      val = $(form).find('textarea[id=my_comment_body_1]').attr('value');
      $(form).find('textarea[id=my_comment_body_1]').attr('value', val + "\n" + data);
    }
  }
  if(form_name == 'form_dynamic'){
    sel = getSelText();

    if(!sel || sel == ''){
      var form = $('#comment_form_dynamic');
      var commentid = $(form).find('input[name=comment_id]').val();
      $(form).find('textarea[id=my_comment_body]').attr('value', 'Подождите......');
      $.ajax({url: '/comments/quote/' + commentid}).done( function (data) {
        if(data)
        {
         val = $(form).find('textarea[id=my_comment_body_2]').attr('value');
         $(form).find('textarea[id=my_comment_body_2]').attr('value', val + "\n" + data);
        }
      });
    }else{
      var form = $('#comment_form_dynamic');
      data = '[QUOTE]' + sel + '[/QUOTE]'
      val = $(form).find('textarea[id=my_comment_body_2]').attr('value');
      $(form).find('textarea[id=my_comment_body_2]').attr('value', val + "\n" + data);
    }
  }
}
  
function show_comment_form(id, quote)
{
  var form_block = $('#switch_form');
  var form = $('#comment_form_dynamic');
  form_block.detach().insertAfter('#clear_' + id);
  form_block.css('display','block');
  $(form).find('input[name=pid]').val(id);
  $(form).find('input[name=comment_id]').val(id);
  if(quote == true)
  {
  }
  else
  {
     $(form).find('textarea[id=my_comment_body_2]').attr('value', '');
  }
  $(form).find('textarea[id=my_comment_body_2]').focus();
  return false;

}

function switch_static_form(obj){
  var parent = obj.parent();
  var parent_id = obj.parent().attr('id');
  var form = $('#static_comment_form');
  var form_parent = $('#static_comment_form').parent();
  $(obj).detach();
  $(form).detach();
  $(form).css('display','none');
  $(form).appendTo(parent);
  $(form).slideToggle();
  $(obj).appendTo(form_parent);
  $(form).find('textarea[id=my_comment_body_1]').focus();
}




