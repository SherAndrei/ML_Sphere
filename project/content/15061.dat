e=1 type=hidden><input name=r value="" type=hidden><input name=button value="add" type=hidden><div id=error_message14 class=tinycommserr></div><textarea rows=5 cols=50 spellcheck=true class="artc tcresizer"></textarea><textarea class="artc_tr" style="height:35px" id=message14 spellcheck=true rows=1 cols=50 name=message maxlength=200000 onkeypress="if(isEnterPressed(event)){this.form.action='http://bloomandflorawi.beon.ru/p/a' + 'dd_comment.cgi';SendFormAddTinyComment('14');return false}" onkeyup="resizeTinyCommentsTextArea('14');" onFocus="resizeTinyCommentsTextArea('14', 0)" onblur="resizeTinyCommentsTextArea('14', 1)" onPaste="resizeTinyCommentsTextAreaDelayed('14')" placeholder="Написать комментарий&hellip;"></textarea><script type="text/javascript">resizeTinyCommentsTextAreaDelayed('14')</script></form></td></table></td></tr></table>
