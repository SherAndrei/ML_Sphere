<!-- Message for pay -->

<script>
    $(document).ready(function() {
        mambo.use('{mambo.utils}Common', function () {
            var url = mambo.utils.getLocationParams(1);
            if (url.msg) {
                var $app = $('#mgsOk');

                var str = window.location.href,
                        str2 = window.location.hash,
                        strnew = str.substr(0, str.indexOf(str2));

                if(url.msg=='qiwi_error'){
                    $app.addClass('payment-error');
                }
                else if(url.msg=='qiwi_ok'){
                    $app.addClass('payment-success');
                }
                require([], function() {
                    Mb.ComponentManager.getComponentInstance({
                        url: 'Web/Modal',
                        params: {
                            content: {
                                htmlElement: $app
                            },
                            outerCssClass: 'web-modal-vitrina',
                            removeResetCssClass: true
                        }
                    }).then(function (modal) {
                        modal.disposed.once(function () {
                            window.location.href = strnew;
                        });
                    });
                });
            }
        });
    });
</script>

