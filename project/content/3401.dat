    <meta itemprop="headline" content=""/>

    <div itemprop="author" itemscope itemtype="http://schema.org/Person">
        
        <div>
            
                <p class="profile_name"><s></s>Wanted dead</p>
                <ul class="actions">
                    
                </ul>
            
            <p class="sub_title" id="ext_title_931759"></p>
