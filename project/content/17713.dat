            $(document).ready(function () {

                var contentColumn = $('[data-role="col-main"]');
                var leftColumn    = $('[data-role="col-right"]');

                // element height + margin (outerHeight(true))
                var itemHeight = 248 + 26;
//                debugger;
                // needed to show elements first time
                showElements();

                // needed only to react if main column changes height (because new img loaded, etc.)
                new ResizeSensor(contentColumn, function () {
                    showElements();
                });

                // show elements, based on columns height
                function showElements()
                {
                    var contentHeight = contentColumn.height();
                    var rightColHeight = leftColumn.height();
                    if (contentHeight > rightColHeight)
                    {
                        var hiddenSliders = leftColumn.find('li[data-slider]:hidden');

                        hiddenSliders.each(function(index, item){

                            // show curr element, if it feets inside free space
                            if (contentColumn.height() > (leftColumn.height() + itemHeight))
                            {
                                var currItem = $(item);

                                // lazy loading images with js
                                var img = currItem.find('img');
                                var realSrc = img.data('realSrc');
                                img.attr('src', realSrc);

                                currItem.show();
                            }
                        });
                    }
                }
            });
        </script>
    

        
