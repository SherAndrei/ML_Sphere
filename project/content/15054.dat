                <script type="text/javascript">
                    $(document).ready(function ()
                    {
                        $(".b-afisha-channels-grid dl a").click(function()
                        {
                            if ($(this).closest("dl").hasClass("b-afisha-channels-grid__card__item_state_opened"))
                            {
                                   $(this).closest("dl").removeClass("b-afisha-channels-grid__card__item_state_opened");
                                   $(this).closest("dl").find(".b-afisha-channels-grid__card__item__caption").hide();
                            }
                            else
                            {
                                   var img = $(this).closest('dd').find('div.b-afisha-channels-grid__card__item__caption__pad img[data-original]');
                                   if(img)
                                       img.attr('src', img.attr('data-original'));
                                   $(this).closest("dl").addClass("b-afisha-channels-grid__card__item_state_opened");
                                   $(this).closest("dl").find(".b-afisha-channels-grid__card__item__caption").show();
                            }
                            return false;
                        });
                    
                        $('ul.b-grey-menu-selectors__container_pos_right a').bind('click', function ()
                        {
                            $("ul.b-grey-menu-selectors__container_pos_right li").removeClass('b-grey-menu-selectors__container__item_state_active');
                            $(this).parent().addClass('b-grey-menu-selectors__container__item_state_active');
                    
                            $('div.b-afisha-channels-grid').attr('viewType', $(this).attr('rel'));
                            $('ul.eventTypesSelector li.b-grey-menu-selectors__container__item_state_active a').click();
                    
                            return false;
                        });
                    
                        var eventTypeList = [];
                    
                        $('ul.eventTypesSelector a').bind('click', function ()
                        {
                            $("ul.eventTypesSelector li").removeClass('b-grey-menu-selectors__container__item_state_active');
                            $(this).parent().addClass('b-grey-menu-selectors__container__item_state_active');
                    
                            var viewType = $('div.b-afisha-channels-grid').attr('viewType'); // get viewtype (now|all)
                            $('div.b-afisha-channels-grid__card').show(); // show all channels
                            if (viewType == 'now') {
                                $('div.b-afisha-channels-grid dl:not(.b-afisha-channels-grid__card__item_time_past)').show(); // show new events
                                $('div.b-afisha-channels-grid dl.b-afisha-channels-grid__card__item_time_past').hide(); // hide old events
                            } else {
                                   $('div.b-afisha-channels-grid dl').removeClass('hidden'); // show all events
                                $('div.b-afisha-channels-grid dl').show(); // show all events
                            }
                    
                            if($(this).attr('rel') != '') {
                                $('div.b-afisha-channels-grid dl:not([typeId='+$(this).attr('rel')+'])').hide(); // hide events with another type
                            }
                            // hide channels without events
                            $('div.b-afisha-channels-grid__card').not($('div.b-afisha-channels-grid__card').has('dl:visible')).hide();
                    
                            if (supports_history_api())
                                history.pushState(null, '', $(this).attr('href').replace(' ','')+'/');
                    
                            return false;
                        })
                        .each(function(i,e)
                        {
                            eventTypeList[$(e).attr('type')] = function() {$('ul.eventTypesSelector a').eq(i).click()};
                        });
                    
                        $.url().doIfInUrl( eventTypeList );
                    
                        function supports_history_api()
                        {
                            return !!(window.history && history.pushState);
                        }
                    
                    });
                </script>
