	<td class="alt1" id="td_post_790" style="border-right: 1px solid #f9f9f9">
	
		
		
			<!-- icon and title -->
			<div class="smallfont">
				<img class="inlineimg" src="images/icons/icon1.gif" alt="По умолчанию" border="0" />
				<strong>Первые тесты ГТА 4</strong>
			</div>
