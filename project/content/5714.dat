    <script>
        banki.utils.domReady.push(function() {
            require(['jquery'], function($) {
                var global = window;
                var popup;
                var popupHtml = $('#auth-popup-template').html();

                global.authPopup = global.authPopup || {};

                require(['ui.popup'], function(Popup) {
                    var originBackUrl;
                    var $backUrlInputField;

                    popup = Popup({
                        content: popupHtml,
                        onAfterShow: function(popupObj) {
                            if (!Modernizr.touch) {
                                popupObj.$popup.find('input').not('[type="hidden"]').first().focus();
                            }
                        },
                        onAfterClose: function() {
                            if (originBackUrl) {
                                $backUrlInputField.val(originBackUrl);
                                originBackUrl = false;
                            }
                        },
                        onBuilded: function(popup) {
                            popup.$popup.find('.b-loginPopup__input').on('click', function() {
                                $('.b-socialAuth__info').hide();
                                $('.b-socialAuth__icon').removeClass('b-socialAuth__icon_active');
                            });
                        }
                    });

                    $backUrlInputField = popup.$popup.find('[name="backurl"]');

                    global.authPopup.popup = popup;
                    global.authPopup.show = function(params) {
                        params = params || {};

                        if (params.backUrl) {
                            originBackUrl = $backUrlInputField.val(); // save previous back url
                            $backUrlInputField.val(params.backUrl); // set new back url
                        }

                        popup.showPopup();
                    };
                });
            });
        });
    </script>


