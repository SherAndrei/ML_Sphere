	// Ф-я для прокладки маршрута по яндекс.карте из пункта "Как доехать"

	function InitYandexRoute()
	{

		ymaps.ready(init);

		function init () {

			if (!ItWasRoute)
			{

				myMap = new ymaps.Map("route-map", {
					center: [56.326944, 44.0075],
					zoom: 12,
					behaviors: ['default', 'scrollZoom'],
					controls: ['zoomControl', 'searchControl', 'typeSelector', 'geolocationControl', 'trafficControl', 'fullscreenControl']
				});
				ItWasRoute = true;
			}

			var start = $("#from-address").val();
			var end = $("#to-address").val();

			ymaps.route(
				[
					// Список точек, которые необходимо посетить
					[start],
					[end]
				],
				{
					// Опции маршрутизатора
					mapStateAutoApply: true, // автоматически позиционировать карту
					avoidTrafficJams: true // маршрут с учётом пробок

				}).then(function (route) {

					myMap.geoObjects.add(route);

					route.editor.start();

					// Получаем первый маршрут
					var way = route.getPaths().get(0),

					// Маршрут состоит из сегментов. Сегмент - участок маршрута, который нужно проехать до следующего изменения направления движения.
						segments = way.getSegments(),

						moveList = '<br/><div class="grid"><div class="unit one-third"><b>Общая длина маршрута:</b> ' + route.getHumanLength() + '</div>';
					moveList += '<div class="unit one-third"> <b>Время в пути:</b> ' + route.getHumanTime() + '</div>';
					moveList += '<div class="unit one-third"> <b>Время в пути с учётом пробок:</b> ' + route.getHumanJamsTime() + '</div></div>';
					moveList += '<hr class="hr-dot-2"><div class="grid align-center"><span class="route-list">Начало маршрутного листа:</span>';

					for (var i = 0; i < segments.length; i++) {
						var street = segments[i].getStreet();
						moveList += ('Едем ' + segments[i].getHumanAction() + (street ? ' на ' + street : '') + ', проезжаем ' + segments[i].getLength() + ' м.,');
						moveList += '</br>'
					}

					moveList += '<span class="route-list">Конец маршрутного листа.</span></div><hr class="hr-dot-2">';

					// Выводим маршрутный лист
					$('.route-map-div').append(moveList);

				}, function (error) {

					alert("Возникла ошибка: " + error.message);

				});


		}
	}

