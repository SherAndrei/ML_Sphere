	// Ф-я для определения местоположения пользователя и подстановки координат в инпут

	function LocateMe() {

		if (navigator.geolocation) {
			navigator.geolocation.getCurrentPosition(handle_geolocation_query,handle_errors);
		} else {
			alert("Ошибка! Пожалуйста, введите адрес вручную — на вашем устройстве не поддерживается геолокация.");
		}
	}

	function handle_errors(error)
	{
		switch(error.code)
		{
			case error.PERMISSION_DENIED: alert("Ошибка! Пожалуйста, введите адрес вручную — разрешение на использование геолокации не предоставлено.");
				break;
			case error.POSITION_UNAVAILABLE: alert("Ошибка! Пожалуйста, введите адрес вручную — координаты определить не получилось.");
				break;
			case error.TIMEOUT: alert("Ошибка! Пожалуйста, введите адрес вручную — определение координат сброшено по тайм-ауту.");
				break;
			default: alert("Ошибка! Пожалуйста, введите адрес вручную.");
				break;
		}
	}

	function handle_geolocation_query(position){
		document.getElementById('from-address').value = position.coords.latitude + ', ' + position.coords.longitude;
		ShowYandexRoute(); // и сразу строим маршрут, если координаты определились
	}


	
