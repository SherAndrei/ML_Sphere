	<script type='text/javascript'>

		var MapCanvas = '';
		var gmapfull = false;

		var gx, gy, panorama, PanoramaLoaded = false;

		function FullScreenControl(controlDiv, map) {

			// Set CSS styles for the DIV containing the control
			// Setting padding to 5 px will offset the control
			// from the edge of the map
			controlDiv.style.padding = '10px';
			controlDiv.style.zIndex = '99999';

			// Set CSS for the control border
			var controlUI = document.createElement('div');
			controlUI.style.backgroundColor = 'white';
			controlUI.style.border = '1px solid #000';
			controlUI.style.cursor = 'pointer';
			controlUI.style.textAlign = 'center';
			controlUI.title = 'Нажмите для просмотра карты в полноэкраном режиме';
			controlDiv.appendChild(controlUI);

			// Set CSS for the control interior
			var controlText = document.createElement('div');
			controlText.innerHTML = '<img id="fullscreenbutton" src="/addon/images/fullscreen.png" alt="На весь экран" style="padding: 4px;"/>';
			controlUI.appendChild(controlText);

			// Setup the click event listener
			google.maps.event.addDomListener(controlUI, 'click', function() {

				if (!gmapfull)
				{

					$("body").prepend('<div class="gmap"></div>');

					$('#map-canvas').css('height', '100%');
					$('#map-canvas').css('width', '100%');
					$('#map-canvas').css('margin', '0px');

					$('#map-canvas').detach().prependTo('div.gmap');

					$('span.filial-address-active')[0].onclick();
					gmapfull = true;

				}
				else
				{

					$('#map-canvas').css('height', '350px');
					$('#map-canvas').css('width', '100%');
					$('#map-canvas').css('margin', '5px');

					$('#map-canvas').detach().prependTo('#gmap-tab');

					$(".gmap").detach();

					$('span.filial-address-active')[0].onclick();
					gmapfull = false;

				}

			});

		}

		function googlepanorama() {

			var current = new google.maps.LatLng(gx, gy);

			var panoramaOptions = {
				position: current
			};

			panorama = new  google.maps.StreetViewPanorama(document.getElementById("map-canvas"), panoramaOptions);

			// Create the DIV to hold the control and
			// call the HomeControl() constructor passing
			// in this DIV.

			/* // my fullscreen button

			var homeControlDiv = document.createElement('div');
			var homeControl = new FullScreenControl(homeControlDiv, panorama);

			homeControlDiv.index = 1;
			panorama.controls[google.maps.ControlPosition.TOP_RIGHT].push(homeControlDiv);
            */

			// Place an organization mark

			var currentMarker = new google.maps.Marker({
				position: current,
				map: panorama,
				icon: 'http://rubrikator.org/addon/images/map_pin_streetview.png',
				title: 'Почта с индексом 200961'
			});

			PanoramaLoaded = true;

			// Определим, есть ли в данной точке картинка

			var streetViewService = new google.maps.StreetViewService();
			var STREETVIEW_MAX_DISTANCE = 50;
			var latLng = new google.maps.LatLng(gx, gy);
			streetViewService.getPanoramaByLocation(latLng, STREETVIEW_MAX_DISTANCE, function (streetViewPanoramaData, status) {
				if (status === google.maps.StreetViewStatus.OK) {
					// StreetView есть!
				} else {
					document.getElementById('GoogleStreetViewNotice').style.display = 'block';
					document.getElementById('GoogleStreetViewNotice').innerHTML = 'Похоже, что на эту улицу посмотреть нельзя.';
				}
			});

		}

		function ShowGooglePanorama(x, y) {

			gx = x;
			gy = y;

			if (!PanoramaLoaded)
			{
				var script = document.createElement('script');
				script.type = 'text/javascript';
				script.src = '//maps.googleapis.com/maps/api/js?key=AIzaSyBeW1OgcmQA_OmvVEN_PKeadMIGDK5OP0E&sensor=false&region=RU&language=ru&callback=googlepanorama';
				document.body.appendChild(script);
			} else {
				googlepanorama();
				setTimeout(function(){ google.maps.event.trigger(panorama, 'resize'); },250);
			}

		}

	</script>

