			/* Меню из ссылок под H1 */

			function scroll(id, clickid, e) {

				if (Modernizr.touch)
				{
					e.stopPropagation();
				}
				else
				{
					e.preventDefault();
					e.stopPropagation();
				}

				var p_el = p_el || document.getElementById(clickid);
				p_el.click();
				$('html, body').animate({
					scrollTop: $(id).offset().top
				}, 400);

				$(id).delay(400).fadeOut(200).fadeIn(100);
			}

