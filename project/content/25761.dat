				},
				{
					suppressMapOpenBlock: true,
					suppressObsoleteBrowserNotifier: true,
					autoFitToViewport: 'always',
					exitFullscreenByEsc: true
				}
				);

				//Настройки метки
				thisOrganization = new ymaps.Placemark([x, y], {
					hintContent: c
				}, {
					preset: 'islands#redDotIcon'
				});

				//Разместить метку
				yandexMap.geoObjects.add(thisOrganization);

				//Триггер - карта загружена
				ItWasMap = true;

			}

			if (ItWasMap && d)
			{

				yandexMap.geoObjects.remove(thisOrganization);

