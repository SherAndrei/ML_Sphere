<script type="text/javascript">
 	function createModal(){
		$("#auth").modal({
			opacity:80,
			overlayCss: {backgroundColor:"#000000"},
			containerCss: {width: "600px", height: "163px"},
			autoPosition: true,
			onShow: function (dialog) {
				$("#autForm").validate({
		               rules : {
		                      login : { required: true, minlength: 5 },
							  password : { required: true, minlength: 5 }
						},
		               messages : {
					   			login : {
							   		required : "Введите логин",
									minlength : "Введите не менее 5 символов"
							  	},
		                       	password : {
							   		required : "Введите пароль",
									minlength : "Введите не менее 5 символов"
							  	}
		               }
				});
			}

		});

	}
	function createRegModal(){
		$("#reg").modal({
			opacity:80,
			overlayCss: {backgroundColor:"#000000"},
			containerCss: {width: "600px", height: "300px"},
			autoPosition: true,
			onShow: function (dialog) {

					$("#registr").validate({
		               rules : {
		                      login2 : { required: true, minlength: 5 },
							  password2 : { required: true, minlength: 5 },
							  name : { required: true, maxlength: 25 },
							  sname : { required: true },
							  mail : { required: true, email: true, maxlength: 30 },
							  capcha : { required: true },
							  accept : { required: true }
						},
		               messages : {
					   			login2 : {
							   		required : "Введите логин",
									minlength : "Введите не менее 5 символов"
							  	},
		                       	password2 : {
							   		required : "Введите пароль",
									minlength : "Введите не менее 5 символов"
							  	},
								name : {
									required : "Введите имя",
									maxlength : "Не более 25 символов"

								},
								sname : { required : "Введите фамилию" },
								capcha : { required : "Введите код!" },
								mail : {
		                        	required : "Введите E-mail",
		                        	email : "Не верный формат e-mail",
									maxlength : "Не более 30 символов"
		                       	},
		                       	accept : { required : "Нужно согласиться с условиями регистрации" }
		               }
				});

			}
		});
	}

	function createPassModal(){
		$("#forgetPass").modal({
			opacity:80,
			overlayCss: {backgroundColor:"#000000"},
			autoPosition: true
		});
		$('.ajax3').ajaxContent({
			target:'#mainConBlock',
			type:'Post',
			bind:'#emailsecond',
			success: function(obj,target,msg){
				if(msg == "Письмо с инструкцией по получению пароля отправлено!"){
					$("#forgetButton").hide(100);
					target.html(msg);
				} else if(msg == "Такого пользователя не существует!"){
					$("#forgetButton").hide(100);
					target.html(msg);
				} else {
					$.modal.close();
					createPassModal()
				}
			}
		});
	}

	function getValid(){
		if($("#autForm").validate().form()){
			$("#helink").attr("href","/login.user?");
			$("#helink").addClass("ajax");
			$("#heimg").attr("src","/$files/pic/enterauth.gif");
			$('.ajax').ajaxContent({
				target:'#errormsg',
				type:'Post',
				bind:'#login,#password,#back',
				success: function(obj,target,msg){
					if(msg == "&nbsp;"){
						document.location.reload();
					} else {
						target.html("Неверный логин или пароль");
					}
				}
			});
		} else {
			$("#helink").attr("href","#");
			$("#heimg").attr("src","/$files/pic/enterauth_gray.gif");
			$("#helink").removeClass("ajax");
		}
	}
	function getValidReg(){
		//alert("try");
		if($("#registr").validate().form()){
			//alert("catch!");
			$("#helink2").attr("href","/reg/?");
			$("#helink2").addClass("ajax2");
			$("#heimg2").attr("src","/$files/pic/registrate.gif");
			$('.ajax2').ajaxContent({
				target:'#regMsg',
				type:'Post',
				bind:'#act,#login2,#password2,#name,#sname,#mail,#capcha,#accept',
				success: function(obj,target,msg){
					if(msg == "Done"){

						msg = "Регистрация прошла успешно! На почту отправленно письмо с регистрационной информацией.";
						$(".notforu").css("display","none");
						$("#regMsg").css("padding-top","30");
					} else {
						remakeCapcha("capchImg");
					}
					target.html(msg);

				}
			});

		} else {
			$("#helink2").attr("href","#");
			$("#helink2").removeClass("ajax2");
			$("#heimg2").attr("src","/$files/pic/registrate_grey.gif");
		}
	}
