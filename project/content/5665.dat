<p style="font-size:12pt;">Dim bd As DAO.Database</p>
<p style="font-size:12pt;">Dim r1 As Recordset</p>
<p style="font-size:12pt;">Dim w1 As Word.Application</p>
<p style="font-size:12pt;">Dim k As String, s As String, m As String</p>
<p style="font-size:12pt;">Set bd = OpenDatabase(Application.ThisWorkbook.Path &amp; &quot;\db1.mdb&quot;)</p>
<br>
<p style="font-size:12pt;">Set w1 = CreateObject(&quot;Word.Application&quot;)</p>
<p style="font-size:12pt;">w1.Documents.Open Filename:=Application.ThisWorkbook.Path &amp; &quot;\Карточка.doc&quot;</p>
<p style="font-size:12pt;">w1.Application.Visible = True</p>
<br>
<p style="font-size:12pt;">If TextBox1.Text &lt;&gt; &quot;&quot; Then</p>
<p style="font-size:12pt;"> ' Очистим старую карточку</p>
<p style="font-size:12pt;"> w1.Selection.WholeStory</p>
