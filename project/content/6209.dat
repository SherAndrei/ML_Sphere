                    (function($){
                        var position = 0,
                            visible = 3,
                            count = $('.b-product__additional-image').length,
                            max_step = count-visible,
                            step = $('.b-product__additional-image').outerWidth(true),
                            duration = 500;

                        var next = $('.js-additional-next'),
                            prev = $('.js-additional-prev'),
                            block = $('.b-product__additional-images-container');


                        next.click(function(){
                            position++;
                            animate();
                        });

                        prev.click(function(){
                            position--;
                            animate();
                        });

                        function setDisable(pos){
                            if (pos == 0) {
                                prev.hide();
                                next.show();
                            } else if (pos == max_step) {
                                next.hide();
                                prev.show();
                            } else {
                                next.show();
                                prev.show();
                            }
                        }

                        function getPosition(){
                            position = position <= 0 ? 0 : position;
                            position = position >= max_step ? max_step : position;
                            setDisable(position);
                            return -position * step;
                        }

                        function animate(){
                            block.stop().animate({marginLeft: getPosition()}, duration);
                        }
                    })(jQuery);
                
