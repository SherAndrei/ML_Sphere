<script>
    window.testLang = function(lang) {
        var langs = [];
        var url = window.location.href;

        if(lang == 'close') {
            var wins = document.getElementsByClassName('langFrame');
            _.each(wins,function(i,e){
                document.body.removeChild(i);
            });
            return;
        }

        if(_.isUndefined(lang)) {
            $('.country-selector li').each(function(i,e){
                langs.push($(e).data('model').code);
            });
        } else {
            langs.push(lang);
        }
        _.each(langs,function(i,e){
            var win = document.createElement('IFRAME');
            win.src = url.replace('/en/', '/'+i+'/');
            win.width = window.innerWidth/langs.length;
            win.height = window.innerHeight/langs.length;
            win.className = win.className + " langFrame";
            document.body.appendChild(win);
        });
    }

</script>
