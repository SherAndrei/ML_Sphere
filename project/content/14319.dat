                            <input type="checkbox" id="1" class="star" name="p_rate" value="1"   disabled><label for="1">&nbsp;</label>
                    <input type="checkbox" id="2" class="star" name="p_rate" value="2"   disabled><label for="2">&nbsp;</label>
                    <input type="checkbox" id="3" class="star" name="p_rate" value="3"   disabled><label for="3">&nbsp;</label>
