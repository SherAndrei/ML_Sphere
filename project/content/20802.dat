    names += pers + "; ";
  }
  if($("#dpbeg").val()!=""){
    names += " c " + $("#dpbeg").val();
  }
  if($("#dpend").val()!=""){
    names += " по " + $("#dpend").val() + "; ";
  }

  if($("#cat_a").prop("checked")){
