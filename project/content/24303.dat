                  <IDXSKIP><td width="10" align="left" valign="top"><img src="/templates/Flexible/images/dlet_artblock_21.gif" width="10" height="23" /></td></IDXSKIP>
                  <td align="left" valign="top" class="a_block_22"><table width="100%" border="0" cellspacing="0" cellpadding="0">
