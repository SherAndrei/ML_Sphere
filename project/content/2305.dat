	<script type="text/javascript">
    
	$(function(){

        var p_interval = setInterval(function(){

            var mgidBlocks = $('.topic_info_wrapper [id^="MarketGidScriptRoot"] .mgline,.topic_info_wrapper [id^="M331394Composite596279"] .mgline');

            if (!mgidBlocks.length) return;

            if (mgidBlocks.eq(0).data('style')) return;

            mgidBlocks.eq(0).data('style',1);

            mgidBlocks.each(function(){
                try {
                    var title = $(this).find('.mctitle a');
                    var descr = $(this).find('.mcdesc a');
                    if (title) {
                        title.text(title.text().replace(' ...',''));
                    }
                    if (descr) {
                        descr.text(descr.text().replace(' ...',''));
                    }

                    var priceEl = $(this).find('div.mcprice');
                    if (priceEl.length) {
                        var divider = $('<div/>');
                        title.prepend(priceEl,divider);
                        if (priceEl.text().replace(/[\s\t]*/, '') == '') {
                            if (priceEl.find('.mcprice').length) {
                                priceEl.find('.mcprice').text('купить');
                            }
                        }
                        if(priceEl.find('.mcpriceold, .mcdiscount').length) {
                            priceEl.find('.mcpriceold, .mcdiscount').prependTo(divider);
                        }
                    }

                } catch(e){}
            });

            clearInterval(p_interval);

        },1000)
	});
	
