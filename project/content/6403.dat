	document.body.innerHTML = document.body.innerHTML.replace(eval("/"+textToFind+"/gi"),"<a name="+textToFind+" style='background:yellow'>"+textToFind+"</a>"); 
	lastResFind=textToFind; 
	
	window.document.getElementById('findname').value = textToFind;
	
	var ie = (document.all);
	if (!ie){ //заглушка для IE
		window.find(textToFind, false, false);
		window.find(textToFind, false, false);
	}
	else{
		window.location = '#'+textToFind;//перемещаем скрол к последнему найденному совпадению
	}
}

function Key_Up(e) {
	//заглушка для IE
	var ie = (document.all);
	if (ie){
		return;
	}
	//временная заглушка для IE
	
	if( !e ) {
		if( window.event ) {
			//Internet Explorer 8-
			e = window.event;
		}
		else {
			//total failure, we have no way of referencing the event
			return;
		}
	}
	
	var kCode = e.keyCode;
	if ( kCode==32 ){
//		var textToFind=window.document.getElementById('findname').value;
//		window.find(textToFind, false, false);
	}
	else if ( kCode==40 ){
		var textToFind=window.document.getElementById('findname').value;
		window.find(textToFind, false, false);
	}
	else if ( kCode==38 ){
		var textToFind=window.document.getElementById('findname').value;
		window.find(textToFind, false, true);
	}
	else if ( kCode==16 || kCode==17 || kCode==18 || kCode==45 ){
		var qw = document.getSelection();
		if ( qw!='' ){
			window.document.getElementById('findname').value = qw;
			window.find(qw, false, false);
		}
	}
}

document.onkeyup=Key_Up;

