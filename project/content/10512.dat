        <script type="text/javascript">
    $(document).ready(function() {
        var fform_wrapper = $('#org_feedback_form_wrapper');
        var fform = $('#org_feedback_form_wrapper form');

        $('.orgFeedback').click(function(event) {
            fform_wrapper.modal();

                        faction="/event/feedback/14091";
            
            fform_wrapper.find('form').attr('action', faction);

            return false;
        } );

        var showError = function(text) {
            fform.find('div.errors').empty().append($('<div class="alert alert-error">' + text + '</div>'));
            window.Recaptcha && Recaptcha.reload();
        } ;

        $('#org_feedback_form_wrapper form').submit(function(event) {
            var fdata = fform.serializeArray();
            $('#org_feedback_form_wrapper input, #org_feedback_form_wrapper textarea, #org_feedback_form_wrapper button').attr('disabled', 'disabled');
            $.post(fform.attr('action'), fdata)
                .success(function(data, textStatus, jqXHR) {
                    if (!data)
                        showError('Не удалось связаться с сервером');

                    else if (data.error)
                        showError(data.error);

                    else if (data.result) {
                        fform_wrapper.modal('hide');
                        bootbox.alert('Ваше сообщение успешно отправлено.');
                    }
                    else
                        showError('Произошла неизвестная ошибка при отправке, попробуйте еще.');
                })
                .error(function() {
                    showError('Произошла неизвестная ошибка при отправке, попробуйте еще.');
                })
                .complete(function() {
                    $('#org_feedback_form_wrapper input, #org_feedback_form_wrapper textarea, #org_feedback_form_wrapper button').removeAttr('disabled');
                });
            return false;
        } );
    } );
</script>

