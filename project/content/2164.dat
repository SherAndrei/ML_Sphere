		function setUnlimCookie(name) {
			document.cookie = ['show_tagline=', name, '; path=/'].join('');
		}

		function getUnlimCookie(name) {
			var verify = String("show_tagline=" + name);
			console.log(verify);
			return document.cookie.indexOf(verify) != -1;
		}

		function setCookie(name) {
			var date = new Date;
			date.setDate(date.getDate() + 1);
			document.cookie = "name=" + name + "; path=/; expires=" + date.toUTCString();
		}

		function getCookie(name) {
			var verify = String("name=" + name);
			console.log(document.cookie.indexOf(verify));
			return document.cookie.indexOf(verify) != -1;
		}


		window.onscroll = function () {
