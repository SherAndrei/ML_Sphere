<script type="text/javascript">
$(document).ready(function () {
  var city_popup = $.cookie('city-popup');
  if (city_popup == null) {
    $('#close').click(function () {
      $('#darkwindow').hide();
    });
    $('#geo_confirm').find('.another').click(function (e) {
      e.preventDefault();e.stopPropagation();
      $('#darkwindow').hide();
      $('#region_dropdown').trigger('click');
    });
    $.cookie('city-popup', '1', {expires: 30, path: '/'});
    $.ajax({
      url: '/geocity.php',
      dataType: 'json',
      success: function (data) {
        if (data.status == 1) {
          $('#geo_city').html(data.title);
          $('#geo_confirm').find('.ok').attr('href', '/changeregion.php?REGION=' + data.id);
          $('#darkwindow').show();
        }
        else if (data.status == 2) {
          $('#region_dropdown').trigger('click');
        }
      }
    });
  }
});
