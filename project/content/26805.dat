				var polling_id = $(this).data('id');
                var data = $(this).serializeArray();
                var option = [];
                for(var i in data) {
                    if(!data.hasOwnProperty(i)) continue;
                    if(data[i]['name'] == 'option') {
                        option = data[i]['value'];
                    }
                    if(data[i]['name'] == 'option[]') {
                        option.push(data[i]['value']);
                    }
                }
                var that = this;
                if(option) $.post($config.uri.ajax + '/pollings/answer/' + polling_id,
                    { option: option },
                    function(response){
	                    response = JSON.parse(response);
