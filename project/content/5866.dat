<script>
jQuery(document).ready(function($) {

  var winwidth = $(window).width();

  $(window).resize(function() {
      clearTimeout(this.id);
      this.id = setTimeout(doneResizing, 500);
  });

  function doneResizing(){
    if ($(window).width() != winwidth) {
      window.location.reload();
    }
  };

  if ( !$('.fix-dog').length && console) {
    console.log('".fix-dog" element not found, please check Sticky Element plugin settings');
    return;
  }
  if ( !$('#footer').length && console) {
    console.log('"#footer" element not found, please check Sticky Element plugin settings');
    return;
  }

  if (winwidth >= 900) {

    var el = $(".fix-dog");
    var elwidthpx = el.css('width');
    var elleftmar = el.css('margin-left');
    var eltop;
    var elheight;
    var elleft = el.offset().left;

    $('<div/>').attr('id', 'clone').css('width',elwidthpx).insertAfter( $('.fix-dog') );

    var style = document.createElement('style');
    style.type = 'text/css';
    style.innerHTML = '.sticky-element-fixed { position: fixed; top: 0; width: ' + elwidthpx + ' !important; }';
    document.getElementsByTagName('head')[0].appendChild(style);

    $(window).scroll(function() {

      if (typeof eltop === "undefined" ) {
        eltop = el.offset().top;
      }
      elheight = el.outerHeight();

      var end = $("#footer");
      endtop = end.offset().top;
      var winscroll = $(window).scrollTop();

      if (winscroll > eltop) {
        $(".fix-dog").addClass('sticky-element-fixed');
        $(".fix-dog").css("left", elleft);
        $(".fix-dog").css("margin-left", 0);
        $('#clone').css('height',elheight);
        $('#clone').css('width',elwidthpx);
      } else {
        $(".fix-dog").removeClass('sticky-element-fixed');
        $(".fix-dog").css("left", "auto");
        $(".fix-dog").css("margin-left", elleftmar);
        $('#clone').css('height',0);
        $('#clone').css('width',0);
      }

      if (winscroll + elheight > endtop) {
        var amount = endtop - (winscroll + elheight);
        $(".fix-dog").css("top", amount + "px");
      } else {
        var amount = endtop - (winscroll + elheight);
        $(".fix-dog").css("top", "");
      }

    });
  }

});
