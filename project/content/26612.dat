        <div class="post_date"><a  class="post_link"  href="/wall195252981_31"  onclick="return showWiki({w: 'wall195252981_31'}, false, event);" ><span class="rel_date">6 окт 2013</span></a></div>
        <div class="ui_actions_menu_wrap _ui_menu_wrap " onmouseover="uiActionsMenu.show(this);" onmouseout="uiActionsMenu.hide(this);">
