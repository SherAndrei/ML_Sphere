    var $overlay;

    function deeplink_do(curr_key) {
        var segments = [];
        var curr_segments = get_segments_from_deeplink($(document.location).attr('href').split("#")[1]);
        if (curr_key === undefined) segments = curr_segments;
        else segments[curr_key.split("=")[0]] = curr_key.split("=")[1];

        for(var key in segments){
            switch(key){
                case "carcass":
                                        $('#overlaywindow').openOverlay({sColor:'#FFFFFF',iOpacity:70});

                    $.ajax({
                        type: "POST",
                        url: "/ocenki/ajax_get_models_data/",
                        data: "brand=mercedes-benz&range=sprinter&mod=iii&carcass_id="+segments[key],
                        success: function(data){
                            $('#models_list').empty();
                            $('#models_list').append(data);
                                                            setTimeout(function(){ deeplink_do('comsort'); }, 200);
                                                    },
                        error: function(jqXHR, textStatus, errorThrown){
                            $.fn.openOverlay('close');
                        }
                    });
                                        break;
                                                case "compg":
                case "comfilter":
                case "comsort":
                    var filter_type = $("select[name=comments_filter] option:selected").val();
                    var sort = $("select[name=comments_sort] option:selected").val();

                    $('#overlaywindow').openOverlay({sColor:'#FFFFFF',iOpacity:70});

                    $.ajax({
                        type: "POST",
                        url: "/ocenki/ajax_get_carscores_comments/",
                        data: "filter_type="+filter_type+"&sort="+sort+"&mod_id=1306&page="+curr_segments['compg']+((curr_segments['carcass'] != undefined && curr_segments['carcass'] > 0) ? "&carcass_id="+curr_segments['carcass'] : ""),
                        success: function(data){
                            $('#score_comments').empty();
                            $('#score_comments').append(data);
                            $.fn.openOverlay('close');
                        },
                        error: function(jqXHR, textStatus, errorThrown){
                            $.fn.openOverlay('close');
                        }
                    });
                    break;
                            }
        }

        //alert(location.href);
    }

    function deeplink(segment, url) {
        if (url === undefined) url = "";
        var curr_url = $(document.location).attr('href').split("#")[0];
        var curr_segments = get_segments_from_deeplink($(document.location).attr('href').split("#")[1]);
        curr_segments[segment.split("=")[0]] = segment.split("=")[1];
        var dpl = get_deeplink_from_segments(curr_segments);
        $(document.location).attr('href', (url == "" ? curr_url : url) + "#" + dpl);
        setTimeout(function(){ deeplink_do(segment); }, 200);
    }

    function get_segments_from_deeplink(link){
        if (link === undefined || link === "") return [];
        var curr_segments = link.split('&');
        var sgm = [];
        for(var i=0; i<curr_segments.length; i++){
            sgm[curr_segments[i].split('=')[0]] = curr_segments[i].split('=')[1];
        }
        return sgm;
    }

    function get_deeplink_from_segments(segments){
        if (segments === undefined || segments.lenght == 0) return "";
        var dpl = "";
        for(var key in segments){
            dpl += (dpl != "" ? "&" : "")+ key + "=" + segments[key];
        }
        return dpl;
    }

    $(document).ready(function(){
                $('#pieBuyAgain').sparkline([24,23], {type: 'pie', width:'44px', height: '44px', offset: 90, sliceColors: ['#E68A05','#68AFCC']});
        
        /*Разворачивание превью*/
        if($('.previewdiva').height()){
            $('.previewdiva').css({display:'block'});
            if($('.previewdiva').find('p').length){
                var height_val=$('.previewdiva').height()+6;
                $('.previewdiv').css({height:height_val+'px',overflow:'hidden'});
            } else {
                $('.previewdiv').css({height:$('.previewdiva').height()+'px',overflow:'hidden'});
            }
        }
        $('.previewdiva a').click(function(){
            $('.previewdiva').css({display:'none'});
            $('.previewdiv').css({height:'auto',overflow:'inherit'});
        });

        $(document.body).append('<div id="overlaywindow" style="display:none;"></div>');

        $("select[name=comments_filter]").change(function () {
          deeplink("comfilter="+$("select[name=comments_filter] option:selected").val());
        });

        $("select[name=comments_sort]").change(function () {
          deeplink("comsort="+$("select[name=comments_sort] option:selected").val());
        });
        $('a.jt').cluetip({
            activation: 'click',
            arrows: true,
            dropShadow: false,
            hoverIntent: true,
            sticky: true,
            mouseOutClose: false,
            closePosition: 'title',
            closeText: '[ X ]',
            width: '290px',
            local: true, hideLocal: true
        });
        
        deeplink_do();
    });
</script>
