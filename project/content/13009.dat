       // настройка модального окна
       $( "#dialog" ).dialog({
            autoOpen: false,
            show: "slide",
            hide: "explode",
            modal: true,
            height: '240',
            width: '400'
        });
         $( "#dialog_mes" ).dialog({
            autoOpen: false,
            modal: true,
            height: '100',
            width: '360'
        });

        // вызов окна
        $( "#mes_error" ).click(function() {
            $('#mse1').val($('#co_name').text()); // имя
            $('#mse2').val($('#co_email').text()); // email
        	$("#dialog").dialog("open");
        	return false;
        });
        $( "#mes_dialog" ).click(function() {
        	$("#dialog_mes").dialog("open");
        	return false;
        });

        // закрытие окна
        $('#mes_er_close').click(function(){
            $("#dialog").dialog("close");
            return false;
        });
        $('#dial_mes_close').click(function(){
            $("#dialog_mes").dialog("close");
            $('#text_mes').text('');
            return false;
        });

        // отправка сообщения
        $('#mes_er_send').click(function(){
            var names = $('#mse1').val(); // имя
            var emails = $('#mse2').val(); // email
            var texts = $('#mse3').val(); // текст сообщения
            var urls = document.location.href; // url текущей страницы
            if(emails.length > 0) {
                if(!isValidEmail(emails)) {
                    $('#text_mes').text($('#mes_err_email').text());
                    $("#dialog_mes").dialog("open");
                    return false;
                }
            }
            if($.trim(texts).length == 0) {
                $('#text_mes').text($('#mes_err_text').text());
                $("#dialog_mes").dialog("open");
            } else {
                $.ajax({
                    type: 'POST',
                    url: '/carscores/ajax_mes_error/',
                    dataType: 'json',
                    data: {
                        nam: names,
                        ema: emails,
                        tex: texts,
                        url: urls
                    },
                    success: function(data) {
                        console.log(data); // см. консоль JavaScript в браузере

                        $('#load_mes_er').fadeOut(500,function(){
                            $('#load_mes_er').remove();

                            $('#text_mes').text(data.mes);
                            $("#dialog_mes").dialog("open");

                            // очистка полей ввода
                            $('#mse1').val($('#co_name').text()); // имя
                            $('#mse2').val($('#co_email').text()); // email
                            $('#mse3').val(''); // текст сообщения

                            $("#dialog").dialog("close");
                        });

                    },
                    beforeSend:function(data){
                        $('#mes_er_send').before('<img id="load_mes_er" style="margin:9px 0 0 10px; float:right;" src="/images/load.gif"/>');
                    }
                });
            }
            return false;
        });

    });

    function isValidEmail(email) {
        return (/^([a-z0-9_\-]+\.)*[a-z0-9_\-]+@([a-z0-9][a-z0-9\-]*[a-z0-9]\.)+[a-z]{2,4}$/i).test(email);
    }
</script>
