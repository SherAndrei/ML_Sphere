<script type="text/javascript">
    var videoWidgetFitHeight = function(iWinWidth, oFrame)
    {
        var oDoc = jQuery(oFrame.get(0).contentDocument ? oFrame.get(0).contentDocument : oFrame.get(0).contentWindow.document)
            , oBody = oDoc.find('body')
            , oVideoBlock = oDoc.find('.b-video').children()
            , oFrame = jQuery(oFrame)
            , oBlockVrez = oFrame.closest('.b-vrez')
            , bIsVideoHalf = oBlockVrez.hasClass('b-vrez_half')
            ;

        var iFitWidth = oBody.width()
            , iFitHeight = iFitWidth / 1.77
            ;

        if ( oVideoBlock.size() )
        {
            if ( !oVideoBlock.data('oldWidth') )
            {
                oVideoBlock.data('oldWidth', oVideoBlock.css('width'));
                oVideoBlock.data('oldHeight', oVideoBlock.css('height'));
                oFrame.data('oldMinHeight', oFrame.css('min-height'));
                oBlockVrez.data('oldMaxWidth', oBlockVrez.css('max-width'));
            }

            if (iWinWidth<767)// if mobile
            {
                oFrame.css({
                    height: iFitHeight,
                    minHeight:0
                });
                oVideoBlock.css({
                    width: iFitWidth,
                    height: iFitHeight
                });

                if (bIsVideoHalf == true)
                {
                    oBlockVrez.css('maxWidth', '100%');
                }
            }
            else
            {
                oVideoBlock.css({
                    'width': oVideoBlock.data('oldWidth'),
                    'height': oVideoBlock.data('oldHeight')
                });
                oFrame.css('min-height', oFrame.data('oldMinHeight') );
                oBlockVrez.css('maxWidth', oBlockVrez.data('oldMaxWidth'));
            }
        }
    }

    // each iframe iterator
    var eachVideoWidgetFitHeight = function(combineCallback)
    {
        jQuery('.b-material .b-video-widget:not(.b-video-widget_youtube) .video-wrapper>iframe').each(function()
        {
            combineCallback(jQuery(this));
        });
    }

    jQuery(function()
    {
        var iWinWidth = jQuery(window).width();
        // fit height onload each iframe
        eachVideoWidgetFitHeight(function(oFrame)
        {
            oFrame.on('load.fit_videos_size', function()
            {
                // console.log('frame loaded', oFrame);
                setTimeout(function()
                {
                    videoWidgetFitHeight(iWinWidth, oFrame);
                }, 1000);
            });

        });

        // fit iframes on window resize
        jQuery(window).off('.fit_videos_size_on_resize').on('resize.fit_videos_size_on_resize', function()
        {
            setTimeout(function()
            {
                eachVideoWidgetFitHeight(function(oFrame)
                {
                    videoWidgetFitHeight(jQuery(window).width(), oFrame);
                });
            }, 100);
        });

        jQuery(window).on('load.fit_videos_size_on_load', function()
        {
            eachVideoWidgetFitHeight(function(oFrame)
            {
                videoWidgetFitHeight(jQuery(window).width(), oFrame);
            });
        });
    });
</script>
