					$(".jspVerticalBar").fadeIn(500);
				},
				function() {
					$(".jspVerticalBar").fadeOut(500);
				}
			);
			useIdleTimer();
		});

		function updateActivity(){
		  if ( true || $('#uactivity_tab1').is(':visible') ) {
