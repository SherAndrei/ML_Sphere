					 //api.reinitialise();
				}
			});			
		  } // end if
		}

		//window.setInterval(updateActivity, 40000);

		function updateSiteActivity(){ // TODO: when updateActivity must need : make it functional in here
		  //if ( $('#uactivity_tab2').is(':visible') ) {
			if( 'idle'===$.data(document,'idleTimer') ) return;
			var la = $("#uactivity_tab2 div.wrappingtable:first").attr('id');
			// if previous ajax request are already pending - if lastActivity is changed - then abort it, else do nothing: keep those request pending
			if( updateSiteActivity.ajax && updateSiteActivity.ajax.abort ){
				if( updateSiteActivity.la === la ) return;
				updateSiteActivity.ajax.abort
			}
			var ajax = $.get('/activity/getsiteevents/',{lastactivity: la},function(data){
				if (data){
					$("#scroll2 .jspPane").prepend(data);
					$('.newsiteactivity').fadeIn(1000, function(){ 
