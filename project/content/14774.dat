	<script type="text/javascript">
		var iii=0,
		items = [];
		function itemsperrow(args) {
				if ( $(window).width() < 441 ) return 1;
				if ( $(window).width() < 577 ) return 2;
				if ( $(window).width() < 972 ) return 3;
				if ( $(window).width() > 971 ) return 4;
		}

		$(window).bind("resize",function(e){
			e.preventDefault();
			onres(1);
		});


		var button = $('.mix-wrap').next();

		button.click(function(){
				$('#hiddenarticles').show();
				button.hide();
		});

		function onres(ii){
			button = $('.mix-wrap').next();
			var item_per_row = itemsperrow(),relap_count = $(".relap__item a").length;
			$('#hiddenarticles:not(:empty)').children().each(function () {
				var element = $(this).detach();
				$('.mix-wrap').append(element);
			});
			$('.mix-wrap').children().each(function () {
				if ( ii > ( item_per_row * 2 - relap_count ) ) {
					$('#hiddenarticles').append(this);
				}
				ii++;
			});
			if ( ii > ( item_per_row * 2 - relap_count ) ) {
				if ( 1 > ( ii - 1 - ( item_per_row * 2 - relap_count ) ) ) {
					button.children().html("");
					button.hide();
				} else {
					button.children().html("+" + ( ii - 1 - ( item_per_row * 2 - relap_count ) ) );
					$('#hiddenarticles').hide();
					button.show();
				}
			} else {
				button.children().html("");
				button.hide();
			}
			return ii;
		}

		  
	</script>


