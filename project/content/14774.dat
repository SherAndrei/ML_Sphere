        }
        else {
            $('.local-like').show();
        }
        if (isNaN(count)) {
            $('.local-like').hide();
        }
        $('.bLikeInner').on('click', function (e) {
            e.preventDefault();
            if ($('.bLike').hasClass('bLikeClick')) {
                count++;
            }
            else {
                count--;
            }
            $('.local-like').text(count).show();
            if (count == '0') {
                $('.local-like').hide();
            }
            else {
                $('.local-like').show();
            }
            if (isNaN(count)) {
                $('.local-like').hide();
            }
        });
    }    
});
</script>
