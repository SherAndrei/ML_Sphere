<script>
    var commonListOptionValue = -1;

    var idRegExp = new RegExp('^[^_]+_(\\d+)_(\\d+)_(\\d+)');
    
    function GetBookmarkData(nameAttributeValue) {
        var matches = idRegExp.exec(nameAttributeValue);
        if (!matches || matches.length < 4) return null;
        return [matches[1], matches[2], matches[3]];
    }
    
    function GetTitleElements() {
        return $(".js-folder-title");
    }

    function BookmarkForm_CheckFolderExists(folderTitle, titleTextBoxId) {

        var trimmedTitle = $.trim(folderTitle);

        var titleElements = GetTitleElements();

        var exists = false;
        for (var i = 0; i < titleElements.length; i++) {
            if ($(titleElements[i]).html() == trimmedTitle)
                exists = true;
        }

        BookmarkManager.clearWarning(titleTextBoxId);

        if (exists) {
            $('.js-folder-comment').html('Папка с таким названием уже существует');
        }
    }
    
    var firstShowBookmarkForm = true;
    
    function ShowBookmarkForm(url, successHandler) {
        $('.js-folder-comment').html('');
        var titleTextBox = $('#Form_BookmarkFolderTitle');
        $('#NewBookmarkFolderTitle').hide();
        LightBox.show('bookmarkForm', {
            AfterShow: function () {
                titleTextBox.select();
                titleTextBox.focus();

                BindTextChangeEventHandler('Form_BookmarkFolderTitle', function () {
                    if (titleTextBox.val()) {
                        $('#BookmarkFormButton').removeAttr('disabled');
                        BookmarkForm_CheckFolderExists(titleTextBox.val(), titleTextBox.attr('id'));
                    } else {
                        BookmarkManager.clearWarning('Form_BookmarkFolderTitle');
                        $('.js-folder-comment').html('Придумайте название папки');
                        $('#BookmarkFormButton').attr('disabled', true);
                    }
                });

                $('#NewFolderLink').click(function(){
                    if ($('#NewBookmarkFolderTitle').is(':visible'))
                    {
                        $('#BookmarkFolder-1').attr('checked', true);
                        $(".js-folder-title[for='BookmarkFolder-1']").addClass('bookmarks-add-on_item__active');
                    }
                    else
                    {
                        $('#defaultSelection').attr('checked', true);   
                        $(".js-folder-title").removeClass('bookmarks-add-on_item__active');
                    }
                    $('#NewBookmarkFolderTitle').toggle();
                    $(this).toggleClass('bookmarks-add-on_new-folder__active');
                    $('.js-folder-title').removeAttr('checked');     
                    if (titleTextBox.val() == '')
                        $('#BookmarkFormButton').attr('disabled', true);                  
                }).removeClass('bookmarks-add-on_new-folder__active');

                $('.bookmarks-add-on_list').on('dblclick', '.js-folder-title', function() {
                    var elementId = $(this).attr('for');
                    var element = $('#'+elementId);
                    element.attr('checked', true);
                    $('#NewBookmarkFolderTitle').hide();
                    var data = form.serializeArray();
                    $.ajax({
                        type: "POST",
                        url: url,
                        data: data,
                        dataType: "json",
                        contentType: "application/x-www-form-urlencoded; charset=utf-8",
                        success: function (data) {
                            if (successHandler)
                                successHandler(data);
                        }
                    });
                    return false;
                });

                $('.bookmarks-add-on_list').on('click', '.js-folder-title', function() {
                    var elementId = $(this).attr('for');
                    var element = $('#'+elementId);
                    element.attr('checked', true);
                    $('#NewBookmarkFolderTitle').hide();
                    $('#BookmarkFormButton').removeAttr('disabled');
                    return false;
                });
                if (firstShowBookmarkForm) {
                    firstShowBookmarkForm = false;
                    $.post('/ajax/mypage/bookmarks/bookmarkFoldersAsync', function(data) {
                        $('.js-bookmark-folders-container').removeClass('bookmarks-add-on_loading').html(data);
                        titleTextBox.val(BookmarkManager.generateFolderName(GetTitleElements()));
                        $('.bookmarks-add-on__new-folder').css('visibility','visible');
                    });
                }

                var holder = $('#bookmarkForm');
                var form = holder.find('form');

                InitPostSimpleFormAsync(form, {
                    url: url,
                    useDefaultSuccessMessageShowing: false,
                    successCallback: function (data) {
                        if (successHandler)
                            successHandler(data);
                    },
                    beforeSendCallback: function (form) {
                        form.addClass("animate");
                    }
                });
            }
        });

        $('.bookmarks-add-on_list').on('click', '.js-folder-title', function() {
            $('.js-folder-title').removeClass('bookmarks-add-on_item__active');
            $(this).addClass('bookmarks-add-on_item__active');
            return false;
        });
    }

    function ShowAddBookmarkForm(successHandler) {
        $('#bookmarkFormTitle').html('Добавление закладки');
        $('#BookmarkFormButton').val('Добавить');
        ShowBookmarkForm('/ajax/mypage/bookmarks/addBookmarkAsync', successHandler);
    }

    function CancelMoveBookmark(cancelLink) {
        var bookmarkId = $(cancelLink).attr('data-id');
        var listItem = $(".js-bookmark[data-id='" + bookmarkId + "']");

        var form = $('#bookmarkForm form');
        
        var formData = {};
        formData['Form.CurrentBookmarkFolderId'] = '0';
        var currentFolderId = form.find('#Form_CurrentBookmarkFolderId').val();
        if (currentFolderId == 0) currentFolderId = -1;
        formData['Form.BookmarkFolderId'] = currentFolderId;
        formData['Form.BookmarkId'] = bookmarkId;

        $.post('/ajax/mypage/bookmarks/moveBookmarkAsync', formData, 
            function(data){
                var bookmarkHtml = listItem.find("div[name='movedContent']").html();
                listItem.html(bookmarkHtml);

                listItem.find('.js-move-bookmark').click(function () {
                    ShowMoveBookmarkForm($(this));
                    return false;
                });

                listItem.find('.del a').click(function () {
                    DeleteBookmark(this);
                    return false;
                });
                listItem.removeClass('bookmarks-unsort_item__restore');
            });
    }

    function ShowMoveBookmarkForm(link){
        $('#bookmarkFormTitle').html('Перемещение закладки');
        $('#BookmarkFormButton').val('Переместить');

        var bookmarkData = GetBookmarkData(link.attr('name'));

        $('#Form_BookmarkId').val(bookmarkData[0]);
        $('#Form_ItemId').val(bookmarkData[1]);
        $('#Form_TypeId').val(bookmarkData[2]);

        var bookmarkId  = link.attr('data-id');

        ShowBookmarkForm('/ajax/mypage/bookmarks/moveBookmarkAsync', function(data){
            if (data.Failed) return;

            var movedBookmarkElement = $('#movedBookmarkTemplate').clone(true);
            movedBookmarkElement.removeAttr('id');
            movedBookmarkElement.removeClass('b-hide');
            movedBookmarkElement.find('a').attr('data-id', bookmarkId);
            movedBookmarkElement.show();

            var titlePrefixElement = movedBookmarkElement.find('.js-moved-bookmark-title-prefix');
            if (data.ToCommonList)
                titlePrefixElement.html('общий список ');
            else {
                titlePrefixElement.html('папку ');
                movedBookmarkElement.find('.js-moved-bookmark-title').remove();
                titlePrefixElement.after('<a href="'+data.FolderUrl+'" class="js-moved-bookmark-title">'+data.FolderTitle+'</a>');
            }
            
            var listItem = $(".js-bookmark[data-id='" + bookmarkId + "']");

            var oldHtml = listItem.html();
            listItem.html(movedBookmarkElement);
            listItem.append("<div class='b-hide' name='movedContent'>"+oldHtml+"</div>");
            listItem.addClass('bookmarks-unsort_item__restore');
            LightBox.hide();
        });
    }
</script>

