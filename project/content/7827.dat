    (function($, page_place){
        var ga = require('Metrics').ga;
        function thanks(){
             $("#survey_negative_form").hide();
             $("#survey_positive_form").hide();
             $("#survey_ask").hide();
             $("#survey_thanks").show();
        }
        $("#survey-yes").click(function(){
            ga.track_event_at("catalog_survey", 'button_yes_click', page_place);
        });
        $("#survey-no").click(function(){
            ga.track_event_at("catalog_survey", 'button_no_click', page_place);
        });
        $("#negative_survey").click(function(e){
            var selection = $("#survey_negative_form input:checked");
            var response = $("#negative_survey_description").val();
            var support_link = $('#support_link');
            var support_href = selection.data('support_href');
            var action = "negative_response";
            if (!!selection.val()) {
                action = action + "-" + selection.val();
            }
            if (!!response) {
                action = action + '/' + response;
            }
            if (!!support_href) {
                support_link.attr("href", support_href);
                support_link.text(selection.data("support_text"));
            }
            ga.track_event_at("catalog_survey", action, page_place);
            thanks();
            return false;
        });
        $("#positive_survey").click(function(e){
            var response = $("#positive_survey_description").val();
            var action = "positive_response";
            if (!!response) {
                action = action + '/' + response;
            }
            ga.track_event_at("catalog_survey", action, page_place);
            thanks();
            return false;
        });
    })(jQuery, "portal-product_catalog-products");
    

        var ajax_indicator_stop_words = ['static-cache','pv.gif', 'events.gif', 'ev.gif', 'track_infomer_views'];
        var is_ajax_indicator_disabled = function (url) {
            if (SW.c["disable_ajax_indicator"]) {
                return true
            }
            if(!url){
                return false
            }
            for(var i=0; i < ajax_indicator_stop_words.length; i++){
                if (url.indexOf(ajax_indicator_stop_words[i]) != -1){
                    return true
                }
            }
            return false
        }
        $("<div></div>", {class: "b-ajax-loading js-ajax-loading", style: "display: none;"}).appendTo($("body"));

        $(".js-ajax-loading").data('ajax_stack', 0)
        .ajaxSend(function (event, xhr, options) {
            var $this = $(this);
            if (!is_ajax_indicator_disabled(options.url)) {
                if (options.loading) {
                    $(options.loading).css("opacity", 0.7);
                    offset = $(options.loading).offset();
                    w = $(options.loading).outerWidth();
                    h = $(options.loading).outerHeight();
                    $this.css({position: "absolute", margin: 0, width: w, height: h, left: offset.left, top: offset.top, background: "none"});
                }
                $this.slideDown('fast');
                $this.data('ajax_stack', $this.data('ajax_stack') + 1);
            };
        })
        .ajaxComplete(function (event, xhr, options) {
            var $this = $(this);
            if (!is_ajax_indicator_disabled(options.url)) {
                if(options.loading)
                    $(options.loading).css("opacity", 1);

                $this.data('ajax_stack', $this.data('ajax_stack') - 1);
                setTimeout(
                    function() {
                        if ($this.data('ajax_stack') <= 0) {
                            $this.slideUp('fast');
                        };
                    },
                    100);
            };
        });

    
