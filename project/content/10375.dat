// Отображение формы добавления отзыва
function showReviewForm(type) {
 
    if(action_reviews_day_limit) {
        $('#action_reviews_day_limit_popup').show();
        return false;
    }    
      
    $('.t2_entity__review-form').removeClass('hidden');
    
    tt.showTopCheckrates = false;
    topCheckratesOffset('#writeReview');    

    if(type==undefined) type=$('#type').val();

    if(type=='audio') {
        $('.writeAudioReview').hide(); 
        $('.writeReview').show();
        window.location.hash='#writeAudioReview';          
        
        $('#audio_vidjets,#error_audio_vidjets').show();
        
        if($('#audiofile').val()!='') {
            initAudioPlayer('audio_player', {url: '/gallery/tmp/audio/'+$('#audiofile').val()});            
            $('#audio_player_remove').show();
        } else {
            initAudioRecorder('audio_recorder');
        }
        
        $('#review_detail,#review_detail_help,#s2id_purpose,#error_text').hide();
        $('#error_text').next('br').hide();                
    } else {
        $('.writeAudioReview').show();
        $('.writeReview').hide();
        window.location.hash='#writeReview';          
        
        $('#review_detail,#review_detail_help,#s2id_purpose,#error_text').show();
        $('#audio_vidjets,#error_audio_vidjets,#audio_player_remove').hide();          
        $('#audio_vidjets').next('br').hide();        
    }    


    $('#type').val(type);   
    
}

</script>
