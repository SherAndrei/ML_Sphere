var alreadyloading = false;

function showReviewsMore() {
    var current_page = +$("#reviews_current_page").val();
    if (!tt.pageNumToLoad) {
        tt.pageNumToLoad = 3;
    } else {
        if( true ) {
            if(--tt.pageNumToLoad == 0) {
                var nowUrl = window.location.href;
                if(/\?page=\d+/.test(nowUrl)) {
                    window.location.href =  nowUrl.replace(/page=\d+/, 'page=' + (current_page + 1));
                } else {
                    if( /rorder=/.test( nowUrl ))
                        window.location.href = nowUrl + '&page=' + (current_page + 1);
                    else
                        window.location.href = nowUrl + '?page=' + (current_page + 1);
                }
                return;
            }
        } else {
            if(--tt.pageNumToLoad == 0) {
                //var nowUrl = window.location.href.replace(/\/information/ , '/otzyvy');
                var nowUrl = window.location.href+'/otzyvy';                
                if( /rorder=/.test( nowUrl ) ) {
                    window.location.href =  nowUrl + '&page=' + ( (current_page ) / 2 + 1 ) + "#reviews_list" ;
                } else {
                    window.location.href =  nowUrl + '?page=' + ( (current_page ) / 2 + 1 ) + "#reviews_list" ;
                }
            }
        }
    }
    $(".t2_entity__reviews-paginator_current").remove();
    $(".t2_entity__reviews-paginator").remove();
    $.getJSON('/trips/morereviews',
        {
            entity:'attractions',
            page: current_page,
            entity_id: 12260,
            rorder:'date_desc',
            perpage:'20',
            newdesign : true
        },
        function (json) {
            $('#reviews_loading').hide();
            if (json && json.data) {
                $("#reviews_list").append(json.data);
                $("#reviews_list .t2_entity__review--share").each(function() {
                    reviewShareBoxDraw(this);
                });
                $("#reviews_current_page").val(json.page);
                if (json.has_more)
                    alreadyloading = false;
                else
                    $(".t2_entity__reviews_show-more--btn").hide();
                if (window.history && history.pushState && true) {
                    var nowUrl = window.location.href,
                        newUrl,
                        nextUrl;
                    if(/page=\d+/.test(nowUrl)) {
                        newUrl =  nowUrl.replace(/page=\d+/, 'page=' + json.page);
                        nextUrl =  nowUrl.replace(/page=\d+/, 'page=' + (json.page + 1));
                    } else {
                        if( /rorder=/.test( nowUrl )) {
                            newUrl = nowUrl + '&page=' + json.page;
                            nextUrl = nowUrl + '&page=' + (json.page + 1);
                        } else {
                            newUrl = nowUrl + '?page=' + json.page;
                            nextUrl = nowUrl + '?page=' + (json.page + 1);
                        }
                    }
                    history.pushState( null, null, newUrl );
                    $('link[rel=prev]').attr("href", nowUrl);
                    if (json.has_more)
                        $('link[rel=next]').attr("href", nextUrl);
                    else
                        $('link[rel=next]').remove();
                }
                $(".js_active").removeClass("js_active");
            }
        }
    );
}



