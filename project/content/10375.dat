<script>
    // Смена Причины жалобы
    function whinesChangeReason(obj) {     
       
        var value=obj.val();
        
        if(value=='Отзыв не соответствует требованиям') {
            var text='Если вы считаете, что этот отзыв нарушает <a href="/agreement" target="_blank">правила размещения</a> отзывов на TravelTipz.ru, отправьте нам сообщение  c указанием причины. Ваша жалоба будет рассмотрена в ближайшее время. Спасибо!';
        } else if(value=='Подозрительный отзыв') {
            var text='Если  вы  считаете, что этот отзыв не является достоверным, отправьте нам сообщение с подробным описанием  причины. Ваша жалоба будет рассмотрена в ближайшее время. Спасибо!';
        } else {
            var text='Если вы знаете, к  какому объекту относится данный отзыв, пожалуйста, укажите правильную ссылку. Или укажите причину, по которой отзыв не соответствует  данному объекту. Ваша жалоба будет рассмотрена в ближайшее время. Спасибо!';
        }
        $('#whines_reason_note').html(text);                
    }    
    
    // Сохранение жалобы
    function whinesSave() {
       
        $('#whines_form').showLoading();    
        
        $.ajax({
            dataType: 'json',
            type: 'POST',
            data: $("#whines_form").serialize(),                                   
            url: '/review/whine-new',                    
            success: function(data) {
                                                    
               $('#whines_form').hideLoading();     
               $('#whines_form div.error-msg--after-input').remove();
                
               if(data.errors!=null) {                        
                    $.each(data.errors, function(index, item) {                                                                    
                        $('#whines_'+index).after('<div class="error-msg--after-input">'+item+'</div>');
                    });
               } else {
                   $.magnificPopup.close();
               }
            }
        });       
         
    }
                   
    
    
</script>
