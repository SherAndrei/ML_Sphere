<script>
require(['jquery'], function($){
	var $node = $('#socialNetwork');
	var $list = $('ul.feedbacks', $node);
	var $showMoreBox = $('.js-show-more-posts-box', $node);
	var $showMoreButton = $('.js-show-more-posts', $node);
	var $loader = $('.js-loader', $node);
	var count = 100;
	var remain = 91;
	var page_size = 100;
	
	$showMoreButton.on('click', function(e){
		e.preventDefault();
		
		$showMoreButton.prop('disabled', true);
		
		$.ajax({
			url: '/js.php',
			type: 'post',
			data: {
				area: 'service', 
				action: 'SocialList' ,
				owner_id: '5559e59540c0882b298d1168',
				source: 'vk',
				strip: 550,
				skip: count,
				limit: page_size,
			},
			beforeSend: function(){
				$loader.show();
			},
			success: function(R){
				if(!R.success) return;
				if(!R.items) return;
				
				$list.append(R.items);
				lazyload($list);

				count += page_size;
				remain -= page_size;

				if (remain > page_size) {
					$showMoreButton.show().text('–ü–æ–∫–∞–∑–∞—Ç—å –µ—â—ë ' + page_size + ' –∏–∑ ' + remain + ' ' + plural(remain, '–∑–∞–ø–∏—Å–µ–π', '–∑–∞–ø–∏—Å–∏', '–∑–∞–ø–∏—Å–µ–π'));

				} else if (remain > 0) {
					$showMoreButton.show().text('–ü–æ–∫–∞–∑–∞—Ç—å –µ—â—ë ' + remain + ' ' + plural(remain, '–∑–∞–ø–∏—Å–µ–π', '–∑–∞–ø–∏—Å—å', '–∑–∞–ø–∏—Å–∏'));

				} else {
					$showMoreBox.remove();
				}
			},
			complete: function(){
				$loader.hide();
			}
		});
	});
	
	lazyload($list);

	require(['js2/CommentsExtended'], function(CommentsExtended){
		new CommentsExtended(document.getElementById('socialNetwork'));
	});
