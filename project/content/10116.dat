<script type="text/javascript">
    (function ($) {
        var comments = {
            perPage: 15,

            updateAllComments: function (html) {
                var $container = $('#comments');
                $container.replaceWith(html);
                comments.handlePagination();
            },

            scrollToComments: function() {
                var $container = $('#comments');
                var scrollOffset = $container.offset().top;
                $('html, body').animate({
                    scrollTop: scrollOffset
                }, 700);
            },

            refresh: function () {
                comments.loadComments(comments.getCurrentPage());
            },

            getPageCount: function() {
                return $('a.page', '.pagination-number').length + 1;
            },

            updateComment: function (commentId) {
                var toSend = $('#comment-' + commentId + ' .message').html();

                $.ajax({
                    url: "/ajax/updateComment/",
                    type: "POST",
                    data: {
                        text: toSend,
                        id: commentId
                    },
                    success: function (editedComment) {
                        comments.cancelEditComment(commentId);
                        $('#comment-' + commentId + ' .message').html(editedComment);
                    }
                });

                return false;
            },

            editComment: function (commentId) {
                var container = $('#comment-' + commentId + ' .message');
                $('#edit-comment-buttons-' + commentId).removeClass('hidden');
                container.attr('contenteditable', true).addClass('edit-comment-active').focus();
                return false;
            },

            getCurrentPage: function () {
                return parseInt($('.page.current').text());
            },

            getCommentsCount: function () {
                return $('#com').children().length;
            },

            sendComment: function (btn, addComment) {
                $.ajax({
                    url: "/ajax/addComment/",
                    type: "POST",
                    data: {
                        text: addComment.val(),
                        'class': "PhotoForum",
                        entity_id: 27257                    },
                    success: function () {
                        btn.hide();
                        addComment.val("");
                        var page = comments.getPageCount();
                        var scroll = false;
                        if (comments.getCommentsCount() == comments.perPage) {
                            page++;
                            scroll = true;
                        }
                        comments.loadComments(page);
                        if (scroll) {
                            setTimeout(comments.scrollToComments, 1000);
                        }
                    }
                });
                return false;
            },


            showMobileComment: function () {
                $('.mobile-add-comment-block').toggle();
                return false;
            },

            showWindow: function (addComment) {
                $.ajax({
                    url: "/ajax/saveComment/",
                    type: "POST",
                    data: {
                        text: addComment.val(),
                        'class': "PhotoForum",
                        entity_id: 27257,
                        'currentLink': "/fotoforum/palto-nakidki/mo-vtoroe-paltishko--27257/"
                    },
                    success: function () {
                        var $userAuth = $('#user-auth');
                        $userAuth.modal().show();
                        $userAuth.on('hidden.bs.modal', function () {
                            $.ajax ({
                                url: "/ajax/destroySessions/",
                                type: "POST",
                                data: {
                                    'class': "PhotoForum",
                                    entity_id: 27257,
                                    text: addComment.val(),
                                    'currentLink': "/fotoforum/palto-nakidki/mo-vtoroe-paltishko--27257/"
                                }
                            })
                        });
                    }
                });
            },

            cancelEditComment: function (commentId) {
                $('#edit-comment-buttons-' + commentId).addClass('hidden');
                $('#comment-' + commentId + ' .message').removeClass('edit-comment-active').attr('contenteditable', false);
                return false;
            },

            loadComments: function (page) {
                $.ajax({
                    type: "POST",
                    url: "/ajax/loadcomments/?comment_page=" + page,
                    data: {
                        "id": 27257,
                        "class": "PhotoForum",
                        "currentLink": "/fotoforum/palto-nakidki/mo-vtoroe-paltishko--27257/"
                    },
                    success: comments.updateAllComments
                });
            },

            handlePagination: function () {
                $('.paginationControl ').on('click', 'a', function (e) {
                    var page;
                    var $this = $(this);
                    var urlParts = $this.attr('href').split("?comment_page=");
                    if (urlParts.length == 1) {
                        page = 1;
                    } else {
                        page = parseInt(urlParts[1]);
                    }
                    comments.loadComments(page);
                    comments.scrollToComments();
                    e.preventDefault();
                });
            }
        };

        window.burda = window.burda || {};
        burda.comments = comments;

        $(document).ready(function () {
            burda.comments.handlePagination();
                    });
    })(jQuery);


</script>
