	<script type="text/javascript">
	$(function() {
	
		$('#rpersons').change(function() {
			if ($(this).val() != '-')
			{
				$("#reserv_date #rdate").fadeIn();
			}
		});

		$(".rhour").click(function() {
			$("#hourdiscount").val($(this).attr('alt'));
			$("#daytime").val($(this).attr('time'));
			
			$("div").each(function (i) {
				$(this).removeClass('selected');
			});
			
			$(this).toggleClass("selected");
			$("#reserv_form").fadeIn();
		});

