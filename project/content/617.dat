                    $('.ch_usd_ex').html(data.usd.change_curr>0?"+"+data.usd.change_curr:data.usd.change_curr).addClass(change_usd);
                    $('.ch_eur_ex').html(data.eur.change_curr>0?"+"+data.eur.change_curr:data.eur.change_curr).addClass(change_eur);
