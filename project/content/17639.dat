<script type="text/javascript">
    var bull_id = 21206627;

    var myid = 0;
    var has_confirmed_email =  0;
    var has_confirmed_phone =  0;

    function DisplayLength(MesText) {

        var $this = $(MesText),
            rch = $('[data-ta-target="'+$this.data('ta')+'"]'),
            MaxLength = 1000,
            iOstatok = MaxLength - MesText.value.length,
            text_v = iOstatok,
            $questionLengthError = $('[data-hint="'+$this.data('ta')+'"]');

        rch.removeClass('b-text-red');

        if ( iOstatok > 20 ) {
            text_v = '<b>' + iOstatok + '</b>';
        } else {
            rch.addClass('b-text-red');
        }

        if ( $this.val().length > MaxLength ) {

            if ( !$questionLengthError.is(':visible') ) {
                $questionLengthError.css('display','');

            }
        } else {

            if ( $questionLengthError.is(':visible') ) {
                $questionLengthError.css('display','none')
            }
        }

        rch.empty();
        rch.html(text_v);

    }

    function focusQuestion() {
        $('#question_ta').focus();
    }

    function focusName() {
        $('#name_input').focus();
    }

    function focusEmail() {
        $('#email_input').focus();
    }

    function focusAnswer(question_id) {
        $('#answer_ta_question_'+question_id).focus();
    }

    $(document).on('click', '.askButton_js', function() {
        var blockWrapper = $(this).parents('div.aq_js:first'),
            questionWrapper = blockWrapper.find('div.questionWrapper:last'),
            $question_ta = $("#question_ta");


        var question_container  = $question_ta;
        var errorContainer      = $('[data-error-cont]:first', blockWrapper);
        var successContainer    = $('[data-success-cont]:first', blockWrapper);

        if ( $question_ta.val().length > 1000 ) {
            alert('Ваш вопрос получается слишком длинным. Пожалуйста, сократите текст.');
            $question_ta.focus();
            return false;
        }

        if ( /\?my/i.test($question_ta.val()) ) {
            alert("Вы указали ссылку, содержащую ?my. Она доступна только вам, продавец не сможет открыть ее. " +
                "Пожалуйста, откройте свое конкретное объявление, скопируйте его адрес и вставьте вместо этой ссылки.");
            return false;
        }

        var question = question_container.val();
        if ($.trim(question).length < 3) {
            alert('Задайте свой вопрос');
            focusQuestion();
            return;
        }

        errorContainer.html('');
        successContainer.html('');

        var _d = {
            question : question,
            bull_id  : bull_id
        };

                _d.name = $('#name_input', blockWrapper).val();
        
        if ($.isCrossDomainRequest) {
            _d['crossdomain_ajax_request'] = 2;
            _d['request'] = 'ajax_ask_question';
        }

        $.ajax ( {
            url: $.ajaxRequestDir + ($.isCrossDomainRequest ? '' : 'ajax_ask_question.php'),
            data: _d,
            dataType : 'json',
            type : 'POST',
            success: function(data) {
                var execute_callback = data.execute_callback,
                    error = parseInt(data.error),
                    question_id = parseInt(data.question_id),
                    user_message = data.user_message,
                    user_notice = data.user_notice,
                    name = data.name,
//                    phone = data.phone,
                    email_o = data.email,
                    asked_time = data.asked_time,
                    asked_question = data.asked_question,
                    questions = $('#own_questions');


                if (error == 1) {
                    alert(user_message);
                } else {
                    successContainer.html(user_message);
                    var qw = questionWrapper.clone();
                    qw.attr('question_id', question_id);
                    qw.attr('id', "question_" + question_id);
                    qw.find('span.question_asked:first').html(asked_question);
                    qw.find('[data-question-asker-name]').html(name);
                    qw.find('[data-question-asked-time]').html(asked_time);
                    qw.find('[data-messages]').data('messages', data.question_id);

                    $question_ta.val('');
                    $('.question_symbols_count_js').html('1000');

                    qw.append($(
                        '<br><div class="b-text-green b-text-green_size_s b-media-cont">Ваш вопрос добавлен!</div>'
                    ).css({'font-weight' : 'bold' }));

                    if ( user_notice.length > 0 ) {
                        qw.append($('<div>' + user_notice + '</div>').css({'color' : 'red'}));
                    }

                    if ( !questions.length ) {
                        questions = $('<div class="b-media-cont aq_js"></div>')
                            .attr('id', 'own_questions')
                            .addClass('aq_js')
                            .html($('<h3>Мои вопросы продавцу:</h3>')
                            .attr('id', 'own_questions_title'));

                        $('div.aq_js:first').before(questions);
                    }
                    questions.append(qw);

                    if (history !== undefined &&
                        typeof history.pushState === "function") {

                        var href = window.location.href;

                        if (href.indexOf("#") !== -1) {
                            href = href.substr(0, href.indexOf("#"))
                        }

                        history.pushState(null, null, href + "#question_" + question_id);
                    }

                    qw.find('[data-small]').attr('id', 'question_publicate_status_' + question_id);
                    qw.show();
                    $.scrollTo(qw, 800);
                    $('[data-ask-block]').remove();
                    $('#questions_title').html("Другие вопросы к продавцу:");
                }

                // execute callbacks passed from back-end
                execute_callback !== undefined && execute_callback.length && $(execute_callback).each(function() {
                    var exe = this.toString();
                    var fn = window[exe];
                    if (typeof fn == 'function') {
                        fn();
                    }
                });
            }
        });
    });

    
    
</script>

