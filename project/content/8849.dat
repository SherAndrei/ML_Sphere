            window.office_map = map;
            window.current_scale = 14;
            initSwiper();
        }
        else {
            document.addEventListener("DOMContentLoaded", function () {
                addMetrosOnMap(map);
                addOfficessOnMap(map);
                window.office_map = map;
                window.current_scale = 14;
                initSwiper();
            });
        }
    }

    function init_officemap() {
        if (!window.ymaps)
            return;

        if (typeof window.GLOBAL_arMapObjects['officemap'] !== "undefined")
            return;

        var node = BX("BX_YMAP_officemap");
        node.innerHTML = '';

        var map = window.GLOBAL_arMapObjects['officemap'] = new ymaps.Map(node, {
            center: [55.324257287546, 38.674009965332],
            zoom: 14,
            type: 'yandex#map'
        });

                if (map.behaviors.isEnabled("scrollZoom"))
            map.behaviors.disable("scrollZoom");
                if (map.behaviors.isEnabled("dblClickZoom"))
            map.behaviors.disable("dblClickZoom");
                map.behaviors.enable("drag");
                if (map.behaviors.isEnabled("rightMouseButtonMagnifier"))
            map.behaviors.disable("rightMouseButtonMagnifier");
                map.controls.add('zoomControl');
                map.controls.add('typeSelector');
                if (window.onMapReady) {
                        office__map            = map;
            window.onMapReady(map);
                    }
        
    }
    
    (function bx_ymaps_waiter() {
        if (typeof ymaps !== 'undefined')
            ymaps.ready(init_officemap);
        else
            setTimeout(bx_ymaps_waiter, 100);
    })();

    
    /* if map inits in hidden block (display:none)
