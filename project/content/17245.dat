<!-- Base post template: user blog, post's items in feeds -->
<script id="postsTmpl" type="text/x-handlebars-template">
{{! <![CDATA[ }}
{{#if posts}}
    {{#posts}}
    <div name="post_{{_id}}" class="Post">
        <div class="PostBody">
        
        {{#if parent}} <!-- СОДЕРЖИТ родительский пост -->
            {{#if action_header}} <!-- если показывается в ленте (с заголовком действия) -->
                {{{action_header}}}
            {{^}} <!-- иначе показываем просто автора поста [Имя Фамилия] -->
                <div class="Author">
                    <a href="/club/users/{{author.id}}">
                        <img src="{{ buildAvatarUrl author.file_host author.avatar_id '160x160' }}" alt="" /><span>{{ author.name }}</span>
                    </a>
                </div>
            {{/if}}

            {{#is_blog this}}
                {{include "attachmentsTmpl" _photos=photos _videos=videos}}
            {{/is_blog}}

            {{#if text}}
                <p class="PostText">{{formatText text}}</p>
            {{/if}}

            {{include "relationsTmpl" _movies=movies _persons=persons}}

            <div class="LikePost">
                {{#unless action_header}} <!-- action_header в приоритете; если он есть, то показывать автора лайкнутого поста не нужно -->
                
                <div class="User">
                    <a href="/club/users/{{parent.author.id}}">
                        <img src="{{ buildAvatarUrl parent.author.file_host parent.author.avatar_id '160x160' }}" alt="" />{{ parent.author.name }}
                    </a>
                    <a class="Date" href="/club/posts/{{parent._id}}"><span>{{ formatDate parent.created }}</span></a>
                </div>
                
                {{^}} <!-- либо если лайкнули запись из блога/рецензия/мнение, то показывать даже при наличии action_header -->
                    
                {{#is_blog parent}} 
                    <div class="User">
                        <a href="/club/users/{{parent.author.id}}">
                            <img src="{{ buildAvatarUrl parent.author.file_host parent.author.avatar_id '160x160' }}" alt="" />{{ parent.author.name }}
                        </a>
                        <a class="Date" href="/club/posts/{{parent._id}}"><span>{{ formatDate parent.created }}</span></a>
                    </div>
                {{/is_blog}}
                    
                {{#is_opinion parent}} 
                    <div class="User">
                        <a href="/club/users/{{parent.author.id}}">
                            <img src="{{ buildAvatarUrl parent.author.file_host parent.author.avatar_id '160x160' }}" alt="" />{{ parent.author.name }}
                        </a>
                        <a class="Date" href="/club/posts/{{parent._id}}"><span>{{ formatDate parent.created }}</span></a>
                    </div>
                {{/is_opinion}}

                {{#is_review parent}} 
                    <div class="User">
                        <a href="/club/users/{{parent.author.id}}">
                            <img src="{{ buildAvatarUrl parent.author.file_host parent.author.avatar_id '160x160' }}" alt="" />{{ parent.author.name }}
                        </a>
                        <a class="Date" href="/club/posts/{{parent._id}}"><span>{{ formatDate parent.created }}</span></a>
                    </div>
                {{/is_review}}

                {{/unless}}

                {{#if parent.data}}
                    {{#is_article parent}}
                {{include "articleBodyTmpl" article=parent.data _photos=parent.photos}}
                    {{/is_article}}

                    {{#is_gallery parent}}
                {{include "articleBodyTmpl" article=parent.data _photos=parent.photos}}
                    {{/is_gallery}}

                    {{#is_trailer parent}}
                {{include "trailerBodyTmpl" trailer=parent.data}}
                    {{/is_trailer}}

                    {{#is_video parent}}
                {{include "trailerBodyTmpl" trailer=parent.data}}
                    {{/is_video}}
                    
                    {{#is_industry parent}}
                {{include "trailerBodyTmpl" article=parent.data}}
                    {{/is_industry}}

                    {{#is_filmproview parent}}
                {{include "filmproviewBodyTmpl" filmproview=parent.data}}
                    {{/is_filmproview}}

                {{^}}

                {{#is_blog parent.this}}

                    {{include "attachmentsTmpl" _photos=parent.photos _videos=parent.videos}}

                    {{#if parent.text}}
                    <p class="PostText">{{formatText parent.text}}</p>
                    {{/if}}

                {{/is_blog}}

                {{#is_opinion parent.this}}

                    {{#if parent.text}}
                    <p class="PostText">{{formatText parent.text}}</p>
                    {{/if}}
                
                {{/is_opinion}}

                {{#is_review parent.this}}

                    {{#if parent.rate}}
                    <p class="PostRate">Оценка: <span class="PostRatePercent PostRatePercent_{{parent.rate}}">{{parent.rate}} из 5</span></p>
                    {{/if}}

                    {{#if parent.title}}
                    <p class="PostTitle">{{parent.title}}</p>
                    {{/if}}

                    {{include "attachmentsTmpl" _photos=parent.photos}}

                    {{#if parent.text}}
                    <p class="PostText">{{formatBBText parent.text}}</p>
                    {{/if}}

                {{/is_review}}
                
                {{/if}}

                {{include "relationsTmpl" _movies=parent.movies _persons=parent.persons}}

            </div>
        {{^}} <!-- НЕ СОДЕРЖИТ родительский пост -->
            {{#if action_header}} <!-- если показывается в ленте -->
                {{{action_header}}}
            {{^}}
                <div class="Author">
                    <a href="/club/users/{{author.id}}">
                        <img src="{{ buildAvatarUrl author.file_host author.avatar_id '160x160' }}" alt="" /><span>{{ author.name }}</span>
                    </a>
                </div>
            {{/if}}

                {{#if data}}
                    {{#is_article this}}
                {{include "articleBodyTmpl" article=data _photos=photos}}
                    {{/is_article}}

                    {{#is_gallery this}}
                {{include "articleBodyTmpl" article=data _photos=photos}}
                    {{/is_gallery}}

