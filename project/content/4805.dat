function isValidURL(url){
  var RegExp = /(ftp|http|https|href|url|www|.ru|.com|.net|.info|.org):\/\/(\w+:{0,1}\w*@)?(\S+)(:[0-9]+)?(\/|\/([\w#!:.?+=&%@!\-\/]))?/;

  if(RegExp.test(url)){
    return true;
  }else{
    return false;
  }
}

