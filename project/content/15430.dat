        /**
         * Big photo toggle
         */
        $('[data-small-image]').on('click', function(ev) {
            ev.preventDefault();
            var jqSmallImage = $(this);
            var options = {
                href: jqSmallImage.attr('href'),
                src: jqSmallImage.attr('img'),
                srcset: jqSmallImage.attr('data-srcset'),
            };
            var jqImgEl = jqBigImage.find('img');

            jqBigImage.attr('href', options.href);
            jqImgEl.attr('src', options.src);
            jqImgEl[0].srcset = options.srcset;

            currentIndex = getIndexByHref(options.href);
        });

        /**
         * Big photo click -> open photoswipe
         */
        $('[data-big-image]').on('click', function(ev) {
            ev.preventDefault();
            $(document).trigger(
                'open_drom_gallery',
                [{
                    id: 'bullphotos',
                    index: currentIndex
                }]
            );
        });

        function getIndexByHref(href) {
            var jqEl = $('[data-bull-photos="big"] a[href="' + href + '"]');
            if (jqEl.length) {
                return jqEl.parent().index();
            }
            return 0;
        }
    });

    (function() {
