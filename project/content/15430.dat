        $('.img img[src]').on('error', function() {
            var jqImg = $(this);
            clearInterval(timers[jqImg.attr('src')]);
            timers[jqImg.attr('src')] = setInterval(function(){
                jqImg.load(function(){
                    clearInterval(timers[jqImg.attr('src')]);
                });
                jqImg.attr('src', jqImg.attr('src'));
            }, 5000);
        });
