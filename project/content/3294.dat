								$("#chronoform-stomatologiya2").addClass("form-overlayed");
							}
							var form_action = $("#chronoform-stomatologiya2").prop("action");
							var sep = (form_action.indexOf("?") > -1) ? "&" : "?";
							var ajax_url = form_action + sep + "tvout=ajax";
							
