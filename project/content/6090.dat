            $(function()
{
    (function()
    {
        var $slideBlock = $('.slideBlock');
        var blocks = $slideBlock.find('.block');
        var slider = $slideBlock.find('.slider span');
        var length = blocks.length;
        var interval = 0;
        var body = $('body');
        var timer;
        var secKoef = 1000/2;

        var currentElement = blocks.first();


        var slider = $slideBlock.find('.slider span');

        var setInter  = function($sec)
        {
            timer = setTimeout( function()
            {

                blocks.eq(interval).fadeOut();
                slider.removeClass('cur');

                interval++;
                if(interval >= length)
                {
                    interval = 0;
                }

                blocks.eq(interval).fadeIn();
                slider.eq(interval).addClass('cur');
                currentElement = blocks.eq(interval);

                setInter(currentElement.attr('rel')*secKoef);

            }, $sec);
        }

        // TODO: можно отрефакторить
        body.mouseleave(function() {
            clearTimeout(timer);
        });

        body.mouseenter(function() {
            setInter(currentElement.attr('rel')*secKoef);
        });

        slider.click(function()
        {
            var $this = $(this);
            var eq = parseInt($this.attr('title'))-1;
            slider.removeClass('cur').eq(eq).addClass('cur');
            blocks.hide().eq(eq).fadeIn();
            currentElement = blocks.eq(eq);
            interval = eq;
            clearTimeout(timer);
            setInter(currentElement.attr('rel')*secKoef);

        });

    })();
});    </script>
