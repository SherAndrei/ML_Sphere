		$.get($(this).attr('href'), function(msg){
			fadepop(msg);
		})
		
		return false;
	})
	
	$("#msg").ajaxError(function(event, request, settings, ex) {
	  $(this).html("Error requesting page " + settings.url + "!");
	});
	
	function fadepop(mess){
		if($('#fadepop').html()){
			$('#fadepop .text').html(mess);
		}else{
			$('body').append('<div id="fadepop" class="popup"><div class="xbtn">X</div><div style="clear: both"><div class="text">'+mess+'</div></div>');
		}
		
		if($('#opac').attr('id')){}else{
			$('body').append('<div id="opac"></div>');
		}
		
		$('#fadepop, #opac').fadeIn();
	}
	
	$('.xbtn, #opac').live('click', function(){
		$('.popup, #opac').fadeOut();
	})
		
	
})
</script>

