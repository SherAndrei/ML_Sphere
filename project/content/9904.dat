@media (min-width: 600px) {.block-adaptive {width: 560px; height: 300px; } }
@media (min-width: 768px) {.block-adaptive {width: 580px; height: 400px; } }
@media (min-width: 800px) {.block-adaptive {width: 580px; height: 400px; } }
@media (min-width: 1000px) {.block-adaptive { width: 665px; height: 300px; } }
</style>
