                	if (myArray != null && myArray[1]) {
                	  on_downloadFoldersContentList(myArray[1], false);
                	}
                	else if (window.folderId !== undefined && folderId){
                	  on_getListFiles(folderId, false);
                	}
                }  
            }, false)
        }
    </script>
	
