                $('#verticalCol').css('height', mainContentHeight + 'px');
            }
            $("#sticky_banner").stick_in_parent({parent: '#verticalCol'});

            $("[data-toggle='tooltip']").tooltip();

            $.scrollUp({
                scrollText: 'Наверх',
                zIndex: 200,
                topDistance: '300',
                topSpeed: 300,
                animation: 'fade',
                animationInSpeed: 200,
                animationOutSpeed: 200
            });
            setScrollUpRight();

            $(window).on('resize', function () {
                setScrollUpRight();
            });


            var orgElementPos = $('.navbar-static-top').offset();
            if (orgElementPos) {
                var orgElementTop = orgElementPos.top;
                $(window).scroll(function () {
                    if ($(window).scrollTop() > orgElementTop) {
                        $('#nav-scrolled').removeClass('hidden');
                    } else {
                        $('#nav-scrolled').addClass('hidden');
                    }
                });
            }
        });
    </script>
    <script type="text/javascript">
        $(function () {
            var id = '#scrollIndicator';
            if ($(id)) {
                $(id).hide();
            }

            var loading = false;
            var idBtn = '#scrollButton';

            $(idBtn).on('click', function () {
                if (loading == false && scrollPager && scrollPager.nextExists) {
                    $('ul.pager').hide();
                    loading = true;

                    if ($(id)) {
                        $(id).show();
                    }

                    var savedHtml = $(idBtn).html();
                    $(idBtn).html('<img src="https://cloudstatic.eva.ru/design/eva3/eva/images/ajax-loader.gif"/>');
                    $(idBtn).addClass('disabled');

                    $.get(
                        '/jsf/photo/ajax/user-photos.xhtml?userId=116873&albumId=&type=&firstResult=' + scrollPager.nextFirstResult + '&pageSize=' + scrollPager.pageSize + '&ajax=true',
                        function (data) {
                            $(idBtn).removeClass('disabled');
                            $(idBtn).html(savedHtml);

                            if ('' == 'true') {
                                $("#user-photos").prepend(data);
                            } else {
                                $("#user-photos").append(data);
                            }
                            if ($(id)) {
                                $(id).hide();
                            }
                            loading = false;
                        }).always(function(data) {
                            if (!scrollPager.nextExists) {
                                $(idBtn).addClass('disabled');
                            }
                        });
                } else {
                    $(idBtn).addClass('disabled');
                }
            });
        });
    </script>

        
