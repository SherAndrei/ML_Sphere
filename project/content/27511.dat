    function thanks_set_viz(div){
	var disp=$('thanks_'+div).style.display;
	if (disp=='block'){
	    $('thanks_'+div).style.display='none';
	    $('thanks_'+div).style.visibility='hidden';
	    $('thanks_click_'+div).innerHTML='[+]';
	}else{
	    $('thanks_'+div).style.display='block'; 
	    $('thanks_'+div).style.visibility='visible';
	    $('thanks_click_'+div).innerHTML='[-]';
	}
}
