<script type="text/javascript">
    $(document).ready(function() {
        $('#recover-pass-form').submit(function() {
            self = $(this);
            restEmail = $.trim($('#user_email').val());
            
            if ($.trim($('#user_email').val()) != '') {
                showLoading('#popup-remind');
                $.post('http://www.sochi-express.ru/my/_ajax/forgot_user_email.php', {email: restEmail}, function(data) {
                    hideLoading('#popup-remind');
                    if (data.status == 1 || data.status == 'email_sso_exist') {
                        $('#popup-remind, .popup-fade').fadeOut();
                        customAlert('Информация', 'Письмо с инструкцией по восстановлению пароля отправлено на ваш e-mail.')    
                    } else if (data.status == 'email_not_exists') {
                        self.find('.auth-error').remove();
                        self.find('.row').append('<div class="auth-error">Данный e-mail не найден. Проверьте правильность ввода.</div>');     
                    } else {
                        self.find('.auth-error').remove();
                        self.find('.row').append('<div class="auth-error">Произошла ошибка. Проверьте правильность ввода.</div>');     
                    }     
                });
            } else {
                self.find('.auth-error').remove();
                self.find('.row').append('<div class="auth-error">Введите e-mail.</div>');    
            }
            return false;
        })    
    })
</script>
