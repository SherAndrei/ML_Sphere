		$(document).click(function(event) {
			if ($(event.target).closest("#popup-basket").length) return;
			$("#popup-basket").fadeOut("slow");
			event.stopPropagation();
		});
		
		$('.close-popup-basket, .continue-shopping, .to-basket').click(function(){
			$("#popup-basket").fadeOut("slow");
		});
		
		
		// Корзина
		$('.to-basket').click(function(){
			var e = $(this),
				id,f, 
				count = 0, 
				tcount, item, 
				cart2 = [],
				cart = get_cookie("cart");
			
			// Данные корзины
			cart = ( cart ? JSON.parse(cart) : [] );
			
			id		= e.attr('data-id');
			tcount	= parseFloat(e.attr('data-count'));
					
			flag = true;
			for (var i in cart) {
				if( cart[i].id !== id )
					continue;
				
				cart[i].count += tcount;
				flag = false;
			}
			
			if( flag ){

				cart[cart.length] = {
					id : id,
					count : tcount
				};
			
			}

			set_cookie("cart", JSON.stringify(cart), 60*60*24*3*1000, "/");
			reprice();
			
			if(!$(this).hasClass('one-click')){
				$('#title-popup-basket').text($(this).attr('data-title'));
				$('#popup-basket').fadeIn();
				return false;
			}
			
		});
		

		$('.bascet-input-item').on('input', function(){
				
			var val = parseFloat($(this).val());			
			
			if(!val || val < 1)
				$(this).val(1);
			
				
			rebascet();
			
		});
		

		$('.bascet-input-item').on('change', function(){
				
			var val = $(this).val();
			
			if(val == ''){
				$(this).val(1);
				rebascet();
			}
			
		});
		

		$('.trashcan').on('click', function(){
				
			$(this).parents('.basket-table-row').fadeOut(300, function(){
				$(this).remove();
				
				rebascet();
				
				if(!$('.basket-table-row').length){
					location.reload();
				}
			});
			
		});
		

		function rebascet(){

			var cart = [], prices = 0;
			
			$('.bascet-input-item').each(function(){
				
				var e = $(this), price, tcount, par;
			
				id		= e.attr('data-id');
				price	= parseFloat(e.attr('data-price'));
				tcount	= parseFloat(e.val());
				par		= e.parents('.basket-table-row');
			
				cart[cart.length] = {
					id : id,
					count : tcount
				};
				console.log('');
				
				$('.our-price', par).text(price*tcount);
				
				prices += price*tcount;
				
				
			});

			$('#old-prices').text(prices);
			
			set_cookie("cart", JSON.stringify(cart), 60*60*24*3*1000, "/");
			reprice();
			
		}
		

		function reprice(){
		
			$('#cartitems').text('Обновляю...');

			$.post(
				ajaxurl,
				{
					action: 'cartitems'
				},
				function(data) {
					$('#cartitems').html(data);
				}
			);
			
		}
		reprice();
		
		$('#cart-form').submit(function(){
			var e, f = false;
		
			$('.warning').removeClass('warning');
		
			e = $('[name="nam"]');
			if ( e.val() == '' ) {
				e.addClass('warning');
				f = true;
			}
		
			e = $('[name="teleph"]');
			if ( e.val() == '' ) {
				e.addClass('warning');
				f = true;
			}
		
			e = $('[name="check"]');
			if ( !e.is(':checked') ) {
				e.parent().addClass('warning');
				f = true;
			}
			
			if ( f ) {
				return false;
			}
			
		});
		
		
	})(jQuery);
		
	function set_cookie ( name, value, exp, path, domain, secure )
	{
		var cookie_string = name + "=" + escape ( value );

		if ( exp )
		{
			var timeObj = new Date();
			var expires = new Date(timeObj.getTime() + exp);
			cookie_string += "; expires=" + expires.toGMTString();
		}

		if ( path )
			cookie_string += "; path=" + escape ( path );

		if ( domain )
			cookie_string += "; domain=" + escape ( domain );

		if ( secure )
			cookie_string += "; secure";

		document.cookie = cookie_string;
	}


	function get_cookie ( cookie_name )
	{
		var results = document.cookie.match ( '(^|;) ?' + cookie_name + '=([^;]*)(;|$)' );

		if ( results )
			return ( unescape ( results[2] ) );
		else
			return null;
	}
