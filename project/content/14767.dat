            if (win.scrollTop() > win.height() / 3) {
                up.fadeIn();
            } else {
                up.fadeOut();
            }
        }).scroll();

    });
    function ShowContactForm() {
        s = '<div class="b-popup">' +
        '<div class="b-popup-content">' +
        '<div class="title">Задать вопрос</div>' +
        '<a class="boxclose" id="boxclose" onclick="HideContactForm(); return false;"></a>' +
        '<form class="askform">' +
        '<div class="bg-good">' +
        '<label>Ваше имя</label><input type="text" data-field="name"><br>' +
        '<label>Контактный телефон</label><input type="text" data-field="phone"><br>' +
        '<label>Email*</label><input type="email" data-field="email"><br>' +
        '</div>' +
        '<textarea data-field="query" placeholder="Текст вашего вопроса"></textarea>' +
        '<a class="button send" href="#" onclick="sendExpertQuery(); return false;">Задать вопрос &rarr;</a>' +
        '<div style="clear:both;"></div>' +
        '</form>' +
        '</div>' +
        '</div>'
        $(".b-popup").remove();
        $(s).appendTo('body').hide();
        $(".b-popup").fadeIn(100);
    }
    function HideContactForm() {
        $(".b-popup").fadeOut(100);
    }

    function DisplayNoneContactForm() {
        $(".b-popup").hide();
    }

    $('body').on('click', '.b-popup', function (e) {
        HideContactForm();
    });

    $('body').on('click', '.b-popup-content', function (e) {
        e.stopPropagation();
    });

    window.onkeydown = function (event) {
        if (event.keyCode === 27) {
            HideContactForm();
        }
    };

    function sendExpertQuery() {
        var name = $('.askform input[data-field="name"]').val();
        var phone = $('.askform input[data-field="phone"]').val();
        var email = $('.askform input[data-field="email"]').val();
        var query = $('.askform textarea[data-field="query"]').val();
        if (email == '') {
            alert('Необходимо указать email');
            return false;
        }
        if ((query == '') || (query.length < 10)) {
            alert('Вы не указали свой вопрос, расскажите о проблеме подробнее');
            return false;
        }
        var o = {};
        o.name = name;
        o.phone = phone;
        o.email = email;
        o.query = query;
        console.log(o);
        //$.post('/comment/form/', {'obj': o});                                        
        $.ajax({
            type: 'POST',
            url: '/comment/form/',
            data: JSON.stringify(o),
            contentType: 'application/json; charset=utf-8',
        })


        $(".b-popup-content").html('<div class="title">Благодарим за вопрос!</div><p style="clear: both; margin-bottom: 10px;">В течение суток мы ответим на указанный вами e-mail: <span style="font-weight: bold; font-style: italic;">' + email + '</span></p><a class="button send" href="#" onclick="HideContactForm(); return false;">Закрыть окно</a><div style="clear: both;"></div>');

    }

</script>
