  $('.js-show-map').click(function() {
    var
      $this = $(this),
      $buildingCard = $('.js-card'),
      $mapContainer = $('#js-map-container');
  
    function showFiltersTop(top) {
      var
        $topFilter = $('.js-filter-top'),
        $filterCity = $topFilter.find('.filter-city'),
        onlyCity = !!$filterCity.length && !$filterCity.siblings('.tb-wrapper-').length,
