		<script type="text/javascript">
			$(function(){
				
				if('false' == 'true') {
					$('#phone_sms').text('+7 ' + $('#phone').val());
					showPopup('popup_login_sms_pass');
				}
				
				formProperties.strRegisterPath = '/amserver/password?urlLogin=%2Famserver%2FUI%2FLogin%3Fgoto%3Dhttp%3A%2F%2Flk.ssl.mts.ru%2F%26service%3Dlk&service=lk/';
    			formProperties.serviceURI = '/amserver';
    			formProperties.emptyLoginMessage = 'Введите телефон';
    			formProperties.wrongOperatorLabel = 'Личный кабинет доступен только абонентам МТС. <a id="howToRegister" href="http://www.mts.ru/help/mts/?tab=shop">Как стать абонентом МТС</a>';
    			validation.useLogin = 'true' == 'false'
    			function init() {
					
					formProperties.validationFunction = validateForm;
					
					
					var loginErr = '';
					var passwordErr = '';
					
					if(loginErr) {
						setControlError($('#phone'), loginErr, true);
					}
					
					if(passwordErr) {
						setControlError($('#password'), passwordErr, true);
					}
					
					$('a.get-password, a.get-password-small').click(function(e){
						e.preventDefault();
						var login = $('#phone').val().replace(/[^0-9]/g, '');
						window.location.href = $(this).attr('href') + (!!login ? '&initialNumber='+ login : '');
					});

				}
				
				init();
			});
		</script>
