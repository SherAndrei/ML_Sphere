<script>
  function sync_radios(){
    $('input[type=radio]').each(function(){
      $(this).parent().removeClass('radio-wrapper-checked').removeClass('radio-wrapper-unchecked').
        addClass('radio-wrapper-'+($(this).is(':checked')?'checked':'unchecked'));
    });
  }
  function sync_checkbox(){
    $('input[type=checkbox]').each(function(){
      $(this).parent().removeClass('checkbox-wrapper-checked').removeClass('checkbox-wrapper-unchecked').
        addClass('checkbox-wrapper-'+($(this).is(':checked')?'checked':'unchecked'));
    });
  }
  $(function(){
    $('input[type=radio]').wrap('<div class="radio-wrapper"></div>').css('opacity',0).change(function(){
      sync_radios();
    });
    sync_radios();
    $('input[type=checkbox]').wrap('<div class="checkbox-wrapper"></div>').css('opacity',0).change(function(){
      sync_checkbox();
    });
    sync_checkbox();
  })
</script>
