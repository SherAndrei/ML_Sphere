 function move_block(){
  var top = jQuery(document).scrollTop();
  var h = jQuery('#panel').outerHeight() + jQuery('#header').outerHeight() + 20;
  var b1 = jQuery('.block-gray:visible').outerHeight();
  var b2 = jQuery('.block-white:visible').outerHeight();  
  var b3 = jQuery('#yandex_ad:visible').outerHeight();
  var sum = h+b1+b2+b3;
  var b = jQuery('.block-gray');
  var ch = jQuery('.article-page>.content-left').outerHeight();
  
  if (top>= sum && ch>(b1+b2+b3+50) && jQuery(window).width()>1250){
   var pos = top-h+25;

   if (pos+b1<jQuery('#middle>.inner').height())
    b.css({'position':'fixed','top':25, 'width':jQuery('.article-page').width()/3-52});
   else
    b.css({'position':'fixed','top':jQuery('#middle>.inner').height()-pos-b1, 'width':jQuery('.article-page').width()/3-52});
  }
  else{
   b.removeAttr('style');
  }
 }
