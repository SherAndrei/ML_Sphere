            });

        $("div[data-id=" + id + "] .dynamic.promo-convergence_open_control").off(
            "click");
        $("div[data-id=" + id + "] .dynamic.promo-convergence_open_control").on(
            "click",
            function (e) {
                ConvergentDetailsPageHelper.toggleInfo(id);
            });
   };

   ConvergentDetailsPageHelper.toggleInfo = function (id) {
        var block = $("div[data-id=" + id + "].promo-convergence_section.__additional-information");
        if (block.attr("data-state") == "hide") {
            block.css("display", "block");
            block.attr("data-state", "show");
        } else {
            block.css("display", "none");
            block.attr("data-state", "hide");
        }

        var link = $("div[data-id=" + id + "] .dynamic.promo-convergence_open_control");
        if (link.hasClass("opened")) {
            link.removeClass("opened");
        } else {
            link.addClass("opened");
        }
    };
