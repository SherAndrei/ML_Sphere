   var ConvergentDetailsPageHelper = {};
   ConvergentDetailsPageHelper.initInfo = function(id){
      if ($("div[data-id=" + id + "].promo-convergence_section.__additional-information:has(*)").length == 0) {
            $("div[data-id=" + id + "] .promo-convergence_open").remove();
            return;
        } else {
            $("div[data-id=" + id + "] .promo-convergence_open").show();
        }

        $("div[data-id=" + id + "] .promo-convergence_additional-close").off(
