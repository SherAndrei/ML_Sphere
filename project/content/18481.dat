    $j('#login_form').submit(function (ev) {
        ev.preventDefault();
        var e = $j('#email');
        var p = $j('#password');
        $j('#loginerror').hide();
        $j.ajax('/users/xhttpLogin', {
            type: 'POST',
            dataType: 'json',
            data: { e: e.val(), p:p.val() },
            beforeSend: function() {
                $j('#login_error').hide();
                $j('#pass_error').hide();
                $j('#pass2_error').hide();
            },
            success: function(r){
                if(r.user_id) {
//                    Tt.notification('Добро пожаловать', false, true);
                    window.location.reload();
                } else {
                    if (r.login_error) {
                        $j('#login_error').show();
                    }
                    if (r.pass_error) {
                        $j('#pass_error').show();
                    }
                    if (r.pass2_error) {
                        $j('#pass2_error').show();
                    }
