/**
* Insert Thanks
*
* @ param integer		id of the post
*/
function thanks_do(postid)
{
	if (typeof vb_disable_ajax != 'undefined' && vb_disable_ajax == 2)
	{
		// disable all ajax features
		return false;
	}

	if (!thanks_pending)
	{
		if (fetch_object('thanks_progress_' + postid))
