	init_cb = function(){
		setTimeout(function(){
			
			$('.item').each(function(){
				var e = $(this);
				var cb = e.find('input.cb:first');
				if(!cb.length){
					return;
				}
				e.bind('mouseenter', function(){
					cb.show();
				});
				e.bind('mouseleave', function(){
					if (!cb.attr('checked')) {
						cb.hide();
					}	
				});
				if(cb.attr('checked')){
					cb.show();
				}
			});
		}, 0);
	}

