		$.ajax({
			url: 'http://loadzonesnap.fo.ru/blog?delete_action=' + id + '&sk=e6e706adf9a29eae3d6fdb9d64bb41c7', 
			complete: function(transport, textStatus){
				waitDiv_hide();
				eval(transport.responseText);
				if (redirect_url) {
					setTimeout(function(){window.location.href = 'http://loadzonesnap.fo.ru/blog/' + redirect_url}, 2000);
