//cache vars
$fl_menu=$jflyfly("#fl_menu");
$fl_menu_menu=$jflyfly("#fl_menu .menu");
$fl_menu_label=$jflyfly("#fl_menu .label");

$jflyfly(window).load(function() {
	menuPosition=$jflyfly('#fl_menu').position().top;
	FloatMenu();
	$fl_menu.hover(
		function(){ //mouse over
			$fl_menu_label.fadeTo($menu_fade_speed, 1);
			$fl_menu_menu.fadeIn($menu_fade_speed);
		},
		function(){ //mouse out
			$fl_menu_label.fadeTo($menu_fade_speed, $closed_menu_opacity);
			$fl_menu_menu.fadeOut($menu_fade_speed);
		}
	);
});

$jflyfly(window).scroll(function () { 
	FloatMenu();
});

function FloatMenu(){
	var scrollAmount=$jflyfly(document).scrollTop();
	var newPosition=menuPosition+scrollAmount;
	if($jflyfly(window).height()<$fl_menu.height()+$fl_menu_menu.height()){
		$fl_menu.css("top",menuPosition);
	} else {
		$fl_menu.stop().animate({top: newPosition}, $float_speed, $float_easing);
	}
}
</script>

