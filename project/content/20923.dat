<script>
 function uphand(id,s)
  {
   if(localStorage['comment-'+id]!==undefined) return false;
   $.post('/updownhand.php',{id:id,act:1});
   localStorage['comment-'+id]=1;
   $('div[comment='+id+']').find('.up').addClass('colored');

   // $(this).prev().html(t=parseInt($(this).prev().html())?t+1:1);

   var t=parseInt($(s).parent().prev().html());
   $(s).parent().prev().html(t && !isNaN(t) ? ++t : t=1);
   if(t>=0) $(s).parent().prev().css('color','#33FF33');
   return true;
  }

 function downhand(id,s)
  {
   if(localStorage['comment-'+id]!==undefined) return false;
   $.post('/updownhand.php',{id:id,act:-1});
   localStorage['comment-'+id]=-1;
   $('div[comment='+id+']').find('.down').addClass('colored');

   // $(this).next().html(t=parseInt($(this).next().html())?t+1:1);

   var t=parseInt($(s).parent().next().html());
   $(s).parent().next().html(t && !isNaN(t) ? --t : t=-1);
   if(t<0) $(s).parent().next().css('color','red');
   return true;
  }

 $(document).ready(function()
  {
   var i;

   $('div[comment]').map(function(i,e)
    {
     if(localStorage['comment-'+e.getAttribute('comment')]!==undefined)
      if(localStorage['comment-'+e.getAttribute('comment')]>0) $(e).find('.up').addClass('colored');
       else $(e).find('.down').addClass('colored');
    });
  });
