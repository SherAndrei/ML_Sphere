<script>
//conpany page tab nav
$('.comp-tab-nav li').click(function() {
$('.comp-tab-nav li').removeClass('active');
$(this).addClass('active');
var tmpClickIndex = $(this).index();
$('.comp-tab-content .bank-deskription').addClass('hidden');
$('.comp-tab-content .bank-deskription:eq('+tmpClickIndex+')').removeClass('hidden');
});
//drop menu in submenu
$('.drop-menu-parent a').mouseover(function() {
$('.drop-menu').addClass('hidden');
var tmpHoverIndex = $(this).parent().index();
$('.drop-menu:eq('+tmpHoverIndex+')').removeClass('hidden');
});
//curses from nbrb.by
var nbrbBaseURI = "http://www.nbrb.by/API/ExRates/Rates/";
var date = new Date();
date = date.getFullYear() + "-" + (date.getMonth()+1) + "-" + date.getDate();
$.getJSON(nbrbBaseURI + "145" + "?onDate=" + date, function(cur) {
var curUsdRate = cur.Cur_OfficialRate.toFixed(2);
$("#nbrb-cur-usd").text(curUsdRate);
});
$.getJSON(nbrbBaseURI + "292" + "?onDate=" + date, function(cur) {
var curEurRate = cur.Cur_OfficialRate.toFixed(2);
$("#nbrb-cur-eur").text(curEurRate);
});
$.getJSON(nbrbBaseURI + "298" + "?onDate=" + date, function(cur) {
var curRurRate = cur.Cur_OfficialRate.toFixed(2);
$("#nbrb-cur-rur").text(curRurRate);
});
// add active to menu
$('.product-nav li').removeClass('active');
$(function () {
$('.product-nav a').each(function () {
var location = window.location.href;
var link = this.href;
if(location == link) {
$(this).parent().addClass('active');
}
});
});
// height col-----------
function setEqualHeight(columns)
{
var tallestcolumn = 0;
columns.each(
function()
{
currentHeight = $(this).height();
if(currentHeight> tallestcolumn)
{
tallestcolumn = currentHeight;
}
}
);
columns.height(tallestcolumn);
}
window.onload=function(){
setEqualHeight($(".own-height-container> div"));
};
