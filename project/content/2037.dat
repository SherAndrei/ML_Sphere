<script language="javascript">
    function changeScrollType(obj) {
        if ($(obj).attr('checked') == 'checked') {
            scrollIndependent();
        }
        else {
            scrollCommon();
        }
    }

    function scrollCommon() {
        $('#actPage').css('overflow', 'auto');
        $(".lang1, .lang2").removeAttr('overflow');
        $(".lang1, .lang2").css('overflow', 'hidden');
        $(".lang1, .lang2").css('height', 'auto');
        $(".lang1, .lang2").css('padding-right', '0');
        $("body").css('overflow', 'auto'); 
        resizeMenu();
    }

    function scrollIndependent() {
        $('#actPage').removeAttr('overflow');
        $(".lang1, .lang2").css('height', ($(window).height() - 38) + "px");
        $(".lang1, .lang2").css('overflow', 'auto');
        $(".lang1, .lang2").css('padding-right', '5px');
        $("body").css('overflow', 'hidden');
        resizeMenu();
    }

    function actcard() {
        $('.dT').hide();
        $('.dTI').hide();
        $('#actPage').show();
        $('#actHolder').show();
    }
    
    function card(id) {
        $('.dT').hide();
        $('.dTI').hide();
        $('#actPage').hide();
        $('#cardHolder' + id).show();

        var t1 = $('#cardHolder' + id).html();
        if(t1.length == 1 || t1.length < 1){

        $('#cardHolder' + id).html("please wait...");
            $.ajax({
                url: "/Pages/card.aspx?getcard=" + id,
                cache: false,
                success: function (html) {
                    if (html.length > 0 || html != "") {
                        $('#cardHolder' + id).html(html);
                    }
                }
            });
        }
    }
    function showContext() {

    }

    function showLangVersion(lact_id, lang_code) {
        var _w = (parseInt(screen.width) * 0.5) + "px";
        var _h = $(window).height() + "px";
        var _l = (parseInt(screen.width) / 2) - (parseInt(_w) / 2);
        var _feat = "height=" + _h + ",width=" + _w + ", left=" + _l + ", top=50, scrollbars=yes, toolbar=yes";
        var a = window.open("/pages/getpage.aspx?lact_id=" + lact_id, lact_id + "_" + lang_code, _feat);
        if (window.focus) {
            a.focus();
        }
    }

    function scrollToElement(selector, time, verticalOffset) {
        time = typeof (time) != 'undefined' ? time : 1000;
        verticalOffset = typeof (verticalOffset) != 'undefined' ? verticalOffset : 0;
        element = $(selector);
        offset = element.offset();
        offsetTop = offset.top + verticalOffset;
        $('html, body').animate({
            scrollTop: offsetTop
        }, time);
    }
              
    function getTarget(id) {
        $.fancybox.close();
        $.scrollTo(id, 800, { offset: { top: -50, left: -1000} });
    }


    function clickAllChk(item_id, parent_id, chk) {
        $.each($('#' + parent_id).find("INPUT[type='checkbox']"), function () {
            if ($(this).attr('id') != item_id) {
                $(this).attr('checked', chk.checked);
                if (!chk.checked) {
                    $.each($('div').find('#s' + $(this).attr('id')), function () { $(this).parent().hide(); });
                }
                else {
                    $.each($('div').find('#s' + $(this).attr('id')), function () { $(this).parent().show(); });
                }       
            }
        });
    }

    function hideItem(chk) {
        if (!chk.checked) {
            $.each($('div').find('#s' + chk.id), function () {
                $(this).parent().hide();
            });
        }
        else {
            $.each($('div').find('#s' + chk.id), function () { $(this).parent().show(); });
        }
    }

    function saveToWord() {
        try {
            $('#hdWord').val($('#divCont').html());
            return true;
        }
        catch (err) {
            alert(err.message);    
        }
    }

    function getActByDate() {
        var val1 = $("#var_date").val();
        var _url = '/Pages/GetPage.aspx?lact_id=3212192&ONDATE=' + val1;
        window.location.href = _url;
    }

    jQuery.fn.extend({
        scrollToMe: function () {
            var x = jQuery(this).offset().top - 100;
            jQuery('html,body').animate({ scrollTop: x }, 500);
        } 
    });
    function opentInAct(id) {
        $("#" + id).scrollToMe();
        try { $.fancybox.close(); } catch (err) { }
    }

    function addToSet()
    {
        $('<a href="/pages/ajax_methods.aspx?action=my_list">Friendly description</a>').fancybox({
            overlayShow: true
        }).click();
    }

    $(document).ready(function () {
        $("#theDefCssID TABLE TD").css('display', '');
        $(".fancybox").fancybox({ autoDimensions: false, 'width': '500', 'height': '400' });

        var _w = $(window).width() > 1280 ? 1280 : ($(window).width() > 1024 ? 1024 : $(window).width());
        $(".fancybox-big").fancybox({ autoDimensions: false, 'width': _w, 'height': '500' });
        
    });

    $(document).keydown(function (e) {
        if (e.ctrlKey && e.keyCode == 13) {
            
                showRegAlert();
            
        }
    });
    
</script>
