            <h1>Места и сроки сбора меда</h1>
            <div class="clear"></div>

            <div class="excerpt"><script async src="//pagead2.googlesyndication.com/pagead/js/adsbygoogle.js"></script>
<!-- babushkinadacha-336x280-TOP -->
<ins class="adsbygoogle"
     style="display:inline-block;width:336px;height:280px"
     data-ad-client="ca-pub-4918206702928756"
     data-ad-slot="9525844628"></ins>
<script>
