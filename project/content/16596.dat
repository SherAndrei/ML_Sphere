  <div class="_post_content">
    
    <div class="post_header">
      <a class="post_image" href="/jimmy">
        <img src="https://pp.userapi.com/c629425/v629425420/3fbc4/AiVilnnvbJk.jpg" data-post-id="844420_445" data-post-click-type="post_owner_img" width="50" height="50" class="post_img" />
