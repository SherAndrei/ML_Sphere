    <script type="text/javascript">
        $(document).ready(function () {
            $("#issueSelect").change(function () {
                $.getJSON("/ajax/issues/", { year: this.value }).done(function (data) {
                    $('#ajaxArchive').html("");
                    $("<option>").attr("value", 0).text("-").appendTo("#ajaxArchive");
                    $.each(data, function (i, item) {
                        $("<option>").attr("value", item.value).text(item.text).appendTo("#ajaxArchive");
                    });
                })
            })
            $("#ajaxArchive").change(function () {
                document.location = this.value == 0 ? "/gazeta/" : "/gazeta/issues/" + this.value + "/";
            })
        })
    </script>

    <script>
        $(document).ready(function () {
            $('.most-viewed__part').each(function (index) {
                $(this).click(function () {
                    $('.most-viewed__part').removeClass('most-viewed__activ');
                    $(this).addClass('most-viewed__activ');
                    $('.most-viewed__conteiner').hide();
                    $('.most-viewed__conteiner:eq(' + index + ')').show();
                })
            })

            var detectDesctopBlock = $('.is-desctop');
            if (detectDesctopBlock.is(":visible") && $('.aside-fixed').length > 0) {
                var
                    docViewTop,

                    fixedTop,
                    fixedBottom,

                    isFixed = false,
                    isFixedBottom = false;

                    fixedElement = $('.aside-fixed'),

                    fixedElementStart = $('.aside-fixed__start'),
                    fixedElementEnd = $('.aside-fixed__end'),
                    scrollToEnd = !(fixedElementEnd.length > 0);

                $(document.all ? window : document).bind('scroll', function () {
                    docViewTop = $(window).scrollTop();

                    fixedTop = fixedElementStart.offset().top - 15;


                    if (docViewTop > fixedTop && !isFixed) {
                        fixedElement.addClass('aside-fixed__fixed');
                        isFixed = true;
                    }
                    else if (docViewTop <= fixedTop && isFixed) {
                        fixedElement.removeClass('aside-fixed__fixed');
                        isFixed = false;
                    }

                    if (!scrollToEnd) {
                        fixedBottom = fixedElementEnd.offset().top - fixedElement.height() - 15;

                        if (docViewTop >= fixedBottom && isFixed && !isFixedBottom) {
                            fixedElement.addClass('aside-fixed__absolute');
                            fixedElement.removeClass('aside-fixed__fixed');
                            fixedElement.css({
                                'top': fixedBottom - fixedTop
                            });
                            isFixedBottom = true;
                        }

                        if (docViewTop < fixedBottom && isFixed && isFixedBottom) {
                            fixedElement.removeClass('aside-fixed__absolute');
                            fixedElement.addClass('aside-fixed__fixed');
                            fixedElement.css({
                                'top': 15
                            });
                            isFixedBottom = false;
                        }
                    }

                });

            }
        })
    </script>
