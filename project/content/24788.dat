<script type="text/html" id="comment-item-template">
    <div class="b-comment <%= (isAuthor) ? 'b-comment_style_author' : ((isModer) ? 'b-comment_style_moderator' : '') %>">
        <div class="b-comment__anchor" id="comment_<%= id %>"></div>

        <div class="b-comment__content"><%= content %></div>

        <div class="b-comment__meta">
            <div class="meta-info-group pull-left">
                <div class="meta-info-group__item meta-info">
                    <img class="meta-info__avatar" src="<%= authorAvatarUrl %>" />
                    <a class="border-link text-capitalize" target="_blank" href="/user/<%= authorId %>"><%= authorName %></a>
                </div>

                <div class="meta-info-group__item meta-info">
                    <span class="meta-info__icon fa fa-calendar"></span>
                    <%= timeCreated %>
                </div>

                <div class="meta-info-group__item meta-info">
                     <span class="<% if (canVote) { %>border-link border-link-dashed<% } %>" data-action="vote" data-title="Сказать спасибо">
                        <span class="meta-info__icon fa fa-thumbs-up"></span>
                        <%= votesCount %>
                     </span>
                </div>
            </div>

            <div class="b-comment__controls meta-info-group pull-right">
                <div class="meta-info-group__item meta-info">
                    <a class="border-link border-link-dashed" data-action="reply" href="#" onclick="return false">
                        <span class="meta-info__icon fa fa-reply"></span>
                        Ответить
                    </a>
                </div>

                <% if (canUpdate) { %>
                    <div class="meta-info-group__item meta-info">
                        <a data-action="update" title="Редактировать" href="#" onclick="return false">
                            <span class="meta-info__icon fa fa-pencil"></span>
                        </a>
                    </div>
                <% } %>

                <% if (canDelete) { %>
                    <div class="meta-info-group__item meta-info">
                        <a data-action="delete" title="Удалить" href="#" onclick="return false">
                            <span class="meta-info__icon fa fa-trash-o"></span>
                        </a>
                    </div>
                <% } %>
            </div>
        </div>
    </div>
</script>

<script type="text/html" id="comment-item-deleted-template">
