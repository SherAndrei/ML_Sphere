            $(document).ready(function(){
            	 var cntr = 0;
            	 var regn = 0;
            	 var city = 0;
                var changeCountry = function(id_country){
                        $("#region").html('');
                        $("#region").html('<option value="">Выберите регион');
                        $.post("/cities.php", {id_country: id_country},
                            function (xml) {
                                $(xml).find('region').each(function() {
                                id = $(this).find('id_region').text();
                                $("#region").append("<option value='" + id + "'" + (id == regn ? " selected='selected' ":"") +">" + $(this).find('region_name_ru').text() + "");
                            });
                        });
                        $(".region").show();
                }
                var changeRegion = function(id_region){
	                  $("#city").html('');
	                  $("#city").html('<option value="">Выберите город');
	                  $.post("/cities.php", {id_region: id_region},
	                      function (xml) {
	                          $(xml).find('city').each(function() {
	                          id = $(this).find('id_city').text();
	                          $("#city").append("<option value='" + id + "'" + (id == city ? " selected='selected' ":"") +">" + $(this).find('city_name_ru').text() + "");
	                      });
	                  });
                }
            	 $.post("/cities.php", {find_city: $("#cityid").text()},
            	 	  function (xml) {
            	 	  		$(xml).find('city').each(function() {
            	 	  			cntr = $(this).find('id_country').text();
		            	 	  	regn = $(this).find('id_region').text();
            	 	  			city = $(this).find('id_city').text();
            	 	  });
            	 });
                $.post("/cities.php", {},
                    function (xml) {
                        $(xml).find('country').each(function() {
                            id = $(this).find('id_country').text();
                            $("#country").append("<option value='" + id + "'"+(id==cntr?" selected='selected' ":"")+">" + $(this).find('country_name_ru').text() + "");
                    });
                    if(cntr > 0){
                	      changeCountry(cntr);
                	      changeRegion(regn);
                	  }
                });
                $("#country").change(function() {
                    id_country = $("#country option:selected").val();
                    if (id_country == "") {
                        //$(".region, .city, #submit").hide();
                    }else {
                    		changeCountry(id_country);
                    }
                });
                $("#region").change(function() {
                    id_region = $("#region option:selected").val();
                    if (id_region == "") {
                        //$(".city").hide();
                    }else {
                    		changeRegion(id_region);
                    }
                    $(".city").show();
                });
                $("#city").change(function() {
                    if ($("#city option:selected").val() == "") {
                        //$("#submit").hide();
                    }else {
                        $("#submit").show();
                    }
                });                
            });
        </script><link rel="stylesheet" type="text/css" href="/css/main.css">
