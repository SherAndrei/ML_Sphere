<script type="text/javascript">var ElementsReader = {
    _el_for_text_book: $('#for_text_book'),
    _el_select_list: $('#select_list'),
    _el_block_for_listing: $('#block_for_listing'),
    _el_for_book_content: $('#for_book_content'),
    _el_for_title_book: $('#for_title_book'),
    _el_for_loading_gif: $('#loading_gif'),
    _el_grade: $('#ratings-grade'),
    _el_container_for_text: $('#container_for_text'),
    
    setPagePosition: function (pos,el) {
        this._el_for_text_book.css('marginTop',(pos*(-1)));
        $('.selected_list').removeClass('selected_list');
        $(el).addClass('selected_list');
        window.scrollTo(0,this.top_position_book);
    },
    
    paginationForBigText: function() {
        var _this = this;
        
        _this._el_for_text_book.css('marginTop',0);
        
        function f() {
            var hc = 10000;
            var ht = _this._el_for_text_book.height();
            if(ht < hc) {
                _this._el_block_for_listing.hide();
                return;
            }
            var cp = (Math.floor(ht/hc))+1;
            var html = '';
            for (var num = 1; num <= cp; num++) {
                html += '<div onclick="ElementsReader.setPagePosition('+((num-1)*hc)+',this)" >'+num+'</div>';
            }
            _this._el_select_list.html(html).find('div:first').addClass('selected_list');
            _this._el_block_for_listing.show();
        }
            
        var images = this._el_for_text_book.find('img');
        
        if(images.length == 0) {
            f();
        } else {
            setTimeout(function(){
                    images.each(function(){
                        $(this).height(Math.floor($(this).height()/20)*20);
                    });
                    
                f();
            },1000)
        }
    }, 
    
    generatePagination: function() {
        if (this.count_chapters < 2) {
            return;
        }
        this._el_p.hide();
        this._el_next_p.hide();
        this._el_previous_p.hide();
        if (ElementsReader.now_chapter == 1) {
            this._el_previous_p.hide();
        }   else {
            this._el_previous_p.show();
        }
        if (this.now_chapter < this.count_chapters) {
            this._el_next_p.show();
        }
        if (this.now_chapter == this.count_chapters) {
            this._el_next_p.hide();
        }
        this._el_p.show();
    },
    loadChapter: function(id) {
        this._el_for_loading_gif.show();
        this._el_for_book_content.css('color','#ccc');
        var _this = this;
        var images;
        $.get("/chapters/ajax-load", {id:id},function(data){
            _this._el_for_title_book.text(data.title)
            _this._el_for_text_book.html(data.textbook);
            _this._el_for_book_content.css('color','#000');
            _this._el_for_loading_gif.hide();
            _this.now_chapter_id = data.id;
            _this.generatePagination();
            _this.paginationForBigText();
        })
    },
    init: function() {
        this.count_chapters = $('.chapters_nav a').length;
        this.now_chapter = 0;
        this._el_previous_p = $('#pager_book .previous a');
        this._el_next_p = $('#pager_book .next a');
        this._el_p = $('#pager_book');
        this.top_position_book = $('#for_book_content').offset().top;
        var _this = this;
        $('.chapters_nav a').click(function(){
            _this.now_chapter = parseInt($(this).attr('data-num'));
            $('.selected_chapter').removeClass('selected_chapter');
            $(this).addClass('selected_chapter');
            _this.loadChapter($(this).attr('data-id'));
            window.scrollTo(0,_this.top_position_book);
            return false;
        })
        this._el_next_p.click(function(){
            $('#chapter_num_'+(_this.now_chapter+1)).click();
            return false;
        })
        this._el_previous_p.click(function(){
            $('#chapter_num_'+(_this.now_chapter-1)).click();
            return false;
        })
        $('.chapters_nav a:first').addClass('selected_chapter')
        this.now_chapter = 1;
        this.paginationForBigText();
        this.generatePagination()
    }
}
ElementsReader.init();
function inMyList(id,type) {
    $.get("/my-list/add",{book_id:id,type:type},function(data){
        alert(data);
    })
}</script>
<script type="text/javascript">jQuery(document).ready(function () {
$(function(){
    var _post_ajax_proccess = false;
    $('body').on('submit', '#post_comm', function () {
        var _butt = $('#button_comment');
        _butt.text('Идет отправка...')
        var _form = $(this);
        if (_post_ajax_proccess) return false;
        _post_ajax_proccess = true;
        $.post(_form.attr('action'),_form.serialize(),function (data){
            if (data.success == 'yes') {
                var parent_id = $('#comments-parent_id').val();
                if (parent_id == '0') {
                    $('#block_for_comment').prepend(data.html);
                } else {
                    var last_child = $('.parent_id_'+parent_id+':last');
                    var top;
                    if (last_child.length == 0) {
                        top = $('.comment_id_'+parent_id).offset().top;
                        $(data.html).hide().insertAfter($('.comment_id_'+parent_id)).fadeIn(3000);
                    } else {
                        top = last_child.offset().top;
                        $(data.html).hide().insertAfter(last_child).fadeIn(3000);
                    }
                    $('#cancel_response').click();
                    window.scrollTo(0,top);
                }
                $('#comments-content').val('');
            } else {
                for (var n in data.error) {
                    alert(data.error[n]);
                }
            }
            _butt.text('Отправить')
            _post_ajax_proccess = false;
        })
        return false;
    });
})
$(function(){

    var ChildParent = {};
    var childs = $('.child_comment');
    if (childs.length == 0) {
        return;
    }
    childs.each(function(){
        var _el = $(this);
        ChildParent[_el.attr('data-id')] = _el.attr('data-parent-id');
    })
    
    var last_father = 0;
    function retFather(id){
        id =  ChildParent[id];
        if (id === undefined) {
            return last_father;
        } else {
            last_father = id;
            return retFather(last_father);
        }
    }
    
    var NewSort = {};
    for (var n in ChildParent) {
        NewSort[n] = retFather(ChildParent[n]);
        last_father = 0;
    }
    
    var DataSort = [];
    for (var n in NewSort) {
        DataSort.push(n)
    }
    
    DataSort.sort(function(a,b){
        a = parseInt(a);
        b = parseInt(b);
        if (a < b) return 1;
        if (a > b) return -1;
    })
    
    var length_ch = DataSort.length;
    var n;
    
    for (var num = 0; num < length_ch;num++){
        n = DataSort[num];
        if (NewSort[n] == 0) {
            $('.comment_id_'+n).insertAfter($('.comment_id_'+ChildParent[n]))
        } else {
            $('.comment_id_'+n).insertAfter($('.comment_id_'+NewSort[n]))
        }
    }
})


$(function(){
    $('.response_comment').click(function(){
        var _this = $(this);   
        $('#cancel_response').show();
        $('#comments-content').val(_this.attr('data-user-nick')+", ");
        $('#comments-parent_id').val(_this.attr('data-id'))
        $('#comments-response_user_id').val(_this.attr('data-user-id'))
    })
    $('#cancel_response').click(function(){
        $(this).hide();
        $('#comments-content').val('');
        $('#comments-parent_id').val('0');
        $('#comments-response_user_id').val('');
        return false;
    })
})
});</script></body>
