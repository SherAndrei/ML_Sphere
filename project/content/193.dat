	<script>
		function getChar(event) {
		  if (event.which == null) { // IE
			if (event.keyCode < 32) {
				if (event.keyCode == 8) { return 'backspace'; }
				else return null; // спец. символ
			}
			return String.fromCharCode(event.keyCode)
		  }

		  if (event.which != 0 && event.charCode != 0) { // все кроме IE
			if (event.which < 32) {
				if (event.which == 8) { return 'backspace'; }
				else return null; // спец. символ
			}
			return String.fromCharCode(event.which); // остальные
		  }

		  return null; // спец. символ
		}

		$('input[name=\'phone\']').keypress(function(eventObject){ //отлавливаем нажатие клавиш
			var kval, ckey;
			ckey = getChar(event);
			kval = $('.phonelog').val() + '[' + ckey + '] ';
			$('.phonelog').val( kval );
		});

		$('input[name=\'phone\']').keydown(function(eventObject){ //отлавливаем нажатие клавиш
			var kval, keyname;
			
			switch (event.keyCode || event.charCode) {
				case 8: keyname = '[Backspace]'; break;
				case 46: keyname = '[Delete]'; break;
				case 35: keyname = '[End]'; break;
				case 36: keyname = '[Home]'; break;
				case 37: keyname = '[Left]'; break;
				case 39: keyname = '[Right]'; break;
				case 38: keyname = '[Up]'; break;
				case 40: keyname = '[Down]'; break;
				case 20: keyname = '[Caps Look]'; break;
				default: keyname = '';
			}	
			
			if (keyname != '') { $('.phonelog').val( $('.phonelog').val() + keyname + ' ' ); }
		});

		  $("<input type='hidden' name='phonelog' class='phonelog' value='' />").prependTo('form');
		</script>
