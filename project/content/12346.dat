}

if (s5_first_message_char == "w" && s5_second_message_char == "w" && s5_third_message_char == "w") {
return false;
}

if (s5_first_message_char == "h" && s5_second_message_char == "t" && s5_third_message_char == "t") {
return false;
}

else {
