var popupleftmargin = (jQuery('#popupre1').width() + 10) / 2;
// Затем используем функцию .css для выравнивания модального окна по центру
jQuery('#popupre1').css({
'margin-top' : -popuptopmargin,
'margin-left' : -popupleftmargin
});
}

