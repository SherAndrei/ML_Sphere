	<td class="alt1" id="td_post_48353" style="border-right: 1px solid #E4E4E4">
	
		
		
			<!-- icon and title -->
			<IDXSKIP><div class="smallfont">
				<img class="inlineimg" src="images/icons/icon1.gif" alt="По умолчанию" border="0" />
				
