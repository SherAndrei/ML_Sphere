		if(!confirm('Удалить?')){
			return;
		}
		
		waitDiv_show();
		$.ajax({
			url: 'http://imeropente.fo.ru/blog?delete_action=' + id + '&sk=2de4225dfe27fb69c2eff4a0c0e6e29e', 
			complete: function(transport, textStatus){
				waitDiv_hide();
				eval(transport.responseText);
				if (redirect_url) {
					setTimeout(function(){window.location.href = 'http://imeropente.fo.ru/blog/' + redirect_url}, 2000);
					return;
				}
				if(transport.getResponseHeader('X-SUCCESS') == 1){
					$('#item_' + id).remove();
				}
				$(window).trigger('scroll');
				
				/** Event handler */
				if(typeof customEvents.onAfterSingleItemDelete == 'function') {
					customEvents.onAfterSingleItemDelete(id);
				}
			}
