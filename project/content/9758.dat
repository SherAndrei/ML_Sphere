            $('#form_comment').defVal();
        }else{
            //alert(d);
            var obj_css = { "border": "1px solid #FF0000", 
            		        "background-color": "#FFf5f5", 
            		        "color":"#ff0000" };
            $('#alert').fadeOut().css(obj_css).html(d).fadeIn();
        } 
    });
    return false;     
};


Comment.reset_defaults  = function () {
    $("#submit_comment").unbind('click').bind('click', function (){Comment.save();});

