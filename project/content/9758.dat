    tab = part;    
    $('#tab_'+tab).addClass('tabe');
    $('#t_'+tab).css({display:''});

    if( part == 'add' ){
    	$('#t_list').css({display:''});

        //$('html, body').animate({scrollTop:$('div.comments').offset().top - 10}, 'slow');
    }
    
    $('#alert').html('').hide();
};

Comment.save = function() {
	$('#form_comment').clearDefVal();
	
