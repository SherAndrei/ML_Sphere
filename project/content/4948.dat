        $arrowLinks.on('mouseenter', function (event) {
            if (isTouchEvent) {
                isTouchEvent = false;
                return;
            }

            var stillOnTarget = true;
            outOfActiveArea = false;
            $link = $(event.currentTarget);
            headerNavigation.toggleAnimated(true);

            $link.one('mouseleave', function () {
                stillOnTarget = false;
                outOfActiveArea = true;

                setTimeout(function () {
                    if (isTouchEvent) {
                        isTouchEvent = false;
                        return;
                    }

                    outOfActiveArea && headerNavigation.toggleAnimated(true);
                    outOfActiveArea && headerNavigation.closeMenu();
                }, 210);
            });

            setTimeout(function () {
                headerNavigation.toggleAnimated(!headerNavigation.isOpened());
                stillOnTarget && headerNavigation.openMenu($link);
            }, headerNavigation.isOpened() ? 200 : 300);
        });

        $arrowLinks.on('touchstart', function () {
            var $link = $(event.currentTarget),
                $item = $link.closest(options.selectors.item),
                isClosing = headerNavigation.isActive($item),
                isSwitching = !isClosing && headerNavigation.isOpened();

            headerNavigation.toggleAnimated(!isSwitching);
            headerNavigation.toggleMenu($link);

            isTouchEvent = true;
            event.preventDefault();
        });

        $dropdowns.on('mouseenter', function () {
            outOfActiveArea = false;
        });

        $dropdowns.on('mouseleave', function () {
            outOfActiveArea = true;
            headerNavigation.toggleAnimated(true);

            setTimeout(function () {
                outOfActiveArea && headerNavigation.closeMenu();
            }, 200);
        });

        $('.b-main-navigation__dropdown-news-description').dotdotdot({
            watch: 'window'
        });

        $(document).on('touchstart', function (event) {
            $target = $(event.target);
            var isGesture = false;

            $target.one('touchmove', function () {
                isGesture = true;
            });

            $target.one('touchend', function () {
                if (isGesture) {
                    return;
                }

                if (!$target.closest('.b-main-navigation__item_arrow').length
                    && !$target.closest('.b-main-navigation__dropdown').length) {
                    headerNavigation.toggleAnimated(true);
                    headerNavigation.closeMenu();
                }
            });
        });
    })();
</script>

