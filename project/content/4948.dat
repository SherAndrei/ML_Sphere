<script>
    var $userbar = $('.js-userbar'),
        profileItemSelector = '.b-top-profile__item',
        profileItemOpenedModifier = 'b-top-profile__item_opened',
        bodyStateProfileOpenedModifier = 'body_state-b-top-profile_opened';

    $userbar.on('click', '.js-stop-propagation', function (event) {
        event.stopPropagation();
    });

    $userbar.on('click', '.js-toggle-bar', function (event) {
        event.stopPropagation();

        toggleDropdownVisibility.call(this);
    });

    $userbar.on('click', '.js-close-popup', closeDropdowns);

    $userbar.on('click', '.js-open-submenu', function (event) {
        event.stopPropagation();

        $(this).closest('.b-top-profile__subitem').toggleClass('b-top-profile__subitem_opened');
    });

    $userbar.on('transitionend webkitTransitionEnd oTransitionEnd', '.js-top-profile-dropdown', function () {
        setPopoverHeight();
    });

   $(window).on('resize', _.debounce(setPopoverHeight, 100));

    document.body.addEventListener('click', function (event) {
        !$(event.target).closest('.b-top-profile__item_opened').length && closeDropdowns();
    }, true);

    document.body.addEventListener('touchend', function (event) {
        !$(event.target).closest('.b-top-profile__item_opened').length && closeDropdowns();
    }, true);

    $(document).on('keyup', function (event) {
        var keyCode = event.keyCode || event.which;

        (keyCode === 27) && closeDropdowns();
    });

    function lockScroll(event) {
        var delta = event.originalEvent.wheelDelta || -event.originalEvent.detail,
            height = this.clientHeight,
            scrollHeight = this.scrollHeight,
            scrollTop = this.scrollTop;

        if ((scrollTop === (scrollHeight - height) && delta < 0) || (scrollTop === 0 && delta > 0)) {
            event.preventDefault();
        }
    }

    function toggleDropdownVisibility() {
        var $profileItem = $(this).closest(profileItemSelector),
            $profileItems = $(profileItemSelector);

        $profileItems.not($profileItem).removeClass(profileItemOpenedModifier);
        $profileItem.toggleClass(profileItemOpenedModifier);
        $('body').toggleClass(bodyStateProfileOpenedModifier);

        $profileItem.hasClass(profileItemOpenedModifier) && $userbar.on('mousewheel DOMMouseScroll MozMousePixelScroll', '.js-top-profile-overflow', lockScroll);

        $profileItem.hasClass(profileItemOpenedModifier) && $profileItem.closest('#notifications-list').length && window.userbar.notificationsList.updateNotificationsList();
    }

    function setPopoverHeight() {
        var $profileOverflow = $('.js-top-profile-overflow');

        $profileOverflow.each(function () {
            var $profileOverflow = $(this),
                $profileDropdown = $profileOverflow.closest('.js-top-profile-dropdown'),
                $profileFooter = $profileDropdown.find('.js-top-profile-footer'),
                bottomMargin = 24,
                dropdownMaxHeight = 540,
                footerHeight =  $profileFooter.height() || 0,
                overflowOffsetTop,
                scrollableHeight;

            if (!$profileOverflow.length) {
                return;
            }

            overflowOffsetTop = $profileOverflow.offset().top - $(window).scrollTop();
            overflowOffsetTop = Math.max(overflowOffsetTop, 0);

            scrollableHeight = $(window).height() - overflowOffsetTop - footerHeight - bottomMargin;
            scrollableHeight = Math.min(scrollableHeight, dropdownMaxHeight);

            $(this).css('max-height', scrollableHeight);
        });
    }

    function closeDropdowns() {
        $('body').removeClass(bodyStateProfileOpenedModifier);
        $(profileItemSelector).removeClass(profileItemOpenedModifier);

        $userbar.off('mousewheel DOMMouseScroll MozMousePixelScroll', '.js-top-profile-overflow', lockScroll);
    }
</script>
