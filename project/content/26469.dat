		create_layer('report_error', { popstate: false });
		return false;
	};

	$('.js-report-error').off('click.report_error').on('click.report_error', reporter);

	$(document).on('keydown', function(e) {
		if (e.ctrlKey && (e.keyCode == 13 || e.keyCode == 10)) {
			e.preventDefault();
			reporter();
		}
	});
});
</script>
