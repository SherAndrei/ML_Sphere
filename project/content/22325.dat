<script type="text/javascript">
    $(function () {
        var $form = $('#modal-login form');
        var $gmodal = $('#modal-login .g-modal');
        // линкер
        $('.login-linker').click(function (e) {
            e.preventDefault();
            jv.modal.open('modal-login');

        });
        // инициализация формы
        $form.ajaxForm(function (result) {
            $gmodal.removeClass('active');
            if (result && result.success == true) {
                jv.auth.UserId(result.user_id);
                if ( $('#user_login_success').size() > 0  ) {
                    $form.resetForm();
                    $(document).trigger('user_login_success', result);
                } else {
                    if ( typeof(jv.modal.callback) != 'undefined' && typeof(jv.modal.callback) == 'function' ) {
                        jv.modal.callback();
                    } else {
                        location.reload();
                    }
                }
            } else {
                //ошибка
                for (var name in result.errors) {
                    var $error = $form.find('input[name="' + name + '"]').parent().parent().find('.error');
                    for (first in result.errors[name]) break;
                    $error.html(result.errors[name][first]);
                    $error.addClass('visible');
                }

            }
        });
        $form.submit(function () {
            $gmodal.addClass('active');
            $form.find('.error').text('').removeClass('visible');
        })
        // клик на ОК
        $('#modal-login a.button').click(function (e) {
            e.preventDefault();
            $form.submit();
        })

        // обработка Enter'а
        $form.keydown(function (e) {
            if (e.keyCode == 13) {
                $form.submit();
            }
        })

    });
</script>


