            $('input[name="is[last_name]"]').attr('req', true);
            $('input[name="is[first_name]"]').attr('req', true);
            selectRegion.attr('req', true);
            variant = 1;
        }

        if (r2.attr('checked')) {
            reset_required();
            $('input[name="is[drtr_name]"]').attr('req', true);
            selectRegion.attr('req', true);
            variant = 2;
        }

        if (r3.attr('checked')) {
            reset_required();
            $('input[name="is[ip_number]"]').attr('req', true);
            variant = 3;
        }

        r1.click(function(){
            if (r1.attr('checked')) {
                reset_required();
                $('input[name="is[last_name]"]').attr('req', true);
                $('input[name="is[first_name]"]').attr('req', true);
                selectRegion.attr('req', true);
