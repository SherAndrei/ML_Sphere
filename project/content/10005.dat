            if (error != '') {
                var message = '<div class="b-fixed_top"><div class="b-msg b-msg--error"><div class="b-msg__title">Ошибка</div>';
                message += error;
                message += '</div></div>';
                message += '<script>$(".b-fixed_top").click(function(){$(this).remove()});<\/script>';
                iss.append(message);
                fn_scrollto($('.f-error').first());
            }

            if (error != '') return false;
        });

        if ((last_name && first_name) || drtr_name || ip_number) {
            setTimeout(function(){$('#btn-sbm').trigger('click');},1000);
        }

        $('.results-frame td.first').live({
