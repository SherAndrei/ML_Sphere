                reset_required();
                $('input[name="is[drtr_name]"]').attr('req', true);
                selectRegion.attr('req', true);
                variant = 2;
            }
        });

        r3.click(function(){
            if (r3.attr('checked')) {
                reset_required();
                $('input[name="is[ip_number]"]').attr('req', true);
                variant = 3;
            }
        });

        function reset_required(){
            $('input').each(function(){
                $(this).attr('req', false);
            });
