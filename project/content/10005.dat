        var drtr_name = get('is%5Bdrtr_name%5D');
        var ip_number = get('is%5Bip_number%5D');
        var address = get('is%5Baddress%5D');
        var patronymic = get('is%5Bpatronymic%5D');
        var date = get('is%5Bdate%5D');

        var selectRegion = $('#region_id');

        if (variant) {
            variant = decodeURIComponent(variant);
            $('#r'+variant).trigger('click');
        }

        if (region_id) {
            region_id = decodeURIComponent(region_id);
            list = $('#region_id');
            list.val(region_id);
            list.trigger('kladr-update');
            list.trigger("chosen:updated");
        } else {
                    }

        if (last_name) {
            setValue('last_name', last_name)
        }

        if (first_name) {
            setValue('first_name', first_name)
        }

        if (drtr_name) {
            setValue('drtr_name', drtr_name)
        }

        if (ip_number) {
            setValue('ip_number', ip_number)
        }

        if (address) {
            setValue('address', address)
        }

        if (patronymic) {
            setValue('patronymic', patronymic)
        }

        if (date) {
            setValue('date', date)
        }

        function setValue(name, value) {
            value = decodeURIComponent(value).replace(/\++/g,' ');
            $('input[name="is['+name+']"]').val(value);
        }

        if (r1.attr('checked')) {
