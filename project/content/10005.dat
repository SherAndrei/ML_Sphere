                var val = $(this).val();

                if ($.trim) {
                    $(this).val(val.trim());
                } else {
                    $(this).val(val.replace(/(^\s*)|(\s*$)/,""));
                }

                $(this).removeClass('f-error');
                if ($(this).attr('req') == 'true') {
                    if ($(this).val() == '') {
                        $(this).addClass('f-error');
                        error += '<div class="b-msg__text">Поле '+ names[$(this).attr('name')] + ' должно быть заполнено</div>';
                    }

                    var pattern = $(this).data('pattern');
                    if (pattern) {
                        var a = new RegExp('^' + pattern + '$',"i");
                        var test = a.test($(this).val());
                        if (!test) {
                            $(this).addClass('f-error');
                            error += '<div class="b-msg__text">'+$(this).data('title')+'</div>';
                        }
                    }
                }
            });

            selectRegion.removeClass('f-error');
            if ((variant == 1 || variant == 2) && !selectRegion.val()) {
