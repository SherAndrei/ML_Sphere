    <script src="js/libs/tinycolor-min.js"></script>
    <script src="js/jquery_plugins/chromoselector.min.js"></script>


    <script>
      $(function(){
        var btnClass = 'sf-helper-btn',
            bgColor = '',
            colors = ['#9cd600', '#69ae00', '#609f00', '#416B00'],
            base = colorBlend(colors),
            hslOffset = [],
            opacityOffset = [20, -10, 50, -20],
            timer = 0,
            styleLight = {}, styleDark = {},
            fontFamily = {
              arial: 'Arial,Helvetica,sans-serif',
              tahoma: 'Tahoma,Geneva,sans-serif',
              georgia: 'Georgia,serif',
              verdana: 'Verdana,Geneva,sans-serif',
              trebuchet: "'Trebuchet MS',Helvetica,sans-serif",
              helvetica: 'Helvetica,Arial,sans-serif'
            };

        if(!bgColor)
          bgColor = base;

        var params = {
          type: 1,
          text: 'Скачать',
          borderRadius: 0,
          font: '',
          fontSize: 12,
          textOpacity: 60        };

        calcHslOffset();
        initConstructor();
        updateButton();

        // color picker
        $('#color').chromoselector({
          target: '.colorpicker',
          autoshow: false,
          width: 130,
          ringwidth: 10,
          preview: false,
          resizable: false,
          create: changeColor,
          update: changeColor
        }).chromoselector('show', 0).chromoselector('setColor', bgColor);

        //
        $('.buttons a').on('click', function(){
          $('input', this.parentNode).click();
          return false;
        });

        $('.buttons input').on('change', function(){
          params.type = parseInt(this.value);
          updateButton();
        });

        $('#text-opacity').on('change', function(){
          changeTextOpacity(this.value);
          updateButton();
        });

        $('body').on('change click keyup', '#text, #font-size', function(e){
          clearTimeout(timer);
          if(this.value)
          {
            var _this = this;
            timer = setTimeout(function(){
              if(_this.id == 'text')
              {
                params.text = _this.value;
              }
              else if(_this.id == 'font-size')
              {
                params.fontSize = parseInt(_this.value);
              }

              updateButton();
            })
          }
        });

        $('.font-size .action').on('click', function(){
          $('<input/>').attr({
            type: 'number',
            name: 'font-size',
            id: 'font-size',
            value: params.fontSize,
            min: 7,
            max: 40,
            size: 2
          }).replaceAll(this);
        });

        $('input[name="font"]').on('change', function(){
          if(fontFamily[this.value])
          {
            params.font = this.value;
            updateButton();
          }
        });


        function initConstructor()
        {
          var color = base;
          $('.buttons input[value="' + params.type + '"]').attr('checked', '');

          if(!params.font || !fontFamily[params.font])
            params.font = 'arial';

          $('.font-family input[value="' + params.font + '"]').attr('checked', '');

          applyNewColor(color);
          changeTextOpacity(params.textOpacity);
        }

        function getLineHeight(fontSize)
        {
          return Math.round(fontSize * 1.75);
        }

        function getBorderRadius(type, height)
        {
          type %= 3;

          if(type == 1)
            return 4;

          if(type == 2)
            return 21;

          return 2;
        }

        function getColorStyle(type)
        {
          var s = (type < 4) ? styleLight : styleDark;
          for(var i in s)
            params[i] = s[i];
        }

        function calcHslOffset()
        {
          var baseHsl = tinycolor(base).toHsl();
          for(var i = 0; i < colors.length; i++)
          {
            var hsl = tinycolor(colors[i]).toHsl();
            hslOffset.push({
              h: baseHsl.h - hsl.h,
              s: baseHsl.s - hsl.s,
              l: baseHsl.l - hsl.l
            });
          }
        }

        function calcNewColors(baseClr)
        {
          var baseHsl = tinycolor(baseClr).toHsl(),
              newColors = [];

          for(var i = 0; i < hslOffset.length; i++)
          {
            var hsl = {
              h: (baseHsl.h - hslOffset[i].h) % 360,
              s: Math.min(1, baseHsl.s - hslOffset[i].s),
              l: Math.min(1, baseHsl.l - hslOffset[i].l)
            };

            if(hsl.h < 0)
              hsl.h = 360 - hsl.h;

            newColors[i] = tinycolor(hsl).toHexString();
          }

          return newColors;
        }

        function changeColor()
        {
          var color = $(this).chromoselector('getColor').getHexString();
          applyNewColor(color);
          updateButton();
        }

        function applyNewColor(color)
        {
          var clrs = calcNewColors(color);

          styleLight.gFrom = clrs[0];
          styleLight.gTo = clrs[1];
          styleLight.backgroundColor = colorBlend([clrs[0], clrs[1]]);

          styleDark.gFrom = clrs[2];
          styleDark.gTo = clrs[3];
          styleDark.backgroundColor = colorBlend([clrs[2], clrs[3]]);

        }

        function changeTextOpacity(value)
        {
          value = parseInt(value);
          var c = [10, -20, 40, -30];
          for(var i = 0; i < c.length; i++)
          {
            c[i] = value + c[i];
            if(c[i] < 0)
              c[i] = 0;
            else if(c[i] > 100)
              c[i] = 100;

            c[i] /= 100;
          }

          styleLight.textColor = 'rgba(0,0,0,' + c[0] + ')';
          styleLight.textShadow = '1px 1px 1px rgba(255,255,255,' + c[1] + ')';
          styleDark.textColor = 'rgba(255,255,255,' + c[2] + ')';
          styleDark.textShadow = '-1px -1px 1px rgba(0,0,0,' + c[3] + ')';
        }

        function updateButton()
        {
          $('.result-btn').html(getBtnHtmlCode());
          $('#result-code').val(getBtnApiCode());
        }

        function getBtnHtmlCode()
        {
          updateParams();

          return '<a class="' + btnClass +
              '" style="' + getBtnStyle() + '" data-savefrom-get-links="ссылка_на_видео">' +
              '<span class="' + btnClass + '-text">' + params.text + '</span>&nbsp;' +
              '<span class="' + btnClass + '-arrow" style="' + getArrowStyle() +
              '">»</span></a>';
        }

        function getBtnApiCode() {
          console.log('params', params);

          var styles = [
            'sfButton.styles = {'
          ];

          if (params.textColor) styles.push(' color: \'' + params.textColor + '\',');
          if (params.textShadow) styles.push(' textShadow: \'' + params.textShadow + '\',');
          if (params.fontFamily) styles.push(' fontFamily: \'' + params.fontFamily + '\',');
          if (params.fontSize) styles.push(' fontSize: \'' + params.fontSize + 'px\',');
          if (params.borderRadius) styles.push(' borderRadius: \'' + params.borderRadius + 'px\',');
          if (params.gFrom && params.gTo) {
            styles.push(
                ' backgroundColor: \'' + (params.backgroundColor || params.gTo) + '\',',
                ' \'background-image\': \'-webkit-linear-gradient(' + params.gFrom + ', ' + params.gTo + ')\',',
                ' \'background-image\': \'-moz-linear-gradient(' + params.gFrom + ', ' + params.gTo + ')\',',
                ' \'background-image\': \'linear-gradient(' + params.gFrom + ', ' + params.gTo + ')\','
            );
          }

          styles.push(
              ' border: \'none\'',
              '};'
          );

          return '' +
            '\<script\>\n' +
            '// Вставка скрипта:\n' +
            '(function(w, d, s, p, a, c) {\n' +
              'w.sfButton = w.sfButton || function(){};\n' +
              'c = d.getElementsByTagName(s)[0];\n' +
              'a = d.createElement(s);\n' +
              'a.async=1;\n' +
              'a.src=p;\n' +
              'c.parentNode.insertBefore(a, c);\n' +
            '})(window, document, \'script\', \'//savefrom.net/js/sf-helper-agent.min.js\');' +
            '\n\n' +
            '// Текст:\n' +
            'sfButton.text = \'' + params.text + '\';' +
            '\n\n' +
            '// Cтили:\n' +
            styles.join('\n') +
            '\n' +
            '\<\/script\>';
        }

        function updateParams()
        {
          params.lineHeight = getLineHeight(params.fontSize);
          params.borderRadius = getBorderRadius(params.type, params.lineHeight);
          getColorStyle(params.type);

          params.fontFamily = fontFamily[params.font];
        }

        function getBtnStyle()
        {
          var style = [
            'border-width: 0;',
            'border-radius: {borderRadius}px;',
            'display: inline-block;',
            'font: {fontSize}px/2em {fontFamily};',
            'font-weight: bold;',
            'padding: 0 0.7em;',
            'text-decoration: none;',
            'vertical-align: middle;',
            'color: {textColor};',
            'background-color:{backgroundColor};',
            'background-image:-webkit-gradient(linear,0 0,0 100%,from({gFrom}),to({gTo}));',
            'background-image:-webkit-linear-gradient({gFrom},{gTo});',
            'background-image:-moz-linear-gradient({gFrom},{gTo});',
            'background-image:-o-linear-gradient({gFrom},{gTo});',
            'background-image:linear-gradient({gFrom},{gTo});',
            'text-shadow: {textShadow};'
          ];

          style = style.join(' ');
          style = style.replace(/\{([\w]+)\}/g, function(s, m1){
            if(params[m1])
              return params[m1];

            return s;
          });

          return style;
        }

        function getArrowStyle()
        {
          var style = [
            'display: inline-block;',
            'vertical-align: middle;',
            'font-size: 1.5em;',
            'margin-left: 0.2em;',
            '-webkit-transform: rotate(90deg);',
            '-moz-transform: rotate(90deg);',
            '-ms-transform: rotate(90deg);',
            '-o-transform: rotate(90deg);',
            'transform: rotate(90deg);'
          ];

          return style.join(' ');
        }

        function colorBlend(colors)
        {
          var res = {h: 0, s: 0, l: 0};
          for(var i = 0; i < colors.length; i++)
          {
            var hsl = tinycolor(colors[i]).toHsl();
            res.h += hsl.h;
            res.s += hsl.s;
            res.l += hsl.l;
          }

          res.h /= colors.length;
          res.s /= colors.length;
          res.l /= colors.length;

          return tinycolor(res).toHexString();
        }

        function normalizeNumber(n, min, max)
        {
          if(n < min)
            return min;
          if(n > max)
            return max;

          return n;
        }

        // full-range YCbCr http://www.equasys.de/colorconversion.html
        function yuv2rgb(yuv)
        {
          var y = yuv.y,
              u = yuv.u - 128,
              v = yuv.v - 128;

          var rgb = {
            r: Math.floor(y + 1.4 * v),
            g: Math.floor(y - 0.343 * u - 0.711 * v),
            b: Math.floor(y + 1.765 * u)
          };

          normalizeNumber(rgb.r, 0, 255);
          normalizeNumber(rgb.g, 0, 255);
          normalizeNumber(rgb.b, 0, 255);

          return rgb;
        }

        function rgb2yuv(rgb)
        {
          var color = (typeof(rgb) == 'object') ? rgb : tinycolor(rgb).toRgb();
          var r = color.r, g = color.g, b = color.b;

          var yuv = {
            y: Math.floor(r *  .299 + g *  .587 + b *  .114),
            u: Math.floor(r * -.169 + g * -.331 + b *  .500) + 128,
            v: Math.floor(r *  .500 + g * -.419 + b * -.081) + 128
          };

          return yuv;
        }
      });
    </script>

