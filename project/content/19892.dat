        <!-- REPORT / DELETE / EDIT / QUOTE DIV -->
        <div align="right"> <span> Отправлено 
          <a title="Ссылка этого сообщения" href="#" onclick="link_to_post(55722); return false;">#12</a> 
          </span> </div></td>
