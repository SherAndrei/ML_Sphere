<IDXSKIP><table width="100%" border="0" align="center" cellpadding="2px" cellspacing="0"><tr><td align="left" bgcolor="#1A3A80" background="/icon/41vb.gif" class="tblock" title="Все публикации" style="padding-left:30px;height:25px"><a href="/common/archive.php?table=publ" style="text-decoration:none">ПУБЛИКАЦИИ</a></td></tr></table></IDXSKIP>
