<div class=" c-user-card c-user-card--h c-user-card--nano"  itemscope itemtype="http://schema.org/Person">
    <div class="c-user-card__pic-container">
        <div class="c-user-card__pic u-vignette-tiny"><a href="/users/kir1705/"><img src="https://a.d-cd.net/a4b176u-100.jpg" width="25" height="25" itemprop="image" alt=""></a></div>
