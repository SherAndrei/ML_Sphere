    function totalprice(str) {
        var count = parseFloat(str.val());
        var price = parseFloat(str.parents().eq(5).find(".price span").html().replace(/\s+/g, ''));
        var totalprice = (price * count).toString();
        totalprice = totalprice.replace(/(\d{1,3})(?=((\d{3})*([^\d]|$)))/g, " $1 ");
        str.parents().eq(5).find(".totalprice span").html(totalprice);
    }
    $('.orderCart .count input[name=count]').each(function () {
        totalprice($(this));
    });
    $("body").on("change",".orderCart .count input[name=count]", function(e) {
        totalprice($(this));
    });



