        $('.modification-img').val($('.itemPage__left__big a img').attr('src'));
        $('.itemPage__left__list').hide();
        if($(".js-mod option").length >0) {
            var $eventSelect = $(".js-mod").select2();
            function img(val) {
                $('.owl-item').hide();
                var i = 0;
                $('.owl-item > .owlItem').map(function (index, elem) {
                    var e = $(elem).parent(), imgval = $(elem).data('imgmod');
                    imgval = imgval.slice(1);
                    var arrimgval = imgval.split(',');
                    var element = $(this);
                    element.find('a').removeClass('fancy');
                    $.each(arrimgval, function (key, value) {
                        if (val == value) {
                            if (i == 0) {
                                $('.modification-img').val(element.find('img').attr('src'));
                                $('.itemPage__left__big a').attr('href', element.find('img').attr('src'));
                                $('.itemPage__left__big a img').attr('src', element.find('img').attr('src'));
                            }
                            e.show();
                            element.find('a').addClass('fancy');
                            i++;
                            return false;
                        }
                    });
                });
                if (i > 6) {
                    $('.owl-prev').show();
                    $('.owl-next').show();
                } else {
                    $('.owl-prev').hide();
                    $('.owl-next').hide();
                }
                if (i == 1) {
                    $('.itemPage__left__list').hide();
                } else {
                    $('.itemPage__left__list').show();
                }
            }
            setTimeout(function(){
                if ($(".js-mod option").length > 0) {
                    $(".ping.base-procuct").hide();
                    $(".ping.mod-product").eq(0).show();
                }
                val = $(".js-mod option:selected").val();
                img(val);
            }, 10);
            $eventSelect.on("change", function (e, v) {
                $('.ping').hide();
                var val = $('.js-mod').val();
                $('[data-mod="'+val+'"]').show();
                img(val);
            });
        } else {
            $(".ping.base-product").show();
        }
    });

