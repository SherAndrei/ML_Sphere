<script>
	jQuery(document).ready(function ($) {

		var iTopHeight = 1000;
		var iCurrent = 0;
		var iPositionSticky = null;
		var sticky = $(".sticky .Blocksmaster");
		var heightArray = { } ;
		var heightCurrent = 0 ;

		$.each(sticky, function(k, s) {
			heightArray[k] = $(s).height();
			if (k == iCurrent) {
				heightCurrent = heightArray[k];
				$(s).css({
					display: 'block',
					visibility: 'visible',
					height: heightArray[k] + 'px'
				});
			}
		});
		console.log({
			heightArray: heightArray
		});
		setTimeout(function () {

			var windowHeight = $(window).height();
			var contentHeight = $('#content').height();
			var sidebarHeight = $('#sidebar').height();
			var addHeight = Math.floor((contentHeight - sidebarHeight) - 400 * sticky.length);

			console.log({
				length: sticky.length,
				addHeight: addHeight
			});

			$(window).scroll(function(){

				/*console.log({
					oh: $(".sticky-banner").outerHeight(),
					ot: $(".sticky-banner").offset().top,
					st: $(window).scrollTop(),
					iPositionSticky:iPositionSticky
				});*/

				if ($(window).scrollTop() >= $(".sticky-banner").offset().top && !iPositionSticky) {
					iPositionSticky = $(".sticky-banner").offset().top;
					$(".sticky-banner .sticky").height(heightCurrent);
				}

				var ii = $('#content').height()-heightCurrent;
				if (iPositionSticky) {
					if (ii > $(window).scrollTop() - (heightCurrent - 100)) {
						if ($(window).scrollTop() >= iPositionSticky + 46) {
							$(".sticky-banner .sticky").css({
								position: 'fixed',
								top: '50px'
							});
						} else {
							$(".sticky-banner .sticky").css({
								position: 'relative',
								top: 'auto'
							});
						}
					} else {
						$(".sticky-banner .sticky").css({
							position: 'absolute',
							top: (contentHeight - sidebarHeight - 46) + 'px'
						});
					}

					var scrollTop = $(window).scrollTop();
					var iNum = parseInt((scrollTop - iPositionSticky) / iTopHeight);

					var iLastSticky = sticky.length;
					if (iCurrent != iNum) {
						iCurrent = iNum;
						sticky.css({
								display: 'none',
								visibility: 'hidden',
								height: 0
						});
						if (iLastSticky > iCurrent) {
							$.each(sticky, function (k, s) {
								if (k == iCurrent) {
									$(s).css({
										display: 'block',
										visibility: 'visible',
										height: heightArray[k] + 'px'
									});
								}
							});
						} else {
							sticky.eq(-1).css({
								display: 'block',
								visibility: 'visible',
								height: heightArray[k] + 'px'
							});
						}
					}

				}

			});
		}, 500);


	});
</script>
