  function collectComments(block, arr) {
     var finished = true;
     for (var i=0; i<packToCollect.length; i++) {
         if (packToCollect[i][0] == block) {
            packToCollect[i][1] = 1;
            packToCollect[i][2] = arr;
         }
         if (packToCollect[i][1] != 1) {
            finished = false;
         }
     }
     if (finished) {
        comments = [];
        for (var i=0; i<packToCollect.length; i++) {
            for (var j=packToCollect[i][3]; j>(packToCollect[i][3]-packToCollect[i][4]); j--) {
              if (packToCollect[i][2][j]) {
                comments.push(packToCollect[i][2][j]);
              }
            }
        }
        showComments();
     }
  }

