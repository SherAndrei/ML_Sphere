  function reply_chars_counter() {
    var limit = 9999;
    if ($('textarea#comment_reply_textarea').size() == 0) return;
    var ta = $('textarea#comment_reply_textarea');
    var len = $(ta).val().length;
    var all = '' + len;
    var c = $('#reply_chars_counter');
    if ( len > limit ) {
      $(c).css({color: 'red'});
      $('#reply_comment_submit_button').attr({disabled: 'disable'}).css({color: 'lightgray'});
      all = len - limit;
      if (all > limit) all = limit;
      all = '' + all;
    } else {
      $(c).css({color: 'black'});
      $('#reply_comment_submit_button').removeAttr('disabled').css({color: 'black'});
    }
    for(;all.length < 4;all = '0'+all){};
    var symbol_height = $(c).find('.chars_counter:eq(0) div:eq(0)').height() + 1;
    $('#reply_chars_counter').css({height: (symbol_height+1)+'px'});
    for(var i=0; i < 4 ;i++) {
      var o = $(c).find('.chars_counter').eq(i);
      var n = -symbol_height*Number(all[i])+'px';
      if(n != o.offset().top) o.stop().animate({top: n});
    }
    setTimeout('reply_chars_counter()', 750);
  }

