<script type="text/javascript">

    var recaptchaCallback = function () {
        grecaptcha.render("g-recaptcha", {
            sitekey: '6LcNmSETAAAAAJZhqRQIW_tnLaWQwYip5hL3X7dV',
            callback: function (jQuery) {
                $('#recaptcha').valid();
            }
        });
    };

    $(function(){
        $(document).ready(function() {

            var showCommentList = function () {

                var startCommentsCount = 10, // How much comments will be shown at start time
                    showCommentsCount  = 20; // How much comments will be shown in one time

                var commentObjects = [],
                    shownCommentsCount = 0,
                    moreButton = $('.show-more-comments-button .btn-default');

                $('.comment-post').each(function (key, value) {
                    commentObjects[key] = value;
                });

                for (var i=0; i<=startCommentsCount-1; i++) {
                    $(commentObjects[i]).removeClass('hidden');
                    $(commentObjects[i]).addClass('visible');
                    shownCommentsCount++;
                }
                if (shownCommentsCount >= commentObjects.length) {
                    moreButton.hide();
                }
                moreButton.on('click', function (e) {
                    e.preventDefault();
                    for (var i=shownCommentsCount; i<(shownCommentsCount+showCommentsCount); i++) {
                        $(commentObjects[i]).removeClass('hidden');
                        $(commentObjects[i]).addClass('visible');
                    }
                    shownCommentsCount = shownCommentsCount + showCommentsCount;

                    if (shownCommentsCount >= commentObjects.length) {
                        $(this).hide();
                    }
                    $(this).blur();
                });
            };

            var ajaxPostComment = function (form) {
                $.ajax({
                    type: "POST",
                    data: form.serialize(),
                    url: "/ru/comments/post/",
                    cache: false,
                    dataType: "html",
                    success: function(html, textStatus) {
                        $('.comments-addcomment').replaceWith(html);
                        prepareCommentForm();
                    },
                    error: function (XMLHttpRequest, textStatus, errorThrown) {
                        if ($('#comment_alert').length == 0) {
                            
                            
                            $('<div id="comment_alert" class="alert alert-warning alert-dismissable"><strong>Предупреждение</strong> Ваш комментарий не может быть опубликован. Извините за доставленные неудобства.</div>').insertBefore( $('.comments-form') );
                        }
                        $('.comments-form .fa-spinner').hide();
                        $('.comments-form .btn').removeClass('disabled');
                    }
                });
            };

            var prepareCommentForm = function() {
                var form = $('.comments-form');

                form.validate({
                    ignore: '',
                    rules: {
                        name: { required: true },
                        email: {
                            email: true,
                            required: true
                        },
                        comment: { required: true },
                        recaptcha: {
                           required: function() {
                               if(grecaptcha.getResponse() === '') {
                                   $('.recaptcha_block').addClass('has-error');
                                   $('span[for="recaptcha"]').insertBefore( $('.recaptcha_block div div') );
                                   return true;
                               } else {
                                   $('.recaptcha_block').removeClass('has-error');
                                   return false;
                               }
                           }
                        }
                    },
                    messages: {
                        name: {
                            required: 'Пожалуйста, введите свое имя',
                        },
                        email: {
                            required: 'Пожалуйста, введите свой емейл'
                        },
                        comment: {
                            required: 'Пожалуйста, напишите ваш комментарий'
                        },
                        recaptcha: {
                            required: 'Пожалуйста, проверьте reCAPTCHA'
                        }
                    },
                    highlight: function(element) {
                        $(element).closest('.form-group').addClass('has-error');
                    },
                    unhighlight: function(element) {
                        $(element).closest('.form-group').removeClass('has-error');
                    },
                    errorElement: 'span',
                    errorClass: 'help-block',
                    errorPlacement: function(error, element) {
                        if(element.parent('.input-group').length) {
                            error.insertAfter(element.parent());
                        } else {
                            error.insertAfter(element);
                        }
                    }
                });

                form.submit(function() {
                    if (form.valid()) {
                        $('.comments-form .fa-spinner').show();
                        $('.comments-form .btn').addClass('disabled');

                        $.ajaxSetup({
                            beforeSend: function(xhr, settings) {
                                function csrfSafeMethod(method) {
                                    return (/^(GET|HEAD|OPTIONS|TRACE)$/.test(method));
                                }
                                function getCookie(name) {
                                    var cookieValue = null;
                                    if (document.cookie && document.cookie != '') {
                                        var cookies = document.cookie.split(';');
                                        for (var i = 0; i < cookies.length; i++) {
                                            var cookie = jQuery.trim(cookies[i]);
                                            if (cookie.substring(0, name.length + 1) == (name + '=')) {
                                                cookieValue = decodeURIComponent(cookie.substring(name.length + 1));
                                                break;
                                            }
                                        }
                                    }
                                    return cookieValue;
                                }
                                if (!csrfSafeMethod(settings.type) && !this.crossDomain) {
                                    xhr.setRequestHeader("X-CSRFToken", getCookie('csrftoken'));
                                }
                            }
                        });
                        $.ajax({
                            type: "POST",
                            url: '/api/recaptcha/post/',
                            cache: false,
                            data: {recaptcha_response: grecaptcha.getResponse()},
                            success: function(data) {
                                grecaptcha.reset();
                                ajaxPostComment(form);
                            },
                            error: function (error) {
                                grecaptcha.reset();
                                alert('Вы робот? Пожалуйста, проверьте reCAPTCHA');
                            }
                        });
                    } else {
                        $('.comments-form').validate();
                    }
                    return false;
                });
            };
            showCommentList();
            prepareCommentForm();
        });

        $(window).load(function() {
            var setCommentDatetime = function () {
                function dateFromNow(commentDate) {
                    return moment(commentDate).locale("ru").fromNow();
                }
                $('.comment-datetime').each(function (key, value) {
                    var commentDate = new Date( $(value).data("comment-datetime") );
                    $(value).text( dateFromNow(commentDate) );
                    $(value).prev().append('<span class="comment-separator"></span>');
                });
            };
            
            $.getScript("https://static.4kdownload.com/3rdparty/moment/moment.min.03c1d3ad0acf.js").done(function() {
                $.getScript("https://static.4kdownload.com/3rdparty/moment/locale/ru.82a9d4e1f275.js").done(function() {
                        setCommentDatetime();
                    });
                });
        });
    });
</script>
