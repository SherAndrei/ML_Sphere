        <script>
            $(document).ready(function () {
                initBigBanner();
                initFilterMenu();
                setTextSize();
                showSocials();
                initBrandSlider();
            });

            // Показывает кнопки социальных сетей в заголовке
            // на главной странице
            function showSocials() {
                $('.social2').addClass('social2--show');
            }

            // Устанавливает размер текст в зависимости от длинны текста
            function setTextSize() {
                $('.standart-block .text-block h3').each(function (idx, el) {
                    if (el.clientWidth < el.scrollWidth)
                        $(el).textfill();
                });
            }

            // Показывает и скрывает большой баннер в шапке сайта при скроллинге страницы  
            function initBigBanner() {
                var $banner = $('.big-poster-emul').first();
                var $window = $(window).first();
                var isSlideUp = false;
                var isSlideDown = true;
                $window.scroll(function () {
                    var scrollTopVal = $window.scrollTop();
                    if (scrollTopVal > 0 && !isSlideUp) {
                        isSlideUp = true;
                        isSlideDown = false;
                        //$banner.toggle();
                        //$banner.stop(true, false).slideToggle("slow");
                        $banner.velocity('stop').velocity('slideUp', 250);
                    }
                    else if (scrollTopVal <= 0 && !isSlideDown) {
                        isSlideUp = false;
                        isSlideDown = true;
                        //$banner.toggle();
                        //$banner.stop(true, false).slideToggle("slow");
                        $banner.velocity('stop').velocity('slideDown', 250);
                    }
                });
            }

            // Показывает и скрывает меню с фильтром фильмов
            function initFilterMenu() {
                var $margin = $('.zal_main_container').width() / 2;
                $('.zal_main_container').css('margin-left', -$margin);

                $(".open-menu").on("click", function () {
                    $(".search_film_slider").stop().slideToggle(500);
                    $('.open-menu').toggleClass('active');
                });

                $('.hidden-menu').hover(
                    function () {
                        $('.dop-menu-link').toggleClass('alert')
                    })
            }

            // Инициализирует слайдер брендирования
            function initBrandSlider() {

                // Удаляем дублирующиеся баннеры
                var slides = $('.big-poster__bxslider').find('li');
                var uniqSlides = [];
                for (var i = 0; i < slides.length; i++) {
                    var link = $(slides[i]).find('a').first().attr('href');
                    if (uniqSlides.indexOf(link) == -1)
                        uniqSlides.push(link);
                    else
                        $(slides[i]).remove();
                }

                // Если только один слайдер, то отключаем слайдшоу 
                // и скрываем кнопки слайдера
                var autoSlide = true;
                if (uniqSlides.length <= 1) {
                    $('.big-poster__control').hide();
                    autoSlide = false;
                }

                // Изначально скрыт что бы не ломать верстку
                $('.big-poster__bxslider').show();

                $('.big-poster__bxslider').bxSlider({
                    auto: autoSlide,
                    pause: 5000,
                    touchEnabled: false,
                    nextSelector: '#big-poster__next',
                    prevSelector: '#big-poster__prev',
                    nextText: '',
                    prevText: '',
                    onSlideAfter: function (slide) {
                        $('.big-promo-link').attr('href', slide.find('a').first().attr('href'));
                    }
                });
            };
        </script>
        
