		flashName: 'flashmediaelement.swf',
		silverlightName: 'silverlightmediaelement.xap',
		alwaysShowControls: true,
		duration: value.duration,
		startVolume: 1,
		success: function(player, node) {

			var textTitle = $('<span>').text(value.filmTitle);
			var textSub = $('<div>').css({'font-size':'15px','margin-top':'5px'}).text(value.filmSlogan);
			var ID = $(node)[0].player.id;
			$('#'+ID).css('position', 'relative');
			$('#'+ID).append(
				$('<div>')
					.attr('class', 'wsPlayerPanel')
					.css({
						'background-color': 'rgba(0, 0, 0, 0.6)',
						'display': 'block',
						'position': 'absolute',
						'left': '0px',
						'bottom': '30px',
						'z-index': '10',
						'width': '100%',
						'color': '#F2F2F2',
						'text-align': 'center',
						'padding-top': '5px',
						'padding-right': '0px',
						'padding-bottom': '5px',
						'padding-left': '0px',
						'font-size': '20px',
						'font-family': 'Arial, sans-serif',
						'opacity': '1'
					}).append(textTitle).append(textSub));

			var time = 0;
			player.addEventListener('timeupdate', function(e) {
				var newTime = Math.abs(player.currentTime - time);
				if (newTime < 1) {
					time = player.currentTime;
				}
			}, false);

			player.addEventListener('pause', function(e) {
				//console.log(e);
			}, false);

			player.addEventListener('play', function(e) {
				//console.log('play');
								//console.log(value.openType);
				if (value.openType == 3) {
					if (!regForm.getUniqueFlag()) {
						if (document.exitFullscreen) {
							document.exitFullscreen();
						}
						else if (document.mozCancelFullScreen) {
							document.mozCancelFullScreen();
						}
						else if (document.webkitCancelFullScreen) {
							document.webkitCancelFullScreen();
						}
						else if (document.cancelFullScreen) {
							document.cancelFullScreen();
						}
						if (player.isFullScreen) {
							player.exitFullScreen();
						}
						setTimeout(function(){
							regForm.setCurrentPlayer(node);
							player.pause();
							$('.mejs-poster').css({display:'block'});
							regForm.init(value);
							regForm.show();
						}, 500);
					}
				} else if (value.openType == 4) {
					if (!regForm.getUniqueFlag()) {
						if (document.exitFullscreen) {
							document.exitFullscreen();
						}
						else if (document.mozCancelFullScreen) {
							document.mozCancelFullScreen();
						}
						else if (document.webkitCancelFullScreen) {
							document.webkitCancelFullScreen();
						}
						else if (document.cancelFullScreen) {
							document.cancelFullScreen();
						}
						if (player.isFullScreen) {
							player.exitFullScreen();
						}
						setTimeout(function(){
							regForm.setCurrentPlayer(node);
							player.pause();
							$('.mejs-poster').css({display:'block'});
							regForm.init(value);
							setTimeout(function () {
								if (getCookie('UniqueFlag') == '') {
									setCookie('UniqueFlag', 'true', 30 * 24);
									if(value.alertText != ''){
										alert(value.alertText);
									}
								}
								regForm.show();
							}, 3000)
						}, 500);
					}
				} else if (value.openType == 5) {
					if (document.exitFullscreen) {
						document.exitFullscreen();
					}
					else if (document.mozCancelFullScreen) {
						document.mozCancelFullScreen();
					}
					else if (document.webkitCancelFullScreen) {
						document.webkitCancelFullScreen();
					}
					else if (document.cancelFullScreen) {
						document.cancelFullScreen();
					}
					if (player.isFullScreen) {
						player.exitFullScreen();
					}
					setTimeout(function(){
						regForm.setCurrentPlayer(node);
						player.pause();
						regForm.init(value);
						//console.log(12);
						regForm.show();
					}, 500);
				} else {
					if (value.openType == 7) {
						regForm.setCurrentPlayer(node);
					}
					if (playFlag == 0) {
						var ID = $(node)[0].player.id;
						$('#' + ID).children('.wsPlayerPanel').css('display', 'block');
						$('#' + ID).children('.wsPlayerPanel').css('opacity', '0');
						$('#' + ID).children('.wsPlayerPanel').animate({opacity: 1}, 5000, function () {
							$(this).animate({opacity: 0}, 2000);
						});
						playFlag = 1;
					}
				}

			}, false);

			player.addEventListener('seeked', function(e) {
				//console.log('seeked');
			}, false);

			player.addEventListener('error', function(e) {
				//console.log('error');
				// останавливаем плеер
				player.pause();
				$('.mejs-poster').css({display:'block'});
				if (document.exitFullscreen) {
					document.exitFullscreen();
				}
				else if (document.mozCancelFullScreen) {
					document.mozCancelFullScreen();
				}
				else if (document.webkitCancelFullScreen) {
					document.webkitCancelFullScreen();
				}
				else if (document.cancelFullScreen) {
					document.cancelFullScreen();
				}
				if (player.isFullScreen) {
					player.exitFullScreen();
				}
				setTimeout(function() {
					if ($.inArray(value.openType, [2, 3, 4, 5, 7]) != -1) {
						// вызываем регистрацию
						regForm.init(value);
						regForm.show();
					} else {
						regForm.initOldFormReg(node, value);
					}
				},500);
			}, false);

			player.addEventListener('seeking', function(e) {
				if(player.currentTime != 0){
					// убираем полноэкранный режим
					if (document.exitFullscreen) {
						document.exitFullscreen();
					}
					else if (document.mozCancelFullScreen) {
						document.mozCancelFullScreen();
					}
					else if (document.webkitCancelFullScreen) {
						document.webkitCancelFullScreen();
					}
					else if (document.cancelFullScreen) {
						document.cancelFullScreen();
					}
					if (player.isFullScreen) {
						player.exitFullScreen();
					}
					setTimeout(function() {
						if ($.inArray(value.openType, [2, 3, 4, 5, 7]) != -1) {
							var regexp = /.+\.flv$/i;
							if (regexp.test(player.src)) {
								// для flash перезагружаем ролик
								player.setSrc(player.src);
								player.load();
								player.play();
								setTimeout(function () {
									player.pause();
								}, 2000);

							} else {
								// для html5 сбрасываем время
								player.pause();
								player.setCurrentTime(0);
							}
							regForm.init(value);
							regForm.show();
							$('.mejs-poster').css({display: 'block'});
						} else {
							// для flash перезагружаем ролик
							player.setSrc(player.src);
							player.load();

							if (!$('#spPlayerReg').length) {
								//alert("Перемотка доступна после регистрации");
								regForm.initOldFormReg(node, value);
							}
						}
					},500);
				}
			}, false);

			player.addEventListener('ended', function(e) {
				time = 0;

				// останавливаем плеер
				player.pause();
				player.setCurrentTime(0);
				$('.mejs-poster').css({display:'block'});
				if (document.exitFullscreen) {
					document.exitFullscreen();
				}
				else if (document.mozCancelFullScreen) {
					document.mozCancelFullScreen();
				}
				else if (document.webkitCancelFullScreen) {
					document.webkitCancelFullScreen();
				}
				else if (document.cancelFullScreen) {
					document.cancelFullScreen();
				}
				if (player.isFullScreen) {
					player.exitFullScreen();
				}
				setTimeout(function() {
					if ($.inArray(value.openType, [2, 3, 4, 5, 7]) != -1) {
						// вызываем регистрацию
						regForm.init(value);
						regForm.show();
					} else {
						if (!$('#spPlayerReg').length) {
							regForm.initOldFormReg(node, value);
						}
					}
				},500);
			}, false);

			player.addEventListener('loadeddata', function(e) {
				//console.log('loadeddata');
			}, false);

			player.addEventListener('loadedmetadata', function(e) {
				//console.log('loadedmetadata');
			}, false);

			player.addEventListener('progress', function(e) {
			}, false);

			player.addEventListener('canplay', function(e) {
				//console.log('canplay');
			}, false);

			player.addEventListener('playing', function(e) {
				//console.log('playing');
			}, false);

			$(".mejs-overlay-loading").css({ display: "none" });
			setTimeout(function(){
				$(".mejs-time-buffering").css({ display: "none" });
			},1000);
		},
		error: function () {
			//console.log('error');
			$('.video-player').remove();
		}
	});
});
</script>
</body>
