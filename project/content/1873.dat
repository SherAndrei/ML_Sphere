function start_elem(el) {
	var online = parseInt($(el).find('.spCommentsCountId').html()) + 1;
	$(el).find('.spCommentsCountId').html(online);

	var now = new Date();
	var hour = now.getHours();
	var min = now.getMinutes();
	var sec = now.getSeconds();
	if (hour.toString().length == 1) hour = '0' + hour;
	if (min.toString().length == 1) min = '0' + min;
	if (sec.toString().length == 1) sec = '0' + sec;
	$(el).find('.spMesElem:last .spComTime').html(hour + ':' + min + ':' + sec);

	$(el).find('.spBlockMes').css('display', 'block');
	$(el).find('.spMesElem:last').prependTo($(el).find('.spBlockMes'));
	var height0 = $(el).find('.spMesElem:eq(0)').height();
	var height1 = $(el).find('.spMesElem:eq(1)').height();
	var height2 = $(el).find('.spMesElem:eq(2)').height();
	var height3 = $(el).find('.spMesElem:eq(3)').height();
	var height = height0 + height1 + height2 + height3;
	$(el).find('.spBlockMes').css("height", height + "px");
	setTimeout(function() {
		start_elem(el);
	}, 23000);
}

