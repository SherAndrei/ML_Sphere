function AjaxAction(script, element, params, result, func) {
	var req = new JsHttpRequest();
    req.onreadystatechange = function() {
    	if (req.readyState == 4) {
			if (req.responseJS) {
				elem = $('#'+element);
				if (elem) elem.html(req.responseJS[result]);
				if (func) func(req.responseJS);
			}
			if (req.responseText) console.log(req.responseText);
     	}
    }
    req.caching = false;
    req.open(null, script, true);
    req.send(params);
}

var popoverTimeout = null;
var catalogTimeout = null;

function CartAction(action, id, brand, model, gen, data, dataAction) {

	var in_order = false;
	if (window.UpdateOrder) in_order = true;

    // dataLayer
    if (window.dataLayer && data && dataAction) {
        var ecommerce = {};
        ecommerce[dataAction] = data;
        dataLayer.push( { "ecommerce": ecommerce } );
    }

	AjaxAction('http://www.bestparts.ru/ajax.cart.html', in_order ? '' : 'top_basket', { action: action, id: id, brand: brand, model: model, gen: gen }, 'cart', function(res) {
		if (action == 'add') {
		 	HidePopover(3000);
		}
		if(in_order) UpdateOrder();
	});
	return false;
}

function CartAdd(id, brand, model, gen, data) { CartAction('add', id, brand, model, gen, data, "add"); }
function CartPlus(id, brand, model, gen, data) { CartAction('plus', id, brand, model, gen, data, "add"); }
function CartRemove(id, brand, model, gen, data) { CartAction('remove', id, brand, model, gen, data, "remove"); }

function ClearPopoverTimeout() {
	if (popoverTimeout) clearTimeout(popoverTimeout);
}

function ClearCatalogTimeout() {
	if (catalogTimeout) clearTimeout(catalogTimeout);
}

function HidePopover(timeout) {
	ClearPopoverTimeout();
	popoverTimeout = setTimeout(function() { $('#popover').hide(); }, timeout ? timeout : 500);
}

function ShowPopover() {
	ClearPopoverTimeout();
	popoverTimeout = setTimeout(function() { $('#popover').show(); }, 300);

}

function ShowCatalog() {
	ClearCatalogTimeout();
	catalogTimeout = setTimeout(function() { $('#catalog').show(); }, 100);
}

function HideCatalog() {
	ClearCatalogTimeout();
	catalogTimeout = setTimeout(function() { $('#catalog').hide(); }, 100);
}


$(document).click(function(e){
	if(
		(
			$(e.target).closest('#popover').length != 0 ||
			$(e.target).closest('#catalog').length != 0
		)
		&& $(e.target).closest('A').length == 0
	) return false;

	$('#popover').hide();
 	ClearPopoverTimeout();
	$('#catalog').hide();
	ClearCatalogTimeout();
});

function SpinUp(ctl) {
	var val = parseInt(ctl.val());
	ctl.val(val+1);
	ctl.keyup();
}

function SpinDown(ctl) {
	var val = parseInt(ctl.val());
	if (val > 1) {
		ctl.val(val - 1);
		ctl.keyup();
	}
}

