<script>
	function agreeSwith(){
		if ( $( '.chkAgree' ).attr( 'ch' ) === '1' ){
			$( '.chkAgree' ).attr( 'ch', '0' );
		} else{
			$( '.chkAgree' ).attr( 'ch', '1' );
		}
		agreeUpdate();
	}
	function agreeUpdate(){
		if ( $( '.chkAgree' ).attr( 'ch' ) === '0' ){
			$( '.chkAgree' ).attr( 'ch', '0' );
			$( '.chkAgree' ).css( 'background-position', '0px 0px' );
			$( '.btnSend' ).prop( 'disabled', true );
		} else{
			$( '.chkAgree' ).attr( 'ch', '1' );
			$( '.chkAgree' ).css( 'background-position', '-24px 0px' );
			$( '.btnSend' ).prop( 'disabled', false );
		}
	}
	function agreeOpen( isForcedOpen ){
		if ( $( '.chkAgree' ).attr( 'ch' ) === '1' && isForcedOpen !== true )
		{
			agreeSwith();
			return false;
		}

		$('.modal_agree').fadeIn(200);
	}
	function agreeClose(){
		$('.modal_agree').fadeOut(200);
	}
	function agreeAccept(){
		$( '.chkAgree' ).attr( 'ch', '1' );
		agreeUpdate();
		agreeClose();
	}
	function agreeReject(){
		$( '.chkAgree' ).attr( 'ch', '0' );
		agreeUpdate();
		agreeClose();
	}
</script>

