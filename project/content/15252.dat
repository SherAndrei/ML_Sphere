    function FeedbackClick() {
        if ($("#ctl00_FeedbackComment").val().length == 0) {
            alert("Поле \"Сообщение\" должно быть заполнено");

            return false;
        }
        if ($("#ctl00_FeedbackEmail").val().length == 0) {
            alert("Поле \"Email\" должно быть заполнено");
            return false;
        }
        $(this).unbind('click');
        date = "{'FeedbackName':'" + $("#ctl00_FeedbackName").val() + "','FeedbackPhone':'" + $("#ctl00_FeedbackPhone").val() + "','FeedbackEmail':'" + $("#ctl00_FeedbackEmail").val() + "','FeedbackComment':'" + $("#ctl00_FeedbackComment").val() + "'}";
        $.ajax({
            type: "POST",
            url: 'https://www.gnicpm.ru:443/Default.aspx/btFeedbackClick',
            data: date,
            contentType: "application/json; charset=utf-8",
            dataType: "json",
            success: function (msg) {
                if (msg.d.length > 0) {
                    $("#FeedbackMsg").html(msg.d);
                }
                else {

                    $("#FeedbackMsg").html("<span style=''>Ваше сообщение отправлено!</span>");
                }
                $("#ctl00_pnFeedback").css("display", "none");
                $("#ctl00_pnFeedbackMsg").css("display", "block");
                $(this).click(function () {
                    FeedbackClick();
                });

            },
            error: function (msg) {

                $("#FeedbackMsg").html("<span style=''>Произошла неизвестнная ошибка!</span>");
                $("#ctl00_pnFeedback").css("display", "none");
                $("#ctl00_pnFeedbackMsg").css("display", "block");
                $(this).click(function () {
                    FeedbackClick();
                });
            }

        });

