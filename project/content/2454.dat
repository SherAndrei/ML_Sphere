<SCRIPT type= "text/javascript">
function	TownNewSet()
	{
	objhTownURL=document.getElementById('hTownUrlName');
	if(!objhTownURL.value)
		{
		alert('Вы не выбрали город!');
//		$_SESSION['fTownNewSet']=false;
		return;
		}
		
	//Если поменяли город, то надо обновить кукис 'city_to'	
//	$_SESSION['fTownNewSet']=true;
	
	DivTownChoice.style.display='none';
var	WinLoc=window.location.toString();
	pos = WinLoc.indexOf("?");
	if(pos!=-1)
                WinLoc=WinLoc.substring(0,pos);
        window.location=WinLoc+'?city='+objhTownURL.value;
	}
function	TownSelect(tTownName,tOblName,tTownUrlName)
	{
	if(tTownUrlName=="msk")
		tTownUrlName="moscow";       
	tdSelTown.innerHTML='<SPAN STYLE="color:BLUE;">'+tTownName+'</SPAN> ('+tOblName+')'+'<INPUT TYPE="hidden" name="hTownUrlName" id="hTownUrlName" VALUE="'+tTownUrlName+'">';
	RegionName.value=tOblName;
	TownNewSet();
	}

var	aTown=new Array();
var	TABLE_TOWN_MAX_NO_COLS=3;
var	NaselLim=400;
function	TownLoad()
	{
var	xmlhttp = getXmlHttp(); // Объект XMLHttpRequest
var	request = "/TownChoice.php?TownName="+encodeURIComponent(eTown.value);
        tdTown.innerHTML="Загружаю список городов...";
	xmlhttp.open('GET', request, false); // Передача запроса
	xmlhttp.setRequestHeader('Content-Type', 'application/x-www-form-urlencoded')
	xmlhttp.send(null);
	if(xmlhttp.status != 200)
		{
		tError="Сервис не реализован... \nкод ошибки:="+xmlhttp.status+"\nописание ошибки:"+xmlhttp.statusText;
		tdTown.innerHTML=tError;
		alert(tError);
		return;
		}
var	sTown=(xmlhttp.responseText.toString()).split("\n");
var	TownID=0;
	for (Idx in sTown)
		{
	var	pTown=sTown[Idx].split("\t");
		if(parseInt(pTown[0])==784)
			{
                        aTown[TownID]=aTown[0];
                        aTown[0]=pTown;
			}
		else if(parseInt(pTown[0])==75)
			{
                        aTown[TownID]=aTown[1];
                        aTown[1]=pTown;
			}
		else
              		aTown[TownID]=pTown;
			
                TownID++;
		}
	tdTown.innerHTML="загружено городов:"+TownID;
	}
function	onTownKeyPress()
	{
var	tSrchTown=(eTown.value.toString().toLowerCase()).replace(/ /g,"");
var	ItemsCnt=0;
var	aTownIdx=new Array();
var	TownMskIdx=-1,TownSpbIdx=-1;
	for (Idx in aTown)
		{
		if(typeof(aTown[Idx][1])=="undefined")
			continue;
		if(tSrchTown=="")
			{
			if(parseInt(aTown[Idx][4])<NaselLim
				|| isNaN(parseInt(aTown[Idx][4])))
				continue;
			}
		else
			{
	                if(aTown[Idx][1].toString().toLowerCase().replace(/ /g,"").substr(0,tSrchTown.length) !=tSrchTown
	                	&& aTown[Idx][5].toString().toLowerCase().replace(/ /g,"").substr(0,tSrchTown.length) !=tSrchTown)
	                	continue;
			}
		aTownIdx[ItemsCnt]=Idx;
		ItemsCnt++;
		}
var	NoOfColums=parseInt(ItemsCnt/20)+((ItemsCnt/20)%1 ? 1:0);
//var	NoOfColums=ItemsCnt/20+1;
	if(TABLE_TOWN_MAX_NO_COLS<=0)
		TABLE_TOWN_MAX_NO_COLS=2
	if(NoOfColums>TABLE_TOWN_MAX_NO_COLS)
		NoOfColums=TABLE_TOWN_MAX_NO_COLS;
var	tHTML="<TABLE>\n";
var	NoOfRows=parseInt(ItemsCnt/NoOfColums)+1;
var	Row,Col;
	for(Row=0;
		Row<NoOfRows;
		Row++)
		{
                tHTML+="<TR>";
		for(Col=0;
			Col<NoOfColums;
			Col++)
			{
			Idx=aTownIdx[(Col)*NoOfRows+Row];
//			Idx=aTownIdx[(Row)*NoOfColums+Col];
			if(typeof(Idx)=="undefined")
				{
				tHTML+="<TD>&nbsp</TD><TD>&nbsp</TD>\n";
				continue;
				}
		var	tTDClass=(parseInt(aTown[Idx][0])==784 || parseInt(aTown[Idx][0])==75)  ? "tdTownMain":"tdTown";
			tHTML+="<TD class=\""+tTDClass+"\" VALIGN=\"bottom\"><A HREF=\"\" onclick=\"TownSelect('"+aTown[Idx][1]+"','"+aTown[Idx][3]+"','"+aTown[Idx][2]+"');return false;\">"+aTown[Idx][1]+"</A></TD><TD class=\""+tTDClass+"\" VALIGN=\"bottom\">&nbsp"+aTown[Idx][3]+"</TD>\n";
			}
                tHTML+="</TR>";
		}
	tHTML+="</TABLE>\n";
//alert("html="+tHTML+" Row="+Row+" NoOfRows:"+NoOfRows+" ItemsCnt="+ItemsCnt+" NoOfColums="+NoOfColums+" TABLE_TOWN_MAX_NO_COLS="+TABLE_TOWN_MAX_NO_COLS);
	if(!ItemsCnt)
	        tHTML+="<SPAN STYLE=\"color:BLUE;\">К сожалению, ни одного города не найдено!</SPAN>";
	tdTown.innerHTML=tHTML;
	}
</SCRIPT>

