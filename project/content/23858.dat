					if (loginResp.authResponse) {
						var fbAccessToken = loginResp.authResponse.accessToken;
						window.location.href = "/login?mode=fb&forward="
								+ encodeURIComponent(forwardURL) + "&token="
								+ loginResp.authResponse.accessToken;
					}
					//else {
					//  console.log('User cancelled login or did not fully authorize.');
					//}
				}, {
					scope : 'email'
				});
			}
		}, true);
	}
		
