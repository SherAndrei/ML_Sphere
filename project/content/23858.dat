$("#note_edit_delete").click(function() {
	var box = $("#note_edit_box:visible");
//	alert("box: " + box.length + ", by id: " + $("#note_edit_box").length);
	if (box.length == 0) {
		return;
	}
	var key = box.find("#note_edit_id").val(). trim();
	if (key == "") {
		return;
	}
	var caption = box.find("#note_edit_caption").val().trim();
	if (caption == "") {
	    caption = key;
	}
	hidePopup();
	messageBox({
		text: "Хотите удалить сообщение \"" + caption + "\"?"
		, ok_text : "Да"
		, cancel_text: "Нет"
		, ok_callback: function() {
			box.find("#note_edit_mode").val("delete");
			box.find("#note_edit_confirmdel").val("Y");
			box.find("form")[0].submit();
		}
	});
});

$("form[nesbook_form=blob]").on("submit", function(evt) {
	evt.preventDefault();
	var form = $(this);
	$.get("/document?getbloburl=" + encodeURIComponent($(this).attr("action")), function(data) {
	    form.attr("action", data);
	    form.removeAttr("nesbook_form");
	    form[0].submit();
	});
});
});

function submitPayment(mode) {
    var form = $('#header_payment_form');
	if (mode == "yandex") {
        var yandexForm = $('#yandex_kassa_form');
        var amount = form.find("#header_payment_amount").val();
        yandexForm.find("#yandex_kassa_amount").val((+amount).toFixed(2));
        yandexForm[0].submit();
	} else {
	    form.find("#payment_mode").val(mode);
	    form[0].submit();
	}
}

function startKassaPayment() {
    var formdiv = $('#header_payment_div');
    var kassadiv = $('#header_kassa_div');
    formdiv.hide();
    kassadiv.show();
}

function submitKassaPayment(paymode) {
    var yandexForm = $('#yandex_kassa_form');
    yandexForm.find("#yandexKassaPaymentType").val(paymode);
	submitPayment('yandex');
}
