<script type="text/javascript">

    function setCookie (name, value, expires, path, domain, secure) {
      document.cookie = name + '=' + escape(value) +
        ((expires) ? '; expires=' + expires : '') +
        ((path) ? '; path=' + path : '') +
        ((domain) ? '; domain=' + domain : '') +
        ((secure) ? '; secure' : '');
    }

    function getCookie(name) {
        var cookie = ' ' + document.cookie;
        var search = ' ' + name + '=';
        var setStr = null;
        var offset = 0;
        var end = 0;
        if (cookie.length > 0) {
            offset = cookie.indexOf(search);
            if (offset != -1) {
                offset += search.length;
                end = cookie.indexOf(';', offset)
                if (end == -1) {
                    end = cookie.length;
                }
                setStr = unescape(cookie.substring(offset, end));
            }
        }
        return(setStr);
    }

    function cleanStyleByClass(cls) {
      var container = document.getElementsByClassName(cls)[0];

      if (container.children){
        var childrens = [].slice.call(container.children);
        
        childrens.forEach(function ccc(node, i, container) {
          node.removeAttribute('style');
          //node.setStyle('line-height', 'normal');
          
          if (node.children){
            var childrens = [].slice.call(node.children);
            childrens.forEach(ccc);
          }
        });
      };
    }

    function setNormalLineHeight(tagName) {
      var Collection = document.getElementsByTagName(tagName);
      var Arr = [].slice.call(Collection);
      Arr.forEach(function (node) {node.setStyle('line-height', 'normal')});
    }

    function setNormalLineHeightAllTag() {
      setNormalLineHeight('a');
      setNormalLineHeight('p');
      setNormalLineHeight('dd');
      setNormalLineHeight('dt');
      setNormalLineHeight('h1');
      setNormalLineHeight('h2');
      setNormalLineHeight('h3');
      setNormalLineHeight('h4');
      setNormalLineHeight('h5');
      setNormalLineHeight('h6');
      setNormalLineHeight('div');
      setNormalLineHeight('li');
    }

    //перевод в одну колонку

    function setOneColumn() {
      var Collection = document.getElementsByClassName('row');
      for (var i = 0; i < Collection.length; i++) {
        var nodes = [].slice.call(Collection[i].children);
        nodes.forEach(function (node) {
          node.className = 'span12';
          return;
        })
      };
    }

    //


      // переход на версию для слабовидящих
    function changeTemplateToBlack() {

      var st = document.head.getElementsByTagName('link');
      [].forEach.call(st, function (elem) {
        if (elem.href.search('/templates/constructor/css/template_') > 0) {
          var newElem = document.createElement('link');
          
          var colorFont = getCookie('colorFont', 'black');
					if (colorFont == 'black') {
						newElem.href = '/templates/whiteblack/css/black.css';
					} else if (colorFont == 'white') {
						newElem.href = '/templates/whiteblack/css/white.css';
					}

          newElem.rel = 'stylesheet';
          newElem.type = 'text/css';

          document.head.replaceChild(newElem, elem);
          setCookie('defaultTemplateHref', elem.href);
        };
      });

      function ready() {
        if (document.getElementById('btnVersionStyle')){
          document.getElementById('btnVersionStyle').style.display = 'none';
          document.getElementById('bw-eye').style.display = 'block';
          
          cleanStyleByClass('component');

          // убираем выбранный школой фон сайта
          document.body.style.background = 'none';
          
          //jQuery('#resetFont').click();

          var FontSize = getCookie('fsize', 18);
          if (FontSize === null) {
            FontSize = 18;
          }
          fontSizeLabel.innerHTML = FontSize + 'px';
          document.body.style.fontSize = FontSize + 'px';

          setNormalLineHeightAllTag();
        }
      }

      document.addEventListener("DOMContentLoaded", ready);
      ready();
    }

  // переход на обычную версию
    function changeTemplateToNormal() {
      var date = new Date(0);
      setCookie('versionStyle', '', date.toUTCString(), '/');
      setCookie('fsize', '', date.toUTCString(), '/');
      setCookie('colorFont', 'black');
      var st = document.head.getElementsByTagName('link');
      [].forEach.call(st, function (elem) {
        if (elem.href.search('/templates/whiteblack/css/black.css') > 0|| elem.href.search('/templates/whiteblack/css/white.css') > 0) {
          var newElem = document.createElement('link');
          newElem.href = getCookie('defaultTemplateHref');
          newElem.rel = 'stylesheet';
          newElem.type = 'text/css';
          document.head.replaceChild(newElem, elem);
          setCookie('defaultTemplateHref', elem.href);
        };
      });
        //location.reload();
      if (document.getElementById('btnVersionStyle')){
        document.getElementById('btnVersionStyle').style.display = 'block';
        document.getElementById('bw-eye').style.display = 'none';
      }
      
      //check font-size;
      document.body.style.fontSize = "";
    }

  function changeTemplate() {
    setCookie('colorFont', 'black');
    
    //Если версия для слабовидящих, то меняем на обычную
    if (getCookie('versionStyle') == 'black') {
      changeTemplateToNormal();
      return false;
    }
    setCookie('versionStyle', 'black', undefined, '/');
    changeTemplateToBlack();
  }

    // изменение размера шрифта
  window.addEvent('domready', function() {
    // Buttons Size
    var originalFontSize = 18;
    var FontSize = getCookie('fsize', 18);

    jQuery('body').css('font-size', FontSize);

    jQuery('#resetFont').click(function(){
        jQuery('body').css('font-size', originalFontSize+'px');
        setCookie('fsize', originalFontSize, undefined, '/');
        fontSizeLabel.innerHTML = originalFontSize + 'px';
    });
    
    jQuery('#increaseFont').on('click', function (event){
        var currentFontSize = jQuery('body').css('font-size');
        var currentFontSizeNum = parseFloat(currentFontSize, 10);
        var newFontSize = currentFontSizeNum+2;
        if (newFontSize > 32) return false;
        jQuery('body').css('font-size', newFontSize);
        setCookie('fsize', newFontSize, undefined, '/');
        fontSizeLabel.innerHTML = newFontSize + 'px';
        return false;
    });
    
    jQuery('#decreaseFont').click(function(){
        var currentFontSize = jQuery('body').css('font-size');
        var currentFontSizeNum = parseFloat(currentFontSize, 10);
        var newFontSize = currentFontSizeNum-2;
        if (newFontSize < 16) return false;
        jQuery('body').css('font-size', newFontSize);
        //document.cookie = 'fsize='+newFontSize+'; path=/;';
        setCookie('fsize', newFontSize, undefined, '/');
        fontSizeLabel.innerHTML = newFontSize + 'px';
        return false;
    });
    
    jQuery('#invertFontWhite').click(function() {
				var st = document.head.getElementsByTagName('link');
				[].forEach.call(st, function (elem) {
					if (elem.href.search('/templates/whiteblack/css/black.css') > 0 || elem.href.search('/templates/constructor/css/template_') > 0) {
						var newElem = document.createElement('link');
						newElem.href = '/templates/whiteblack/css/white.css';
						newElem.rel = 'stylesheet';
						newElem.type = 'text/css';
						document.head.replaceChild(newElem, elem);
						setCookie('defaultTemplateHref', elem.href);
						setCookie('colorFont', 'white');
					};
				});
				return false;
		});
		
		jQuery('#invertFontBlack').click(function() {
				var st = document.head.getElementsByTagName('link');
				[].forEach.call(st, function (elem) {
					if (elem.href.search('/templates/whiteblack/css/white.css') > 0 || elem.href.search('/templates/constructor/css/template_') > 0) {
						var newElem = document.createElement('link');
						newElem.href = '/templates/whiteblack/css/black.css';
						newElem.rel = 'stylesheet';
						newElem.type = 'text/css';
						document.head.replaceChild(newElem, elem);
						setCookie('defaultTemplateHref', elem.href);
						setCookie('colorFont', 'black');
					};
				});
				return false;
		});
});



