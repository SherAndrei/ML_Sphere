	<script type="text/javascript">
								(function(cnt, mdl, url){
									var oForm = mdl.find('form');
									var oCredent = oForm.find('input[type="text"][data-required="required"]');
									var oTel = oForm.find('input[type="tel"][data-required="required"]');
									cnt.find('.payOkay__link').on('click', function(e){
										e.preventDefault();
									});
									cnt.on('click', function(e){
										if($(this).attr('id')=='payokay_img'){
											try {
												ga('send','event','PayOkay','Popup_open','Logo');
											 } catch(err) {}
										}
										if($(this).attr('id')=='payokay_rate'){
											try {
												ga('send','event','PayOkay','Popup_open','Course');
											 } catch(err) {}
										}
										if($(this).attr('id')=='payokay_detail'){
											try {
												ga('send','event','PayOkay','Popup_open','Button');
											 } catch(err) {}
										}
										mdl.modal({keyboard: true});
									});
									oForm.on('submit', function(e){
										//try {
										//	ga('send','event','PayOkay','Popup','Lead_sent');
										// } catch(err) {}
										e.preventDefault();
										var sData = oForm.serialize();
										var oError = $();
										function isName(o) {
											var n = o.val(),
												i = /^[А-Я][а-я]+/;
											if (!n)
											return false;
											return n.match(i) && 1 === n.match(i).length ? true : false;
										}
										function isBelPhone(o) {
											var n = o.val(),
												i = /\+375\s*\(\d{2}\)\s*\d{3}\-\d{2}\-\d{2}/;
											if (!n)
											return false;
											return n.match(i) && 1 === n.match(i).length ? true : false;
										}
										oCredent.each(function(i){
											if (!isName($(this))) {
												$(this).addClass('error');
												oError = oError.add($(this));
											} else {
												$(this).removeClass('error');
											}
										});
										oTel.each(function(i){
											if ($(this).val().length < 3 || !isBelPhone($(this))) {
												$(this).addClass('error');
												oError = oError.add($(this));
											} else {
												$(this).removeClass('error');
											}
										});
										if (oError.length < 1) {
											//alert('Заявка отправлена');
											mdl.modal('hide');
											$.ajax({
												url : url,
												type : 'POST',
												data : oForm.serialize(),
												success : function(msg) {
													alert(msg.mess);
													if (msg.result > 0) {
														try {
															ga('send', 'event', 'PayOkay', 'Popup', 'Lead_sent');
														    } catch(err) {}
													}																																			
												}
											});
											return true;
										}
										return false;
									});
									var internals = {
										format: function(n) {
											n = n.replace(/[^0-9]/g,'');
											var pref = n.substr(3,2);
											if(n.length > 4&&!(pref=='29'||pref=='33'||pref=='44'||pref=='25'||pref=='17'))
												pref = '';
											var val = '+375 ('+pref;
											if(n.length > 5)
												val += ') '+n.substr(5,3);
											if(n.length > 8)
												val += '-'+n.substr(8,2);
											if(n.length > 10)
												val += '-'+n.substr(10,2);
											return val;
										},
										getCaretPosition: function(ctrl) {
											var CaretPos = 0;
											// IE Support
											if (document.selection) {
												ctrl.focus();
												var Sel = document.selection.createRange();
												Sel.moveStart('character',-ctrl.value.length);
												CaretPos = Sel.text.length;
											}
											// Firefox support
											else if (ctrl.selectionStart || ctrl.selectionStart == '0')
												CaretPos = ctrl.selectionStart;
											return (CaretPos);
										},
										setCaretPosition: function(ctrl, pos) {
											if(ctrl.setSelectionRange) {
												ctrl.focus();
												ctrl.setSelectionRange(pos,pos);
											}
											else if (ctrl.createTextRange) {
												var range = ctrl.createTextRange();
												range.collapse(true);
												range.moveEnd('character', pos);
												range.moveStart('character', pos);
												range.select();
											}
										}
									};
									oTel.focus(function() {
										if(this.value.length < 6)
											this.value='+375 (';
									});
									oTel.keydown(function() {
										var pos=internals.getCaretPosition(this);
										if (pos < 6) {
											internals.setCaretPosition(this,6);
											return false;
										}
									});
									oTel.keyup(function() {
										var val = $(this).val().toString().replace(/[^0-9]/g,'');
										if(isFinite(val)) {
											var pos=internals.getCaretPosition(this);
											var len=this.value.length;
											$(this).val(internals.format(val));
											if (pos > 7)
												pos = pos+this.value.length-len;
											if (pos < 6)
												pos = 6;
											internals.setCaretPosition(this,pos);
										}
										return true;
									});
								})($('.payokay__td'),$('#payOkay-modal'),'https://finance.tut.by/json/send_info_api.php');
</script>

