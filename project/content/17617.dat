	getHexColor: function(color) {
		color = color.replace(/\s/g, "");
		var colorRGB = color.match(/^rgb\((\d{1,3}[%]?),(\d{1,3}[%]?),(\d{1,3}[%]?)\)$/i), colorHEX = '';
		for (var i = 1; i <= 3; i++) {
