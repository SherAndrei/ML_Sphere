<script type="text/javascript">var url='http://b.stranamam.ru/show.php?placeid=113&zone=&rnd='+Math.round(Math.random()*100000);document.write('<scr'+'ipt type="text/javascript" src="'+url+'"><\/scr'+'ipt>');</script>
