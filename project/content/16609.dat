	(function($) { 
		
		$(document).ready(function() {
			$('body').append($('#myModal'));
			$('body').append($('#myModal_success'));
			var keys = {};
			
			$(document).keydown(function (e) {
				keys[e.which] = true;		
				printKeys();
			});			
			
			function printKeys() {
				var is_true = '';
				for (var i in keys) {
					if (!keys.hasOwnProperty(i)) continue;
					if(i == 13){
						is_true += 1;
					}
					if(i == 17){
						is_true += 1;
					}			
				}
				
				if(is_true == '11'){
					
					var selection = window.getSelection();
					var selection_text = selection.toString();
					if(selection_text != ""){
						$('#myModal').modal('show');
						var selection_block = selection.getRangeAt(0).commonAncestorContainer.innerText;
						if(typeof(selection_block) == "undefined"){
							var selection_block = selection.focusNode.data.toString();
						}
						
						selection_block = selection_block.replace(selection_text,'<span style="background: #dfeffc;font-weight: bold;">'+selection_text+'</span>')
						jQuery('#selection_block').html(selection_block);
						$('input[name=error_text]').val(selection_block);					
						$('input[name=error_page]').val(window.location.href);
					}					
					is_true = '';
					keys = {};
				}
			}
			
			$("button#send").click(function( event ) {
				
				event.preventDefault();
				
				$.ajax({
					type: "POST",
					url: "assets/mail/error_form_mail.php",
					data: $('form#error-in-text').serialize(), // serializes the form's elements.
					success: function(data)					
					{  
						$('#myModal').modal('hide'); 
						$('#myModal_success').modal('show');
						setTimeout(function () {
							$('#myModal_success').modal('hide');
						}, 2000); // время в мс			  
					},
					error:  function(){
						alert('Возникла ошибка');
					}
				});
				
			});
		});
	})(jQuery);
</script>
