		var scrollToElem = targetPos - winHeight;
		jQuery(window).scroll(function(){
			var winScrollTop = jQuery(this).scrollTop();
			if(winScrollTop <= targetPos){
				jQuery('#itemBackToTop').fadeOut();
			}
			else{
				jQuery('#itemBackToTop').fadeIn();
			}
		});
	})
</script>

