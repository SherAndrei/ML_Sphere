        <div class="post_date"><a  class="post_link"  href="/wall-89112050_6008"  onclick="return showWiki({w: 'wall-89112050_6008'}, false, event);" ><span class="rel_date">22 ноя 2015</span></a></div>
        <div class="ui_actions_menu_wrap _ui_menu_wrap " onmouseover="uiActionsMenu.show(this);" onmouseout="uiActionsMenu.hide(this);">
