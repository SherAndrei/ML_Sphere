  <div class="_post_content">
    
    <div class="post_header">
      <a class="post_image" href="/jesustxt">
        <img src="https://pp.userapi.com/c621218/v621218082/18c24/wLePyxWtTvI.jpg" data-post-id="-89112050_5994" data-post-click-type="post_owner_img" width="50" height="50" class="post_img" />
