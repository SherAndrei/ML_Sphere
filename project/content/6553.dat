		<script type="text/javascript">

			jQuery(document).ready(function($) {
				var $marker = $('.js-donate__marker' ),
					marker_top = $marker.offset().top,
					$window = $(window ),
					$target = $('.js-donate__header' ),
					default_margin_int = -200,
					default_margin_px = '-200px',
					default_duration = 700,
					speed = (default_margin_int * -1) / default_duration,
					cur_duration;


				$window.scroll( hh_init_float_header );
				$window.resize( hh_init_float_header );

				hh_init_float_header();


				function hh_init_float_header() {

					if ( $window.width() < HH.breakpoints.tablet_horizontal ) {
						$target
							.stop(true)
							.css({
								'margin-left' : 0,
								'opacity' : 1
							});

						return;
					}

					if ( break_line() > marker_top ) {

						cur_duration = ( parseInt( $target.css('margin-left') ) * -1 ) / speed;

						$target
							.stop(true)
							.animate({
								'margin-left' : 0
							},
							cur_duration,
							'swing'
						)
							.animate({
								opacity : 1
							},{
								duration : cur_duration,
								queue : false,
								easing : 'swing'
							});
					} else {
						$target
							.stop(true )
							.css({
								'margin-left' : default_margin_int,
								'opacity' : 0
							});
					}
				}


				function break_line () {
					return $window.scrollTop() + $window.height();
				}
			});

		</script>

