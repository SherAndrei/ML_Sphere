<script type="text/javascript">
var ai, xmlhttp, rows , globalTmpMustRemoved;

(window.addEventListener)?window.addEventListener('load', start_script, false):window.attachEvent('onload', start_script);

function start_script() {

	var logoButton = window.document.getElementById('logo'), menuButton, cnt;
	
	if(logoButton.getElementsByClassName) {
		menuButton = logoButton.getElementsByClassName('menu')[0];
	} else { //FOR IE
		menuButton = logoButton.getElementsByTagName('DIV');
		for(cnt = 0; cnt < menuButton.length; cnt++) {
			if(menuButton[cnt].className == 'menu') {
				menuButton = menuButton[cnt];
				break;
			}
		}
	}
	menuButton = menuButton.getElementsByTagName('DIV')[0].getElementsByTagName('DIV')[0];
	(menuButton.addEventListener)?menuButton.addEventListener('click', displayLogoMenu, false):menuButton.attachEvent('onclick', displayLogoMenu);
	function displayLogoMenu() {
		if(logoButton.className) {
			logoButton.removeAttribute('class');
		} else {
			logoButton.className = 'show';
			window.document.body.focus();
			
			if (window.getSelection) {
				if (window.getSelection().empty) { // Chrome
					window.getSelection().empty();
				} else if (window.getSelection().removeAllRanges) { // Firefox
					window.getSelection().removeAllRanges();
				} else if (document.selection) { // IE?
					document.selection.empty();
				}
			}
		}
	}
	
	(window.document.addEventListener)?window.document.addEventListener('click', closeLogoMenu, false):window.document.attachEvent('onclick', closeLogoMenu);
	function closeLogoMenu(event) {
		var searchMenu = event.target || event.srcElement;
		while(searchMenu && (searchMenu != window.document.body)) {
			searchMenu = searchMenu.parentNode;
			if(searchMenu.className == 'menu') {
				return;
			}
		}
		logoButton.removeAttribute('class');
	}	
	var tmp = new Object();
	xmlhttp = getXmlHttp();
	ai = {'list':window.document.getElementById('additional-info')};
	tmp['points'] = ai['list'].getElementsByTagName('TR');
	try {
		rows = {
			'location':{
				row:tmp['points'][tmp['points'].length-2],
				content:window.document.createElement('SPAN')
			},
			'provider':{
				row:tmp['points'][tmp['points'].length-1],
				content:window.document.createElement('SPAN')
			}
		};
		rows.location['cell'] = rows.location.row.getElementsByTagName('TD')[0];
		rows.provider['cell'] = rows.provider.row.getElementsByTagName('TD')[0];
		
		while(rows.location.cell.firstChild) rows.location.cell.removeChild(rows.location.cell.firstChild);
		while(rows.provider.cell.firstChild) rows.provider.cell.removeChild(rows.provider.cell.firstChild);
		
		rows.location.cell.appendChild(rows.location.content);
		rows.provider.cell.appendChild(rows.provider.content);
		
		
		
		
	} catch(e){
		console.error('Ошибка: '+e);
		return;
	};
	getIpInfo();
	tmp = null;
}

function getIpInfo() {

	xmlhttp.open('POST', 'http://router.kz/ajax/get-header-info.php', true);
	xmlhttp.setRequestHeader('Content-Type', 'application/x-www-form-urlencoded; charset=utf-8');
	xmlhttp.send('ip='+'217.69.133.242');
	
	
	if(xmlhttp.addEventListener) {
		xmlhttp.addEventListener('readystatechange', get_header_info, false);
	} else {
		//attachEvent не канает для «onreadystatechange»
		xmlhttp.onreadystatechange = function() {
			get_header_info();
		}
	}
	
	function get_header_info() {
		if(xmlhttp.readyState != 4) return;
		var imgObj, responsed = xmlhttp.responseXML.documentElement, countryInfo = getElementAttributes(responsed.getElementsByTagName('country')[0]), providerInfo = getElementAttributes(responsed.getElementsByTagName('provider')[0]);
		
		if(countryInfo) {
			if(countryInfo['image']) {
				imgObj = new Image();
				imgObj.src = countryInfo['image'];
				(imgObj.addEventListener)?imgObj.addEventListener('load', load_location_icon, false):imgObj.attachEvent('onload', function() {load_location_icon(imgObj);});
			}
			if(countryInfo['name']) rows.location.content.appendChild(window.document.createTextNode(countryInfo['name']));
			rows.location.row.removeAttribute('style');
		}
		
		if(providerInfo) {
			if(providerInfo['image']) {
				imgObj = new Image();
				imgObj.src = providerInfo['image'];
				(imgObj.addEventListener)?imgObj.addEventListener('load', load_provider_icon, false):imgObj.attachEvent('onload', function() {load_provider_icon(imgObj);});
			}
			if(providerInfo['name']) {
				rows.provider.row.getElementsByTagName('TH')[0].appendChild(window.document.createTextNode((providerInfo['is_provider'] == '1')?'Провайдер':'Организация'));
				rows.provider.content.appendChild(window.document.createTextNode(providerInfo['name']));
			}
			rows.provider.row.removeAttribute('style');
		}
		if(xmlhttp.removeEventListener) xmlhttp.removeEventListener('readystatechange', get_header_info, false);
		xmlhttp.close;
	}
	tmp = null;
}

function load_provider_icon() {
	var eventObj = createLoadedImage(arguments[0]);
	var imgNode = createLoadedImage(arguments[0]);
	if(!imgNode) return;
	imgNode.setAttribute('alt', 'Провайдер');
	if(rows.provider.content.firstChild) {
		rows.provider.content.insertBefore(imgNode, rows.provider.content.firstChild);
	} else {
		rows.provider.content.appendChild(imgNode);
	}
}

function load_location_icon() {
	var eventObj = createLoadedImage(arguments[0]);
	var imgNode = createLoadedImage(arguments[0]);
	if(!imgNode) return;
	imgNode.setAttribute('alt', 'Флаг');
	if(rows.location.content.firstChild) {
		rows.location.content.insertBefore(imgNode, rows.location.content.firstChild);
	} else {
		rows.location.content.appendChild(imgNode);
	}
}

function createLoadedImage(eventObj) {
	var imgNode, loaded;
	if(eventObj['target'] || eventObj['currentTarget']) {
		imgNode = window.document.createElement('IMG');
		loaded = (eventObj['target'])?eventObj['target']:eventObj['currentTarget'];
		imgNode.setAttribute('src', loaded.src);
		imgNode.setAttribute('width', loaded.width);
		imgNode.setAttribute('height', loaded.height);
	} else if(eventObj['nodeType'] == 1) {
		imgNode = eventObj;
	} else {
		return false;
	}
	return imgNode;
}

function getElementAttributes (elementNode) {
	if(!elementNode) return null;
	var attList = new Object();
	elementNode = elementNode.attributes;
	for(var cnt = 0; cnt < elementNode.length; cnt++) attList[elementNode[cnt]['name']] = elementNode[cnt]['value'];
	return attList;
}

function getXmlHttp(){
	var xmlhttp;
	try {
		xmlhttp = new ActiveXObject("Msxml2.XMLHTTP");
	} catch (e) {
		try {
			xmlhttp = new ActiveXObject("Microsoft.XMLHTTP");
		} catch (e) {
			xmlhttp = false;
		}
	}
	if (!xmlhttp && typeof XMLHttpRequest!='undefined') {
		xmlhttp = new XMLHttpRequest();
	}
	return xmlhttp;
}
