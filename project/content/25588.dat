<script>
	$('.js-raty-booking').on('click', function () {
		var self = $(this)
		var timeOut = 4000;
		var postId = $("#commentform input[name='comment_post_ID']").val();
		var $rate = self.children('input')

		var newRating = $rate.attr('value')

		$.ajax({
			type: 'GET',
			url: '/json/api?apiCall=addRating&id=' + postId + '&rating=' + newRating
		}).done(function (response) {
//			console.log(response)
			$('.booking__rating-text').html('Cпасибо!');
			setTimeout(function () {
				$('.booking__rating-text').html('Рейтинг: ' + response.result)
				$rate.attr('data-value', response.result)
				self.attr('data-value', response.result)
				self.raty('score', response.result);
			}, timeOut)
		}).fail(function (textStatus) {
//			console.log(textStatus)
		});

	});
	$("#commentform").on('submit', function (e) {
		e.preventDefault()
		var timeOut = 4000;
		var $textarea = $("#commentform textarea");
		var rating = $("#commentform input[name='score']").val();
		var postId = $("#commentform input[name='comment_post_ID']").val();
		if (rating == "") {
			rating = 0
		}
		var commentText = $.trim($textarea.val());
		var $topRating = $('.js-raty-booking');
		if (rating !== 0) {
			$.ajax({
				type: 'GET',
				url: '/json/api?apiCall=addRating&id=' + postId + '&rating=' + rating
			}).done(function (response) {
//				console.log(response)
				$('.write-comment__rating-note').html(' cпасибо, ваш рейтинг учтён!');
				$('.booking__rating-text').html('Рейтинг: ' + response.result)
				$topRating.attr('data-value', response.result)
				$topRating.raty('score', response.result);
				$topRating.children('input').attr('data-value', response.result)
				setTimeout(function () {
					$('.write-comment__rating-note').html('– укажите рейтинг')
				}, timeOut)
			}).fail(function (textStatus) {
//				console.log(textStatus)
			});
		}
		if (commentText == '') {
			$textarea.val('')
			$textarea.attr('placeholder', 'Пожалуйста, введите текст отзыва!')
		}
		if (commentText != '') {
			$.ajax({
				type: 'GET',
				url: '/json/api?apiCall=addComment&id=' + postId + '&comment=' + commentText + '&rating=' + rating
			}).done(function (response) {
				var article = response.response;
				var $ajaxContainer = $('.ajax-container');
				$ajaxContainer.prepend(article);
				$('.ajax-container .comment:first-child').slideDown('slow');
                $textarea.val('');

				var $commentsCount = $('.comments__count');
                $('.comments__head').html('Комментарии');
				if ($commentsCount.length == 0) {
					$('.comments__header').append('<span class="comments__count"></span>')
				}
                $('.comments__count').show();
				$commentsCount = $('.comments__count');
				var countInt = parseInt($commentsCount.html(), 10) + 1;
				$commentsCount.html($commentsCount.html() == 'Отзывов нет' || $commentsCount.html() == '' ? 1 : countInt);

				var $newsCountButton = $('.article-page__content-footer .button--gotoall');
				if ($newsCountButton.length != 0) {
					var innerText = $newsCountButton.html();
					var resultText;
					switch (innerText) {
						case '0 комментариев':
							resultText = '1 комментарий';
							break;
						case '1 комментарий':
							resultText = '2 комментария';
							break;
						case '2 комментария':
						case '3 комментария':
							resultText = countInt + ' комментария';
							break;
						default:
							resultText = countInt + ' комментариев';

					}
					$newsCountButton.html(resultText)
				}

			}).fail(function (response) {
				var message;
				switch (response.status) {
					case 429:
						message = 'Извините, вы отправляете отзыв слишком часто. Попробуйте позже.';
						break;
					case 409:
						message = 'Извините, вы уже отправляли такой отзыв. Пожалуйста, измените его.';
						break;
					default:
						message = 'Неизвестная ошибка. Пожалуйста, попробуйте позже.';
						break
				}
				if ($('.write-comment__footer span').length == 0) {
					$('.write-comment__footer').append('<span>' + message + '</span>')
				}
				setTimeout(function () {
					$('.write-comment__footer span').remove()
				}, timeOut)
			});
		}

	})
</script>
