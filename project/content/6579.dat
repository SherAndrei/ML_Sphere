<script>

    function printDiv(divName) {
    var printContents = document.getElementById(divName).innerHTML;
    var originalContents = document.body.innerHTML;
    document.body.innerHTML = printContents;
    window.print();
    document.body.innerHTML = originalContents;
    }

    document.querySelector('#viewer_page').addEventListener('dblclick', function (event) {
    ga('send', 'event', 'zoom', 'dblclick', '1');
    event = event || window.event;
    var mouse_x = y = 0;
    mouse_x = event.clientX;
    mouse_y = event.clientY;
    zoom.to({
    x: mouse_x - 150,
            y: mouse_y - 150,
            width: 300,
            height: 300
    });
    });</script>


<script type="text/javascript">
    if($("a").is("#next")){
    document.querySelector('#next').addEventListener('click', function (event) {
    ga('send', 'event', 'next', 'click', '1');
    });
    }
    if($("a").is("#prev")){
    document.querySelector('#prev').addEventListener('click', function (event) {
    ga('send', 'event', 'prev', 'click', '1');
    });
    }
    document.querySelector('#showqrcode').addEventListener('click', function (event) {
    ga('send', 'event', 'showqrcode', 'click', '1');
    });
    document.querySelector('#printpage').addEventListener('click', function (event) {
    ga('send', 'event', 'printpage', 'click', '1');
    });
</script>

<script type="text/javascript">
    $(document).ready(function () {
    var w = 131 * 100;
    var b = 131 * 2;
    var wpb = w + b - 4;
    var del = 131 / 12;
    var go = wpb / del;
    $("#navigate_bar2").scrollTo(go, 1000, {axis:'x'});
    });
    $('.tt1').qtip({
    content: {
    text: 'После добавления устройство появится в списке в личном кабинете. Требуется регистрация и авторизация'
    },
            style: {
            tip: {
            corner: true
            }
            }
    });
    $('.tt1').mousemove(function () {
    ga('send', 'event', 'mydevices', 'viewtip', '1');
    });
    function windowSize(){
    if ($(window).width() <= 1010){
    var need = $(window).width() / 1010;
    $("#navigate_bar2").width($(window).width()-50);
    $('#viewer_page').css({
    '-webkit-transform' : 'scale(' + need + ')',
            '-moz-transform'    : 'scale(' + need + ')',
            '-ms-transform'     : 'scale(' + need + ')',
            '-o-transform'      : 'scale(' + need + ')',
            'transform'         : 'scale(' + need + ')'
    });
    $('.col-viewer').height($('#viewer_page').innerHeight() * need + 250);
    //$('.scroll_child>img').width($('.scroll_child').innerHeight()*need);
    //$(window).scrollTop($('#navigate_bar2').offset().top);
    }
    };
    $(window).load(windowSize);
    $(window).resize(windowSize);
    $(window).on('load resize', windowSize);
    if ($(window).width() > 1010){
    $('h1').css({'font-size' : '20px'});
    }
    if ($(window).width() > 700 && $(window).width() <= 1010){
    $('.scroll_child img').width(60);
    $('h1').css({'font-size' : '18px'});
    }
    if ($(window).width() > 300 && $(window).width() <= 700){
    $('.scroll_child img').width(40);
    $('h1').css({'font-size' : '16px'});
    }
    
</script>

