		{
			var ourBlock = $('#comments-newsComments').offset().top + $('#comments-newsComments').height();
			var block = $('.next-article');
			var blockCss = block.css('right');

			if (ourBlock < ($(window).scrollTop()+$(window).height()) && blockCss=='-335px')
			{
				showArrow();
			}


			if(ourBlock > ($(window).scrollTop()+$(window).height()) && blockCss=='0px')
			{
				hideArrow();
			}

		}
	});

	function hideArrow()
	{
		$('.next-article').animate({right:"-335px"},"fast");
		$('.next-article__popout').addClass('hide-block');
		$('.next-article__popout').removeClass('show-block');
