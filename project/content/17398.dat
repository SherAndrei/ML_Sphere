		<script>
			(function() {
				var submitCounter = '5719689';
				var suggestCounter = '20398679';

				function getCounter (id, click) {
					var type = click ? 'sb' : 'd';
					new Image().src = '//rs.mail.ru/' + type + id + '.gif?rnd=' + parseInt(Math.random()*100000);
				}

				__PM.getItems(function (menuItems) {
					var search = menuItems.toolbar.items[1].itemsByName.search;
					var suggest;

					if (search) {
						search.form.el.setAttribute('accept-charset', 'UTF-8');
						submitCounter && getCounter(submitCounter);

						search.input.on('input', function (e) {
							var query = e.target.value;
							var suggests = search.suggests;

							if (query.length > 2) {
								if (!suggest) {
									suggestCounter && getCounter(suggestCounter);
									suggest = true;
								}
								$.ajax({
									url: '/search/suggest/',
									data: {
										q: query
									},
									success: function (data) {
										suggests.list({
											searchText: query,
											items: data.items
										});
									}
								});
							} else {
								suggests.hide();
							}
						});

						search.suggests.on('select', function (e, item) {
							suggestCounter && getCounter(suggestCounter, 'click');

							// Из-за Opera Presto подставляем локейшн вместо отправки формы
							search.input.el.value = item.text;
							setTimeout(function () {
								window.location = item.url;
							}, 0);
						});

						search.form.on('submit', function (e) {
							submitCounter && getCounter(submitCounter, 'click');
						});
					}
				});
