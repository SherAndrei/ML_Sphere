<script type="text/javascript">
	function catalog_bind_listing_ajax(bind_id, dir, page) {
	
		var go_page = 1;
		var limit = 15;
		var curr_page = $('#bind-list-' + bind_id).data('currpage');
		var overall = $('#bind-list-' + bind_id).data('overall');
		var overall_pages = parseInt(overall / limit) + ((overall % limit) ? 1 : 0);
		var from = 0;
		var to = 15;
		
		if (dir == 'forward') {
			go_page = curr_page + 1;
			if (page) {
				go_page = page;
			}
			if (go_page > overall_pages) return false;
			$('#bind-list-' + bind_id + ' .nav .backward .disabled').removeClass('disabled');
		} else if (dir == 'backward') {
			if (curr_page == 1) return false;
			go_page = curr_page - 1;
			$('#bind-list-' + bind_id + ' .nav .forward .disabled').removeClass('disabled');
		} else {
			return false;
		}
		
		element_disable('bind-list-' + bind_id);
		
		$('#bind-list-' + bind_id).data('currpage', go_page);
		
		if (go_page == 1) {
			$('#bind-list-' + bind_id + ' .nav .backward a').addClass('disabled');
		}
		if (go_page == overall_pages) {
			$('#bind-list-' + bind_id + ' .nav .forward a').addClass('disabled');
		}
		
		from = go_page * limit - limit;
		to = from + limit;
		
		$.ajax({
			type     : 'post',
			url      : '/catalog/',
			data     : 'do=binds_listing&section_id=1568&bind_id=' + bind_id + '&page=' + go_page + '&limit=' + limit + '&get=' + encodeURIComponent(location.search.replace(/^\?/, '')),
			dataType : 'json',
			success  : function(data, ans) {
				if (data && data.status == 'success') {
				
					if (!data.rows) {
						return true;
					}
					
					var html = '';
					
					for (i = 0; i < data.rows.length; i++) {
						html += '<li class="default"><a href="/mobilnye-i-telefony-smartfony-otzyvy/' + ( (data.rows[i].query) ? '?' + data.rows[i].query : '' ) + '" title="' + htmlspecialchars(data.rows[i].title) + '">' + htmlspecialchars(data.rows[i].name) + '</a></li>';
					}
					
					$('#bind-list-' + bind_id + ' .list').empty().html(html);
					
					to = to - (limit - data.rows.length);
					
					$('#bind-list-' + bind_id + ' .note .from').text(from+1);
					$('#bind-list-' + bind_id + ' .note .to').text(to);
				
				}
				
				element_disable('bind-list-' + bind_id);
			}
		});
		
		return true;
		
	}
	
	function catalog_bind_show_full_list() {
		
