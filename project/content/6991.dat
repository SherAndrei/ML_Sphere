                    window.setTimeout(function () {
                        $(document).on('click.dialog', function (e) {
                            if (!$(e.target).closest('.js-dialog').length) {
                                $dialog.trigger('hideDialog');
                            }
                        });
                    });
                }).on('hideDialog', function () {
                    $.data(this, 'visible', false);
                    $BODY.removeClass('has-online-call');
                    $dialog.removeClass('is-visible').attr('aria-hidden', true);
                    trackEvent('close', csrf, trackerUrl);
                    $(document).off('.dialog');
                }).on('toggleDialog', function () {
                    if (!Boolean($.data(this, 'visible'))) {
                        $dialog.trigger('showDialog');
                    } else {
                        $dialog.trigger('hideDialog');
                    }
                }).on('click', '.js-dialog-close', function (e) {
                    e.preventDefault();
                    $dialog.trigger('hideDialog');
                }).on('click', '.js-dialog-button', function (e) {
                    e.preventDefault();
                    trackEvent('submit', csrf, trackerUrl);

                    var popupUrl = 'https://lk.mango-office.ru/widget/call-from-site-auto-dial/MTAwMDIwNDY=';
                    var width = 238;
                    var height = 400;
                    var left = (screen.width / 2) - (width / 2);
                    var top = (screen.height / 2) - (height / 2);
                    var popupParams = [
                        'width=' + width,
                        'height=' + height,
                        'top=' + top,
                        'left=' + left,
                        'resizable=no',
                        'toolbar=no',
                        'menubar=no',
                        'location=no',
                        'status=no'
                    ];
                    window.open(popupUrl, '_blank', popupParams.join(','));
                });


                $('.js-online-buttons').on('toggle', function () {
                    var $container = $(this);
                    var isVisible = $.data(this, 'isVisible');

                    $container.trigger(isVisible ? 'hide' : 'show');
                }).on('show', function () {
                    var $container = $(this);
                    var isVisible = $.data(this, 'isVisible');
                    $.data(this, 'isVisible', !isVisible);
                    $container.toggleClass('is-visible', !isVisible);
                }).on('hide', function () {
                    var $container = $(this);
                    var isVisible = $.data(this, 'isVisible');
                    $.data(this, 'isVisible', !isVisible);
                    $container.toggleClass('is-visible', !isVisible);
                }).on('click', '.js-online-buttons-toggle', function (e) {
                    e.preventDefault();

                    $(e.delegateTarget).trigger('toggle');
                }).on('click', '.js-online-chat-button', function (e) {
                    e.preventDefault();

                    if (window.vocalizeChat && $.isFunction(window.vocalizeChat.openChat)) {
                        window.vocalizeChat.openChat();
                    }

                    $(e.delegateTarget).trigger('hide')
                }).on('click', '.js-online-call-button', function (e) {
                    e.preventDefault();
                    $dialog.trigger('toggleDialog');
                    $(e.delegateTarget).trigger('hide')
                });


                if (!isWebRTCSupported()) {
                    $('.js-online-call-button').hide();
                }
            });
        }(jQuery));
    </script>







    
        
