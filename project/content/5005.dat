var nocache = Math.random();
var browser = navigator.appName;
http.open('get', '/inc/ajax_opinions.php?cid='+oC+'&rid='+oR+'&mp='+mP+'&nocache = '+nocache);

http.onreadystatechange =  LastOpinionsReply;
http.send(null);

}

