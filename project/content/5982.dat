        $('.like').click(function (event) {
            event.preventDefault();
            var target = $(this);
            $.ajax({
                type: "GET",
                url: $(this).attr('href'),
                success: function (data) {
                    if (data == 'error')
                        alert('Error! Can`t save!');
                    else {
                        if (data === '1')
                            target.html('<i class="fa fa-heart"></i> Unlike');
                        else
                            target.html('<i class="fa fa-heart"></i> Like');
                    }
                },
                error: function (data) {
                    alert('Error!!! Can`t save!' + data);
                }
            });

        });

