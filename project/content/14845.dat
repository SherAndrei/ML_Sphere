var def_val=[];
function sel_repl(arg,hlp,s_wi) {

	var info="";
	if(hlp>0) {info='<i onclick="he_we();"></i>';}
	
	def_val[arg]=vals[arg][0];
	gebi(arg).className='js_sel';
	
	gebi(arg).innerHTML+='<div style="width:'+s_wi+'px;" id="div_'+arg+'" class="nea">'+def_val[arg]+'</div><span style="width:'+(s_wi+3)+'px;" id="span_'+arg+'"></span>';
	hide("span_"+arg);
	gebi("div_"+arg).onclick=function() {
		var ima=this.id.substring(4);    
		var len=keys[ima].length;
  
		var aas='<b onclick="ch_reset(\''+ima+'\');" onmouseover="clearTimeout(s_ti);" onmouseout="s_hide();">'+info+def_val[ima]+'</b>';
		
		var nlc="";
    if((arg == 'brand')||(arg == 'weight'))
    {
  		var o="brand";
  		if(ima==o) o="weight";
    }
    
    if((arg == 'brand_1')||(arg == 'weight_1'))
    {
  		var o="brand_1";
  		if(ima==o) o="weight_1";
    }
   
