.ya-site-suggest-list {background:#fff!important;}

a {transition:color .4s ease 0s,background-color .4s ease 0s;}

#plushka {position:fixed;left:0;top:0;width:100%;height:100%;z-index:1000000;background-color:rgba(100,100,100,0);transition:background .3s ease 0s;}
#plushka.opened {background-color:rgba(100,100,100,.9);}

#plushka img {bottom:0;right:0;left:0;top:0;margin:auto;max-height:90%;max-width:90%;position:absolute;vertical-align:top;opacity:0;transition:opacity .5s ease 0s;border-radius:1em;border:3em solid #fff;}

#plushka img.opened {opacity:1;}

#plushka span {font-family:Segoe UI,sans-serif;display:block;width:100%;bottom:-1em;position:absolute;background-color:rgba(255,255,255,1);text-align:center;padding:.4em 0;opacity:0;transition:opacity .5s ease 0s,bottom .5s ease 0s;font-size:160%;}

#plushka span.opened {opacity:1;bottom:0;}

#plushka b {display:block;position:absolute;opacity:0;cursor:pointer;transition:opacity .5s ease 0s;width:4em;height:4em;}
#plushka #plbt_nxt {right:3%;top:50%;margin-top:-2em;}
#plushka #plbt_prw {left:3%;top:50%;margin-top:-2em;}
#plushka #plbt_cls {right:3%;top:3%;}
#plushka b.opened {opacity:.7;}
#plushka b:hover {opacity:1;}
#plushka b svg {vertical-align:top;width:100%;height:100%;}
#plushka b svg polygon {fill:#000;}
#plushka b svg circle {fill:#fff;fill-opacity:1;transition:all .2s ease 0s;}
#plushka b:active svg circle {fill-opacity:0.8;}

@media screen and (max-width:1200px) {
#plushka span {font-size:140%;}
}

@media screen and (max-width:700px) {
#plushka img {max-height:100%;max-width:100%;box-shadow:none;}
#plushka span {font-size:120%;background-color:rgba(255,255,255,.5);}
#plushka b svg circle {fill-opacity:.5;}
}

@media screen and (max-height:400px) {
#plushka img {max-height:100%;max-width:100%;box-shadow:none;}
#plushka span {font-size:100%;background-color:rgba(255,255,255,.5);}
#plushka b svg circle {fill-opacity:.5;}
}</style>

