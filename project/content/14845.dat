			nlc=","+we_br[o][pf_a[o]]+",";
		}
    
		for(var i=1;i<len;i++) {
			if((nlc.indexOf(","+keys[ima][i]+",")>=0)||(pf_a[o]=="")) {
				var clss=[];
        
				if(vals[ima][i]==gebi("div_"+ima).innerHTML) clss.push('act');
