			<span class="name"> &#8212; <span itemprop="reviewRating" itemscope itemtype="http://schema.org/Rating"><span itemprop="worstRating" content="1"></span><span itemprop="ratingValue">3</span><span itemprop="bestRating" content="5"></span></span>, <span itemprop="author">Динара</span>.</span>
