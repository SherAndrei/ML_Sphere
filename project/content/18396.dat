                        }
                    }
                }
            }
        }
    }
    var S=(parseFloat(alfa)*(parseFloat(P)*parseFloat(L))/parseFloat(dU))/parseFloat(par);
    var MassS=[0,0.5,0.75,1,1.2,1.5,2,2.5,3,4,5,6,8,10,16,25,35,50,70,95,120,150,185,240];
    var x=1;
    var i=MassS[x];
    while(i<S)
    {
        x++;
        i=MassS[x]
    }
