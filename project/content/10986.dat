<script type="text/javascript">

var c_top = 0;
var move_p = 50; // px

var rp = 0;

var delta_y = 5;
var delta_ms = 5;

var div_y = 0;

var bottom_limit = -200;

var timerId = null;

function scrollUp() {
	stopAllScroll();
	setUpActive();
	rp = 0;
	setDownActive();
	if(div_y <= bottom_limit) {
		setDownUnActive();
		return;
	}
	timerId = setInterval(moveDivUp, delta_ms)
}

function scrollDown() {
	stopAllScroll();
	setDownActive();
	rp = 0;
	setUpActive();
	if(div_y >= 0) {
		setUpUnActive();
		return;
	}
	timerId = setInterval(moveDivDown, delta_ms)
}

function moveDivUp() {
	if(rp >= move_p) {
		stopAllScroll();
	} else {
		div_y = parseInt($('movePhotos').style.top);
		rp = rp+delta_y;
		div_y -= delta_y;
		if(div_y <= bottom_limit) {
			div_y = bottom_limit;
			setDownUnActive();
			
		}
		$('movePhotos').style.top = div_y+'px';
	}
	
}


function moveDivDown() {
	if(rp >= move_p) {		
		stopAllScroll();
	} else {
		div_y = parseInt($('movePhotos').style.top);
		rp = rp+delta_y;
		div_y += delta_y;
		if(div_y > 0) {
			div_y = 0;
			setUpUnActive();
		}
		$('movePhotos').style.top = div_y+'px';
	}
	
}

function setUpUnActive() {
	$('but_img_st_up').src = PATH_CONTENT+'';
}

function setUpActive() {
	$('but_img_st_up').src = PATH_CONTENT+'';
}

function setDownUnActive() {
	$('but_img_st_down').src = PATH_CONTENT+'';
}

function setDownActive() {
	$('but_img_st_down').src = PATH_CONTENT+'';
}


function stopAllScroll() {
	clearInterval(timerId);
	timerId = null;
}

addEvent($('but_up'),'click', scrollDown);
addEvent($('but_down'),'click', scrollUp);

</script><div class="feedback">
