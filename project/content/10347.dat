		waitDiv_show();
		$.ajax({
			url: 'http://tolideshoipros.fo.ru/blog?delete_action=' + id + '&sk=7f39eee5fd7ec31d77c71f8c6017379c', 
			complete: function(transport, textStatus){
				waitDiv_hide();
				eval(transport.responseText);
				if (redirect_url) {
					setTimeout(function(){window.location.href = 'http://tolideshoipros.fo.ru/blog/' + redirect_url}, 2000);
					return;
				}
				if(transport.getResponseHeader('X-SUCCESS') == 1){
					$('#item_' + id).remove();
				}
				$(window).trigger('scroll');
				
				/** Event handler */
				if(typeof customEvents.onAfterSingleItemDelete == 'function') {
					customEvents.onAfterSingleItemDelete(id);
				}
			}
		});
	}
	
