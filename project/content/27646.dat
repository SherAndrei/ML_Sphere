var q = '%d0%b8%d0%bd%d1%81%d1%82%d1%80%d1%83%d0%ba%d1%86%d0%b8%d1%8f+%d0%bf%d0%be+%d0%bf%d0%b5%d1%80%d0%b5%d0%b2%d0%be%d0%b7%d0%ba%d0%b5+%d0%ba%d0%b8%d1%81%d0%bb%d0%be%d1%82%d1%8b';
