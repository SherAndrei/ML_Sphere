    if (window.location.href.match('search=') && 0 != 0) {
        // Iterato through all lists and shorten the list with show more buttons
        $('#taxonomyTree, #taxonomyTree .listProductSubMenu, #taxonomyTree .listProductSubSubMenu').each(function(index, list) {
            $maxItems = $(list).is('#taxonomyTree') ? 2 : 5;
            // Just append the button if we have enough entries
            if ($(list).children('li').length > $maxItems) {
                // Hide entries
                $(list).children('li:gt(' + ($maxItems - 1) + '):not(.current-product-item)').addClass('hidden').hide();
                $(list).append(
                    // Build the show more button
                    $('<li>', {'class': 'product-menu-item more-sub-categories collapsed'}).append(
                        $('<span>').text(Message.get('show.more'))
                    ).append(
                        $('<i>', {'class': 'icon16 icon-down-dir inline'})
                    ).bind('click', function(event) {
                        // If we set the class visible our items need to be hidden
                        if ($(this).is('.collapsed')) {
                            // Show the items
                            $(list).children('li.hidden').removeClass('hidden').addClass('visible').show();
                            $(this).removeClass('collapsed');
                            // Update text inside first span
                            $(this).find('span:eq(0)').text(Message.get('show.less'));
                            $(this).find('.icon-down-dir').removeClass('icon-down-dir').addClass('icon-up-dir');
                        }
                        else {
                            // Hide the items
                            $(list).children('li.visible').removeClass('visible').addClass('hidden').hide();
                            $(this).addClass('collapsed');
                            // Update text inside first span
                            $(this).find('span:eq(0)').text(Message.get('show.more'));
                            $(this).find('.icon-up-dir').removeClass('icon-up-dir').addClass('icon-down-dir');
                        }
                    })
                );
            }
        });
    }

