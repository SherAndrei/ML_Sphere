		// Checking if a search sentence matches with a taxonomy suggestion (with inverting words)
		function matchWordsWithTaxonomies(inputWords, taxonomyWords) {
			if (inputWords.split(' ').length != taxonomyWords.split(' ').length) {
				return false;
			}
			// Same number of words, check if they are contained in each others
			else {
				var allInputWords = inputWords.split(' ');
				for(var index = 0; index < allInputWords.length; index ++) {
					inputWord = allInputWords[index];
					var matchExpr = new RegExp ('\\b' + inputWord + '\\b','i');
					if (taxonomyWords.match(matchExpr) == null) {
						return false;
					}
				}

				return true
			}
			return false;
		}

		// Returns the sluf og a taxo page having a matching with the searched performed by the user
		function getExactMatchingTaxonomy(inputName, suggestions) {
			inputName = inputName.trim()
			inputName = inputName.replace('-', ' ')
			var matchingTaxoUrl = null;
			if (suggestions != 'undefined') {
				// Iterating over all suggestions
				for (var index = 0; index < suggestions.suggestions.length; index ++) {
					var suggestedText = suggestions.suggestions[index].value
					suggestedText = suggestedText.replace('-', ' ')
					var matching = matchWordsWithTaxonomies(inputName, suggestedText)
					if (matching == true )
						return suggestions.suggestions[index].data;
				}

			}
			return matchingTaxoUrl
		}
	
