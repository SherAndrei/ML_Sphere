<script type="text/javascript">
    (function(){

                    var Steps = {
                ENTER_EMAIL: 1,
                CONFIRM_EMAIL: 2
            };

            $('body').on('click', '[data-step] [data-next-step]', function(e){
                e.preventDefault();
                var $this = $(this);

                var $parent = $this.closest('[data-step]'),
                    $input = $parent.find('input'),
                    email = $.trim($input.val()),
                    $notify_block = $parent.closest('.b-modal').find('[data-notification]'),
                    step = parseInt($this.attr('data-next-step')),
                    $nextBlock = $('.fancybox-inner [data-step="' + $this.attr('data-next-step') + '"]');

                switch (step) {
                    case Steps.ENTER_EMAIL:
                    {
                        $('#subscriptions_email').val('');
                        $notify_block.html('Будем отправлять вам письма о новых объявлениях. Это бесплатно. <a class="b-link b-link_type_important" href="http://www.drom.ru/misc/subscriptions/" target="_blank">Подробнее...</a>').removeAttr('class').addClass('normal-notification normal-notification_modified').show();
                        $parent.toggle();
                        $nextBlock.toggle();
                    }
                        break;

                    case Steps.CONFIRM_EMAIL:
                    {
                        if ( $this.attr('data-block-process') ) {
                            return false;
                        } else {
                            $this.attr('data-block-process', '1');
                        }

                        if (!email) {
                            $notify_block.html("Поле не заполнено, укажите e-mail").removeAttr('class').addClass('red-notification red-notification_modified').show();
                            $this.removeAttr('data-block-process');
                            return false;
                        }

                        if (!isValidEmail(email)) {
                            $notify_block.html("Вы указали неверный e-mail").removeAttr('class').addClass('red-notification red-notification_modified').show();
                            $this.removeAttr('data-block-process');
                            return false;
                        }

                        $.ajax({
                            url: "//www.drom.ru/common_ajax_processor.php",
                            dataType: "json",
                            type: "POST",
                            data: {
                                mode: 'subscription_email_confirm',
                                subscriptions_email: email,
                                subscriptions_name: 'Chery Tiggo, Казань',
                                subscriptions_params: '{"mid":[2878],"fid":[38],"cid":[58]}',
                                subscriptions_keywords: ''
                            },
                            success: function(data){
                                if ( !data.status ) {
                                    $notify_block.html(data.result.message).removeAttr('class').addClass('red-notification red-notification_modified').show();
                                    $this.removeAttr('data-block-process');
                                } else {
                                    $notify_block.html(data.result.message).removeAttr('class').addClass('normal-notification normal-notification_modified').show();
                                    $this.removeAttr('data-block-process');
                                    $parent.toggle();
                                    $nextBlock.toggle();
                                }
                            },
                            error: function (xhr, ajaxOptions, thrownError) {
                                $notify_block.html('При проверка e-mail произошла ошибка. Попробуйте еще раз.').removeAttr('class').addClass('red-notification red-notification_modified').show();
                                $this.removeAttr('data-block-process');
                            }
                        });

                        return false;
                    }
                        break;
                }

            });

        
