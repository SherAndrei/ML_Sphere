<script>
    $(document).ready(function () {
        var rates = [{"code":"BYN","value":30.3393},{"code":"EUR","value":63.531},{"code":"USD","value":56.5373}];
        rates.unshift({code: 'RUB', value: 1});



        $.each( rates, function( i, currency ) {
            $('#rateValute_1').append('<option value="'+currency.value+'" class="'+currency.code+'">'+currency.code+'</option>');
        });

        $.each( rates.reverse(), function( i, currency ) {
            $('#rateValute_2').append('<option value="'+currency.value+'" class="'+currency.code+'">'+currency.code+'</option>');
        });

        ex_rate();
    });

    function ex_rate() {
        var currency_amount = $('#ex_first_in').val();
        var left_valute = $('#rateValute_1 :selected');
        var right_valute = $('#rateValute_2 :selected');
        var result = Math.round(left_valute.val() * currency_amount / right_valute.val() * 100) / 100;

        $('#ex_result_out').text(result);
        $('#ex_first_in_label').text(left_valute.text());
        $('.exchange-converter .result-area .label').text(right_valute.text());
    }
</script>

