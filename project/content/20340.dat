<script>
$().ready(function(){
	$(".clip_complain_btn").click(function(){
		$("#complain_form_error").hide();
		$("#complain_form_sent").hide();
		$("#complain_form_enter_message").hide();
		$("#complain_form").show();
		$("#popup_clip_id_field").val($(this).attr("data-clip_id"));
		$("#popup_complain_text_field").val("");
		ShowPopup($("#popup_complain_form"));
		return false;
	});
});

function send_complain_message(){
	if($(".complain_with_message:checked").size() && !$("#popup_complain_text_field").val()){
		$("#complain_form_enter_message").show();
		return false;
	}
	var payload=$("#popup_complain_form").find('form').serializeArray();
	$.post('/ajax/?f=clip_complain', payload, function(data) {
			if(data.status == 0) {
				$("#complain_form_error").hide();
				$("#complain_form_sent").show();
				$("#complain_form").hide();
			} else {
				$("#complain_form_error").show();
				$("#complain_form_sent").hide();
				$("#complain_form").hide();
			}
			$("#complain_form_enter_message").hide();
			window.setTimeout(function(){
				HidePopup($("#popup_complain_form"));
				$("#popup_complain_text_field").val("");
			},5000);

		});
	return false;
}
