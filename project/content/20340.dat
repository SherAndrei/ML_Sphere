	<script type="text/javascript">
	var comment_default_text="Введите текст комментария";
	var dribble_timer;
	var noclick=0;
	$().ready(function(){
		var $add_comment_container = $('#add_comment_container');
		var $submit_button_changer = $add_comment_container.find('input[type=submit]');
		var $comment_id_field = $add_comment_container.find('input[name=comment_id]');
		var form_visible = false;
		$('.x-b_add_comment').live('click',function() {
			if(noclick){return false}
			noclick=1;
			dribble_timer=window.setTimeout("noclick=0",1000);
			var $this = $(this);
			var comment_id = parseInt($this.attr('data-comment_id'));
			if(comment_id == $comment_id_field.val() && form_visible) { // form already shown for this comment => hide form
				$comment_id_field.val(0);
				$('#add_comment_0').append($add_comment_container);
				$submit_button_changer.val('Добавить комментарий');
				$add_comment_container.show().find('textarea').focus();
			} else { // show form
				$comment_id_field.val(comment_id);
				$('#add_comment_' + comment_id).append($add_comment_container);
				if(comment_id) {
					$submit_button_changer.val('Ответить');
				} else {
					$submit_button_changer.val('Добавить комментарий');
				}
				$add_comment_container.show().find('textarea').focus();
				form_visible = true;
			}
			return false;
		});
		$('#add_comment_form').submit(function() {
			var $this = $(this);
			var txt=$this.find('textarea').val();
			if(!txt || txt == comment_default_text){return false}
			$this.find('input[type=submit]').attr('disabled', true);
			$.post('/ajax?f=add_comment', $this.serialize(), function(resp) {
				$this.find('input[type=submit]').attr('disabled', false);
				if(resp.no_reload){
					$("#add_comment_"+resp.comment.parent_id).after(create_comment_item(resp.comment));
					$("#add_comment_container").find("textarea").val("");
					$("#add_comment_container").hide();
					$("#no_comments_message_div").hide();
					return false;
				}else{
					var add_to_url = 'allcomments=1&thread=' + $this.find('input[name=comment_id]').val() + '&r=' + Math.random();
					 // random part to url is added for case when user do comment in thread and url is not changed after adding comment, just hash changed
					var hash = '';
					if(resp.last_page) {
						add_to_url += '&page=' + resp.last_page;
					}
					if(resp.comment_id) {
						hash = 'comment_' + resp.comment_id;
					}
					window.location.assign(url_plus(add_to_url, '', hash));
				}
			});
			return false;
		});

		$('#add_comment_container').find('textarea').blur(function(){
			if(!$(this).val()){
				$(this).val(comment_default_text);
			}
		}).focus(function(){
			if($(this).val() == comment_default_text){
				$(this).val("");
			}
		});
	});

	function create_comment_item(comment){
		var formlevel=comment.level + 1;
		if(formlevel>3){formlevel=3}
		return "<div class=\"block color_wt smp\" style=\"margin-left: " + (comment.level * 20) + "px\"> \
				<u class=\"block_t\"><i></i><b></b></u> \
					<div class=\"block_c\"> \
						<div class=\"comment\"> \
							<div class=\"comment_pic\"><a href=\"/profile/\" rel=\"nofollow\"><img src=\"/avatar/_50x50\" width=\"50\" height=\"50\" alt=\"\" /></a></div> \
							<div class=\"comment_cnt\"> \
								<div class=\"comment_head\"> \
									<div class=\"comment_inf\"><span>" + comment.created + "</span> <a href=\"/profile/\" rel=\"nofollow\"></a></div> \
								</div> \
								<div class=\"comment_body\">" + comment.content + "</div> \
								 \
							</div> \
							<div class=\"cltb\"></div> \
						</div> \
					</div> \
				<u class=\"block_b\"><i></i><b></b></u> \
			</div> \
			<div id=\"add_comment_" + comment.id + "\" style=\"margin-left: " + (formlevel * 20) + "px\"></div>";
	}
	</script>
</div>

