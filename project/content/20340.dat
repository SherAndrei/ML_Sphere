	player.on("stop",function(){
		lock_playlist_buttons=0;
		if(video_playlist.length <= 1){
			return;
		}
		video_playlist_position=Math.floor(video_playlist_position);
		if(video_playlist.length > video_playlist_position + 1){
			video_playlist_position++;
		}else{
			video_playlist_position=0;
		}
		update_clip_titles(video_playlist_position);
		var next_clip=video_playlist[video_playlist_position];
		auto_play=1;			// play next clip after loading
		player.reload(next_clip);
	});

	player.on("start",function(){
		lock_playlist_buttons=0;
		if(!auto_play){
			return;
		}
		video_playlist_position=Math.floor(video_playlist_position);
		if($(".playlist_entry_"+video_playlist_position)){
			$(".playlist_entry_"+video_playlist_position).addClass('active');
		}
		player.play();
	});

	function stop_player(){
		lock_playlist_buttons=0;
		auto_play=0;
		player.stop();
	}

	function shuffle_playlist(){
		var tmp_ids=Array(video_playlist.length);
		var tmp_playlist=Array(video_playlist.length);
		var tmp_clips=Array(video_playlist.length);
		for(var i=0; i<video_playlist.length; i++){
			tmp_ids[i]=i;
		}
		for(var j, x, i = tmp_ids.length; i; j = parseInt(Math.random() * i), x = tmp_ids[--i], tmp_ids[i] = tmp_ids[j], tmp_ids[j] = x);
		for(var i=0; i<tmp_ids.length; i++){
			tmp_playlist[i]=video_playlist[tmp_ids[i]];
			tmp_clips[i]=clips[tmp_ids[i]];
		}
		video_playlist=tmp_playlist;
		clips=tmp_clips;
		move_in_playlist(0);
		return false;
	}

	function update_clip_titles(i){
		$("#playlist_position_"+i).click();
		if(!clips || !clips[i]){return}
		if($("#current_artist_name")){
			$("#current_artist_name").html(clips[i]['artist']);
		}
		if($("#current_clip_title")){
			$("#current_clip_title").html(clips[i]['title']);
		}
		if($("#current_clip_rating")){
			$("#current_clip_rating").html('<b>'+clips[i]['rating']+'</b> '+show_rating(clips[i]['rating'])+' <b>'+clips[i]['votes_cnt']+' '+sclonyator(clips[i]['votes_cnt'],'голос','голоса','голосов')+'</b>');
		}
		if($("#current_clip_views")){
			$("#current_clip_views").html('<b>Просмотров: '+clips[i]['hits']);
		}
	};

	function move_in_playlist(step){
		if(lock_playlist_buttons){return};
		lock_playlist_buttons=1;
		video_playlist_position+=step;
		if(video_playlist_position>=video_playlist.length){
			video_playlist_position=0;
		}
		if(video_playlist_position<0){
			video_playlist_position=video_playlist.length-1;
		}
		update_clip_titles(video_playlist_position);
		var next_clip=video_playlist[video_playlist_position];
		auto_play=1;
		player.reload(next_clip);
		return false;
	};

	$(document).on("click",".x-playlist-entry",function(){
		lock_playlist_buttons=0;
