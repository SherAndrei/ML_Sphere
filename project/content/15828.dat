<div class="answer handle_hovers  " id="answer5436" itemprop="suggestedAnswer" itemscope itemtype="http://schema.org/Answer">
	<div class="body rad5">
		<div class="shadow rad4">

			<div class="border in_answer rad4_tl rad4_tr">
			<div class="second_border in_answer">
