<script>
$('.dayrecipes_p').css('background-image', 'url(' + dayRecipesPopupBgImage + ')');

function showDailyRecipesPopup(force)
{
	force = force || false;
	if (force || (popupBgImageLoaded && !daily_recipes_popup_shown)) {
		daily_recipes_popup_shown = true;
		$('.wrpqwe, .dayrecipes_p').show();
		return true;
	}
	return false;
}


function closeDailyRecipesPopup(clb)
{
	clb = clb || null;
	$('.dayrecipes_p, .wrpqwe').hide();
	if (clb) {
		clb();
	}
}

(function($) {

function createCookie(name, value, days)
{
	var expires;

	if (days) {
		var date = new Date();
		date.setTime(date.getTime() + (days * 24 * 60 * 60 * 1000));
		expires = "; expires=" + date.toGMTString();
	} else {
		expires = "";
	}

	document.cookie = encodeURIComponent(name) + "=" + encodeURIComponent(value) + expires + "; path=/; domain=.povarenok.ru";
}

$('.dayrecipes_p').submit(function() {
	var $this = $(this);
	var $btn = $this.find('button');
	var name = $.trim($this.find('input[name="name"]').val());
	var email = $.trim($this.find('input[name="email"]').val());

	if (!name) {
		alert('Укажите своё имя');
		return false;
	}

	if (!email.match(/.+@.+/)) {
		alert('Введите корректный адрес e-mail');
		return false;
	}

	var _data = {
		name: name,
		email: email,
		promo: popupImgId
	};

	$btn.attr('disabled', 'disabled').text('Отправка…');
	$.post('http://hoster1srv.povarenok.ru/ajax/subscribe/dailyrecipesmailer/', _data, function(response) {
		$btn.removeAttr('disabled').text('Хочу получать вкусные рецепты!');
		if (response.success) {
			closeDailyRecipesPopup(function() {
				$this.find('input[name="name"]').val('');
				$this.find('input[name="email"]').val('');
			});
			// Set a cookie to prevent showing this popup again
			createCookie('drs_done', 1, 365);
		} else {
			alert(response.error);
		}
	}, 'json');
	return false;
});



if (screen.width>800) interval = setInterval(function() {
  if ($(window).scrollTop() >= 400) {
    if (showDailyRecipesPopup(false)) {
      clearInterval(interval);
    }
  }
}, 2000);





})(jQuery);
