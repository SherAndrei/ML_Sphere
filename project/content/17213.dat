        $('.user.one').click(function (e, ob) {
            $('.user').removeClass('active-user');
            $('.user.one').addClass('active-user');
            $('#user_type').val('1');
            e.stopPropagation();
        });
        $('.user.two').click(function (e, ob) {
            $('.user').removeClass('active-user');
            $('.user.two').addClass('active-user');
            $('#user_type').val('2');
            e.stopPropagation();
