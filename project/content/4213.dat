<script type="text/javascript">
$(".b-rating img").click(function(){
	var id = this.id.replace(this.className+'_','');
	var rate = this.className=='up'?1:-1;
	var span = $(this).parent().find('.b-num');
	span.html('<img alt="" src="/design/img/ajax.gif" height="18"/>');
	$.post("/ajax_block/comment_rate.php", {'id':id,'rate':rate}, function(data){		
		data = parseInt(data);
		if(data>=0){
			span[0].className='b-num b-num-cool';
			span.html('+'+data);
		} else {
			span[0].className='b-num b-num-suxx';
			span.html(data);
		}
	});
});
$(".commentform").live('submit',function(){
	var errors = [];	
	$(".req",this).each(function(){
		if($(this).val()=='' || $(this).val()==this.title)
			errors.push("Не заполнено обязательное поле "+this.title);
	});
	$(".chk_email", this).each(function(){
		if($(this).val()!='' && $(this).val()!=this.title && !isValidEmailAddress($(this).val()))
			errors.push("Поле E-mail заполнено неверно.");
	});
	if(errors.length){
		$('.errortext',this).remove();
		$(this).prepend('<div class="errortext">'+errors.join("<br/>")+'</div>');
		return false;
	}
});
</script>
