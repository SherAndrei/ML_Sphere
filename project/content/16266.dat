function setCenter(b) {
	windowHeight = document.documentElement.clientHeight;
	currentOffset = document.documentElement.scrollTop || document.body.scrollTop;
	currentTop = currentOffset + parseInt((windowHeight - ($(b).height() + 40)) / 2);
	pLeft = ($("#wrapper").width() - $(b).width()) / 2;
	if (currentTop < (currentOffset + 20)) {
