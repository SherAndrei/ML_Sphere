<div  class="response good" id="div_review_162494" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
    
    <meta itemprop="headline" content=""/>

    <div itemprop="author" itemscope itemtype="http://schema.org/Person">
        
        <div>
            
                <p class="profile_name"><s></s>Герой романа</p>
                <ul class="actions">
                    
                </ul>
            
            <p class="sub_title" id="ext_title_162494"></p>
        </div>
