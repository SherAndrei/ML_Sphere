			type: "POST",
			dataType: "json",
			success : function(comment){
				$('#commarea').append( comment.content ).slideDown('slow');
				$('textarea').sceditor('instance').val( '' );
				
				$('#captcha_img').html( comment.captcha );
				$('#captcha').val('');
			}
		})
		return false;
	})
	
	$("textarea").sceditor({
		plugins: "bbcode",
		style: "/am/editor/jquery.sceditor.default.min.css",
		emoticonsRoot: "/am/editor/",
		toolbar: "bold,italic,underline|maximize,source|quote"
	});
});
