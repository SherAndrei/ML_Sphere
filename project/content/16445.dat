            <img id="ctl95_article_image" src="/2014/11/22/kak_pravilno_zakapyvat_kapli_v_glaza.jpg" alt="как правильно закапывать капли в глаза" style="border-width:0px;" />
            <div id="ctl95_article_left">
		<div style="padding:0px; position: relative;">
<script async src="//pagead2.googlesyndication.com/pagead/js/adsbygoogle.js"></script>
<!-- 200x200 Seaside -->
<script type="text/javascript">
