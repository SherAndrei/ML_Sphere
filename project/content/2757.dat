    $('body').on('click', 'a.nofollow', function () {
        var link = $(this).data("link");
        if (link) {
            window.open(link);
        }
        return false;
    });

    $('body').on('click', 'button.button-survey-choose-card', function () {
        var del_id = $(this).data('del_id');
        var add_id = $(this).data('add_id');

        l4basket.preloader();
        if (del_id) {
            l4basket.delItem(del_id).success(function (html) {
                l4utils.yandex_goal('BASKET_ITEMS');
                ga('send', 'event', 'basket', 'items', 'delete');
                l4basket.updHtml(html);
            });
        } else {
            l4basket.addItem({id: add_id}).success(function (html) {
                l4utils.yandex_goal('BASKET_ITEMS');
                ga('send', 'event', 'basket', 'items', 'add');
                l4basket.updHtml(html);
            });
        }
    });

    l4callbacks.basket_upd.add(function (params) {
        var $item = $('[data-test_id]');
        var $b_add = $item.find('button[data-add_id]');
        var $b_del = $item.find('button[data-del_id]');

        $b_add.show();
        $b_del.data('del_id', "").hide();

        $('[data-test_id_in_basket]').each(function () {
            var test_id = $(this).data('test_id_in_basket');
            var basket_id = $(this).data('basket_id');

            if (test_id == $item.data('test_id')) {
                $b_add.hide();
                $b_del.data('del_id', basket_id > 1 ? basket_id : test_id).show();
            }
        });

        l4utils.addQTip('#discountBaloon');
    });

});</script>
