<script type="text/javascript">l4test = new l4obj('test');

l4test.question = function (params) {
    params = typeof (params) === 'object' ? l4utils.serializeParams(params) : {test_id: params};

    l4test._post(l4test._prepareParams('question', params)).success(function (response) {
        l4popups.show(response.popup);
    });
};

l4test.answer = function (params) {
    params = typeof (params) === 'object' ? l4utils.serializeParams(params) : false;

    var consultation_id = params.consultation_id;
    var answer = params.answer;

    var $c = $('[data-consultation_id="' + consultation_id + '"]');
    var $reply = $c.find('.place-layer-comment__reply').first();

    var $f = $c.find('[name="consultation_add_answer"]');
    var $b = $f.find('[type="submit"]');
    var $l = $f.find('img.answer__loader');

    $b.hide();
    $l.show();

    l4test._post(l4test._prepareParams('answer', params)).success(function (response) {
        if (response.result && response.result === true) {
            $reply.find('.place-layer-comment__body').html(answer);
            $reply.find('.place-layer-comment__header').html('Ответ Lab4u');
        } else {
            alert(response.error ? response.error : 'Ошибка при ответе. Обновите страницу.');
        }

        $b.show();
        $l.hide();
    });
};

l4test.deleteQuestion = function (test_id, consultation_id) {
    if (!confirm('Удалить вопрос?')) {
        return false;
    }

    l4test._post(l4test._prepareParams('delete_question', {test_id: test_id, consultation_id: consultation_id})).success(function (response) {
        if (response.result && response.result === true) {
            $('[data-consultation_id="' + consultation_id + '"]').remove();
        } else {
            alert(response.error ? response.error : 'Ошибка при попытке удаления вопроса. Обновите страницу.');
        }
    });
};

l4test.moveQuestion = function (params) {
    params = typeof (params) === 'object' ? l4utils.serializeParams(params) : false;

    if (!confirm('Переместить вопрос?')) {
        return false;
    }

    var consultation_id = params.consultation_id;

    l4test._post(l4test._prepareParams('move_question', params)).success(function (response) {
        if (response.result && response.result === true) {
            $('[data-consultation_id="' + consultation_id + '"]').remove();
        } else {
            alert(response.error ? response.error : 'Ошибка при попытке пемещения вопроса. Обновите страницу.');
        }
    });
};

l4test.updateCaptcha = function () {
    var params = false;

    l4test._post(l4test._prepareParams('update_captcha', params)).success(function (response) {
        if (response.code) {
            $('.captcha_pic').attr('src', '/bitrix/tools/captcha.php?captcha_sid=' + response.code);
            $('[name="captcha_sid"]').val(response.code);
        } else {
            alert(response.error ? response.error : 'Ошибка при попытке обновить капчу. Обновите страницу.');
        }
    });
};

$(document).ready(function () {
    $('body').on('click', '.slog-link', function () {
        var
                l = $(this),
                p = l.parents('.slog-block'),
                b = p.find('.slog-info'),
                h = p.find('.slog-hide');

        b.slideToggle(function () {
            l.toggleClass('selected');
            h.toggle();

            h.click(function () {
                b.slideUp(function () {
                    h.hide();
                    l.removeClass('selected');
                });
            }).animate();
        });
    });
});
