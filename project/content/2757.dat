l4user.auth_url = false;
l4user.next_step = false;

l4user._action = function (form, action, param) {
    var params = false;

    if (form instanceof jQuery) {
        params = l4utils.serializeParams(form);
    } else if (typeof (form) === 'object') {
        params = form;
    }

    if (param && l4user[param]) {
        if (params === false) {
            params = {};
        }
        params[param] = l4user[param];
        l4user[param] = false;
    }

    if (l4user.next_step) {
        params.next_step = l4user.next_step;
        l4user.next_step = false;
    }

    var post_params = params === false ? this._prepareParams(action) : this._prepareParams(action, params);

    l4popups.preloader();
    this._post(post_params).success(function (response) {
        l4user.user = response.params && response.params.user ? response.params.user : false;

        l4popups.show(response);

        if (action === 'auth' || action === 'reg') {
            l4callbacks.user_upd && l4callbacks.user_upd.fire();
        }
    });
}

l4user.auth = function (form) {
    l4user._action(form, 'auth', 'reg_url');
};

l4user.reg = function (form) {
    l4user._action(form, 'reg', 'auth_url');
};

l4user.forgot = function (form) {
    l4user._action(form, 'forgot', 'auth_url');
};

l4user.unsubscribe = function (form) {
    l4user._action(form, 'unsubscribe');
};

l4user.change_password = function (form) {
    l4user._action(form, 'change_password');
};

l4user.recover_password = function (form) {
    l4user._action(form, 'recover_password');
};

l4user.delete_patient = function (form) {
    l4user._action(form, 'delete_patient');
};

l4user.change_phone = function (form) {
    l4user._action(form, 'change_phone');
};

l4user.add_patient = function (form) {
    l4user._action(form, 'add_patient');
};

$(document).ready(function () {
    $('body').on('keyup', '[name="U_PASS"]', function (event) {
        if (event.which === 13) {
            l4user.auth($('[name="U_AUTH_FORM"]'));
        } else {
            //alert(event.which);
        }
    });
});

l4callbacks.user_upd = $.Callbacks();

l4callbacks.user_upd.add(function () {
    if (!l4user.user) {
        return;
    }

    if (l4user.user.GROUPS) {
        var r = false;

        for (var x in l4user.user.GROUPS) {
            var g = parseInt(l4user.user.GROUPS[x]);

            if (g === 12) {
                r = server_root_url + "/doctor/order/";
            } else if (g === 7) {
                r = server_root_url + "/partner/order/";
            } else if (g === 8) {
                r = server_root_url + "/hospital/order/";
                //} else if (_G($_SESSION['BASKET'], 'LPU_INTERVAL_ID')) {
                //	r = "/store/?patient_choise=yes";
            }
        }

        if (r) {
            window.location.assign(r);
        }
    }
});</script>
