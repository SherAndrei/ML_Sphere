<script type="text/javascript">

var PicturesResult = "";
var Count = 0;
var UploadedFiles = 0;
function pictures_fileDialogComplete(numFilesSelected, numFilesQueued) {
    try {
        if (numFilesQueued > 0) {
            Count = parseInt(numFilesQueued);
            $('#picture_upload').html('<div id="progress_inner"><div id="Progress" /></div></div>');
            this.startUpload();
        }
    } catch (ex) {
    }
}

function pictures_uploadProgress(file, bytesLoaded) {
    try {
        var pw = 240;
        var w = Math.ceil(pw * (UploadedFiles / Count + (bytesLoaded / (file.size * Count))));
        $('#Progress').stop().animate({ width: w });
    } catch (ex) {
    }
}
function pictures_uploadSuccess(file, serverData) {
    try {
        UploadedFiles++;
		var resp=serverData;
		$('#comment_form_img_div').html(resp);
    } catch (ex) {

    }
}

function pictures_uploadComplete(file) {
    try {
        if (this.getStats().files_queued > 0) {
            this.startUpload();
        } else {
            $('#Progress').css('width','0');
			$('#picture_upload').html('Прикрепить изображение');
        }
    } catch (ex) {
    }
}

