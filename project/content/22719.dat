            case SWFUpload.QUEUE_ERROR.ZERO_BYTE_FILE:
            case SWFUpload.QUEUE_ERROR.INVALID_FILETYPE:
                break;
        }
    } catch (ex) {
    }

}

function swfuploadLoaded() {
    $('#UploadArea object').hover(
        function() {
            $(this).next().addClass('hover');
        },
        function() {
            $(this).next().removeClass('hover');
        });

}
var ASPSESSID = "";
var swfuPictures;
function BindSWFUpload() {
    var swfuPicturesSettings = {
        file_dialog_complete_handler: pictures_fileDialogComplete,
        upload_progress_handler: pictures_uploadProgress,
        upload_success_handler: pictures_uploadSuccess,
        upload_complete_handler: pictures_uploadComplete,
