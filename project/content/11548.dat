<script type="text/javascript">
	function getUsersOnline() {
		if( !window.GetUsersProcess ) {
			window.GetUsersProcess = true;
		
			$('#modal-moderatorOnlineInformer .hover').show();
			
		    $.ajax({
				url: '/util/?action=moderatorOnlineInformer&mode=fullList',
				dataType: 'json',
				success: function( data ) {
		            if( data ) {
			            $('.moderatorOnlineInformer').show();
			            
			            $('.moderatorOnlineInformer b').text( data['num'] );
		            	$('.modal-content-moderatorOnlineInformer').html( data['html'] );
		            	$('#modal-moderatorOnlineInformer .hover').hide();
		            }
					window.GetUsersProcess = false;
				}
			});
		} else { }
	}
</script>


