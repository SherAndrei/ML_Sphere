<script>
    mambo.uid = 632920355;
    
    function redirectToRegPayPage(listener, className) {
        if($(listener).hasClass(className)) {
            $.ajax({
                url: 'https://www.mamba.ru/ru/ajax/pay_for_registration.phtml',
                data: {page: encodeURIComponent(window.location), action: 'setRedirect'},
                dataType: 'json',
                success: function(data) {}
            });

            return true;
        }

        return false;
    }

    $(function() {
        if(window.jQuery && window.$){
            $(document).on('custom.vipPaymentDone',function() {
                $('._openChateg.needPayForRegClass').unbind('click');
            });
        }
    });

	mambo.use('{mambo.ui}Overlay', '{mambo.utils}Cookie', function ($) {
		// dropdown
		var $doc = $(document),
		    selectedClass = 'selected',
		    $advertBoxes = $('.dropdown-wrapper.advert-box'),
		    $dropDownWrappers = $('.dropdown-wrapper').not($advertBoxes),
		    $dropDowns = $dropDownWrappers.find('.dropdown').not($advertBoxes);
        
		// functions
		function setDropdown($dropDownWrapper) {
			var $dropDown = $dropDownWrapper.find('.dropdown');
			$dropDownWrapper.bind('click.dropdown', function (e) {
				var $t = $(e.target);
				if (!$t.closest($dropDown).length) {
					$dropDownWrapper.toggleClass(selectedClass);
					$advertBoxes.trigger('otherDropDownAction');
				}
			});
			$doc.bind('click.dropdown', function (e) {
				var $t = $(e.target);
				if (!$t.is($dropDownWrapper) && !$t.closest($dropDownWrapper).length) {
					$dropDownWrapper.removeClass(selectedClass);
					$advertBoxes.trigger('otherDropDownAction');
				}
			});
		}
        
		function setAdvertBox($advertBox) {
			// vars
			var cookieName = $advertBox.attr('data-cookie-name');
			var $btnCloseHint = $advertBox.find('.hint-close');
			var $btnToClientPage = $advertBox.find('.btn-to-client-page');
			var $btnBig = $advertBox.find('.button');
			var closeHintPostUrl = $btnCloseHint.attr('data-post-url');
			var clientPageUrl = $btnBig.attr('data-redirect-url');
			var advertVisibilityFlag = 0;
			if(!mambo.utils.Cookie(cookieName)){//set first view
				$advertBox.addClass(selectedClass);
				advertVisibilityFlag = 1;
			}
            
			// events
			$btnCloseHint.on('click',function(e){
				e.preventDefault();
				mambo.utils.Cookie(cookieName,'1');// set cookie
				$advertBox.removeClass(selectedClass);
				advertVisibilityFlag = 0;
				if(closeHintPostUrl){
					$.get(closeHintPostUrl);
				}
			});
            
			$btnBig.on('click',function(e){
				e.preventDefault();
				if(clientPageUrl){
					window.location = clientPageUrl;
				}
			});
            
			$advertBox.on('otherDropDownAction',function(){
				if($dropDownWrappers.filter('.'+selectedClass).length){// other dropDowns is visible
					if(advertVisibilityFlag){
						$advertBox.removeClass(selectedClass);
					}
				}else{// no dropDowns is visible
					if(advertVisibilityFlag){
						$advertBox.addClass(selectedClass);
					}
				}
			});
		}
        
		// init dropdowns
		$dropDownWrappers.each(function (i) {
			setDropdown($(this));
		});
        
		$advertBoxes.each(function (i) {
			setAdvertBox($(this));
		});
        
		// winked
		if (mambo.auth) {
			var $link = $('#wink-link'),
			    $winkedText = $('#winked-text'),
                addToFavoriteBtn = $('.js-add-to-favorite'),
                favoriteForm = $("<form class='hide' method='post'><input type='hidden' name='s_post' value='A4zaV7mnohKKqxgu5m5WrCm0DKNxTlI7'></form>").appendTo('body');
            
			$link.bind('click', function (e) {
                if(redirectToRegPayPage(this, 'needPayForReg')) {
                    return false;
                }

				if (Boolean("true")) {
					$winkedText.show();
					$link.hide();
					$link.addClass('winked').unbind('click').click(function (){return false;});
					var $linkUk = $link.find('.text-container');
					if ($linkUk.length) {
						$linkUk.text("");
					} else {
						$link.text("");
					}
				}
			});
            
            addToFavoriteBtn.on('click', function(e) {
                var action = this.href;
                
                e.preventDefault();
                favoriteForm.attr('action', action).submit();
            });
		}

        $('._openChateg.needPayForRegClass').unbind('click').bind('click', function(e) {
            redirectToRegPayPage(this, 'needPayForRegClass');
            return false;
        });

        $("#PhotoList .s-border").click(function() {
            if($("#FotoLayer").hasClass('noShowbigPhoto')) {
                $.ajax({
                    url: 'https://www.mamba.ru/ru/ajax/pay_for_registration.phtml',
                    data: {page: encodeURIComponent(window.location), action: 'setRedirect'},
                    success: function(data) {}
                });
            }
        });

	});
</script>
