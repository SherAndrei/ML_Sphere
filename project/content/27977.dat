                    (function() {
                        var classDropdown = 'js-menu-dropdown',
                                classDropdownSwitcher = 'js-menu-dropdown-switcher',
                                classDropdownActive = 'menu-dropdown_active',
                                $dropdowns = $('.' + classDropdown);

                        $dropdowns.on('click.menu_toggler', function(e) {
                            var $currentMenu = $(this),
                                    $target = $(e.target);

                            if (!$target.hasClass(classDropdownActive) && $target.closest('.' + classDropdownActive).length === 0) {
                                $dropdowns.removeClass(classDropdownActive);
                            }

                            if ($target.hasClass(classDropdownSwitcher) || $target.closest('.' + classDropdownSwitcher).length !== 0) {
                                $currentMenu.toggleClass(classDropdownActive);

                                $(document).on('click.menu_body_toggler', function(e) {
                                    var $target = $(e.target);

                                    if (!$target.hasClass(classDropdownActive) && $target.closest('.' + classDropdownActive).length === 0) {
                                        $dropdowns.removeClass(classDropdownActive);
                                        $(document).off('click.menu_body_toggler');
                                    }
                                });

                                e.preventDefault();
                            }
                        });
                    })();
                    </script>



			
