<script>

mambo.use('{mambo.ui}BlockRejected', function($){

	var blockedClass = 'b-blocked';

	function init (linkSelector, valueSelector, $layer, $layerTextarea, $rejectedBlock) {

		$rejectedBlock.on('submit_success', function () {
			$(this)[!$(linkSelector).length ? 'removeClass' : 'addClass'](blockedClass);
		});

		mambo.ui.BlockRejected({
			linkSelector: linkSelector,
			valueSelector: valueSelector,
			layer: $layer,
			submitHandler: function () {

				var formAction = $(this).attr('action'),
					formData = $(this).serialize();

				$.post(formAction, formData).done(function (R) {
					if (R && R.s) {
						$rejectedBlock.removeClass(blockedClass);
						$(valueSelector).html($layerTextarea.val().replace(/[\r\n]+/ig, '<br>'));
						$(linkSelector).closest('.b-anketa_blocked').remove();
					}
				});

				return false;
			}
		});
	}

	(function lookforBlockHandler() {

		var linkSelector = '.js-link-lookfor-rejected',
			valueSelector = '.js-value-lookfor-rejected';

		var $layer = $('.js-layer-lookfor-rejected'),
			$layerTextarea = $('textarea', $layer),
			$rejectedBlock = $(linkSelector).closest('.' + blockedClass);

		init(linkSelector, valueSelector, $layer, $layerTextarea, $rejectedBlock);

	}());

	(function sexexcitationBlockHandler() {

		var linkSelector = '.js-link-sexexcitation-rejected',
			valueSelector = '.js-value-sexexcitation-rejected';

		var $layer = $('.js-layer-sexexcitation-rejected'),
			$layerTextarea = $('textarea', $layer),
			$rejectedBlock = $(linkSelector).closest('.' + blockedClass);

		init(linkSelector, valueSelector, $layer, $layerTextarea, $rejectedBlock);

	}());
});

</script>

