			<script type="text/javascript">
				$(function() {
					var $form = $('#registration').find('form');

					$(document).on('click', '.login_trigger', function() {
						$('[data-target="#login"]').click();
						return false;
					});

					$form.submit(function() {
						var sub = $(this).find('input[name=create_sub]').is(':checked');

						$form.find('.form-group').removeClass('has-error');

						$.ajax({
							url: $form.attr('action'),
							data: $form.serialize(),
							method: 'POST',
							dataType: 'json'
						}).done(function(data) {
							var idx;
							if (data.success) {
								yaCounterAll.reachGoal('REGISTER', { registrationType: 'Регистрация' });
								if (sub) {
									yaCounterAll.reachGoal('SUBSCRIBE', { subscriptionType: "Регистрация" });
								}

								if (data.redirectTo) {
									window.location.href = data.redirectTo;
								} else {
									window.location.reload();
								}
							} else {
								for (idx in data.message) {
									if ('login' === idx && data.tryLogin) {
										data.message[idx] += ('. ' + $('#try_login').html());
									}
									setError(idx, data.message[idx]);
								}
							}
						}).fail(function(data) {
							$('#register_error').html('Не удалось произвести регистрацию. Повторите попытку позднее.');
						});
						return false;
					});

					function getWrongField(name)
					{
						name = name || 'login';
						return $form.find('[name=' + name + ']');
					}

					function setError(fieldName, text)
					{
						getWrongField(fieldName).parent().addClass('has-error').find('.form-group__hint').html(text);
					}
				});
			</script>

