            <script type="text/javascript">
            $(function () {
                var $blogSubscribe = $('.js-blog-subscribe');

                $blogSubscribe
                    .on('submit', 'form', function(e) {
                        var
                            $form = $(this),
                            $genericError = $blogSubscribe.find('div[data-message="generic.error"]');

                        e.preventDefault();

                        $blogSubscribe.find('div[data-message]').hide();

                        $.ajax({
                            type: $form.attr('method'),
                            url: $form.attr('action'),
                            data: $form.serializeArray(),
                            success: function(data) {
                                var $message = $blogSubscribe.find('div[data-message="' + data.code + '"]');
                                if ($message.length) {
                                    $message.show();
                                    yaCounterAll.reachGoal('SUBSCRIBE', { subscriptionType: "Подписка в блоге" });//Удачная подписка в блоге
                                } else {
                                    $genericError.show();
                                }
                            }
                        }).fail(function() {
                            $genericError.show();
                        });
                    })
                    .on('click', '.js-subscribe-show', function (e) {
                        e.preventDefault();

                        $blogSubscribe.find('div[data-message]').hide();
                        $blogSubscribe.find('div[data-message="form"]').show();
                    });

            });
        </script>

