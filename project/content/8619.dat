	var $layerVideoBonus = $('.js-bonus-layer');

	if ($layerVideoBonus.length > 0)
	{
		var layer = $layerVideoBonus.data('layer');
		var layerName = $layerVideoBonus.data('layerName');

		if (typeof layer === "string" && layer.length>0)
		{
			var overlay = Modal({
				content: $('#'+layer),
				modalWidth: 430,
				modalClass: layerName + '-modal',
				useScrolling: true
			});

			$layerVideoBonus.on('click', function (e) {
				e.preventDefault();
				overlay.show();
			});
		}
	}
});

