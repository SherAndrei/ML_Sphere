<script type="text/javascript">
$(function() {
    var unexpectedErrText = 'Произошла ошибка, пожалуйста попробуйте позже';
    var containerHtml = '<div class="b-media-cont b-media-cont_no-margin"></div>';
    var containerInnerHtml = '<div class="b-media-cont b-media-cont_no-margin b-media-cont_relative"></div>';
    var btnLoadingClass = 'b-button_state_loading';
    var commentReplyLinkAttr = 'data-comment-reply';
    var commentAddBtnAttr = 'data-add-comment';
    var $htmlBody = $('html, body');
    var $cancelBtn = $('<span class="b-link">Передумал отвечать</span>');
    var $closeBtn = $('<div class="b-close-btn b-close-btn_pos-abs"></div>');
    var $formContainerInner = $(containerInnerHtml)
        .append($('[data-comment-form-add]').clone())
        .prepend($closeBtn)
    var $formContainer = $(containerHtml)
        .hide()
        .append($formContainerInner)
        .appendTo($('body'));
    var $form = $formContainer.find('form');
    var $addBtn = $formContainer.find('[' + commentAddBtnAttr + ']');
    var lastScrollPosition = 0;

    $addBtn.children().text('Ответить');
    $addBtn.wrap('<div class="b-random-group b-random-group_margin_r-size-l"></div>');
    $addBtn.parent().append($cancelBtn);

    $cancelBtn.on('click', function() {
        $formContainer.hide();
    });

    $closeBtn.on('click', function() {
        $formContainer.hide();
    });

    if (typeof performance === 'object' &&
        performance.navigation &&
        performance.navigation.type === 1) {
        // forceHashChangeTrigger();
        $(window).on('load', forceHashChangeTrigger);
    }

    $(document).on('click', '[data-comment-reply-navigate]', function(ev) {
        ev.preventDefault();
        var href = $(this).attr('href');
        var loc = document.location;

        loc.href = href;
        if (href.split('#')[0] === loc.href.replace(loc.hash, '')) {
            loc.reload();
        }
    });

    $form.on('submit', function(ev) {
        ev.preventDefault();
        var $this = $(this);
        var id = $this.find('[' + commentAddBtnAttr + ']').attr(commentAddBtnAttr);
        var url = '//www.drom.ru/add_comment_new.php';
        var data = $this.serialize() + '&' + $.param({
            response_type: 'json',
            answer_id: id,
        });

        $addBtn.addClass(btnLoadingClass);

        $.ajax({ url: url, type: 'POST', method: 'POST', data: data, xhrFields: { withCredentials: true } })
            .done(function(res) {
                if (res.status && !res.errors.length && res.comment_url.length) {
                    $formContainer.hide();
                    $htmlBody.scrollTop(lastScrollPosition);

                    var $notifContainer = $('[data-notification-cont]').eq(0);
                    if (!$notifContainer.length) {
                        var $menu = $('.b-menu').eq(0);
                        if (!$menu.length) {
                            $menu = $('.m-header').eq(0);
                        }
                        if (!$menu.length) {
                            $menu = $('[id=info_div]').eq(0);
                        }
                        $notifContainer = $(
                            '<div data-notification-cont class="b-notifications-group b-notifications-group_relative"></div>'
                        );
                        $menu.after($notifContainer);
                    }
                    $notifContainer.empty();

                    var notif = DromNotificationGroup.getInstance(
                        $notifContainer[0], { useContainerOffset: true }
                    );
                    notif.createNotification(
                        'Комментарий опубликован! <a data-comment-reply-navigate href="' + res.comment_url +
                            '" class="b-link">Перейти к комментарию</a>',
                        'success',
                        'commentsReplySuccesMessage'
                    );
                    setTimeout(function () {
                        notif.removeNotification('commentsReplySuccesMessage');
                    }, 15000);
                } else if (!res.status && res.errors.length) {
                    res.errors.forEach(function(err) {
                        $formContainer.find('[data-comment-form-message]').remove();
                        $formContainer.prepend(
                            '<div class="red-notification red-notification_show no-margin" ' +
                            'data-comment-form-message>' + err + '</div>'
                        );
                    });
                } else {
                    alert(unexpectedErrText);
                }
            })
            .fail(function() {
                alert(unexpectedErrText);
            })
            .always(function() {
                $addBtn.removeClass(btnLoadingClass);
            });
    });

    $('[' + commentReplyLinkAttr + ']').on('click', function() {
        var $link = $(this);
        var id = $link.attr('data-comment-reply');
        var $target = $('#comment_id_' + id);

        lastScrollPosition = document.body.scrollTop;

        resetForm();
        $formContainer
            .find('[' + commentAddBtnAttr + ']')
            .attr(commentAddBtnAttr, id);
        $target.parent().after($formContainer.show());
        $htmlBody.scrollTop($link.offset().top - 70);
    });

    /**
     * [resetForm description]
     */
    function resetForm() {
        $formContainer.find('[data-comment-form-message]').remove();
        $form.find('#comment_text').val('');
    }

    /**
     * [forceHashChangeTrigger description]
     */
    function forceHashChangeTrigger() {
        var tmpHash = document.location.hash;
        document.location.hash = '';
        document.location.hash = tmpHash;
    }
});
</script>

