$('#button-review').bind('click', function() {
	urlr = 'city=' + encodeURIComponent($('input[name=\'cityr\']').val()) + 
'&email=' + encodeURIComponent($('input[name=\'emailr\']').val()) + 
'&name=' + encodeURIComponent($('input[name=\'name\']').val()) + 
'&text=' + encodeURIComponent($('textarea[name=\'text\']').val()) + 
'&rating=' + encodeURIComponent($('input[name=\'rating\']:checked').val() ? $('input[name=\'rating\']:checked').val() : '') + 
'&captcha=' + encodeURIComponent($('input[name=\'captcha\']').val());

	$.ajax({
		url: '/index.php?route=product/product/write&product_id=523',
		type: 'POST',
		dataType: 'json',
		data: $("#tab-review input[type=text], #tab-review input[type=radio]:checked, #tab-review textarea"),
		beforeSend: function() {
			$('.success, .warning').remove();
			$('#button-review').attr('disabled', true);
			$('#button-review').after('<div class="attention"><br/><img src="catalog/view/theme/default/image/loading.gif" alt="" /> Пожалуйста, подождите!</div>');
		},
		complete: function() {
			$('#button-review').attr('disabled', false);
			$('.attention').remove();
		},
		success: function(data) {

			if (data['success']) {
				$('#button-review').after('<div class="success"><br/>' + data['success'] + '</div>');
				$('input[name=\'name\']').val('');
				$('textarea[name=\'text\']').val('');
				$('input[name=\'rating\']:checked').attr('checked', '');
				$('input[name=\'captcha\']').val('');
				$('input[name=\'email\']').val('');
				$('input[name=\'city\']').val('');
			}
			if (data['error']) {
				$('#button-review').after('<div class="warning"><br/>' + data['error'] + '</div>');
			}

		}
	});
});
