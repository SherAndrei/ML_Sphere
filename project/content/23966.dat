<script type="text/javascript">
    $(document).ready(function() {
        fbCookieName = 'fbDoPopup';
        cookieExp = new Date();
        cookieExp.setMonth(cookieExp.getMonth() + 12);
//        cookieDomain = '.'+location.host;
        cookieDomain = '.goodhouse.com.ua';
        currentFbCookie = getCookie(fbCookieName);
        if(!currentFbCookie) {
            setCookie(fbCookieName, 1, cookieExp, '/', cookieDomain);
        }

        $('#FbPopup [data-close]').bind('click', function() {
            $('#FbPopup').fadeOut();
            $('html').removeClass('htmlOverflow');
            return false;
        })
    })

    window.onscroll = function () {
        var scrolled = window.pageYOffset || document.documentElement.scrollTop;
        if (scrolled > 650) {
            showFbPopup();
            window.onscroll = null;
        }
    }

    function showFbPopup() {
        if(currentFbCookie == 1) {
            $('#FbPopup').fadeIn();
            $('html').addClass('htmlOverflow');
            setCookie(fbCookieName, 2, cookieExp, '/', cookieDomain);
        }
    }

    function setCookie(name, value, expires, path, domain, secure) {
        if (!name) return false;
        var str = name + '=' + (value ? encodeURIComponent(value) : '');

        if (expires) str += '; expires=' + expires.toUTCString();
        if (path)    str += '; path=' + path;
        if (domain)  str += '; domain=' + domain;
        if (secure)  str += '; secure';

        document.cookie = str;
        return true;
    }

    function getCookie(name) {
        var pattern = "(?:; )?" + name + "=([^;]*);?";
        var regexp  = new RegExp(pattern);

        if (regexp.test(document.cookie)){

            return decodeURIComponent(RegExp["$1"]);
        }
        return false;
    }

    function deleteCookie(name, path, domain)
    {
        return setCookie(name, null, new Date(0), path, domain);
    }
