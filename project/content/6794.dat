	}

	var jTooltip = $('.js-headline_tooltip_profile');
	var oAvatar = menuItems.toolbar.items[1].itemsByName.avatar;

	// Вешаем на пункт бейдж "new". Скользкий момент, нужно следить за порядком элементов.
