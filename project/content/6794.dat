	// var oAventDropdownItem = oToolbarDropdown.find(':nth-child(11)');

	// oBreastDropdownItem.find('.js-text').append(newBadge);
	// oAventDropdownItem.find('.js-text').append(newBadge);

	var jAvatar = $(oAvatar.el);

	if (window.user && !window.user.hasProfile) {
		jAvatar.append(jTooltip);

		oAvatar.on('mouseenter', function() {
			jTooltip.addClass('headline-tooltip_show');
		});
		oAvatar.on('mouseleave', function() {
			jTooltip.removeClass('headline-tooltip_show');
		});

		if (!$.cookie('pftip')) {
			setTimeout(function(){
				jTooltip.addClass('headline-tooltip_show');
			}, 1000);
		}

		jAvatar.on('click', '.js-headline_tooltip_profile_close', function() {
			$.cookie('pftip', 'hide', {expires: 7, domain: document.domain, path: '/'});
			jTooltip.removeClass('headline-tooltip_show');
		});

		return;
	}

	var jDropdown = jAvatar.find('.js-dropdown');
	var jForum = jDropdown.find('.js-item:eq(0) .js-link');
	var jClub = jDropdown.find('.js-item:eq(1) .js-link');
	var sForumCount = userProfileDropdown.items[0].numbers;
	var sClubCount = userProfileDropdown.items[1].numbers;
	var tmplCount = function(count) {
		return count ? [
			'<span class="headline-dropdown__count">',
				'<span class="pm-toolbar__button__numbers pm-toolbar__button__numbers_visible pm-toolbar__button__numbers_noicon">',
					count,
				'</span>',
			'</span>'
		].join('') : '';
	};

	jDropdown.addClass('headline-drop');
	jForum.prepend(tmplCount(sForumCount));
	jClub.prepend(tmplCount(sClubCount));
});

