            <img src="[img]">
        </div>
        <div class="txt">
            <h5>[title]</h5>
            <span>[qty] шт</span>
            <b>[sum]</b>
        </div>
        <span class="del remove-product"></span>
    </li>
</script>
<script type="text/template" id="product-template-expired">
    <li data-id='[id]' class='product'>
        <div class="img-box fl">
            <img src="[img]">
        </div>
        <div class="txt">
            <h5>[title]</h5>
            <span>[qty] шт</span>
            <strike>[sum_expired]</strike><b>[sum]</b>
