<script type="text/javascript">
/*<![CDATA[*/
jQuery(function($) {

var block=$('#sidebanner'),
    next=block.next(),
    outerHeight=block.outerHeight(),
    parentOuterHeight=block.parent().outerHeight(),
    offset=block.offset(),
    jump=false;

$(window).scroll(function() {
	if(!next.size()) {
		if($(window).scrollTop()>offset.top) {
			if(!jump) {
				block.css('top',0);
				block.css('position','fixed');
				jump=true;
			}
		} else {
			if(jump) {
				block.css('position','static');
				jump=false;
			}
		}
	} else {
		if($(window).scrollTop()>offset.top+parentOuterHeight) {
			if(!jump) {
				next.css('margin-top',outerHeight);
				block.css('top',0);
				block.css('position','fixed');
				jump=true;
			}
		} else {
			if(jump) {
				next.css('margin-top',0);
				block.css('position','static');
				jump=false;
			}
		}
	}
});

});
/*]]>*/
</script>
