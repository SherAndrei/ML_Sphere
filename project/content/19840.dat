	<script>
	$(function () {
			
		$(".tslider").shamanslider(); $('.imagelightbox a').imageLightbox();
		notify = $('.notify').miniNotification();

		auth = $('.auth').authorizzila({
				'trigger': 'authorizzila',
				'reciveCallback': function(data) {
					
					$.post( 
						'/api/login'
						,data
						,function( data ) {	
							if (data.status == 'success'){								
								$("img[data-user='avatar']").attr("src",data.data['avatar']);
								$("[data-user='name']").text(data.data['name']);
								$("[data-user='date']").text(new Date().toJSON().slice(0,10));
								$('.user').removeClass('hide');
								
								auth.api('off');
								auth.api('close');
								notify.api(data.status, 'Вы успешно авторизованы. Добро пожаловать!');
							} else {
								auth.api(data.status, data.data);
							}						
						}
						,"json"
					);
								
				}
			});


		$('.auth').on('click', function(e) {
			e.preventDefault();
			$(this).trigger( "authorizzila" );
		});
		
		//add comment
		$('.contentcomment').on('click', function(e) {
			e.preventDefault();
						
			$.post( 
				'/api/contentcomment'
				,{'entity': $('body').data('id'), 'reply': $('.block_leave_comment_1').data('reply'), 'comment': $('.block_leave_comment_1 textarea').val()}
				,function( data ) {	
					if (data.status == 'success'){								
						notify.api(data.status, data.data);
						$("#commentform")[0].reset();
					} else {
						notify.api(data.status, data.data);
					}						
				}
				,"json"
			);
		});
		
		//reply on comment
		$('.reply').on('click', function(e) {
			replyto = $(this).closest('.comment');
			
			e.preventDefault();			
			$('.block_leave_comment_1')
				.insertAfter(replyto)
				.data('reply', replyto.data('id'));

		});
		
		//rating
		raitingCookies = $('body').data('type') + $('body').data('id') + 'rating';
				
		$('#star-raiting').barrating({ 
			readonly: Cookies.get(raitingCookies) ? true : false,
			showSelectedRating: false,
			initialRating: Math.round($('div[itemprop="ratingValue"]').text()), 
			onSelect: function(value, text) {
				
				if (!Cookies.get(raitingCookies)) 
				{
					
					$.post( 
						'/api/saveContentRating'
						,{'id':$('body').data('id'), 'rating':value}
						,function( data ) {	
							if (data.status == 'success'){								
								Cookies.set(raitingCookies, value);
								$('div[itemprop="ratingValue"]').addClass('glow').text(data.data['value']);
								$('div[itemprop="reviewCount"]').addClass('glow').text(data.data['count']);
								$(".glow").one("transitionend webkitTransitionEnd oTransitionEnd MSTransitionEnd", function(){
									$(this).removeClass('glow');				
								});
								$('#star-raiting').barrating('readonly');
							}						
						}
						,"json"
					)
					
				};
								
			}
		});
		
		//Content voiting
		$('ul.vote').on('click', '.dovote', function(e) {
			e.preventDefault();			
			voteCookies = $('body').data('type') + $('body').data('id') + 'vote';
			
			vote =  $(this).closest('ul').find('input:checked');
			
			if (vote.length !== 0 && !Cookies.get(voteCookies)) {
			
				$.post( 
					'/api/saveContentVote'
					,{'id':$('body').data('id'), 'vote':vote.attr('id')}
					,function( data ) {	
						if (data.status == 'success'){							
							Cookies.set(voteCookies, vote.attr('id'));
							var total = 0;for (var p in data.data) total += data.data[p];
							
							$(".vote input").each(function() {
								percent = data.data[$(this).attr('id')] ? Math.floor((data.data[$(this).attr('id')] / total) * 100) + '%' : '1%';
								progress = $('<span/>', { class: 'progress'});
								
								$(this).closest('li')
									.append( progress );
								
								progress.css('width'); progress.css('width', percent);
							});
							
							notify.api('success', 'Поздравляем, вы успешно проголосовали и ваш голос принят!');
						} else {
							notify.api(data.status, data.data);
						}						
					}
					,"json"
				)
			
			} else {
				notify.api('warning', 'Ошибка глосования. Возможно вы не выбрали ни одного варианта или пытаетесь голосовать повтороно.');
			}
		});
		
		//Yandex Share
		new Ya.share({
			element: 'ya_share',
				elementStyle: {
					'type': 'none',
					'border': false,
					'quickServices': ['vkontakte', 'facebook', 'twitter', 'odnoklassniki', 'gplus']
				},
				onshare: function(e){
					$.post( 
						'/api/addLike'
						,{'id': $('body').data('id'), 'entity': $('body').data('type')}
						,function( data ) {	
							if (data.status !== 'success'){								
								console.log(data);
							}						
						}
						,"json"
					);
				} 
		});
		
	});
	</script>
