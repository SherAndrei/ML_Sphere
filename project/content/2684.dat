<script>
$(window).load(function(){
    function loadLastNews(){
        var itemsGroup = $("aside.right .items"),
            containers = $("aside.right .items"),
            itemsIteration = 1;

        // Перечисляем все контейнеры
        containers.each(function(eq, el){
            // Определяем объект контейнера
            var container = {
                body: $(el),
                father: $(el).parent(),
                brothers: $(el).parent().find(".block_title, .banner"),
                height: $(el).parents(".page:first").find("section.left").height(),
                removeLoader: function(){
                    var loader = this.body.find(".items64loader");
                    if(loader.length > 0) loader.remove();
                },
                getOuterHeight: function(item){
                    var outerHeight, node = $(item);
                    if(node.css('display') != 'none'){
                        outerHeight = node.outerHeight(true);
                    }else{
                        node.css('display','block');
                        outerHeight = node.outerHeight(true);
                        node.hide();
                    }
                    return outerHeight;
                },
                loadItems: function(itemsGroup){
                    var container = this;
                    $(itemsGroup).find(".item:hidden").each(function(i, item){
                        var it = {
                            body: $(item),
                            height: container.getOuterHeight( $(item) )
                        };
                        if(container.height >= it.height){
                            it.body.show();
                            container.height -= it.height;
                        }
                    });
                    // Проверяем есть ли необходимость подгрузить еще новости
                    if(itemsGroup.find(".item:hidden").length <=0 && container.height >= 500){
                        container.duplicateItems(itemsGroup);
                    }
                },
                duplicateItems: function(){
                    var container = this;
                    var clones = containers.find(".item").clone().hide().appendTo( itemsGroup );
                    if(clones.length > 0) container.loadItems(itemsGroup);
                }
            };

            // Высчитываем максимальную высоту контейнера без его братьев
            container.brothers.each(function(i, brother){
                container.height -= $(brother).outerHeight(true);
            });

            // Высчитываем отступы
            container.height -= parseInt(container.father.css("margin-bottom"),10);

            // Подгружаем элементы в свободные колонки
            container.loadItems(itemsGroup);
        });

        // Убираем лишние заголовки
        containers.each(function(eq, el){
            if($(el).find(".item:visible").not(".fixer").length <= 0){
                $(el).find(".block_title").hide();
            }
        });
    }

    loadLastNews();
});

</script>
