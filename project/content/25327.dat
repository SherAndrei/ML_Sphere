<script type="text/javascript">
$(document).ready(function () {

// center the tweets
$('.twitter-tweet').addClass('tw-align-center');

var ShowFixedPopup = false;
var ShowFixedSocialButtons = true;
var SocialNet = '';

    if( /vk\.com/i.test(document.referrer) ) {
	SocialNet = 'vkontakte';
    }

    if( /odnoklassniki\.ru|ok\.ru|m\.ok\.ru/i.test(document.referrer) ) {
	SocialNet = 'odnoklassniki';
    }

    if( /plus\.url\.google\.com/i.test(document.referrer) ) {
	SocialNet = 'googleplus';
    }

    if ($.cookie('no-fixed-popup')) {
	// we don't need popup
	ShowFixedPopup = false;
	//
    }

    if ($.cookie('no-fixed-social-buttons')) {
	// we don't need popup
	ShowFixedSocialButtons = false;
	//
    }

	$(window).scroll(function() {
		var y = $(this).scrollTop();
		if (y > 2600) {
			$('#GoTopX').fadeIn(100,function(){
				$('#GoTopX').removeClass('notopbX');
				$('#GoTopX').addClass('topbX');
			});

		} else{
			$('#GoTopX').fadeOut(100,function(){
				$('#GoTopX').removeClass('topbX');
				$('#GoTopX').addClass('notopbX');
			});
		}

            if (!$.cookie('hide-ads') && (document.body.clientWidth < 769) && !(/rambler.ru|m.rambler.ru|brain.rambler.ru/i.test(document.referrer))) {
		if (y > 2500) {
			$('.mobile_footer').fadeIn(300,function(){
				$('.mobile_footer').show();
			});
		} else{
			$('.mobile_footer').fadeOut(300,function(){
				$('.mobile_footer').hide();
			});
		}
            }


		if ( ShowFixedSocialButtons && ( window.innerWidth > 800 ) ) {
			( y > 4000 ) ? $(".fixed-socialbuttons").animate({right: "50px"}, 500) : $(".fixed-socialbuttons").stop(!0).animate({right: "-800px"}, 300);
		}

		if ( ShowFixedPopup ) {
			var a = $("._endofpost").offset().top;
			(a < y + $(window).outerHeight()) ? $(".fixed-popup."+SocialNet).animate({right: "15px"}, 500) : 
							    $(".fixed-popup."+SocialNet).stop(!0).animate({right: "-350px"}, 300);
		}


	});
});

$('#GoTopX').each(function(){
      $(this).click(function(){ 
          $('html,body').animate({ scrollTop: 0 }, 'slow');
          return false; 
      });
});

var isSafari = Object.prototype.toString.call(window.HTMLElement).indexOf('Constructor') > 0; // At least Safari 3+: "[object HTMLElementConstructor]"

$('#bigpicture-webpush-closer, #bigpicture-webpush-block-button').click(function(){
	$.cookie('GCMPush', -1, {expires: 30, path: '/'});
	$('#bigpicture-webpush-widget').hide();
});

$('#bigpicture-webpush-allow-button').click(function(){
	// $.cookie('GCMPush', -1, {expires: 30, path: '/'});
	newwindow=window.open('https://subscribe.bigpicture.ru/gcm/subscribe.html','Уведомления Bigpicture.ru','height=400,width=560');
	if (window.focus) 
		newwindow.focus();
	$('#bigpicture-webpush-widget').hide();
});

if ( !$.cookie('GCMPush') && !isSafari ) {
	$('#bigpicture-webpush-widget').show();
	// $(".web-push").show( 200 );
}

$(".fixed-popup .close-box,.fixed-popup .hide-box").on("click", function(b) {
    b.preventDefault();
    $.cookie('no-fixed-popup', true, { expires: 90, path: '/'  });
    $(".fixed-popup").hide();
});

$(".fixed-socialbuttons .close-box").on("click", function(b) {
    b.preventDefault();
    $.cookie('no-fixed-social-buttons', true, { expires: 30, path: '/'  });
    $(".fixed-socialbuttons").hide();
});

$(".js-close").click(function(){
    // hide ADs in footer
    $(".mobile_footer").slideUp();
    $.cookie('hide-ads', true, { expires: 1, path: '/'  });
    return false;
});

(function($) {
    $(function() {
        var container = $('.article-content');

        if (!container.length) {
            return;
        }

        var mobile = false;
        var bodyWidth = container.width();

        $('.js-before-after-slider').on('mousemove', function(e) {
            // get the mouse x (horizontal) position and offset of the div
            var offset = $(this).offset();
            var iTopWidth = (e.pageX - offset.left);

            // set width of bottomimage div
            $(this).find('.before').width(iTopWidth);
            $(this).find('.hint').remove();
            e.preventDefault();
        });

        $('.js-before-after-slider').on('touchmove', function(e) {
            e.preventDefault();
            var touch = e.originalEvent.touches[0] || e.originalEvent.changedTouches[0];
            var offset = $(this).offset();
            var iTopWidth = (touch.pageX - offset.left);

            // set width of bottomimage div
            $(this).find('.before').width(iTopWidth);
            $(this).find('.hint').remove();
        });

        var beforeAfterInit = function() {
            $('.js-before-after-slider').each(function() {
                var img = $(this).find('img');
                var imgWidth = mobile ? bodyWidth + 'px' : img.attr('width') + 'px';
                var imgHeight = calcHeight(img.attr('width'), img.attr('height')) + 'px';
                $(this).css('width', imgWidth);
                $(this).css('height', imgHeight);
                img.attr('style', 'width: ' + imgWidth + '!important');

                $(this).find('.before').css('width', '50%');

                $(this).find('.hint').remove();
                $(this).append('<div class="hint">← СКОЛЬЗИ →</div>');
                $(this).find('.hint').css('top', Math.round(parseInt(imgHeight) / 2 - 20) + 'px').css('opacity', 0.9);
            });
        };

        beforeAfterInit();

        if (mobile) {
            $(window).on('resize', function(e) {
                bodyWidth = container.width();
                beforeAfterInit();
            });
        }

        function calcHeight(width, height) {
            if (mobile) {
                var aspect = width / height;
                return Math.round(bodyWidth / aspect);
            }
            return height;
        }
    });
})(jQuery);

</script>

