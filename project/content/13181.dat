	<article class="post post-3938 type-post has-post-thumbnail clearfix" itemscope itemtype="http://schema.org/Article">

		        <h1 class="single_title" itemprop="headline">Маски для лица из киви</h1>
        
				<aside class="entry-meta post-meta"><span class="date">Август 30 2013 в 13:46</span>
			<span class="category">Рубрика: <a href="http://galinanekrasova.ru/category/krasota" rel="category tag">КРАСОТА</a></span>
			<span class="views">Просмотры: 1256</span></aside>
		
        <div class="entry clearfix" itemprop='articleBody'>
						<div class="sp-before-content"></div>
<!-- Quick Adsense WordPress Plugin: http://quicksense.net/ -->
<div style="float:none;margin:10px 0 10px 0;text-align:center;">
<script async src="//pagead2.googlesyndication.com/pagead/js/adsbygoogle.js"></script>
<!-- Блок 4 -->
<ins class="adsbygoogle"
     style="display:inline-block;width:580px;height:400px"
     data-ad-client="ca-pub-7500245730314739"
     data-ad-slot="9056275607"></ins>
<script>
