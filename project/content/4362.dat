                        for(var i in calculated){
                            if(i == 'indexOf')
                                continue;

                            if(calculated[i][0]<calculated[shortest][0])
                                shortest = i;
                            if(calculated[i][1]<calculated[quickest][1])
                                quickest = i;
                        }
                        for(var i in calculated){
                            if(i == 'indexOf')
                                continue;

                            var name = '';
                            if(i == quickest){
                                name = 'Быстрый';
                            }
                            if(i == shortest){
                                var t = 'Короткий';
                                if(name != ''){
                                    name += ', ';
                                    t = 'короткий';
                                }
                                name += t;
                            }
                            if(name != ''){
                                $('#routesVariantControl_'+i + ' .routesVariantButton').html(name);
                            }

                        }

                        $.each($('.routesVariantControl'), function( index, value ) {
                            $(value).width($(value).width()+20);
                        });




                    } else alert(status);
                }

                if(!is_bot || (show_map_for_bot && flagSaveRoute) )
                    initialize();

