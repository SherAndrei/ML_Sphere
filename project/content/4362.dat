            <script type="text/javascript">

                                //sec to time
                function sec2time(sec){
                    dt = new Date();
                    dt.setTime(sec*1000);

                    if(dt.getUTCHours() == 0)
                        return dt.getUTCMinutes()+" мин.";
                    else
                        return dt.getUTCHours()+" ч. "+dt.getUTCMinutes()+" мин.";
                }

                function sec2km(metr){
                    var tmp = metr/1000;
                    return tmp.toFixed(1)+' км.';
                }

                var createCookie = function(name, value, days) {
                    var expires;
                    if (days) {
                        var date = new Date();
                        date.setTime(date.getTime() + (days * 24 * 60 * 60 * 1000));
                        expires = "; expires=" + date.toGMTString();
                    }
                    else {
                        expires = "";
                    }
                    document.cookie = name + "=" + value + expires + "; path=/";
                }

                function getCookie(c_name) {
                    if (document.cookie.length > 0) {
                        c_start = document.cookie.indexOf(c_name + "=");
                        if (c_start != -1) {
                            c_start = c_start + c_name.length + 1;
                            c_end = document.cookie.indexOf(";", c_start);
                            if (c_end == -1) {
                                c_end = document.cookie.length;
                            }
                            return unescape(document.cookie.substring(c_start, c_end));
                        }
                    }
                    return "";
                }

                function getRouteCode() {
                    var pageHref=location;
                    //преобразуем в строку
                    var stroka =pageHref.toString();
                    //ищем индекс символа совпадения "route="
                    // вырезаем всё после этого индекса
                    return stroka.slice(stroka.search(/route=/)+6);
                }

                var flagSaveRoute = true;
                var responseSql = false;
                var show_map_for_bot = false;
                var is_bot = true;

                if(!is_bot || (show_map_for_bot && flagSaveRoute)) {
                                        var directionsDisplay = new google.maps.DirectionsRenderer();
                    var directionsService = new google.maps.DirectionsService();
                    var map;
                }

                var controls = null;
                var selectedRoute = 0;

                function initialize() {

                    if(getCookie('Ferries') != ''){
                        $('#Ferries').attr('checked', getCookie('Ferries') == 'true');
                    }else{
                        $('#Ferries').attr('checked', true);
                    }

                    if(getCookie('DirtRoads') != ''){
                        $('#DirtRoads').attr('checked', getCookie('DirtRoads') == 'true');
                    }else{
                        $('#DirtRoads').attr('checked', true);
                    }
                    if(getCookie('transport_type') != ''){
                        $('#transport_type_'+getCookie('transport_type')).attr('checked', true);
                    }else{
                        $('#transport_type_l').attr('checked', true);
                    }

                    var rendererOptions = {
                        polylineOptions: {
                            strokeOpacity: 0.7,
                            strokeColor: '#00882D',
                            strokeWeight:5
                        },
                        hideRouteList: false
                    };
                    directionsDisplay = new google.maps.DirectionsRenderer(rendererOptions);
                    var myOptions = {
                        mapTypeId: google.maps.MapTypeId.ROADMAP,
                        zoom:1,
                        center: new google.maps.LatLng(0, 0)
                    };
                    map = new google.maps.Map(document.getElementById("map_canvas"), myOptions);
                    directionsDisplay.setMap(map);

                    map.controls[google.maps.ControlPosition.TOP_RIGHT].push(
                        FullScreenControl(map, 'Открыть в полном размере',
                            'Вернуться к сайту'));

                    calcRoute();
                }

                function calcRoute() {

                    createCookie('Ferries', $('#Ferries').attr('checked'));
                    createCookie('DirtRoads', $('#DirtRoads').attr('checked'));
                    createCookie('transport_type',  $('[name=transport_type]:checked').val());

                    var start = document.getElementById("origin").value;
                    var end = document.getElementById("destination").value;

                                        var waypts = [  ];

                    var request = {
                        origin: start,
                        destination: end,
                        waypoints: waypts,
                        optimizeWaypoints: true,
                        travelMode: google.maps.DirectionsTravelMode.DRIVING,
                        provideRouteAlternatives: true,
                        avoidFerries: !$('#Ferries').attr('checked')
                    };

                    var DirtRoads = getCookie('DirtRoads');
                    var Ferries = getCookie('Ferries');

                    directionsService.route(request, function(responsee, status) {

                        if(status == 'OK'){

                            var str = JSON.stringify(responsee);
                            if(flagSaveRoute) {
                                console.log('start save road');

                                $.ajax({
                                    url: '/ajax.index.php',
//                                    async: false,
                                    type: "post",
                                    data: {
                                        action: 'parseroute',
                                        flag: 'saveroute',
                                        route: getRouteCode(),
                                       // fast_route: str,
                                       // short_route: str,
                                        val_route: str,
                                        ferries: Ferries,
                                        dirtroads: DirtRoads
                                    },
                                    success: function (data) {
                                        console.log(data);
                                    }
                                });
                            }

                            responseSql = JSON.parse(str);
//                            responseSql = responsee;
                            mainmap(responseSql, status);
                        }
                        else alert(status);

                    });

                }

                function mainmap(response, status) {

                    var counterItemStep = 0;

                    var start = document.getElementById("origin").value;
                    var end = document.getElementById("destination").value;

                    var data = {};

                                        var waypts = [  ];

                    var request = {
                        origin: start,
                        destination: end,
                        waypoints: waypts,
                        optimizeWaypoints: true,
                        travelMode: google.maps.DirectionsTravelMode.DRIVING,
                        provideRouteAlternatives: true,
                        avoidFerries: !$('#Ferries').attr('checked')
                    };
                    var infodriving_arr = [];
                    var calculated = [];

                    var geocoder = new google.maps.Geocoder;
                    var infowindow = new google.maps.InfoWindow;

                    jQuery(".step").live('click', function(e){
                        var lat = $(this).attr('data-lat');
                        var lng = $(this).attr('data-lng');
                        var id = $(this).attr('id');
                        geocodeLatLng(geocoder,map,infowindow,lat+','+lng, id);

                        var elementClick = $("#map");
                        var destination = $(elementClick).offset().top - 150;
                        if ($.browser.safari) {
                            $('body').animate({ scrollTop: destination }, 1100); //1100 - скорость
                        } else {
                            $('html').animate({ scrollTop: destination }, 1100);
                        }
                    });

                    jQuery(".stepmap").live('click', function(e){
                        e.preventDefault();
                        var step = $(this).attr('href');
                        var id = $('#'+step).attr('id');
                        var lat = $('#'+step).attr('data-lat');
                        var lng = $('#'+step).attr('data-lng');
                        geocodeLatLng(geocoder,map,infowindow,lat+','+lng, id);
                    });

                    var calculateRoute = function(route, index){

                        distance = 0;
                        duration = 0;

                        infodriving_arr[index] = '';
                        var start_km = 0;
                        var start_time='0';

                        var item = 0;
                        for (var i = 0; i < route.legs.length; i++) {
                            var routeSegment = i + 1;
                            distance = distance + route.legs[i].distance.value;
                            duration = duration + route.legs[i].duration.value;

                            for( var j=0; j<route.legs[i].steps.length; j++){

                                //km from start point
                                var tmp_km = route.legs[i].steps[j].distance.value;
                                start_km = parseInt(start_km) + parseInt(tmp_km);

                                //time from start
                                var tmp_time = route.legs[i].steps[j].duration.value;
                                start_time = parseInt(start_time) + parseInt(tmp_time);

                                var temp = sec2time(start_time);

                                //counterItemStep
                                var color = 'background-color: #E7F1F4;';
                                if(item % 2 == 0){
                                    color = 'background-color: #F9FCFD;'
                                }
                                infodriving_arr[index] = infodriving_arr[index]
                                    + '<div style="clear: both;'+color+'" class="step" id="'+counterItemStep+'"'
                                    + 'data-lat="'+route.legs[i].steps[j].start_location.lat
                                    +'" data-lng="'+route.legs[i].steps[j].start_location.lng+'">'
                                    + '<div style="float: left;max-width: 710px;" class="textinfo">'
                                    //+route.legs[i].steps[j].maneuver+ ' |'
                                    + route.legs[i].steps[j].instructions + '</div>'
                                    //+ route.legs[i].steps[j].adress_[0].formatted_address + '</div>'
                                    + '<div style="float: right;min-width: 130px;text-align: center;" class="distan">' + route.legs[i].steps[j].distance.text + ' '+ sec2time(route.legs[i].steps[j].duration.value) + '</div>'
                                    + '<div style="float: right;min-width: 130px;text-align: center;" class="fromstart">'+ sec2km(start_km) + ' '+ temp +'</div>'
                                    + '</div>';

                                item++;
                                counterItemStep++
                            }
                        }

                        duration = duration/60;
                        if($('[name=transport_type]:checked').val() == 'g'){
                            duration*=1.20;
                        }
                        use = Math.round($("#use").val()/100 * (distance/1000));
                        price = use*$("#price").val();

                        return [distance, duration];
                    };

                    var geocodeLatLng = function (geocoder, map, infowindow, str, id) {
                        var input = str;
                        var latlngStr = input.split(',', 2);
                        var latlng = {lat: parseFloat(latlngStr[0]), lng: parseFloat(latlngStr[1])};
                        geocoder.geocode({'location': latlng}, function(results, status) {
                            if (status === google.maps.GeocoderStatus.OK) {

                                if (results[0]) {
                                    var text = results[0].formatted_address;
                                    var tmp = '<br><a class="stepmap" href="'+ (parseInt(id)+1)+'">вперед</a> <a  class="stepmap" href="'+(parseInt(id)-1)+'">назад</a>';
                                    infowindow.setContent(text+tmp);
                                    infowindow.setPosition(latlng);
                                    infowindow.open(map);
                                } else {
                                    window.alert('No results found');
                                }
                            } else {
                                window.alert('Geocoder failed due to: ' + status);
                            }
                        });
                    };

                    var drawRoute = function(routeIndex, click){

                        click = typeof click !== 'undefined' ?  click : false;

                        $('#infodrive').html( infodriving_arr[routeIndex] );


                        $.each($('.routesVariantControl'), function( index, value ) {
                            $(value).removeClass('selected');
                            $(value).removeClass('unselected');
                        });
                        $('#routesVariantControl_'+routeIndex).addClass('selected');
                        $('#routesVariantControl_'+routeIndex + ' .info2').hide();
                        $('#routesVariantControl_'+routeIndex + ' .info1').show();
                        $.each($('.routesVariantControl'), function( index, value ) {
                            if(!$(value).hasClass('selected'))
                                $(value).addClass('unselected');
                        });

                        selectedRoute = routeIndex;
                        directionsDisplay.setDirections(response);
                        directionsDisplay.setRouteIndex(routeIndex);
                        var route = response.routes[routeIndex];

                        distance = 0;
                        duration = 0;
                        for (var i = 0; i < route.legs.length; i++) {
                            var routeSegment = i + 1;
                            distance = distance + route.legs[i].distance.value;
                            duration = duration + route.legs[i].duration.value;
                            $("#rid-"+routeSegment).html( Math.round(route.legs[i].distance.value/1000) + ' км');
                        }
                        if($('[name=transport_type]:checked').val() == 'g'){
                            duration*=1.20;
                        }
                        $(".rdistance").html(Math.round(distance/1000) + ' км');
                        duration = duration/60;
                        $("#rduration").html(Math.floor(duration / 60) + ' ч. ' + Math.round(duration % 60) + ' мин.');
                        use = Math.round($("#use").val()/100 * (distance/1000));
                        $("#ruse").html(use);
                        price = use*$("#price").val();
                        $("#rprice").html(price);

                        if(click)
                            ff.setMap(null);
                    };

                    var ff = new Object();
                    var drawRoute2 = function(routeIndex){

                        var drive2 = [];
                        if(routeIndex == indexNowRoad)
                            ff.setMap(null);
                        else {

                            drive2 =  response.routes[routeIndex].overview_polyline;

                            drive22 = google.maps.geometry.encoding.decodePath(drive2);

                            flightPath = new google.maps.Polyline({
                                path: drive22,
                                strokeColor: '#00882D',
                                strokeOpacity: 0.5,
                                strokeWeight: 3
                            });
                            ff = flightPath;
                            flightPath.setMap(map);
                        }
                    };

                    if (status == 'OK') {

                        var routesVariantsControl = document.createElement('div');
                        // routesVariantsControl.style.width=''
                        if(controls != null){
                            $(controls).remove();
                        }
                        controls = routesVariantsControl;

//                        console.log(response);
                        for(var i = 0 ; i < response.routes.length; i++){
                            var calculatedRoute = calculateRoute(response.routes[i], i);
                            calculated.push(calculatedRoute);

                            var routesVariantControl = document.createElement('div');
                            routesVariantControl.className = 'routesVariantControl';
                            var routesVariantButton = document.createElement('div');
                            routesVariantButton.className = 'routesVariantButton';
                            var routesVariantInfo = document.createElement('div');
                            routesVariantInfo.className = 'routesVariantInfo';

                            routesVariantControl.setAttribute('id', 'routesVariantControl_'+i);

                            routesVariantControl.appendChild(routesVariantButton);
                            routesVariantControl.appendChild(routesVariantInfo);


                            routesVariantButton.innerHTML = response.routes[i].summary;
                            routesVariantInfo.innerHTML = '<span class="info1"><b>' + (Math.round(calculatedRoute[0]/1000) + ' км')+'</b><br/>'+(Math.floor(calculatedRoute[1] / 60) + ' ч. ' + Math.round(calculatedRoute[1] % 60) + ' мин.</span><span class="info2"></span>');



                            // routesVariantControl
                            var indexNowRoad = 0;
                            var flag_change = 0;
                            (function(i, routesVariantControl){

                                if (routesVariantControl.addEventListener) {
                                    routesVariantControl.addEventListener('click', function(){
                                        indexNowRoad = i;
                                        console.log(i);
                                        drawRoute(i, true);
                                    }, false);
                                } else if (routesVariantControl.attachEvent)  {
                                    routesVariantControl.attachEvent('onclick', function(){
                                        indexNowRoad = i;
                                        console.log(i);
                                        drawRoute(i, true);
                                    });
                                }

                                if (routesVariantControl.addEventListener) {
                                    routesVariantControl.addEventListener('mouseenter', function(){

                                        if(i != selectedRoute && flag_change != i){
                                            var diff = [(calculated[i][0]-calculated[selectedRoute][0]), (calculated[i][1]-calculated[selectedRoute][1])];
                                            $(routesVariantControl).find('.info2').html('<span style="font-weight: bold; pointer-events:none;">' + (diff[0]<0?'-':'+') + (Math.abs(Math.round(diff[0]/1000)) + ' км')+
                                                '</span><br/>'+ (diff[1]<0?'-':'+') +
                                                (Math.abs(diff[1])>59?(Math.abs(Math.floor(diff[1] / 60)) + ' ч. '):'') +
                                                Math.abs(Math.round(diff[1] % 60)) + ' мин.');
                                            $(routesVariantControl).find('.info1').hide();
                                            $(routesVariantControl).find('.info2').show();
                                            flag_change = i;
                                            drawRoute2(i);
                                        }
                                    }, false);
                                } else if (routesVariantControl.attachEvent)  {
                                    routesVariantControl.attachEvent('mouseover', function(){

                                        if(i != selectedRoute && flag_change != i){
                                            var diff = [(calculated[i][0]-calculated[selectedRoute][0]), (calculated[i][1]-calculated[selectedRoute][1])];
                                            $(routesVariantControl).find('.info2').html('<span style="font-weight: bold; pointer-events:none;">' + (diff[0]<0?'-':'+') + (Math.abs(Math.round(diff[0]/1000)) + ' км')+
                                                '</span><br/>'+ (diff[1]<0?'-':'+') +
                                                (Math.abs(diff[1])>59?(Math.abs(Math.floor(diff[1] / 60)) + ' ч. '):'') +
                                                Math.abs(Math.round(diff[1] % 60)) + ' мин.');
                                            $(routesVariantControl).find('.info1').hide();
                                            $(routesVariantControl).find('.info2').show();
                                            flag_change = i;
                                            drawRoute2(i);
                                        }
                                    });
                                }


                                if (routesVariantControl.addEventListener) {
                                    routesVariantControl.addEventListener('mouseleave', function(){
                                        $(routesVariantControl).find('.info2').hide();
                                        $(routesVariantControl).find('.info1').show();
                                        flag_change = indexNowRoad;
                                        drawRoute2(indexNowRoad);
                                    }, false);
                                } else if (routesVariantControl.attachEvent)  {
                                    routesVariantControl.attachEvent('mouseout', function(){
                                        $(routesVariantControl).find('.info2').hide();
                                        $(routesVariantControl).find('.info1').show();
                                        flag_change = indexNowRoad;
                                        drawRoute2(indexNowRoad);
                                    });
                                }

                            })(i, routesVariantControl);



                            routesVariantsControl.appendChild(routesVariantControl);



                        }




                        routesVariantsControl.index = 1;
                        // map.controls[google.maps.ControlPosition.TOP_CENTER].push(routesVariantsControl);
                        $('#variant').append(routesVariantsControl);

                        drawRoute(selectedRoute);



                        //analize and name
                        var shortest = 0;
