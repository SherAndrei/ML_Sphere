		checkCookieNameAndPath = function(name, path){
			try{
		    	cookie = JSON.parse($.cookie(name));
		    }
		    catch(e){
		    	cookie = false;
		    }
		    if (cookie && cookie.path == path){
		    	return true;
		    }
		    return false;
		}

       
        $("textarea#text").focus(function(){set_mank(1);});
        $("textarea#text").focusout(function(){set_mank(0);});

        if ( USER.is_blocked == false){
            if (checkCookieNameAndPath("comment", window.location.pathname)) {
                comment = JSON.parse($.cookie("comment"));
                $.cookie("comment", null);

                if (comment.data.reply_id_level > 0) {
                    /* находит нужный комментарий и открывает форму ответа */
                    comments.get_focus_to_new_comment(comment.data.reply_id);
                    $("#comment_"+comment.data.reply_id+" > footer > a").click();
                }

                window.setTimeout(
                    function(){
                        /*добавляет комментарий форму*/
                        if ($("textarea#text").length > 1) {
                            $("textarea#text").last().val(comment.data.text);
                        }
                        else {
                            $("textarea#text").val(comment.data.text);
                        }
                    },
                    1000
                );
            }
        }
    })
</script>


