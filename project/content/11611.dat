	.message.user_status {
		background: #f9f7e0;
		color: #6c6141;
		font-size: 1em;
		border: 1px solid #eadca0;
		padding: 5px 5px 5px 15px;
		margin-bottom: 10px;
	}
	
		.message.user_status.in_profile {
			font-size: 1.2em;
			position: relative;
			padding-left: 15px;
			overflow: auto;
		}
		
	.message.user_status #update_status {
		background: #243f5c;
		font-size: 0.8em;
		font-weight: bold;
		padding: 3px 8px;
		margin-left: 10px;
	}
	
	.message.user_status .cancel {
		font-size: 0.75em;
	}

        .message.success {
	        border: 1px solid #DDD;
        	margin-bottom: 1em;
        	padding: 10px 10px 10px 10px;
        }

        .success {background: #E6EFC2; color: #264409; border-color: #C6D880;
        }

        .success a {color: #264409;}
	
