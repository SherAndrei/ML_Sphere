/* Floats */
.right { float: right; }
.left { float: left; }

/* Visibility */
.hide { display: none; }

/* Bullets */
.bullets ul, .bullets ol,
ul.bullets, ol.bullets {
	list-style: disc;
	margin-left: 30px;
	line-height: 150%;
	list-style-image: none;
}

