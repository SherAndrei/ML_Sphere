body .maintitle {
	font-size: 17px;
	font-weight: normal;
	border-radius: 5px 5px 0 0;
	-moz-border-radius: 5px 5px 0 0;
	-webkit-border-top-left-radius: 5px;
	-webkit-border-top-right-radius: 5px;
	padding: 8px !important;
	margin: 0 !important;
	overflow: hidden;
}

/* Photo */
.photo {
	border: 1px solid #d8d8d8;
}

/* Moderation */
em.moderated {
	background: transparent;
	font-size: 0.8em;
	font-weight: bold;
	margin-top: 8px;
	display: block;
	clear: both;
}

