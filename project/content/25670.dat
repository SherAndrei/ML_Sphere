	var emailStat = 0;
	$(".login_r").focusout(function()
	{
		//alert("wd");
		var login = $(".login_r").val();
		var expLogin = /^[a-zA-Zа-яА-Я0-9_]+$/g;//[a-zа-я0-9]
		var resLogin = login.search(expLogin);
		if((resLogin == -1))
		{
			// alert("Неадекватный ник");
			loginStat = 0;
			buttonOnAndOff();
		}
		else
