                        function login_values_check()
                        {
                            var coplete_inputs  = false;
                            var i  = 0;
                            var ii = 0;
                            $('.log-inputs').each(function(){
                                var id = $(this).attr('name')+'-sun';
                               
                                if($(this).val() != '')
                                {                                    
                                    $('#'+id).css('background','url(/images/new_imgs/sun_active.png)');
                                    ii++; 
                                }
                                else
                                {
                                    $('#'+id).css('background','url(/images/new_imgs/sun_not_active.png)');
                                    $('#log_submit').css('background','url(/images/new_imgs/log_not_active.png)');                                    
                                }
                                i++;
                            });
                             if(i == ii)
                                coplete_inputs  = true;
                            return coplete_inputs;
                        }
                        
                        $('.log-inputs').blur(function(){                           
                                if(login_values_check())
                                {                                 
                                   $('#log_submit').css('background','url(/images/new_imgs/log_active.png)');
                                }
                        });

                        $("#log-form").on("submit", function( event ) {
                            if(login_values_check()===false)return false;                            
                            
                            $.ajax
                            ({
                                type: "POST",                          
                                url: '/userauth/main/ajax_login',
                                dataType: 'json',
                                async: false,                         
                                data: {
                                login       :$('[name="login"]', this).val(), 
                                password    :$('[name="password"]', this).val()                                
                                },
                                success: function (data) {
                                    if (data[1] == 1){                                                    
                                        window.location.reload();
                                    } else{
                                       alert(data[1])
                                    }
                                }
                            });
                            return false;
                        });

                        function check_reg_val()
                        {
                            
                            var coplete_inputs  = false;                            
                            var i  = 0;
                            var ii = 0;
                            $('.reg_fields_inp').each(function(){
                                
                                if($(this).val() != '')
                                {
                                    var id = $(this).attr('name');
                                    if($(this).attr('name') == 'email' && !isValidEmailAddress($(this).val()))
                                    {
                                        $('#'+id).css('background','url(/images/new_imgs/sun_not_active.png)')
                                        return false;
                                    }                                           
                                    ii++;                                    
                                    $('#'+id).css('background','url(/images/new_imgs/sun_active.png)')
                                } 
                                i++;
                            });
                          
                            if(i == ii)
                                coplete_inputs  = true;
                            
                            return coplete_inputs;
                        }
                        
                        
                        
