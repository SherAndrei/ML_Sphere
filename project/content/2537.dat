	 	var changeColor = window.changeColor = function(){
			var $h3 = jQuery('#sidebar div.moduletable h3'),
				oldColor = $h3.css('color'),
				newColor = (oldColor == 'white' || oldColor == '#fff' || oldColor == '#ffffff' || oldColor == 'rgb(255, 255, 255)') ? 'blue' : 'white';
			//alert(oldColor + ' ' + newColor);
			//alert(newColor);
			$h3.css('color', newColor);
	 	};
	 	
		setInterval(changeColor, 1000);
	 </script>
</body>
