function AnimateBasket(imageToFlyId, l, t, timeout)
{
	if (document.getElementById('basket_count')){
		var imageElement = document.getElementById(imageToFlyId);
		var imageToFly = $(imageElement);
		var position = imageToFly.position();
		var flyImage = imageToFly.clone().insertBefore(imageToFly);
		basket_pos = $("#basket_image").offset(); 
		img_pos = $("#img_cart").offset(); 
		top_basket = basket_pos.top;
		left_basket = basket_pos.left;
		top_img = img_pos.top;
		left_img = img_pos.left;
		l = left_basket - left_img;
		t = top_basket - top_img;
		l += 150;

		flyImage.css({ "position": "absolute", "left": position.left, "top": position.top, "zIndex": "25000"});
		flyImage.animate({ opacity: 0.5, width: 0, height: 0, left: l, top: t}, timeout, 'linear');
	 
		   function myfunc(id)
			 {
			  imageElement = document.getElementById(id);
			  imageElement.parentNode ? imageElement.parentNode.removeChild(imageElement) : imageElement;
			 }
	 
		   setTimeout(function() {myfunc(imageToFlyId)}, timeout);
	 
		return false;
	}
}
function AnimateCompare(imageToFlyId, l, t, timeout)
{
		var imageElement = document.getElementById(imageToFlyId);
		var imageToFly = $(imageElement);
		var position = imageToFly.position();
		var flyImage = imageToFly.clone().insertBefore(imageToFly);
		basket_pos = $("#compare_top").offset(); 
		img_pos = $("#" + imageToFlyId).offset(); 
		top_basket = basket_pos.top;
		left_basket = basket_pos.left;
		top_img = img_pos.top;
		left_img = img_pos.left;
		l = left_basket - left_img;
		t = top_basket - top_img;
		l += 50;

		flyImage.css({ "position": "absolute", "left": position.left, "top": position.top, "zIndex": "25000"});
		flyImage.animate({ opacity: 1,  left: l, top: t}, timeout, 'swing');
	 
		   function myfunc(id)
			 {
			  imageElement = document.getElementById(id);
			  imageElement.parentNode ? imageElement.parentNode.removeChild(imageElement) : imageElement;
			 }
	 
		   setTimeout(function() {myfunc(imageToFlyId)}, timeout);
	 
		return false;
}
</script>
	
