function ValidateNewsComment(form){
		var text = FCKeditorAPI.GetInstance("message").GetXHTML();
		if(text.length == 0){
                alert("Напишите комментарий");
                return false;
        } else{
                SendNewsComment(form,text);
                FCKeditorAPI.GetInstance("message").SetHTML("");
                return false;
        }
}
function SendNewsComment(form,text){
        var post="";
        post += "message=" + encodeURIComponent(text) + "&";
        xmlhttp.open("POST", "/n_news.php?action=putcomment&id="+form.id.value, true);
        xmlhttp.setRequestHeader("Cache-Control", "no-store, no-cache, must-revalidate");
        xmlhttp.setRequestHeader("Content-Type", "application/x-www-form-urlencoded");
        xmlhttp.onreadystatechange = putComment;
        xmlhttp.send(post);
