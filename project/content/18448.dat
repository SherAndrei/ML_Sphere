        <div class="post_date"><a  class="post_link"  href="/wall-70923364_23525"  onclick="return showWiki({w: 'wall-70923364_23525'}, false, event);" ><span class="rel_date">26 янв 2015</span></a><a onmouseover="wall.postTooltip(this, '-70923364_23525', {i: '-1', dark: 1, className: ' post_source_tt'})" href="/app3882244" class="wall_post_source_icon wall_post_source_default"></a></div>
        <div class="ui_actions_menu_wrap _ui_menu_wrap " onmouseover="uiActionsMenu.show(this);" onmouseout="uiActionsMenu.hide(this);">
