<script>

    var foto_offer_path = [];

    $(function() {
        if (currentUser != null && currentUser.is_logged) {
            $(".add_m_comment .submit_block .non_auth").hide();
            $(".add_m_comment .submit_block .auth").show();
            $('.add_m_comment .check_label').show();
        }
        else {
            $('.add_m_comment .check_label, .add_m_comment .add_mat_form').hide();
            $(".add_m_comment .submit_block .auth").show();
            $(".add_m_comment .submit_block .non_auth").show();
        }

        $('#offer_check').on("click", function() {
            if ($(this).is(':checked')) {
                $('.offer_auth').show();
            } else {
                $('.offer_auth').hide();
            }
        });

        $('.ajx-submit-button-offer').click(function() {
            if (currentUser != null && currentUser.is_logged) {
                var offer_file_data = $('.offer_file_data').serializeArray();
                var offer_file_titles = $('.offer_file_title').serializeArray();
                var offer_filenames = $('.offer_filename').serializeArray();
                var tab_id = $('#m_tab_1').hasClass('tabswitcher_on2') ? 1 : 2; // Смотрим номер таба
                $.post('/form/offer', {
                    text: $('#offer_text').val(),
                    files: offer_file_data,
                    titles: offer_file_titles,
                    site_user_id : currentUser.id,
                    tab_id : tab_id,
	                filenames: offer_filenames
                }, function(data) {
                    if (data.result == 'ok') {
                        $('#offer_text').val('');
                        $('.offer-mess').empty().append('Материал принят');
                        $('.offer-mess-err').empty();
                        $('#offer_file_block').empty();
                        $('.char_counter').empty();
                        $('#Offer_file_load_url').val('');
                        $('#offer_text').removeClass('error_input');
	                    $('#nofile').show();
                    }  else {
                        var error_msg = [];
                   
                        $('#offer_text').addClass('error_input');
                        if (data.user) {
                            error_msg.push(data.user);
                        }
                        if (data.text) {
                            error_msg.push(data.text);
                        }
                        if (data.files) {
                            error_msg.push(data.files);
                        }

                        $('.offer-mess-err').empty().append(error_msg);
                        $('.offer-mess').empty();
                    }
                });
            } else {
				authPanel.showPanel('login');
            }
        });

        $("#Offer_file_load").uploadifive({
            'buttonText': 'Выберите файл',
            'uploadScript': '/form/offerfile',
            'progressData': 'speed',
            'multi': false,
            'onUploadComplete': function(file, data) {
                uploadFileOffer(data);
                clean_message_offer_form();
            }
        });

        $('#Offer_file_load_bt').click(function() {
            var file_url = $('#Offer_file_load_url').val();
            if (file_url != '') {
                $.post('/form/offerfileurl', {path: file_url}, function(data) {
                    uploadFileOffer(data);
                });
            }
        });


        $('.add_m_comment #offer_text, #Offer_file_load_url').click(function() {
            clean_message_offer_form();
        });

    });

    function clean_message_offer_form() {
        $('.add_m_comment .offer-mess-err, .add_m_comment .offer-mess').empty();
        $('.add_m_comment #offer_text').removeClass('error_input');
    }

    function offer_block_file_close(id) {
        $('#' + id).remove();
    }

    function uploadFileOffer(data) {
        if (!(data instanceof Object)) data = JSON.parse(data);
        
        if (data.upload == 'ok') {
            var offer_block_start = '<div id="' + data.hash + '" class="blue_block"><i class="photo_file"></i>';
            var offer_block_hide = '<input type="hidden" value="' + data.file + '" class="offer_file_data" name="offer_file_data[]">';
            var offer_block_filename = '<input type="hidden" value="' + data.name + '" class="offer_filename" name="offer_filename[]">';
            var offer_block_end = ' <img style="cursor:pointer"  onclick="offer_block_file_close(\'' + data.hash + '\')" src="http://stat.aif.ru/img/closer.jpg?9c0"><i class="delet_file"></i>';
            var offer_block_title = '<span class="file_descript">Описание:</span><input type="test" name="offer_file_title[]" class="offer_file_title"> </div>';
            $('#offer_file_block').append(offer_block_start + offer_block_hide + offer_block_filename + data.name + offer_block_end + offer_block_title);
            $('#Offer_file_load_url').val('');
	        $('#nofile').hide();
        } else {
            $('.offer-mess-err').empty().append(data.error);
            $('.offer-mess').empty();
        }
        $('.uploadifive-queue').hide();
    }

</script>
