    $(document).ready(function() {
     
      var owl = $("#owl-demo");
     
      owl.owlCarousel({
          items : 13,
		  navigation: true,
      });

      $("#next").click(function(){
        $('.owl-next').click();
      })
      $("#prev").click(function(){
        $('.owl-prev').click();
      })
     
      $('#trig').click(function(){
        $('#blockMenu').toggle();
        $('#trig').toggleClass('close');
      });
    });


</script>


