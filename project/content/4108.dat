<script type="text/javascript">
    var reloadFilters;
    var isDiscount, isWholesale, isSpecial, isCheckedCompany, isNotChecked;
    $(function () {
        window.deliveryScopeChanged = false;
        window.filtersQueue = 0;
        window.opened_attributes = {};
        window.shown_attributes = {};
        window.opened_vendors = false;
        window.filtersUrl = '';
        var category = '3780';
        var get = {"display":"list"};
        var selectedFilters = (get.filters) ? get.filters.split('-') : [];
        var priceFrom = get.priceFrom ? parseFloat(get.priceFrom).toFixed(2) : 0.00;
        var priceTill = get.priceTill ? parseFloat(get.priceTill).toFixed(2) : 0.00;
        isDiscount = get.is_discount ? 1 : false;
        isWholesale = get.wholesale ? 1 : false;
        isSpecial = get.special ? 1 : false;
        isCheckedCompany = get.checked_company ? 1 : false;
        isNotChecked = get.is_not_checked ? 1 : false;
        var search = "";

        if ($('#v-delivery').find('.b-filter__link_state_checked').length > 0) {
            selectedFilters.push('delivery');
        }

        reloadFilters = function (clicked, follow) {
            if ($('#attributes-list').length == 0) {
                return false;
            }

            follow = !!(follow);
            if (selectedFilters.length > 0) {
                var get_filters = selectedFilters.slice(0);
                for (var i in get_filters) {
                    if (get_filters[i] == 'delivery') {
                        delete get_filters[i];
                    }
                }
                get.filters = get_filters.join('-');
            } else {
                delete get.filters;
            }
            delete get.priceFrom;
            delete get.priceTill;
            delete get.is_discount;
            delete get.wholesale;
            delete get.checked_company;
            delete get.is_not_checked;
            if (priceFrom) {
                get.priceFrom = priceFrom;
            }
            if (priceTill) {
                get.priceTill = priceTill;
            }
            if (isDiscount) {
                get.is_discount = isDiscount;
            }
            if (isWholesale) {
                get.wholesale = isWholesale;
            }
            if (isCheckedCompany) {
                get.checked_company = isCheckedCompany;
            }
            if (isNotChecked) {
                get.is_not_checked = isNotChecked;
            }
            if (isSpecial) {
                get.special = isSpecial;
            }
            var clearPriceLink = $('#clear_price_filter a');
            if (priceFrom || priceTill) {
                clearPriceLink.removeClass('b-filter__link_cancelled_yes');
            } else {
                clearPriceLink.addClass('b-filter__link_cancelled_yes');
            }

            delete get.search;
            if (search) {
                get.search = search;
                $('#clear_search_filter a').removeClass('b-filter__link_cancelled_yes');
            } else {
                $('#clear_search_filter a').addClass('b-filter__link_cancelled_yes');
            }

            if (isWholesale) {
                $('#clear_wholesale_filter a').removeClass('b-filter__link_cancelled_yes');
            } else {
                $('#clear_wholesale_filter a').addClass('b-filter__link_cancelled_yes');
            }

            if (isCheckedCompany) {
                $('#clear_checked_company_filter a').removeClass('b-filter__link_cancelled_yes');
            } else {
                $('#clear_checked_company_filter a').addClass('b-filter__link_cancelled_yes');
            }

            if (isDiscount) {
                $('#clear_discount_filter a').removeClass('b-filter__link_cancelled_yes');
            } else {
                $('#clear_discount_filter a').addClass('b-filter__link_cancelled_yes');
            }

            if (isNotChecked) {
                $('#clear_is_not_checked_filter a').removeClass('b-filter__link_cancelled_yes');
            } else {
                $('#clear_is_not_checked_filter a').addClass('b-filter__link_cancelled_yes');
            }

            var data = {
                category         : category,
                priceTill        : priceTill,
                priceFrom        : priceFrom,
                searchInCat      : search,
                                filters          : $.parseJSON('{"categories":{"values":[3780],"exclude":false},"type":{"values":[1],"exclude":false},"priority_delivery":{"values":["367"],"exclude":false}}'),
                selectedFilters  : selectedFilters,
                                isDiscount       : isDiscount,
                isWholesale      : isWholesale,
                isCheckedCompany : isCheckedCompany,
                isNotChecked     : isNotChecked
            };

            $.parseJSON('{"categories":{"values":[3780],"exclude":false},"type":{"values":[1],"exclude":false},"priority_delivery":{"values":["367"],"exclude":false}}');

            window.filtersUrl = '/t/kotly-gazovye-3780/' + (!$.isEmptyObject(get) ? '?' + $.param(get) : '');
            if (follow) {
                document.location.href = window.filtersUrl;
            }
            window.filtersQueue++;
            $.ajax({
                url      : '/ajax/filters/get-filters/',
                data     : data,
                dataType : 'JSON',
                type     : 'POST',
                success  : function (r) {
                    if (window.filtersQueue > 1) {
                        window.filtersQueue--;
                        return true;
                    }
                    window.filtersQueue--;
                    var container = $('#attributes-list');
                    container.empty();
                    var l = r.attributes.length;

                    if (l > 0) {
                        for (var i = 0; i < l; i++) {
                            var type = parseInt(r.attributes[i].type);
                            var aid = parseInt(r.attributes[i].id) * 10 + type;
                            var is_closed = false;
                            if (window.opened_attributes[aid] !== true) {
                                if (r.attributes[i].is_common == 1 && window.opened_attributes[aid] === false) {
                                    is_closed = true;
                                } else if (r.attributes[i].is_common == 0) {
                                    is_closed = !(!(aid in window.opened_attributes) && r.attributes[i].hasSelected);
                                }
                            }
                            var attribute = $('<ul class="b-filter__list' + (r.attributes[i].multi_column_catalog == 1 ? ' b-filter__list_view_columns' : '' ) + (is_closed ? ' b-filter__list_state_closed' : '' ) + '"></ul>');
                            var values = r.attributes[i].values;
                            for (var j = 0; j < values.length; j++) {
                                var value = values[j];
                                var id = parseInt(value.id) * 10 + type;
                                var itemClasses = ['b-filter__item'];
                                var classes = ['b-filter__link'];
                                if (value.selected) {
                                    itemClasses.push('b-filter__item_state_checked');
                                    classes.push('b-filter__link_state_checked');
                                }

                                if (!value.count) {
                                    classes.push('b-filter__link_state_empty');
                                }

                                attribute.append('<li class="' + itemClasses.join(' ') + '" id="v-' + id + '"><a href="#" data-id="' + id + '" class="' + classes.join(' ') + '">' + value.value + '</a></li>');
                            }
                            container.append(attribute);
                            if (attribute.find('li').length > 15 && (!window.opened_vendors || !window.shown_attributes[aid]) && !is_closed) {
                                var showAllText = ( type == 3 ? 'Все производители' : 'Показать все' );
                                if (!attribute.find('li:gt(14)').hasClass('b-filter__item_state_checked')) {
                                    attribute.find('li:gt(14)').hide();
                                    attribute.find('li:eq(14)').after('<li><a role="button" class="dashed attribute-show-all" id="attribute-show-all-' + aid + '" data-text="' + showAllText + '">' + showAllText + '</a></li>');
                                }
                                else {
                                    attribute.append('<li><a role="button" class="dashed attribute-show-all opened" id="attribute-show-all-' + aid + '" data-text="' + showAllText + '">Свернуть</a></li>');
                                }
                                $('#attribute-show-all-' + aid).click(function () {
                                    var thisUl = $(this).parents('ul:first');
                                    var showLink = $(this).parent().detach();
                                    if ($(this).hasClass('opened')) {
                                        if (type == 3)
                                            window.opened_vendors = false;
                                        else
                                            window.shown_attributes[aid] = false;
                                        thisUl.find('li:gt(14)').hide();
                                        thisUl.find('li:eq(14)').after(showLink);
                                        thisUl.find('.attribute-show-all').text(thisUl.find('.attribute-show-all').data('text'));
                                        $(this).removeClass('opened');
                                    } else {
                                        if (type == 3)
                                            window.opened_vendors = true;
                                        else
                                            window.shown_attributes[aid] = true;
                                        thisUl.find('li').show();
                                        thisUl.append(showLink);
                                        thisUl.find('.attribute-show-all').text('Свернуть');
                                        $(this).addClass('opened');
                                    }
                                    return false;
                                });
                            }
                            attribute.data('aid', aid);
                            var description = '';
                            if (r.attributes[i].description.length) {

                                if (r.attributes[i].description.length > 500 || r.attributes[i].description.indexOf('&lt;img ') + 1) {
                                    /**
                                     * Используем диалог для больших описаний атрибутов или если вложено изображение в описание атр
                                     */
                                    description = '&nbsp;<span class="sprite question-click pict-click-dialog" data-id="' + i + '"></span>';
                                    var idDialogAttr = 'help-filter-dialog-' + i;
                                    var $divAttrHelp = $('<div />')
                                        .attr('id', idDialogAttr)
                                        .addClass('adaptive-dialog categories-dialog')
                                        .data('title', r.attributes[i].title)
                                        .html($("<div/>").html(r.attributes[i].description).text());

                                    $("body").append($divAttrHelp);
                                    var dialogHeight = $('body').height() * 0.95;

                                    $('#' + idDialogAttr).dialog({
                                        width    : 800,
                                        height   : dialogHeight,
                                        modal    : true,
                                        autoOpen : false,
                                        title    : $('#' + idDialogAttr).data('title')
                                    });
                                } else {
                                    /**
                                     * Для маленьких описаний используем балон
                                     */
                                    description = '&nbsp;<span class="sprite question balloon" alt="' + r.attributes[i].description + '"></span>';
                                }
                            }
                            attribute.before('<h3 class="b-filter__title' + (is_closed ? ' b-filter__title_state_closed' : '') + '"><span>' + r.attributes[i].title + description + '</span></h3>');
                            window.opened_attributes[aid] = !is_closed;
                        }

                        /**
                         * Инициализация диалогов для описаний атрибутов с изображениями
                         */
                        $('.pict-click-dialog').on('click', function () {
                            var id = $(this).data('id');
                            $('#help-filter-dialog-' + id).dialog("open");
                            return false;
                        });
                    }

                    if (clicked) {
                        var $balloon = $('#b-filter__balloon');

                        $('#filter-result-count').html(r.total);
                        if (r.total > 0) {
                            $('#follow-filters-link').show(0);
                        } else {
                            $('#follow-filters-link').hide(0);
                        }
                        var el = $(clicked);

                        var offset = el.offset();

                        /* offset element where was click */
                        var elOffsetLeft = el.offset().left;
                        var elChildWidth = el.children().width();

                        // left balloon margin
                        var leftBubbleMargin = 35;

                        if (el.hasClass('wd-filters')) {
                            leftBubbleMargin = leftBubbleMargin + el.next().width();
                        }

                        // Calculate left offset
                        var left = el.offset().left + el.children().width() + leftBubbleMargin;

                        var windowWidth = $(window).width();
                        var top = offset.top - ($balloon.outerHeight() - el.outerHeight()) / 2;

                        if (windowWidth <= 360 && (left + $balloon.width()) > windowWidth) {
                            left = 50;
                            top += $balloon.outerHeight(true);
                            $balloon
                                .removeClass('b-filter__balloon_left')
                                .addClass('b-filter__balloon_arrow_top');

                            if (el.hasClass('b-price-filter')) {
                                left = 40;
                                $balloon
                                    .removeClass('b-filter__balloon_left')
                                    .addClass('b-filter__balloon_arrow_top');
                                top += 10;
                            }
                        } else {
                            $balloon
                                .removeClass('b-filter__balloon_arrow_top')
                                .addClass('b-filter__balloon_left');
                        }

                        $balloon.css({
                            position : 'absolute',
                            left     : left,
                            top      : top,
                            zIndex   : 999
                        });
                        var t = $balloon.data('timeout');
                        if (t) {
                            window.clearTimeout(t);
                            $balloon.removeData('timeout');
                        } else {
                            $balloon.fadeIn(300);
                        }

                        var timeout = window.setTimeout(function () {
                            $balloon.fadeOut(300);
                            $balloon.removeData('timeout');
                        }, 5000);
                        $balloon.data('timeout', timeout);
                    } else {
                        if ($(window).width() < COLLAPSE_WIDTH) {
                            $(".b-filter__title:not(.b-filter__title_state_closed)").click();
                        }
                    }
                    attach_balloon();
                    attachZoom();

                }
            });
        };
        $(document)
            .on('click', '.b-filter__link', function () {
                if ($(this).parents('#clear_price_filter').length > 0) {
                    if ($(this).hasClass('b-filter__link_cancelled_yes')) {
                        $('#priceFrom').val('');
                        $('#priceTill').val('');
                        priceFrom = $('#priceFrom').val();
                        priceTill = $('#priceTill').val();
                    } else {
                        $('#priceFrom').val('');
                        $('#priceTill').val('');
                        priceFrom = 0;
                        priceTill = 0;
                    }

                    reloadFilters('#clear_price_filter');
                    return false;
                }
                if ($(this).parents('#clear_search_filter').length > 0) {
                    if ($(this).hasClass('b-filter__link_cancelled_yes')) {
                        search = 'null';
                    } else {
                        search = '';
                    }
                    reloadFilters('#clear_search_filter');
                    return false;
                }
                if ($(this).parents('#clear_wholesale_filter').length > 0) {
                    if (isWholesale) {
                        $("#wholesale_filter").removeProp("checked");
                    } else {
                        $("#wholesale_filter").prop("checked", true);
                    }
                    isWholesale = !isWholesale;
                    reloadFilters("#clear_wholesale_filter");
                    return false;
                }
                if ($(this).parents('#clear_checked_company_filter').length > 0) {
                    if (isCheckedCompany) {
                        $("#checked_company_filter").removeProp("checked");
                    } else {
                        $("#checked_company_filter").prop("checked", true);
                    }
                    isCheckedCompany = !isCheckedCompany;
                    reloadFilters("#clear_checked_company_filter");
                    return false;
                }
                if ($(this).parents('#clear_discount_filter').length > 0) {
                    if (isDiscount) {
                        $("#discount_filter").removeProp("checked");
                    } else {
                        $("#discount_filter").prop("checked", true);
                    }
                    isDiscount = !isDiscount;
                    reloadFilters("#clear_discount_filter");
                    return false;
                }
                if ($(this).parents('#clear_is_not_checked_filter').length > 0) {
                    if (isNotChecked) {
                        $("#is_not_checked_filter").removeProp("checked");
                    } else {
                        $("#is_not_checked_filter").prop("checked", true);
                    }
                    isNotChecked = !isNotChecked;
                    reloadFilters("#clear_is_not_checked_filter");
                    return false;
                }
                var id = $(this).data('id');
                var el = $(this);
                var target = '#v-' + id;

                if ($(this).hasClass('b-filter__link_type_selected')) {
                    el = $('#attributes-list').find('[data-id="' + $(this).data('id') + '"]');
                    target = '#sv-' + id;
                }

                var pos = $.inArray(id.toString(), selectedFilters);
                if (pos == -1) {
                    selectedFilters.push(id.toString());
                    el.addClass('b-filter__link_state_checked');
                } else {
                    delete selectedFilters[pos];
                    el.removeClass('b-filter__link_state_checked');
                }
                selectedFilters = selectedFilters.filter(function (element, index, array) {
                    return !!element;
                });

                var selectedV = $('#sv-' + id);
                if (selectedV.length > 0) {
                    selectedV.find('.b-filter__link').toggleClass('b-filter__link_cancelled_yes');
                }

                reloadFilters(target);
                return false;
            })
            .on('click', '.b-filter__title', function () {
                var c = $(this).next();
                if ($(this).hasClass('b-filter__title_state_closed')) {
                    c.removeClass('b-filter__list_state_closed');
                    $(this).removeClass('b-filter__title_state_closed');
                    window.opened_attributes[c.data('aid')] = true;
                } else {
                    c.addClass('b-filter__list_state_closed');
                    $(this).addClass('b-filter__title_state_closed');
                    window.opened_attributes[c.data('aid')] = false;
                }

                return false;
            });

        $('#follow-filters-link,.b-filter__submit').click(function () {
            if ($('#v-delivery').length > 0) {
                var deliveryCookie = 0;
                if ($('#v-delivery').find('.b-filter__link_state_checked').length > 0) {
                    deliveryCookie = 1;
                }
                var date = new Date();
                date.setTime(date.getTime() + (60 * 60 * 1000));
                $.cookie('delivery_scope_' + '1', deliveryCookie, {
                    expires : date,
                    path    : '/',
                    domain  : '.satom.ru'
                });
            }
            document.location.href = window.filtersUrl;
            return false;
        });

        $('#priceFrom, #priceTill').blur(function () {
            var $this = $(this);
            var val = $.trim($this.val()).replace(/,/, '.');
            $this.removeClass('b-price-filter__input_state_wrong');
            var testRegexp = /^[0-9]+(\.[0-9]+)?$/;

            if (testRegexp.test(val) || !val) {
                if (val) {
                    val = parseFloat(val).toFixed(2).replace(/\.00/i, '');
                }
                $this.val(val);
                var id = this.id;
                var follow = $this.data('follow') ? 1 : 0;
                $this.removeData('follow');

                eval('if (val.toString() !== ' + id + ' || ' + follow + ') { ' + id + ' = val;reloadFilters(\'#v-price\', ' + follow + '); }');
            } else {
                $this.addClass('b-price-filter__input_state_wrong');
            }

            return false;
        }).keydown(function (e) {
            if (e.keyCode == 13) {
                $(this).data('follow', true);
                $(this).blur();
            }
        });

        var $popularManufactorersList = $('.b-filter__popular-manufactorers');
        if ($popularManufactorersList.children().length > 17) {
            var $showBtn = $('.b-filter__popular-manufactorers-show-all');
            $popularManufactorersList.addClass('b-filter__popular-manufactorers_closed');
            $showBtn.removeClass('b-filter__popular-manufactorers-show-all_inactive');
            $showBtn.find('a').text('Все производители');
            $showBtn.on('click', function () {
                if ($popularManufactorersList.hasClass('b-filter__popular-manufactorers_closed')) {
                    $popularManufactorersList.removeClass('b-filter__popular-manufactorers_closed');
                    $showBtn.find('a').text('Скрыть');
                }
                else {
                    $popularManufactorersList.addClass('b-filter__popular-manufactorers_closed');
                    $showBtn.find('a').text('Все производители');
                }
            });
        }
    });
</script>
