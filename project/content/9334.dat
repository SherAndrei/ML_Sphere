<script>

	function openOverlayAuthorize() {
		var cbx = $("#authorize").find('input[name=user_agreement]');
		if(cbx.hasClass("ui-helper-hidden-accessible")){
			cbx.button("destroy");
		}
		cbx.button();
	}

	function openOverlayRemind() {
		var $form = $("#remind").find(".remind-main-form").formvalidate("reset");
		$form.get(0).reset();
		$form.find("input[name=remind]").show();
		$form.find("input[type=submit]").show();
		$(".remind-message", $form).text("");
	}

	function closeOverlayAuthorize() {
		this.find(".authorization-main-form").formvalidate("reset").get(0).reset();
	}

	function getDataUserRating($form)
	{
		var product_rating = $('input[type="hidden"][name="product-rating"]').val();
		var product_type = $('input[type="hidden"][name="product-type"]').val();
		var product_id = $('input[type="hidden"][name="product-id"]').val();
		return  $form.serialize() + "&product_rating=" + product_rating + "&product_type=" + product_type +	"&product_id=" + product_id;
	}

	$(function() {

		var authValidator = $(".authorization-main-form").formvalidate({
			rules: {
				"input[name=code]": { required: true, minlength: 12 }
			},
			errmess: {
				"input[name=code]" : { required: "Укажите код скидки", minlength: "Укажите код скидки" }
			},
			keyup: true,
			invalidkeyup: false,
			submitHandler: function(form) {

				var $form = $(form), $input = $form.find("input[name=code]"), $button = $form.find("input[type=submit]"), msg = $(".authorize-message", $form), $agr = $("input[name=user_agreement]", $form.parents(".iform-auth"));

				if ($agr.length && !$agr.prop("checked")) {
					if (!confirm("Вы даете добровольное согласие на обработку своих персональных данных?"))
						return false;
					$agr.click();
				}

				$button.attr("disabled", true).addClass("btn-disabled");

				msg.text("");
				var dataForm = getDataUserRating($form);
				$.ajax({
					type: "POST",
					url: $form.attr("action"),
					data: dataForm,
					dataType: "json",
					success: function(a,b,c) {
						$button.removeAttr("disabled");
						if(a && a.error){
							$form.formvalidate("errors", {code: a.error});
							$button.removeClass("btn-disabled");
						}
						else {
							msg.addClass("lab-loader").loadingTxt({"txt": "Идет авторизация"});
							$input.hide();
							$button.hide();
							if (form.actionurl && form.actionurl.value) {
								window.location = form.actionurl.value;
							}
							else {
								window.location = window.location.href;
							}
						}
					}
				});

				return false;
			}
		});

		var remindValidator = $(".remind-main-form").formvalidate({
			autorange: false,
			rules: {
				"input[name=remind]": {
					required: true,
					email: {
						depends: function(element) {
							return !$.formvalidateobj.methods.mobile.call($.formvalidateobj, element.value, element);
						}
					},
					mobile: {
						depends: function(element) {
							return !$.formvalidateobj.methods.email.call($.formvalidateobj, element.value, element);
						}
					}
				}
			},
			messages: {
				"input[name=remind]": {
					required: "Укажите e-mail или мобильный (9XXXXXXXXX)",
					email: "Укажите e-mail или мобильный (9XXXXXXXXX)",
					mobile: "Укажите e-mail или мобильный (9XXXXXXXXX)"
				}
			},
			keyup: true,
			invalidkeyup: false,
			submitHandler: function(form) {

				var $form = $(form), $input = $form.find("input[name=remind]"), $button = $form.find("input[type=submit]"), msg = $(".remind-message", $form);

				$button.attr("disabled", true).addClass("btn-disabled");

				msg.text("");
				var dataForm = getDataUserRating($form);
				$.ajax({
					type: "POST",
					url:  $form.attr('action'),
					data: dataForm,
					dataType: "json",
					success: function(a,b,c) {
						$button.removeAttr("disabled");
						if(a && a.error){
							$form.formvalidate("errors", {remind: a.error});
							$button.removeClass("btn-disabled");
						}
						else if(a && a.message) {
							msg.text(a.message);
							form.reset();
							$input.hide();
							$button.hide();
						}
					}
				});

				return false;
			}
		});

	});

</script>
