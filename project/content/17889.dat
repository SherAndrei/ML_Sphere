        /* -----------[ DisableControls ]----------- */
        function p_DisableControls(disable)
        {
            if (disable) p_ShowError('<div class=GL_MAR10B><img align=left src="http://i.li.ru/ReActive/i/blog/comm/load.gif" /> Подождите, идет отправка данных...</div>'); else p_ShowError('');

