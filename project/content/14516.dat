        function simple_timer(sec,block){
            var time=sec;
            var hour=parseInt(time/3600);
            if(hour<1){
                $$(".page_item_sale_offer_timer_timercol_cell_2val_one:0",block).innerHTML="0";
                $$(".page_item_sale_offer_timer_timercol_cell_2val_one:1",block).innerHTML="0";
            }
            time=parseInt(time-hour*3600);
            if(hour<10){
                $$(".page_item_sale_offer_timer_timercol_cell_2val_one:0",block).innerHTML="0";
                $$(".page_item_sale_offer_timer_timercol_cell_2val_one:1",block).innerHTML=hour;
            }else{
                $$(".page_item_sale_offer_timer_timercol_cell_2val_one:0",block).innerHTML=~~(hour/10);
                $$(".page_item_sale_offer_timer_timercol_cell_2val_one:1",block).innerHTML=hour%10;
            }
            var minutes=parseInt(time/60);
            if(minutes<1){
                $$(".page_item_sale_offer_timer_timercol_cell_2val_one:2",block).innerHTML="0";
                $$(".page_item_sale_offer_timer_timercol_cell_2val_one:3",block).innerHTML="0";
            }
            time=parseInt(time-minutes*60);
            if(minutes<10){
                $$(".page_item_sale_offer_timer_timercol_cell_2val_one:2",block).innerHTML="0";
                $$(".page_item_sale_offer_timer_timercol_cell_2val_one:3",block).innerHTML=minutes;
            }else{
                $$(".page_item_sale_offer_timer_timercol_cell_2val_one:2",block).innerHTML=~~(minutes/10);
                $$(".page_item_sale_offer_timer_timercol_cell_2val_one:3",block).innerHTML=minutes%10;
            }
            var seconds=time;
            if(seconds<10){
                $$(".page_item_sale_offer_timer_timercol_cell_2val_one:4",block).innerHTML="0";
                $$(".page_item_sale_offer_timer_timercol_cell_2val_one:5",block).innerHTML=seconds;
            }else{
                $$(".page_item_sale_offer_timer_timercol_cell_2val_one:4",block).innerHTML=~~(seconds/10);
                $$(".page_item_sale_offer_timer_timercol_cell_2val_one:5",block).innerHTML=seconds%10;
            }
            sec--;
            if(sec>=0){
                setTimeout(function(){simple_timer(sec,block);},1000);
            }
        }
        if($$(".page_item_sale_offer_timer_timercol_timer:0")!=undefined){
            var now = new Date(), fut;
            if(now.getHours()>=20){
                fut=new Date(now.getYear()+1900,now.getMonth(),now.getDate()+1,8,0);
                var delta = fut - now;
                var back = delta < 0;
                var d = (Math.abs(delta) / 1000).toFixed(0);
