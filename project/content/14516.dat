                    var delta = fut - now;
                    var back = delta < 0;
                    var d = (Math.abs(delta) / 1000).toFixed(0);
                    try{simple_timer(d, $$(".page_item_sale_offer_timer_timercol_timer:0"));}catch(e){alert(e)}
                }
            }
        }
