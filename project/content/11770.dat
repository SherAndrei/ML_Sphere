<script type="text/javascript">
var wpp = $('.popular-posts');
$(document).ready(function()
  {
    $('h4', wpp).remove();
    $(wpp).removeClass();
    $(wpp).css({'font-weight': 'bold', 'font-size': '90%', 'border': '2px solid #FFFFFF', 'border-radius': '5px', 'width':'290px', 'padding':'0 10px'});
    $('ul', wpp).css({'overflow': 'hidden', 'padding': '10px', 'margin':'0px', 'border-radius':'5px', 'background-color': '#E5E5E5', 'clear': 'both'});
    $('<div style="color: #7F7F7F; padding: 10px;"><span style="float: left; margin-top: 5px;">Сегодня читают:</span><div style="border-radius: 5px 5px 0 0; margin-left: 20px; background-color: #E5E5E5; float: left;" class="wpp-pages"></div> </div>').prependTo(wpp);
    k=0;
    i=0;
    $('li', wpp).each(function()
      {
    $('li', wpp).css({'clear': 'both', 'overflow': 'hidden', 'margin-bottom': '5px'});    
        $($('a',this).get(0)).css({'float': 'left', 'width': '50px', 'height': '50px', 'border':'2px solid #FFFFFF','border-radius':'5px'});
        $($('a',this).get(1)).css({'float': 'left', 'width': '206px', 'padding': '5px', 'font-style': 'italic', 'color': '#7F7F7F'});
        $(this).addClass('tab'+(k+1));
        i++;
        if(i>4)
          {
            k++;
            i=0;
          }
      });
    if(i==0) k--;
    for(i=1;i<=k+1;i++)
      {
        $('.wpp-pages',wpp).append('<a class="wpp-nums" onclick="tabs('+i+');"><span style="margin-left: 0px;">'+i+'</span></a>');
      }
    tabs(1);
  });

function tabs(l)
  {
    $('li',wpp).hide(500);
    $('.tab'+l,wpp).show(500);
  }
</script>
