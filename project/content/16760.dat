            if (dlink && email) {
                if (IsEmail(email.val())) {
                    dlink.val(dlink.val() + "&email=" + email.val());
                    $.cookie('is_already_downloaded', 1, {expires: 7, path: '/'});
                    $.cookie('email', email.val(), {expires: 7, path: '/'});
