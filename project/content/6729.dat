<script type="text/javascript">
    var CITY_IN_COL_COUNT = 14;
    var UNIT_25_COL_COUNT = 4;

    adrMgr.regCities = [];

    adrMgr.paramsForDelete = ['searchid'];

    adrMgr.getCites = function (regCode) {
        var request = $.Deferred();
        if (adrMgr.regCities[regCode]) {
            request.resolve(adrMgr.regCities[regCode]);
        } else {
            $.ajax({
                url: '/ajax/getRegionCity',
                type: 'post',
                dataType: 'json',
                data: {
                    regKlId: regCode
                },
                async: true,
                cache: false
            }).done(function (resp) {
                var cities = adrMgr.regCities[regCode] = resp.result;
                request.resolve(cities);
            });
        }
        return request;
    };

    (function () {
        var selectedRegion = adrMgr.selectedRegion.klRegion;

        var show = function (regCode, force) {
            adrMgr.getCites(regCode).done(function (cities) {
                if (regCode != selectedRegion) return;

                var region = adrMgr.regions[regCode];

                cities = cities.slice(0, CITY_IN_COL_COUNT * UNIT_25_COL_COUNT);

                var html = '';

                html += '<div class="location-expd__head">' + adrMgr.regions[regCode].name + '</div>';

                html += '<div class="units-row">';
                {
                    for (var column = 0; column < UNIT_25_COL_COUNT; column++) {
                        html += '<ul class="location-expd__col">';
                        cities.slice(column * CITY_IN_COL_COUNT, (column + 1) * CITY_IN_COL_COUNT).forEach(function (city) {
                            var current = city.id == adrMgr.selectedCity.id;
                            var big = 'г' == (city.prefix || '').toLowerCase();

                            var href = '//' + region.synonym + '.rt.ru/'
                            if (city.synonym != region.synonym) href += city.synonym + '/';
                            href += 'hometel';

                            var params = utils.deparam(location.search.slice(1));
                            if (params) {
                                adrMgr.paramsForDelete.forEach(function (param) {
                                    delete params[param];
                                });
                                params = $.param(params);
                                href += '?' + params;
                            }

                            html += '<li class="' + (current ? 'is-loc-current' : '') + '">';
                            {
                                html += '<a href="' + href + '" class="location-expd__link" onclick="stat.fixStat(\'change_city\');">';
                                {
                                    if (big) html += '<b>';
                                    html += city.name;
                                    if (big) html += '</b>';
                                }
                                html += '</a>';
                            }
                            html += '</li>';
                        });
                        html += '</ul>';
                    }
                }
                html += '</div>';

                $('#location-city-box').html(html);

                if ($('#location-box').hasClass('is-loc-shown') && !force) {
                    $('#location-city-box').addClass('is-show');
                }
            })
        };

        $('#location-region-box').find('[data-region]').click(function () {
            $('#location-region-box').find('.is-loc-current').removeClass('is-loc-current');
            $(this).closest('li').addClass('is-loc-current');
            selectedRegion = $(this).attr('data-region');
            show(selectedRegion);
        });

        $('#header-location').one('click', function () {
            show(adrMgr.selectedRegion.klRegion, true);
        });

        $('#region-btn').one('click', function () {
            show(adrMgr.selectedRegion.klRegion, true);
        });
    })();

</script></div>
    </div>
</div>

