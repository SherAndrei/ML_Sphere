		//$banner.css( 'margin', '0 auto');
		$b_height = $banner.innerHeight();
		$allow = $(document).height() - $('#footer').height() - $banner.height() - 180;
		
		$banner_show = false;
		$isAnimate = false;
		$(window).scroll(function() {
			$b_height = $banner.innerHeight();
