					$banner.stop().animate({top: $b_pos + "px"}, 800, function() {
						$banner.css( "position", "fixed");
						$banner.css( "top", $b_pos + "px");
						$isAnimate = false;
						$banner_show = true;
					});
		    	}
		    	
		    }
