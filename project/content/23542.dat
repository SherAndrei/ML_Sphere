                isEnable = urlMobileRedirectReject && dateNow > dateReject + this.paramDateRejectDuration;
            } catch (e) {
                isEnable = true;
            }

            return isEnable;
        },

        isLocalStorageAvailable: function () {
            try {
                var isEnable, ls;

                ls = "isLocalStorageAvailable";

                localStorage.setItem(ls, ls);
                isEnable = localStorage.getItem(ls) === ls;
