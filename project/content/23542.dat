        smartTvRedirect: function(){
            if (this.isLocalStorageAvailable() && this.isAvailable() && !(document.location.search.indexOf('from=') > -1) && this.isSmartTv() ) {

                var lastRedirect = window.localStorage.getItem(this.LS_KEY);
                var redirectUrl = "/" + lang + "/app?type=smarttv";
                var today = new Date();
                var todayTime = today.getTime();

                if (lastRedirect) {
                    lastRedirect = lastRedirect.replace(/\|/g, ".");

                    var values = lastRedirect.split(".");

                    var differenceInDays = Math.floor((todayTime - values[2]) / 1000 / 60 / 60 / 24);

                    if (differenceInDays > 6) {
                        window.localStorage.setItem(this.LS_KEY, (parseInt(values[0]) + 1) + "." + values[1] + "." + todayTime);
                        document.location.href = redirectUrl;
                    }
                } else {
                    window.localStorage.setItem(this.LS_KEY, "1" + "." + todayTime + "." + todayTime);
                    document.location.href = redirectUrl;
                }
            }
        },
        
        isAvailable: function () {
