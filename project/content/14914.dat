			for(var track_id in tracks[post_id])
			{
				var map_container = null;
				markers[post_id][track_id] = [];
				
				if(map_container = document.getElementById("track_" + post_id + "_" + track_id))
				{
                     var mapOptions = {
                       size: new google.maps.Size(600, 400),                                 
                      scaleControl: false,                     
                      center: new google.maps.LatLng(tracks[post_id][track_id][4], tracks[post_id][track_id][5]),
                      overviewMapControl: false,
                      overviewMapControlOptions:{opened:false},
                      mapTypeId: google.maps.MapTypeId.ROADMAP,
                      draggable: false,
                      scrollwheel: false,
                      disableDoubleClickZoom: true
                    };

                     
                    var map = new google.maps.Map(map_container, mapOptions);
                    directionsDisplay.setMap(map);

                    var decodedPath = google.maps.geometry.encoding.decodePath(tracks[post_id][track_id][0]);
                    var decodedLevels = decodeLevels(tracks[post_id][track_id][1]);

                    var polyline = new google.maps.Polyline({
					    strokeColor: "#FF0000",
					    strokeWeight: 3,
					    strokeOpacity: 0.95,
					    path: decodedPath,
					    levels: decodedLevels,
					    zoomFactor: tracks[post_id][track_id][3],
					    numLevels: tracks[post_id][track_id][2]
			
					});

                    var marker = new google.maps.Marker({
                             position: polyline.getPath().getAt(0),
                             map: map,
                             title: 'Начало маршрута',
							clickable: true
                    });
                    
                    var marker = new google.maps.Marker({
                             position: polyline.getPath().getAt(polyline.getPath().getLength() - 1),
                             map: map,
                             title: 'Конец маршрута',
							clickable: true
                    });
                    
                    var $mapDiv = $("#track_" + post_id + "_" + track_id);
                    var mapDim = { height: $mapDiv.height(), width: $mapDiv.width() };

					map.setCenter(new google.maps.LatLng(tracks[post_id][track_id][4], tracks[post_id][track_id][5]));
                    map.setZoom(getBoundsZoomLevel(getBoundsForPoly(polyline), mapDim));
					
					//map.addControl(new google.maps.LargeMapControl3D());
					//map.addControl(new google.maps.MapTypeControl());

                    if(parseInt(tracks[post_id][track_id][6]) == 0)
					{
					    polyline.setMap(map);
					}
					else
					{
						var points_type = tracks[post_id][track_id][6].split('');
						var waypts = [];
                        var waypts_direction = [];
						var type = points_type[0];
						waypts[0] = polyline.getPath().getAt(0);
                        waypts_direction.push({
                               location:polyline.getPath().getAt(0),
                               stopover:true});

						for(var i = 1; i < points_type.length; i++)
						{
							if(points_type[i] != type)
							{
								if(type == 0)
								{
									var polyline_wp = new google.maps.Polyline({path: waypts, strokeColor: "#ff0000", strokeOpacity:3, strokeWeight:0.95});
                                    polyline_wp.setMap(map);
								}
								else
								{
                                    request = {
								         origin:waypts[0],
                                         destination: waypts[waypts.length - 1],
                                         waypoints: waypts_direction,
                                         optimizeWaypoints: true,
                                         travelMode: google.maps.DirectionsTravelMode.DRIVING
                                    };

                                    directionsService.route(request, function(response, status) {
                                      if (status == google.maps.DirectionsStatus.OK) 
                                      {
                                         directionsDisplay.setDirections(response);
                                      }
                                    });   
                                            
								}
								waypts = [polyline.getPath().getAt(i-1), polyline.getPath().getAt(i)];
                                waypts_direction = [];
                                waypts_direction.push({
                                     location:polyline.getPath().getAt(i-1),
                                     stopover:true});
                                waypts_direction.push({
                                     location:polyline.getPath().getAt(i),
                                     stopover:true});     
 
								type = points_type[i];

								if(i == points_type.length - 1)
								{
									if(points_type[i] == 0)
									{
										var polyline_wp = new google.maps.Polyline({path: waypts, strokeColor: "#ff0000", strokeOpacity:3, strokeWeight:0.95});
                                        polyline_wp.setMap(map);
									}
									else
