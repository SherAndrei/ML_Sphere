<script type="text/javascript">
// <![CDATA[
	//Здесь прописываем какие url будем парсить в bbcode map
	var expr = new RegExp('http(s)?:\/\/(www\.|mapsengine\.|maps\.)?goo(gle)?.(gl|ru|com)\/map(s\?|\/)(.*)', 'i');
	var expr2 = new RegExp('(http(?:s)?:\/\/(?:www\.)?google\.(?:ru|com)\/)maps\/dir\/([^@]+)+\/@([^\/]+),([0-9]+z)\/', 'i');
	
	function parse_map_bbcode()
	{
		$('div.maps').each(function(){
			var cur_map = $(this);
			var cur_url = cur_map.html();
			var cur_match = cur_url.match(expr);
			
			if(cur_match!= null && cur_match[0]!='')
			{
				if(cur_match[3] != 'gle') //имеем короткую ссылку
				{
					$.ajax({
						type: 'GET',
						dataType: 'json',
						url: 'https://www.googleapis.com/urlshortener/v1/url?shortUrl='+cur_url,
						success: function(data){
							if(data['status'] == 'OK' && data['longUrl']!='')
							{
								cur_url = data['longUrl'];
								display_map_iframe(cur_url, cur_map);
							}
							else
							{
								cur_map.html("Не удалось загрузить карту").removeClass('maps');
								return;
							}
						}
					});
				}
				else			
				{
					display_map_iframe(cur_url, cur_map);
				}
			}
			else
			{
				cur_map.html("Не удалось загрузить карту").removeClass('maps');
			}			
 		});
	}
	
	function display_map_iframe(cur_url, cur_map)
	{
		cur_url = cur_url.replace(/&(?!amp;)/g,'&amp;');
		cur_url = cur_url.replace(/output=classic/g,'output=embed');
		cur_url = cur_url.replace(/map\/edit/g,'map/embed');
		if(cur_url.indexOf('/maps/dir/') != -1)
		{
			var cur_match = cur_url.match(expr2);
			var addr = cur_match[2].split('/');
			var add_from = addr[0];
			var add_d = addr[1];
			addr = addr.slice(1);
			addr = addr.slice(1);
			if(addr.length>0)
			{
				var addr_to = '+to:'+addr.join('+to:');
			}
			else
			{
				var addr_to = '';
			}
			
			var zoom = '';//((cur_match[4]!='') ? '&amp;z='+cur_match[4].replace('z',''): '');
			
			cur_url = cur_match[1] + '/maps?t=m&amp;saddr=' + add_from + '&amp;daddr='+add_d + addr_to+'&amp;ll=' + cur_match[3] + zoom +'&amp;output=embed&amp;dg=ntvo';
		}
		else if(cur_url.indexOf('output=embed') == -1)
			{
				cur_url += '&amp;output=embed'; 
			}
		cur_map.html('<iframe width="640" height="480" frameborder="0" scrolling="no" marginheight="0" marginwidth="0" src="'+cur_url+'"></iframe>').removeClass('maps');
	}
	
	$(document).ready(parse_map_bbcode());
// ]]>
</script>

