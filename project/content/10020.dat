<link rel="stylesheet" href="//www.klerk.ru/html5/js/library/jquery.smartbanner/jquery.smartbanner.css"/>
<script type="text/javascript">
    
    (function($) {
        $(function () {
            // Если AdBlock режит smartbanner
            if (!$.smartbanner) {
                return ;
            }

            $.smartbanner({
                title: 'Клерк.Ру - весь для бухгалтера',
                author: 'Клерк.Ру',
                inGooglePlay: 'в Google Play',
                button: 'СКАЧАТЬ',
                price: 'Бесплатно',
                appendToSelector: '.upperPanel',
                scale: 1,
                icon: '/im/logos-klerk/square-128x128.png'
            });

            var $header = $('.upperPanel');

            var smartBannerClose = function () {
                $('body').removeClass('smartbanner-open');
                $header.height($header.data('originalHeight'));
            };

            var intervalCount = 0,
                smartBannerInterval = setInterval(function () {
                    intervalCount++;
                    if (intervalCount >= 20) {
                        clearInterval(smartBannerInterval);
                        return;
                    }
                    if ($('#smartbanner').length) {
                        clearInterval(smartBannerInterval);
                        var originalHeight = $header.height(),
                                originalMarginTop = parseInt($('body').css('margin-top')),
                                bannerHeight = $('#smartbanner').outerHeight();
                        $header.data('originalHeight', originalHeight);
                        $header.height(originalHeight + bannerHeight);
                        $('#smartbanner .sb-close').on('click', smartBannerClose);
                        $('#smartbanner .sb-button').on('click', smartBannerClose);
                        $('body').addClass('smartbanner-open');
                    }
                }, 100);
        });
    })(jQuery)
    
