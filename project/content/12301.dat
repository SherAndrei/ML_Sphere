        <div class="post_date"><a  class="post_link"  href="/wall189426492_364"  onclick="return showWiki({w: 'wall189426492_364'}, false, event);" ><span class="rel_date">7 авг в 13:46</span></a></div>
        <div class="ui_actions_menu_wrap _ui_menu_wrap " onmouseover="uiActionsMenu.show(this);" onmouseout="uiActionsMenu.hide(this);">
