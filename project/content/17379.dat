      // 'Georgia': {
      //   'url': 'http://www.the-village.ge',
      //   'nameFrom': 'Грузии',
      //   'nameFranchize': 'Грузия'
      }
    };

    var currentFranchise = franchisesList[currentCountry];
    var lsName = 'village-hide-bottom-line-' + currentCountry;

    if (currentFranchise && !localStorage.getItem(lsName)) {
      var content =
        "<div id='change-city-line' class='city-line'>" +
          "<div class='city-line__container'>" +
            "<div class='city-line__text'><span>Нам кажется, что вы из " + currentFranchise.nameFrom + ".</span> В вашей стране уже работает <span>The Village <b style='color: #ff652c'>" + currentFranchise.nameFranchize + "</b>.<span></div>" +
