                "<a href='#' class='city-line__btn-border js-cancel-franchise'>оставить как есть</a>" +
              "</div>" +
            "</div>" +
          "</div>" +
        "</div>";

      $('body').append(content);

      setTimeout(function() {
        $('#change-city-line').css('transform', 'translateY(0)');
      }, 1000);

      $('.js-to-franchise').on('click', function() {
        $('#change-city-line').css('transform', 'translateY(100%)');
      });

      $('.js-cancel-franchise').on('click', function(ev) {
        ev.preventDefault();
        $('#change-city-line').css('transform', 'translateY(100%)');

        var ls = localStorage;

        if (ls) {
          localStorage.setItem(lsName, true);
        }
      });
    }

  });
</script>
