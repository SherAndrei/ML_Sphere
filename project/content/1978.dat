				var_rur = document.getElementById("rur_today").value;
				document.getElementById("datepicker").value = document.getElementById("date_today").value;
			}
			
			if (var_usd == 0 && var_eur == 0 && var_rur == 0){
				document.getElementById("form_usd").value = "курсов";
				document.getElementById("form_byr").value = "на эту";
				document.getElementById("form_eur").value = "дату";
				document.getElementById("form_rur").value = "нет !";			
			}
			else{
				//$("#error").fadeOut();
				if (currency == "usd")
				{
					lastChange = "usd";
					if (isNaN(document.getElementById("form_usd").value)) document.getElementById("form_usd").value = 100;
					if ((document.getElementById("form_usd").value == "") || (document.getElementById("form_usd").value == " ")){
						document.getElementById("form_usd").value = "";
						document.getElementById("form_byr").value = 0;
						document.getElementById("form_eur").value = 0;
						document.getElementById("form_rur").value = 0;
						return;
					}
					document.getElementById("form_byr").value	= Math.round(parseFloat(document.getElementById("form_usd").value) * var_usd * 10000) / 10000;
					document.getElementById("form_eur").value	= Math.round(parseFloat(document.getElementById("form_usd").value) * var_usd / var_eur * 10000) / 10000;
					document.getElementById("form_rur").value		= Math.round(parseFloat(document.getElementById("form_usd").value) * var_usd / var_rur * 10000) / 100;
				}
				else if (currency == "eur"){
					lastChange = "eur";
					if (isNaN(document.getElementById("form_eur").value)) document.getElementById("form_eur").value = 100;
					if ((document.getElementById("form_eur").value == "") || (document.getElementById("form_eur").value == " ")){
						document.getElementById("form_eur").value = "";
						document.getElementById("form_byr").value = 0;
						document.getElementById("form_usd").value = 0;
						document.getElementById("form_rur").value = 0;
						return;
					}					   
					document.getElementById("form_byr").value	= Math.round(parseFloat(document.getElementById("form_eur").value) * var_eur * 10000) / 10000;
					document.getElementById("form_usd").value	= Math.round(parseFloat(document.getElementById("form_eur").value) * var_eur / var_usd * 10000) / 10000;
					document.getElementById("form_rur").value		= Math.round(parseFloat(document.getElementById("form_eur").value) * var_eur / var_rur * 10000) / 100;
				}
				else if (currency == "rur"){
					lastChange = "rur";
					if (isNaN(document.getElementById("form_rur").value)) document.getElementById("form_rur").value = 100;
					if ((document.getElementById("form_rur").value == "") || (document.getElementById("form_rur").value == " ")){
						document.getElementById("form_rur").value = "";
						document.getElementById("form_byr").value = 0;
						document.getElementById("form_usd").value = 0;
						document.getElementById("form_eur").value = 0;
						return;
					}					
					document.getElementById("form_byr").value	= Math.round(parseFloat(document.getElementById("form_rur").value) * var_rur * 100) / 10000;
					document.getElementById("form_usd").value	= Math.round(parseFloat(document.getElementById("form_rur").value) * var_rur / var_usd * 100) / 10000;
					document.getElementById("form_eur").value	= Math.round(parseFloat(document.getElementById("form_rur").value) * var_rur / var_eur * 100) / 10000;
				}
				else if (currency == "byr"){
					lastChange = "byr";
					if (isNaN(document.getElementById("form_byr").value)) document.getElementById("form_byr").value = 100;
					if ((document.getElementById("form_byr").value == "") || (document.getElementById("form_byr").value == " ")){
						document.getElementById("form_byr").value = "";
						document.getElementById("form_eur").value = 0;
						document.getElementById("form_usd").value = 0;
						document.getElementById("form_rur").value = 0;
						return;
					}					
					document.getElementById("form_rur").value = Math.round(parseFloat(document.getElementById("form_byr").value) / var_rur * 10000) / 100;
					document.getElementById("form_usd").value = Math.round(parseFloat(document.getElementById("form_byr").value) / var_usd * 10000) / 10000;
					document.getElementById("form_eur").value = Math.round(parseFloat(document.getElementById("form_byr").value) / var_eur * 10000) / 10000;
				}
			}
	}
</script>

