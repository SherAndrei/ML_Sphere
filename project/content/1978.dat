	var var_alert = 0;
	var lastChange = "usd";
	// пересчет курсов локально, когда дата уже выбрана и меняются только суммы
	function calcItLocal(currency) {
	
		if (var_usd == 0 && var_eur == 0 && var_rur == 0)
			if ((document.getElementById("usd_today").value > 0) && (document.getElementById("eur_today").value > 0) && (document.getElementById("rur_today").value > 0))
			{
				var_usd = document.getElementById("usd_today").value;
