function filterBy(fields) {

    $('.dropdown a').each(function (index) {

        switch ( fields.country ) {

            case 'ru':

                if ( $(this).attr('country') == 'ru' ) { 

                    $(this).css('display', 'block');

                } else {

                    $(this).css('display', 'none');

                }

