			send({ev_type: 'pageview'});
			for (var i = 0, len = q.length; i < len; ++i) {
				send(q[i].data, q[i].op);
			}
		});
	});

</script><script>
jqready(function($) {
	var reporter = function() {
		create_layer('report_error', { popstate: false });
