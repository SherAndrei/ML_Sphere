                    }
                  });                  
                  
                  setTimeout("convertDate()", 1000);
                  
                  $(".info_c").each(function() {
                    var comId = $(this).parent().parent().parent().attr('id').replace('com', '');
                    if($("#comment_toggle_" + comId).html() != '') {
                      if($(".comment_toggle_init[attr='" + comId + "']").length <= 0) {
                        if(userHide == 1) {
                          $("#save_" + comId).before('<a href="" class="comment_toggle_init" attr="' + comId + '"><img src="/images/icon_plus.png"></a>');
                        } else {
                          $("#save_" + comId).before('<a href="" class="comment_toggle_init" attr="' + comId + '"><img src="/images/icon_minus.png"></a>');
                        }
                        
                      }
                    }
                    
                  });
                  
                });
                return false;
              });
