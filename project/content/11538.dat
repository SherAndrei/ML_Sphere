function checkMail(email, filed_prefix_id)
{

	if(email)
	{

        if(!filed_prefix_id) {
          filed_prefix_id = "rm_email";
        }

		if(!isValidEmail(email))
		{
			$('#'+filed_prefix_id+'_err').html('Неверный email');
			return false;
		}
		$.ajax({
			url: 'ajax/ajax_login.php',
			data:{
				mode:'test_email',
				email: email
			},
	        type: 'POST',
	        success: function(data,code)
	        {
				if(data==1)
				{
					$('#'+filed_prefix_id+'_err').html('Email уже используется');
				}
	            else
	            {
					$('#'+filed_prefix_id+'_err').html('');
				}
			}
		});
	}
}
function checkLogin(login, filed_prefix_id)
{

        if($("#rv").val()==1) {
            $("#rv").val('89d5ba1dfb');
        }

        if(!filed_prefix_id) {
          filed_prefix_id = "rm_login";
        }

		if(login == '')
		{
			$('#'+filed_prefix_id+'_err').html('Введите логин');
		}
		else if (!isLatinString(login))
		{
			$('#'+filed_prefix_id+'_err').html('Используйте A-z, 0-9');
		}
		else if(login.length < 4)
		{
			$('#'+filed_prefix_id+'_err').html('Логин коротковат');
		}
		else if(login.length > 16)
		{
			$('#'+filed_prefix_id+'_err').html('Логин длинноват');
		}
		else
		{
			$.ajax({
				url: '/signup.php',
				data:{
					avail: login
				},
		        type: 'GET',
		        success: function(data)
		        {
					if(data == login + '|0')
					{
						$('#'+filed_prefix_id+'_err').html('Логин занят');
					}
					else
					{
						$('#'+filed_prefix_id+'_err').html('');
					}
				}
			});
		}
}


