                function stripslashes(str) {
                  return str.replace(/\0/g, '0').replace(/\\(.)/g, "$1");
                }
                
                
                //$("#getCommAnim").show();
                
                $.getJSON(href, function(data) {

                  $("#load_comments").children('a').html(data.message);
                  if(data.count < 500) {
                    $("#load_comments").hide();
                  }
                  
                  $.each(data.comments, function(key, value) {
                    value.text = stripslashes(value.text);
                    
                    if(value.parent_id != 0 && $("#comment_toggle_" + value.parent_id).length > 0) {

                      $("#comment_toggle_" + value.parent_id).append(value.text + '<div id="comment_toggle_' + value.comment_id + '" class="comment_tree"></div>');
                      
                      
                      var el = $("#com" + value.comment_id).parent().parent();
                      $("#com" + value.comment_id).attr('incl', el.parent().parents(".comment_tree").length);

                      
                      var paddNum = el.parent().parents(".comment_tree").length;
                      if (paddNum >= 10) {
                        paddNum = paddNum % 10;
                        
                        if(paddNum == 0) {
                          var elLine = $("#com" + value.comment_id).parent().parent().parent();
                          elLine.html("<tr class='hc_" + value.comment_id + "'><td colspan='10'><img src='http://s.pikabu.ru/images/comm_tree_start.gif' style='position: relative; right: 9px; bottom: 2px;'></td></tr>"+ elLine.html())                        
                        }
                        
                      }
                      
                      if(paddNum > 0) {
                        var html = el.html();
                        el.html('<td style="border-left: dotted 1px #e5e5e5; width: 4%; padding: 0; margin: 0;">&nbsp;</td>' + html);

                        var padd = "";
                        for(i = 1; i <= paddNum; i++) {
                          padd += '<td style="border-left: dotted 1px #e5e5e5; width: 4%; padding: 0; margin: 0;">&nbsp;</td>'
                        }
  //                      console.log(html);
  //                      return;
                        el.html(padd + html);
                      }

                      
                    } else {
                      if(userHide == 1) {
                        $("#commentsDiv").append(value.text + '<div id="comment_toggle_' + value.comment_id + '" class="comment_tree hidden"></div>');
                      } else {                    
                        $("#commentsDiv").append(value.text + '<div id="comment_toggle_' + value.comment_id + '" class="comment_tree"></div>');
                      } 
                    }
                  });
                  
                  $("#load_comments").attr('class','submit_button');
                  
                  $(".comment_tree:hidden").each(function() {
                    if($(this).children('table').length > 0) {
                      $("#comment_slidedown_" + $(this).attr('id').replace('comment_toggle_', '')).css('display' , 'inline');
                    }                  
                  });
                  
                  lastComm = data.comments[Object.keys(data.comments)[Object.keys(data.comments).length - 1]].comment_id;
                  
                  
                  $(".comment_toggle").toggle(
