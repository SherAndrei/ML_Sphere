                            e.preventDefault();
                            $(this).html("показать комментарий");
                            $(this).next("div").hide();
                            setTimeout('redraw_footer()', 100);
                          }
                  );   
                
                  $(".bb_bar a").unbind('click').click(function() {
                    var button_id = attribs = $(this).attr("alt");
                    button_id = button_id.replace(/\[.*\]/, '');
                    if (/\[.*\]/.test(attribs)) {
                      attribs = attribs.replace(/.*\[(.*)\]/, ' $1');
                    } else
                      attribs = '';
                    var start = '[' + button_id + attribs + ']';
                    var end = '[/' + button_id + ']';
                    var elm = $(this).parent().attr("attr");
                    bb_insert(start, end, elm);
                    return false;
                  });                
                  
                  //$("#getCommAnim").hide();
                  
                  $(".comment_b > div > .comment_slidedown").each(function() {
