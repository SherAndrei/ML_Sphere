<script>
jQuery(document).ready(function($){
	$('#site-navigation .dropdown-primary-menu').html( $('.menu-item-has-children .sub-menu').html() );
	$('#primary-menu li.menu-item-has-children').mouseenter(function(){
		$('#site-navigation .dropdown-primary-menu').html( $('.sub-menu', this).html() );
	});

	//mobile-navigation
	$('a.mobile-navigation').toggle(
		function(){
			$('#primary-menu').addClass('show-mobile-menu');
			$('.show-mobile-menu li.menu-item-has-children>a').on('click',
				function(e){
					$('.sub-menu', $(this).parent() ).toggleClass('show-mobile-sub-menu');
					e.preventDefault();
			});
		},
		function(){
			$('#primary-menu').removeClass('show-mobile-menu');
			$('#primary-menu .sub-menu').each(function() {
				$(this).removeClass('show-mobile-sub-menu');
			})
		}
	);
});
</script>

</body>
</html>
