<script type="text/javascript">
function findPos(obj){
	
	var curleft = curtop = 0;
	curleft = obj.offsetLeft;
	curtop = obj.offsetTop;
	if( obj.offsetParent ){
		while( obj = obj.offsetParent ){
			curleft += obj.offsetLeft;
			curtop += obj.offsetTop;
		}
	} 
	return [curleft, curtop];
	
}
function show_enews_popup()
{
var pos = findPos(document.getElementById('main_content'));
	document.getElementById('enewsletter').style.cs-p-left = (pos[0]+200)+'px';
  document.getElementById('enewsletter').style.display = '';
document.getElementById('enewsletter_bg').style.display = '';
}
function close_enews_popup()
{
document.getElementById('enewsletter_bg').style.display = 'none';
 document.getElementById('enewsletter').style.display = 'none';
}
function getCookie(name) {
	var cookie = " " + document.cookie;
	var search = " " + name + "=";
	var setStr = null;
	var offset = 0;
	var end = 0;
	if (cookie.length > 0) {
		offset = cookie.indexOf(search);
		if (offset != -1) {
			offset += search.length;
			end = cookie.indexOf(";", offset)
			if (end == -1) {
				end = cookie.length;
			}
			setStr = unescape(cookie.substring(offset, end));
		}
	}
	return(setStr);
}
function need_show_enews ()
{
    was_clicked = getCookie('was_clicked');
    if (was_clicked == null) 
      was_clicked = 1;
    else
      was_clicked = parseInt(was_clicked) + 1;
console.log(was_clicked);
      if (was_clicked == 2) {
         setTimeout("show_enews_popup()", 6000);
      } 
var date = new Date();
	date.setTime(date.getTime()+(30*24*60*60*10000));
	document.cookie = 'was_clicked='+was_clicked+'; path=/;  expires='+date.toGMTString() ;
      
}
