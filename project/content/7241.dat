<script>
function validatePhone(fld) {
    var error = "";
    var stripped = fld.value.replace(/[\(\)\.\-\ ]/g, '');
 
   if (fld.value == "") {
        error = "Вы не ввели номер телефона.\n";
        fld.style.background = 'Yellow';
        alert(error);
		return false;
 
    } else if (isNaN(parseInt(stripped))) {
        error = "Номер телефона содержит недопустимые символы. Don't include dash (-)\n";
        fld.style.background = 'Yellow';
        alert(error);
		return false;
    } else if (!(stripped.length == 10)) {
        error = "Не правильно введен номер телефона. Введите номер из 10 символов без '8' и '+7' не используя дефисов\n";
        fld.style.background = 'Yellow';
        alert(error);
		return false;
    }
    return true;
}
</script>
<script>
var is_popuped = true;
