                    <a name="message70713990"/>

        <div class="info">
            <IDXSKIP><div class="user">
                                    Anonymous
                    <br/>
                                                </div></IDXSKIP>

            <div class="date">
                Дата:05.11.11 Время:19:43
                            </div>
