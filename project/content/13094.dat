  <div class="_post_content">
    
    <div class="post_header">
      <a class="post_image" href="/id139957706">
        <img src="https://pp.userapi.com/c11294/u139957706/e_687caa8e.jpg" data-post-id="139957706_260" data-post-click-type="post_owner_img" width="50" height="50" class="post_img" />
