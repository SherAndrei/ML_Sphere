            iFrame.addEventListener('load', function () {
                var iFrameWindow = iFrame.contentWindow;

                if (iFrameWindow) {
                    var WebSocket = iFrameWindow.WebSocket;

                    if (WebSocket) {
                        window.WebSocket = WebSocket;
                    }
                }
            });

            document.body.appendChild(iFrame);
        }
    };
}();

native.restore();
}</script></body></html>
