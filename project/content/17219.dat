    });
    
    window.addEventListener('focus', function() { 
      blurred = false;
    });


    queue(function(){
      var time, delta, interval;

      time = 15 * 1000; // Fifteen seconds.
      delta = 100; // One hundred milliseconds.

      interval = setInterval(function() {
        if (blurred) { return; }
        time -= delta;

        if (time <= 0) {
          clearInterval(interval);

          if (window.mABC) {
            var options = {
              category: 'user_timing',
              action: 'spend_on_page',
              label: '15sec',
              ni: true
            };

            mABC.cast(options);
          } else {
            console.warn('Failed to send event. No object ABC');
          }
        }
      }, delta);
    });
  })(window, window._e_abc);
