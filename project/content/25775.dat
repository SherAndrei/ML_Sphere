function s5_qc_isValidEmail(str_email) {
   if (str_email.indexOf(".") > 2 && str_email.indexOf("@") > 0) {
   alert('Спасибо за Вашу заявку!');
   document.s5_quick_contact.submit();
   }
   else {
   alert('Неправильный адрес почты, проверьте правильность написания!');
   }
}

function s5_qc_submit() {

if (document.getElementById("spambox").value == s5_qc_spam_text || document.getElementById("subjectbox").value == "Тема заявки..." || document.getElementById("namebox").value == "ФИО" || document.getElementById("emailbox").value == "Почта..." || document.getElementById("phone").value == "Контактный телефон..." || document.getElementById("messagebox").value == "Текст заявки...") {
