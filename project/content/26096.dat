   function dfpSlotRenderEnded($banner) {
       $banner = $banner || document.querySelector('#div-inread');
       var inreadElement = document.querySelector('.__inread');
       var inreadButtonCloseElement = document.querySelector('.__inread-button.__inread-button--close');
       var $iframe = $banner.querySelector('iframe');
       var iframeWindow = $iframe.contentWindow;
       var toggleAnimationDuration = 600;
       var transition = 'height ' + toggleAnimationDuration + 'ms linear';
       var videoWidth = $banner.clientWidth;
       var videoHeight = $banner.clientWidth / (16/9) || 360;
       var iframeTop = 0;
       
       inreadButtonCloseElement.addEventListener('click', function() {
        inreadElement.style.display = 'none';
       });

      window.setTimeout(function() {
        document.querySelector('.__inread-header').style.display = 'table';
      }, 600);
     
       iframeTop = $iframe.getBoundingClientRect().top + window.pageYOffset;

       window.addEventListener('message', function(event) {
         if (typeof event.data !== 'string') return;
           if (event.data.indexOf('dfpinread') < 0) {
               return true;
           }

           var data;

           try {
               data = JSON.parse(event.data);
           } catch (e) {
               console.log(e);
           }

           if (typeof data === 'undefined' || !data.msgType) {
               return true;
           }

           if (data.msgType === 'dfpinread-started') {
               $banner.style.height = '0px';
               $banner.style.overflow = 'hidden';
               $banner.style.webkitTransition = transition;
               $banner.style.transition = transition;
               $banner.style.margin = '7px 0px'

               $iframe.style.width = videoWidth + 'px';
               $iframe.style.height = videoHeight + 'px';
               $iframe.style.webkitTransition = transition;
               $iframe.style.transition = transition;

               window.addEventListener('scroll', onScroll);
               onScroll();
           }

           if (data.msgType === 'dfpinread-ended') {
               $banner.style.height = '0px';
               $banner.style.margin = '0px';
           }

           if (data.msgType === 'dfpinread-video-not-found') {
               $banner.style.height = videoHeight + 'px';
               $banner.style.margin = '0px';
           }
       }, false);

       function onScroll() {
           if (window.pageYOffset + (window.innerHeight / 2) >= iframeTop) {
               window.removeEventListener('scroll', onScroll);
               $banner.style.height = videoHeight + 'px';               

               iframeWindow.postMessage(JSON.stringify({
                   msgType: 'dfpinread-video-visible'
               }), '*');
