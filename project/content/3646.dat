		}
		if(t==2){
			ge('likespan'+id).className='op50'
			//ge('dislikespan'+id).className=''
		}
        
        if(t==0) 
            $('.likespan'+id).addClass('op50');
		else if(t==1) 
            $('.likespan'+id).removeClass('op50');
		else if(t==2)
            $('.likespan'+id).addClass('op50');
}
