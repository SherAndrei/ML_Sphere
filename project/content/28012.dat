<script>
        var in_process = false;
        var page = 1;

        function get_next_items() {
            if (in_process) {
                return false;
            }

            page = page + 1;
            LastPage = page;

            url = "/local/templates/html_dev/components/db.by/main.news.list/ajax_list/include_component.php"

            in_process = true;
            $.ajax({
                type: "GET",
                dataType: "html",
                data: "dbAjaxNav=Y&PAGEN="+page+"&PARENT_SECTION=99",
                url: url,
                beforeSend: function(){
                    $('.pagin_button').hide();
                    $('.preloader-wrapper').addClass("active");
                },
                success: function( HTML ){
                    if(HTML)
                    {
                        $('.preloader-wrapper').removeClass("active");
                        $(HTML).insertAfter('div.dbAjaxnavItem:last');
                        $('.pagin_button').show();
                    }else{
                        $('.preloader-wrapper').removeClass("active");
                    }
                },
                error:  function(xhr, str) {
                    console.log("ERROR "+xhr.responseText);
                },
                complete: function(){
                    in_process = false;
                }
            });
        }

        var counterLoad = 0;
        function watchAjax() {
            if (counterLoad > 2 || in_process === true) {
                return false;
            }
            if (window.innerWidth > 1023) {
                var block = $('.ajax-news').find('.news-like').last();
                if (block.length) {
                    var bottom = window.innerHeight - block.outerHeight() - block[0].getBoundingClientRect().top;
                    if (bottom > 80) {
                        counterLoad += 1;
                        get_next_items();
                    }
                }
            }
        }
        $(window).on('scroll', function() {
            watchAjax();
        });
    </script>

