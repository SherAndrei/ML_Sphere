    var map;
    var markers=new Array();
    function initmap()
    {
        var mapOptions={center: new google.maps.LatLng(55.755826,37.6173),
        zoom: 10,
        mapTypeId: google.maps.MapTypeId.ROADMAP};
        map=new google.maps.Map(document.getElementById("map_google"),mapOptions);
        for(i=0;i<mapshops.length;i++)
        {
            var coords=new google.maps.LatLng(mapshops[i].lat,mapshops[i].long);
            var contentstring="<p><b>"+mapshops[i].name+"</b><br>Адрес: "+mapshops[i].addr;
            var infowindow = new google.maps.InfoWindow({content: contentstring, width: 500});
            markers[i]=new google.maps.Marker({position: coords, map: map, icon: "/img/map_ballon.png"});
            markers[i].shopid=i;
            markers[i].iw=infowindow;
            google.maps.event.addListener(markers[i], 'click', function() {
                var coords=new google.maps.LatLng(mapshops[this.shopid].lat,mapshops[this.shopid].long);
                map.setCenter(coords);
                map.setZoom(15);
                this.iw.open(map,this);
            });
        }
    }
    function movemap(shopid)
    {
        for(i=0;i<mapshops.length;i++)
        {
            if(mapshops[i].id==shopid)
            {
                var coords=new google.maps.LatLng(mapshops[i].lat,mapshops[i].long);
                map.setCenter(coords);
                map.setZoom(15);
                markers[i].iw.open(map,markers[i]);
                document.getElementById("map_google").scrollIntoView();
                break;
            }
        }
    }
    window.addEventListener("load",function(){ initmap(); },false);

    (function($){
        $('button[type=submit]', $('#form_registration')).on('click', function(event){
            event.preventDefault();

            var form = $('#form_registration'),
                fio = $('#registration_fio'),
                email = $('#registration_email'),
                company = $('#registration_company'),
                password = $('#registration_password'),
                error = 0;

            if(!fio.val().trim()){ fio.addClass('error'); error = 1; } else { fio.removeClass('error'); }
            if(!email.val().trim()){ email.addClass('error'); error = 1; } else { email.removeClass('error'); }
            if(!company.val().trim()){ company.addClass('error'); error = 1; } else { company.removeClass('error'); }
            if(!password.val().trim()){ password.addClass('error'); error = 1; } else { password.removeClass('error'); }

            if(error == 0){

                var sendData = form.serializeArray();
                sendData.push({'name': $('head meta[name="csrf-param"]').prop('content'), 'value': $('head meta[name="csrf-token"]').prop('content')});

                $.ajax({
                    url: '/office/registration/',
                    type: 'POST',
                    dataType: 'json',
                    data: sendData,
                    success: function(ret){
                        if(ret.error == 0){
                            form.html('<p>Благодарим за регистрацию. Вам необходимо подтвердить электронную почту, пройдя по ссылке из письма.</p>');
                        } else {
                            var pError = $('p.error', form),
                                mError = '<p class="error">При регистрации произошла ошибка. ' + ret.message + '</p>';
                            if(pError.length){
                                $('p.error', form).replaceWith(mError);
                            } else {
                                form.prepend(mError);
                            }
                        }
                    }
                });
            }
        });
    }(jQuery))
    
