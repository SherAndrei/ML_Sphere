	<script type='text/javascript'>
		
							$.fn.wait = function(time, type) {
								time = time || 100;
								type = type || "fx";
								return this.queue(type, function() {
									var self = this;
									setTimeout(function() {
										$(self).dequeue();
									}, time);
								});
							};
							function runIt() {
							  $(".blinke").wait()
									  .animate({"opacity": 0.1},500)
									  .wait()
									  .animate({"opacity": 1},300,runIt);
							}
							
						
						
		function get_auth(form) {
			$('#mail_error').html('');
			$('#password_error').html('');
			$('#ca_reglink').removeClass('blinke');
			$.get(

				'/engine/ajax/recept.php?action=auth&amp;'+$(form).serialize(),
				function(res) {
				
					if(res == -1) {
						$('#mail_error').html('адрес не найден в базе данных');
						$('#ca_reglink').addClass('blinke');
						runIt();
						 
					}
					else if(res == -2) {
						$('#password_error').html('пароль неверен');
					}
					else if(res == 1) {
						window.location.reload();
					}
				}
			);
		}
		
		function get_register(form) {
			$('#reg_name_error').html('');
			$('#reg_mail_error').html('');
			$('#reg_password_error').html('');
			$.get(
				'/engine/ajax/recept.php?action=register&amp;'+$(form).serialize(),
				function(res) {
				
					var response = $.parseJSON( res );
						$.each( response, function(key,val)  {
				
							if( val == -1 ) {
								$('#reg_mail_error').html('уже существует');
							}
							else if( val == -2 ) {
								$('#reg_mail_error').html('некорректный e-mail');
							}
							else if( val == -4 ) {
								$('#reg_mail_error').html('введите e-mail');
							}
							else if( val == -3 ) {
								$('#reg_name_error').html('введите имя');
							}
							else if( val == -5 ) {
								$('#reg_password_error').html('введите пароль');
							}
							else if( val == -6 ) {
								$('#reg_name_error').html('некорректное имя');
							}
							else if( val == 1 ) {
								window.location.reload();
							}
						});
				}
			
			);
		}
	</script>
	
	
