<script>
function send_error(that)
{
	$.post(that.attr('action'), that.serialize(), 
		function(d,t,j){
			if(d.status == 'error')
				alert(d.message)
			else {
				alert('Спасибо!');
				that[0].reset();
				$('#modal-errors').modal('hide');
			}
		},"json");

	return false;
}
</script>


