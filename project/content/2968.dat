<script>
    $(function() {
        $('#comment_form').submit(function() {
            var message = $('textarea').sceditor('instance').val();
            message = $('textarea').sceditor('instance').fromBBCode(message);

            $.ajax({
                url: '/comments/add',
                data: {
                    'target': $("#target").val(),
                    'target_id': $("#target_id").val(),
                    'username': $("#username").val(),
                    'message': message,
                    'captcha': $("#captcha").val()
                },
                type: "POST",
                dataType: "json",
                success: function(comment) {
                    $('#comments ul').append(comment.content).slideDown('slow');
                    $('textarea').sceditor('instance').val('');

                    $('#captcha_img').html(comment.captcha);
                    $('#captcha').val('');
                }
            })
            return false;
        })

        $("textarea").sceditor({
            plugins: "bbcode",
            style: "/km/editor/jquery.sceditor.default.min.css",
            emoticonsRoot: "/km/editor/",
            toolbar: "bold,italic,underline|maximize,source"
        });
    });
</script>
