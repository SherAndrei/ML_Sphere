function editPost(type, id) {
    // Форма редактирования
    $('#text-'+type+'-'+id+'-new').val($('#text-'+type+'-'+id).text());
    if (type == 'answer') {
        $('#container-'+type+'-'+id+'-edit').find('.inputs').trigger('keyup');
    }
    $('#container-'+type+'-'+id).hide();
    $('#container-'+type+'-'+id+'-edit').show();
}
// Отмена редактирования записи
function cancelEditPost(type, id) {
    $('#container-'+type+'-'+id+'-edit').hide();
    $('#text-'+type+'-'+id+'-new').empty();
    $('#container-'+type+'-'+id).show();
}
// Сохранение отредактированной записи
function saveEditPost(type, id, time) {
    // Отправляем post-запрос
    $.post('/ajax',{'type':'setEditPost','param':{'type':type,'id':id, 'time':time, 'text':$('#text-'+type+'-'+id+'-new').val()}},function(data){
        $('#container-'+type+'-'+id+'-edit').hide();
        $('#container-'+type+'-'+id).replaceWith(data);
        $('#container-'+type+'-'+id).show();
	}, "json");
}</script>
    
