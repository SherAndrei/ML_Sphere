// Центровка окна
function center_w(w, sp){
	dh = $(window).height();
	dw = $(window).width();
	wh = w.height();
	ww = w.width();
	wleft = Math.round((dw-ww)/2);
	wtop = Math.round((dh-wh)/2-150);
	if (wtop < 0) wtop = 10;
	w.animate({"left":wleft+"px","top":wtop+"px"}, sp);
}
