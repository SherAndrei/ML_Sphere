                    if ((date.getHours() >= 6 && date.getHours() < 18)) {
                        if (win <= 20) {
                            $(child).append('<div class="event"><a href="/pages/about#konkurs" class="event-day-second">'+rating+'*0.2='+ win.toFixed(1) +' руб.</a></div>');
                        } else {
                            $(child).append('<div class="event"><a href="/pages/about#konkurs" class="event-day-second">20 руб.</a></div>');
                        }
                    }
                }
            } else if (key == 2) {
                var win = rating  * 0.1;
                if (type == 'day') {
                    if (win <= 20) {
                        $(child).append('<div class="event"><a href="/pages/about#konkurs" class="event-day-third">'+rating+'*0.1='+ win.toFixed(1) +' руб.</a></div>');
