<script>

	var regValidator = $(".registration-main-form").formvalidate({
		rules: {
			"input[name=email]": { required: true, email: true },
			"input[name=firstname]": { required: true },
			"input[name=sex]": { required: true }
		},
		errmess: {
			"input[name=email]": { required: "Укажите e-mail" },
			"input[name=firstname]": { required: "Укажите имя" },
			"input[name=sex]": { required: "Укажите Ваш пол" }
		},
		keyup: true,
		invalidkeyup: false,
		submitHandler: function(form) {

			var $form = $(form), $button = $form.find("input[type=submit]"), $panel = $(".submit-panel", $form), $agr = $("input[name=user_agreement]", $form.parents(".iform-auth"));

			if ($agr.length && !$agr.prop("checked")) {
				if (!confirm("Вы даете добровольное согласие на обработку своих персональных данных?"))
					return false;
				$agr.click();
			}

			$button.attr("disabled", true).addClass("btn-disabled").loadingTxt({txt: "Идет регистрация"});

			$panel.addClass('lab-loader');

			JsHttpRequest.query("/ajax.php", {
				cl_name: "cabinet",
				me_name: "registration",
				firstname: form.firstname.value,
				email: form.email.value,
				sex: $form.find("[name=sex]:checked").val()
			}, function(result, errors) {
				if (result && result.errors != undefined) {
