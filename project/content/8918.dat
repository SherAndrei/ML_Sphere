<script>
	jQuery(document).ready(function(){
		jQuery(document).on('submit','#simple-subscribe-form', function(){
			var $form = jQuery(this);
			var arFields = $form.serializeArray();
			var request = {
				action : 'subscribe_weekly',
			};
			var servicesURL = '/common/subscribe/services.php';
			jQuery.each(arFields, function(index, arField){
				request[arField.name] = arField.value;
			});
			jQuery.post(servicesURL, request, function(response){
				var res = jQuery.parseJSON(response);
				var content = '';
				switch(res.status){
					case 'subscribed':
						content = 'Вы успешно подписаны!';
					break;
					case 'cleaned':
						content = 'Извините, вы не можете подписаться на эту рассылку.';
					break;
					case 'exists':
						content = 'Вы уже подписаны на эту рассылку.';
					break;
				}
				var $input = $form.find('[name="email"]');
				$input.popover({
					//title : 'Подписка',
					placement: 'top',
					content : content,
				});
				$input.popover('show');
				setTimeout(function(){
					$input.popover('hide');
					$input.popover('destroy');
				},5000);
			})
			return false;
		});
	});
</script>
