.tab_bar {
	background-color: #e4ebf2;
	color: #4a6784;
}

	.tab_bar li.active {
		background-color: #243f5c;
		color: #fff;
	}
	
	.tab_bar.no_title.mini {
		border-bottom: 8px solid #243f5c;
	}

/* Menu popups */
.ipbmenu_content, .ipb_autocomplete {
	background-color: #f7f9fb;
	border: 1px solid #d5dde5;
