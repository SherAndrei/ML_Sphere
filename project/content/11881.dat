                            '<tr><th>Пн</th><th>Вт</th><th>Ср</th><th>Чт</th><th>Пт</th><th>Сб</th><th>Вс</th></tr>'+
                            '<tr>';
                    if ( weekday ) str +=
                                '<td colspan="'+weekday+'">&nbsp;</td>';
                    for ( var i = 1; i <= day_count; i++ )
                    {
                        weekday++;
                        str +=
                                '<td>';
                        if ( data.data.days[i] != undefined ) str +=
                                    '<a href="http://courier.crimea.ua/calendar/'+year+'-'+(month > 9 ? month : '0'+month)+'-'+(i > 9 ? i : '0'+i)+'.html">'+i+'</a>';
                        else str +=
                                    i;
                        str +=
                                '</td>';
                        if ( weekday == 7 )
                        {
                            weekday   = 0;
                            str +=
                            '</tr><tr>';
                        }
                    }
                    if ( weekday != 7 ) str +=
                                '<td colspan="'+(7-weekday)+'">&nbsp;</td>';
                    str +=
                            '</tr>';
                    $('#cal_body').empty();
                    $('#cal_body').append(str);
                }
                cal_disabled = false;
            },
            'json'
        );
        return false;
    }

    function AjaxCalendarYear(side)
    {
        year = side ? ++year : --year;
        AjaxCalendar();
    }

    function AjaxCalendarMonth(m)
    {
        document.getElementById('month_'+month).className = '';
        month = m;
        document.getElementById('month_'+month).className = 'active';
        AjaxCalendar();
    }

    AjaxCalendar();
</script>

