<script type="text/javascript">

$(function(){
    function resetInfoForm() {
        $(".info-type").attr('checked', '');
        $("#addinfo").val('');
    }
    var iform = $('#info_form');
    $('.info-edit-link').click(function(){
        resetInfoForm(); 
        var type   = $(this).attr('data-type');
        var id = $(this).attr('data-id');
        $.get(dle_root + 'engine/ajax/post_info.php', {
            id     : id,
            action : 'get'
        }, function(result) {
            $(".info-type[value='"+result.type+"']").attr('checked', 'checked'); 
            $("#addinfo").val(result.content);
            var dialog = $("#info_form_wrapper").dialog({
                title : 'Изменение дополнительной информации о фильме',
                width : '500px'
            });
            $('#info_form').unbind('submit').submit(function(){
                var f_data  = $(this).serializeArray();
                var data    = {};
                for(i in f_data) {
                    data[f_data[i].name] = f_data[i].value;
                }
                if(!data.addinfo) {
                    alert("Вы не заполнили текстовое поле.");
                    return false;
                }
                $.post(dle_root + 'engine/ajax/post_info.php', {
                    action  : 'edit',
                    id      : id,
                    type    : data.type,
                    content : data.addinfo
                }, function(result) {
                    if(result) {
                       alert("Произошла ошибка. Сервер вернул неожиданный результат."); 
                    }
                    window.location.reload();
                });
                return false;
            });
        });
        return false;
    });
    $('.info-remove-link').click(function(){
        var id = $(this).attr('data-id');
        DLEconfirm("Вы действительно хотите удалить эту запись? Действие необратимо.", "Подтверждение удаления", function(){
            $.post(dle_root + 'engine/ajax/post_info.php', {
                id     : id,
                action : 'remove'
            }, function(result) {
                if(result) {
                    alert("Произошла ошибка. Сервер вернул неожиданный результат."); 
                }
                window.location.reload();
            });
        });
        return false;
    });
    $('.info-add-link').click(function(){
        resetInfoForm(); 
        var type   = $(this).attr('data-type');
        var newsid = $(this).attr('data-newsid');
        $(".info-type[value='"+type+"']").attr('checked', 'checked'); //.prop('checked', true);
        var dialog = $("#info_form_wrapper").dialog({
            title : 'Добавление дополнительной информации о фильме',
            width : '500px'
        });
        
        $('#info_form').unbind('submit').submit(function(){
            var f_data  = $(this).serializeArray();
            var data    = {};
            for(i in f_data) {
                data[f_data[i].name] = f_data[i].value;
            }
            if(!data.addinfo) {
                alert("Вы не заполнили текстовое поле.");
                return false;
            }
            $.post(dle_root + 'engine/ajax/post_info.php', {
                action  : 'add',
                newsid  : newsid,
                type    : data.type,
                content : data.addinfo
            }, function(result) {
                if(result) {
                    alert("Произошла ошибка. Сервер вернул неожиданный результат."); 
                }
                window.location.reload();
            });
            return false;
        });
        return false;
    });
});
</script>
	
    

