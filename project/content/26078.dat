{
var ves=document.getElementById("ves").value;
var rost=document.getElementById("rost").value;
var ind=ves/((rost/100)*(rost/100));
var t_ind=round(ind,2);
var res=document.getElementById("result");
res.innerHTML=t_ind;

}

</script>
