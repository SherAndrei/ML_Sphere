                var X1 = Xpage,
                    X2 = Xwindow,
                    Y1 = Ypage,
                    Y2 = Ywindow;

                if ($('.post-hint[data="'+dataId+'"]').length < 1)
                {
                    setTimeout(function () {
                        $('.fixed-hint').removeClass('hidden');

                        if (X2 > 0.5 * $(window).width())
                        {
                            $('.fixed-hint').addClass('right-hint');
