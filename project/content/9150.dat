				$(".pass-box #user-pass").attr("action", "/user/password?registration=1");
			
			// Подстановка верстки для кастомного input - file
				$("#edit-picture-upload-register-wrapper .description").remove();
				$("#edit-picture-upload-register-wrapper label").remove();
