					if(".registration-box:visible"){
						$(".registration-box").slideUp();
					}
					$(".pass-box").slideToggle();
					return false;
				})
				$("#user-login-form .item-list .first").click(function(){
					if(".pass-box:visible"){
						$(".pass-box").slideUp();
					}
					$(".registration-box").slideToggle();
					return false;
				})
			
			// Смена атрибута action у формы регистриции
				$("#user-register").attr("action", "/user/register?destination=user&registration=1");
