    <script>
        $(function(){
            $('.b-fixed_top').hide();

            $('#subs_form').submit(function(){
                var form = $(this);
                var url = form.attr('action');
                var error = $('.b-fixed_top');
                form.fadeTo('fast', 0.5);
                form.ajaxSubmit({
                    url:url,
                    dataType: 'json',
                    type:'POST',
                    success: function (data) {
                        if(!data.success) {
                            form.find('#email').addClass('f-error');
                            form.fadeTo('fast', 1);

                            error.show();
                            $('.b-msg__text').html( data.errors.join('<br/>') );
                        } else {
