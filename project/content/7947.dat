<br />&#1052;&#1085;&#1086;&#1078;&#1077;&#1089;&#1090;&#1074;&#1086; &#1088;&#1072;&#1079; &#1073;&#1083;&#1080;&#1079;&#1082;&#1080;&#1077; &mdash; &#1084;&#1091;&#1078;&#1095;&#1080;&#1085;&#1099; &#1080; &#1078;&#1077;&#1085;&#1097;&#1080;&#1085;&#1099; &mdash; &#1086;&#1078;&#1080;&#1076;&#1072;&#1083;&#1080; &#1086;&#1090; &#1084;&#1077;&#1085;&#1103; &#1076;&#1086;&#1082;&#1072;&#1079;&#1072;&#1090;&#1077;&#1083;&#1100;&#1089;&#1090;&#1074; &#1089;&#1091;&#1097;&#1077;&#1089;&#1090;&#1074;&#1086;&#1074;&#1072;&#1085;&#1080;&#1103; &#1089;&#1077;&#1082;&#1089;&#1080;&#1079;&#1084;&#1072;, &#1085;&#1086; &#1085;&#1080;&#1082;&#1086;&#1075;&#1076;&#1072; &#1085;&#1077; &#1086;&#1078;&#1080;&#1076;&#1072;&#1083;&#1080; &#1076;&#1086;&#1082;&#1072;&#1079;&#1072;&#1090;&#1077;&#1083;&#1100;&#1089;&#1090;&#1074; &#1088;&#1072;&#1089;&#1080;&#1079;&#1084;&#1072; (&#1088;&#1072;&#1079;&#1091;&#1084;&#1077;&#1077;&#1090;&#1089;&#1103;, &#1085;&#1072;&#1081;&#1076;&#1091;&#1090;&#1089;&#1103; &#1090;&#1077;, &#1082;&#1086;&#1084;&#1091; &#1085;&#1091;&#1078;&#1085;&#1099; &#1080; &#1090;&#1072;&#1082;&#1080;&#1077; &#1076;&#1086;&#1082;&#1072;&#1079;&#1072;&#1090;&#1077;&#1083;&#1100;&#1089;&#1090;&#1074;&#1072; &mdash; &#1085;&#1086; &#1085;&#1077; &#1074; &#1084;&#1086;&#1077;&#1084; &#1082;&#1088;&#1091;&#1075;&#1091; &#1086;&#1073;&#1097;&#1077;&#1085;&#1080;&#1103;). &#1052;&#1085;&#1086;&#1078;&#1077;&#1089;&#1090;&#1074;&#1086; &#1088;&#1072;&#1079; &#1083;&#1102;&#1076;&#1080; &#1074;&#1086;&#1082;&#1088;&#1091;&#1075; &#1084;&#1077;&#1085;&#1103; &#1076;&#1077;&#1083;&#1072;&#1083;&#1080; &#1074;&#1080;&#1076;, &#1095;&#1090;&#1086; &#1089;&#1077;&#1082;&#1089;&#1080;&#1089;&#1090;&#1089;&#1082;&#1072;&#1103; &#1089;&#1080;&#1090;&#1091;&#1072;&#1094;&#1080;&#1103; &mdash; &#1089;&#1086;&#1074;&#1077;&#1088;&#1096;&#1077;&#1085;&#1085;&#1086; &#1085;&#1086;&#1088;&#1084;&#1072;&#1083;&#1100;&#1085;&#1086;&#1077; &#1076;&#1077;&#1083;&#1086;.
