<script type="text/javascript">     
$(document).ready(function(){ 
    $("#SupEmail").attr("placeholder","name@email.com");
    $("#TrNasPunkt").hide();
    $("#SupBtn").attr("disabled","disabled");
    $("#SupSubmit").change(function(){
        if (!$(this).hasClass("checked")) {
            $("#SupBtn").removeAttr("disabled");
            $(this).addClass("checked");
        } else {
            $("#SupBtn").attr("disabled","disabled");
            $(this).removeClass("checked");
        }
    });

    $("input.tipOtcheta").change(function(){

        if ($("input[name='tipOtcheta']:checked").val() == 1) {
            $("#TrEmail").show();
            $("#SupEmail").addClass("obyaz");
        } else {
            $("#TrEmail").hide();
            $("#SupEmail").removeClass("obyaz");

        }
    });

    $("#SupRayon").change(function(){
        if ($(this).val() > 2) {
            $("#TrNasPunkt").show();
            $("#SupNasPunkt").addClass("obyaz");
        } else {
            $("#TrNasPunkt").hide();
            $("#SupNasPunkt").removeClass("obyaz");

        }
    });


$("#support").submit(function(){

    $("#SupBtn").attr('disabled', 'disabled');
    $(".obyaz").removeClass("SupError");
    var total = ($("#SupRayon").val()>2)? 7:8;
    total = ($("input[name='tipOtcheta']:checked").val() == 2)? total-1:total;
    $(".obyaz").each(function(i, val){
        if ($(val).val() == "") {
            $(val).addClass("SupError");

        }
    });

    if (0 < ($(".obyaz.SupError").length-6)) {
        $("#SupBtn").removeAttr('disabled');
        return false;
    }
    $(".obyaz").removeClass("SupError");
    $("#SupIframe").location = $("#SupIframe").attr("src");

        var myForm = {
        familiya: $("#SupFamiliya").val(),
        imya: $("#SupImya").val(),
        otchestvo: $("#SupOtchestvo").val(),
        type: $("input[name='tipOtcheta']:checked").val(),
        index: $("#SupIndex").val(),
        rayon: $("#SupRayon").val(),
        naspunkt: $("#SupNasPunkt").val(),
        adres: $("#SupAdres").val(),
        mail: $("#SupEmail").val(),
        telefon: $("#SupTelefon").val(),
        text: $("#SupText").val(),
        upfile: $("#SupUpFile").val()
    };

    $.ajax({
        type: "POST",
        url: "support/go.php",
        data: myForm,
        success: function() {
            $("#quest").hide();
            $("#answ").show();
        }
    });
    $("#SupBtn").removeAttr('disabled');
    return false;
});});
</script>
