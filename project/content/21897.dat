    <script type="text/javascript">
        var winTop = document.getElementById("winDownloadButtonsTop");
        var winBottom = document.getElementById("winDownloadButtonsBottom");
        var macTop = document.getElementById("macDownloadButtonsTop");
        var macBottom = document.getElementById("macDownloadButtonsBottom");

        function getParameterByName(name, url) {
            if (!url) {
                url = window.location.href;
            }
            name = name.replace(/[\[\]]/g, "\\$&");
            var regex = new RegExp("[?&]" + name + "(=([^&#]*)|&|#|$)"),
                results = regex.exec(url);
            if (!results) return null;
            if (!results[2]) return '';
            return decodeURIComponent(results[2].replace(/\+/g, " "));
        }

        function showMac() {
            winTop.style.display = "none";
            winBottom.style.display = "none";

            macTop.style.display = "block";
            macBottom.style.display = "block";
        }

        function showWin() {
            winTop.style.display = "block";
            winBottom.style.display = "block";

            macTop.style.display = "none";
            macBottom.style.display = "none";
        }

        if (getParameterByName("osType", document.location.href) == "mac"
            || navigator.userAgent.indexOf("Mac OS X") != -1) {
            showMac();
        }

    </script>

