<div class="objects" ng-controller="commentaryCtrl">
    
    
        <div id="parent_template" ng-non-bindable>
            <div class="item dynamic-item level-[[comment.depth ]]" ng-cloak
    
        ng-repeat="comment in commentaries['1070044689643869605']['parent_id_for_replace'] track by comment.id"
    

    on-finish-render 
    ng-class="{
        'implode-wrapper': imploded[comment.id]
        
        , 'is-active': activeComment == comment.id}">

    <div id="comment-stub" ng-class="{'imploded-stub': imploded[comment.id]}" ng-click="imploded[comment.id] = false"></div>

    <div class="b-comment" 
        ng-class="{'b-comment--imploded': imploded[comment.id]}"
        id="[[comment.id]]" 
        data-path="[[comment.path]]"
        data-user='[[ formatUserName(comment.user, {"truncation": false, "onlyFirstName": true}) ]]'
        ng-init="imploded[comment.id] = (comment.vote_value == '-' || comment.mark_curval < -5 || comment.mark_spam > 5 || comment.user_mark_as_spam);">

        <IDXSKIP><div class="b-user__avatar">
            <img ng-src='[[::comment.user.avatar_url]]' width="60" height="60"/>
        </div></IDXSKIP>

        <header class="b-comment__info">
            <figure class="b-user b-user--full">
                <a ng-href="[[::comment.user.profile_url]]" target="_self">
                    <b class="b-user__name" ng-bind="::formatUserName(comment.user, {'max_length': activeOrder == 'tree' ? 26 - (comment.depth - 1) : 26})"></b>
                </a>
                <span class="b-user__rank b-user__rank--inline" ng-bind="::comment.badge"></span>

                <span class="b-comment__addressee"
                      ng-if="parent_id_for_replace && comment.parent_comment.user_name"
                      ng-click="scrollToCommentMethod('parent_id_for_replace')"
                      ng-attr-title="Ответ пользователю [[comment.parent_comment.user_name]]">
                      <u ng-bind="::comment.parent_comment.user_name"></u>
                </span>
            </figure>
            <span class="b-comment__date" title="[[::comment.formatted_ctime]]" ng-bind="::comment.humanized_ctime"></span>
        </header>

        <IDXSKIP><div class="b-comment__placeholder">
            <p class="b-comment__placeholder-reason" ng-bind="imploded[comment.id]"></p>
            <span class="is-clickable">Показать комментарий</span>
        </div></IDXSKIP>

        <div class="b-comment__text">
            <p ng-bind-html="::escapeText(comment.text, {withEmoticons: true, formatLines: true, replaceWithLinks: true})"></p>
        </div>

        <IDXSKIP><div class="b-attachments b-attachments--comment"
             ng-init="galleriesIndexes[comment.id] = 0"
             thumbnails rel="[[comment.id]]" ng-if="comment.image_list">
            <span class="b-attachments__control b-attachments__control--prev is-disabled"
                ng-click="galleriesIndexes[comment.id] = galleriesIndexes[comment.id] + 1"
                ng-class="{'is-disabled': galleriesIndexes[comment.id] == 0 }"></span>

            <span class="b-attachments__control b-attachments__control--next"
                ng-click="galleriesIndexes[comment.id] = galleriesIndexes[comment.id] - 1"
                ng-class="{'is-disabled': galleriesIndexes[comment.id] == images_per_page - getLength(comment.image_list) || getLength(comment.image_list) <= images_per_page }"></span>

            <div class="b-attachments__shaft" lightboxed-images
                ng-class="{'is-clipped': galleriesIndexes[comment.id] !== images_per_page - getLength(comment.image_list) && getLength(comment.image_list) > images_per_page }">
                <div class="b-attachments__item" 
                    ng-repeat='image in comment.image_list track by $index'
                    ng-style="{'transform': 'translateX(' + ( galleriesIndexes[comment.id]*100 + $index * 100 ) + '%)'}">
                    <a class="b-attachments__img" ng-href="[[::image]]" target="_blank"
                        ng-style="::getImageStyle(comment)">
                        <img ng-src="[[::wrap_maxsz(image)]]" alt=""  unselectable="on">
                    </a>
                </div>
            </div>
