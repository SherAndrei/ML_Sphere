    <script type="text/ng-template" id="/notifications/captcha.html"
            ng-init="$root.captchaShow = false; $root.captchaError = false">
        <div class="b-modal-aligner ng-cloak ng-hide" ng-cloak ng-show="$root.captchaShow == true">
            <div class="b-modal b-modal--captcha" ng-class="{'captcha-error': $root.captchaError == true}">
                <figure class="b-modal__holder">
                    <div class="b-modal__content">
                        <p class="b-modal__text">В целях безопасности нам необходимо убедиться, что действия совершает обычный человек.</p>

                        <div class="b-captcha__error with-border" ng-show="$root.captchaError == true">
                            <p class="b-captcha__error-text"
                                ng-init="$root.captchaErrorMsg='Ой! Вы ошиблись при выборе изображения.'"
                                ng-bind-html="trustAsHtml($root.captchaErrorMsg)">
                            </p>
                        </div>

                        <label class="b-modal__text" ng-bind="$root.captchaData.question"></label>

                        <ul class="b-captcha">
                            <li class="b-captcha__item" ng-repeat="el in $root.captchaData.values track by $index">
                                <input  class="b-captcha__input" id="captcha_[[ $index ]]" name="[[ $root.captchaData.imageFieldName ]]" value="[[ el ]]" type="radio"/>
                                <label class="b-captcha__label" for="captcha_[[ $index ]]">
                                    <img ng-src="/captcha/image/[[ $index ]]/?r=[[ el ]]"/>
                                </label>
                            </li>
                        </ul>
                    </div>
                    <div class="b-modal__footer">
                        <span class="btn btn--white" ng-click="hideCaptcha()">Отменить</span>
                        <button class="btn" ng-click="$root.captcha.successCallback()">Продолжить</button>
                    </div>
                </figure>
                <div class="b-modal__bg ng-cloak" ng-click="hideCaptcha()"></div>
            </div>
        </div>
    </script>

