<script type="text/javascript">
(function(){
    var elems = $('#link_social a');

    elems
        .on('mouseenter', function(){                 
            var t = $(this),
                s = t.siblings(),
                i = 0;             

            t
                .stop()
                    .animate({
                        width: 120,
                        left: t.index() * 36
                    }, 50)
                    .find('.label')
                        .show();

            for (i; i < s.length; i++) {
                var e = $(s[i]);

                e.stop().animate({
                    left: e.index() <= t.index() ? e.index() * 36 : e.index() * 36 + 120 - 36,
                    width: 36
                }, 50);               
            };
        })
        .on('mouseleave', function(){
            var t = $(this),
                s = t.siblings().andSelf();

            for(var i = 0; s.length > i; i++) {
                var e = $(s[i]);

                e.animate({
                    width: 50,
                    left: e.index() * 50
                }, 50);       
            };

            t
                .find('.label')
                    .fadeOut(30);
        })
})();
</script>

