<script>
	$(document).ready(function() {
		$('.productFixBlock').each(function() {
			
			function getFloatCssAttr(item, attr) {
				var attr = item.css(attr).replace(/[^0-9\.,]/g, '')
				if(attr == '') {
					return 0;
				}
				return parseFloat(attr);
			}
			
			function getItemHeight(item) {
				return parseFloat(item.height()) 
							+ getFloatCssAttr(item, 'padding-top')
							+ getFloatCssAttr(item, 'padding-bottom')
							+ getFloatCssAttr(item, 'margin-top')
							+ getFloatCssAttr(item, 'margin-bottom');
			}
			
			var carouselloItemsShow = 5
			
			if(parseFloat($(window).height()) < 768) {
				carouselloItemsShow = 3;
			}
			
			var carousello = $(this);
			var carouselloContainer = carousello.find('.bg');
			var carouselloApplied = false;
			var carouselloUp = carouselloContainer.find('.up');
			var carouselloDown = carouselloContainer.find('.down');
			var carouselloImages = carouselloContainer.find('.block > div');
			var carouselloItems = carouselloImages.find('a');
			var carouselloItemsSizes = [];
			var carouselloItemsCount = carouselloImages.find('a').length;
			var carouselloDelay = 200;
			var carouselliOnAction = false;
			var carouselloSlideDownSize = 0;
			var carouselloLastItemCount = 0;
			
			carouselloItems.each(function(i) {
				carouselloItemsSizes.push(getItemHeight($(this)));
				if(i < carouselloItemsShow) {
					carouselloSlideDownSize += carouselloItemsSizes[i];
					carouselloLastItemCount = i;
				}
			});
			carouselloContainer.find('.block').height(carouselloSlideDownSize);
			
			// раскрытие/сокрытие карусельки
			carousello.find('.button > a').each(function() {
				$(this).attr('href', 'javascript: void(0);').click(function() {
					if(carouselloApplied) {
						carouselloContainer.animate(
							{
								'height' : 0,
								'padding-top' : 0
							},
							200
						);
						carouselloApplied = false;
					} else {
						carouselloContainer.animate(
							{
								'height' : carouselloSlideDownSize + 50,
								'padding-top' : 45
							},
							200
						);
						carouselloApplied = true;
					}
				})
			});
			
			// скроллинг карусельки
			carouselloUp.attr('href', 'javascript: void(0);').click(function() {
				var carouselloNextItem = carouselloLastItemCount - (carouselloItemsShow - 1) - 1;
				if(carouselloItemsSizes[carouselloNextItem] != undefined) {
					if(carouselloApplied && !carouselliOnAction) {
						carouselliOnAction = true;
						var top = 0;
						for(var i = 0; i <= carouselloNextItem - 1; i++) {
							top -= carouselloItemsSizes[i];
							
						}
						carouselloImages.animate(
							{
								'top' : top
							}, 
							carouselloDelay,
							function() {
								carouselliOnAction = false;
								carouselloLastItemCount = carouselloNextItem + (carouselloItemsShow - 1);
							}
						);
					}
				}
			})
			
			// скроллинг карусельки
			carouselloDown.attr('href', 'javascript: void(0);').click(function() {
				var carouselloNextItem = carouselloLastItemCount + 1;
				if(carouselloItemsSizes[carouselloNextItem] != undefined) {
					if(carouselloApplied && !carouselliOnAction) {
						carouselliOnAction = true;	
						var top = 0;
						for(var i = 0; i <= carouselloNextItem; i++) {
							top -= carouselloItemsSizes[i];
							
						}
						top += carouselloSlideDownSize;
						carouselloImages.animate(
							{
								'top' : top
							}, 
							carouselloDelay,
							function() {
								carouselliOnAction = false;
								carouselloLastItemCount = carouselloNextItem;
							}
						);
					}
				}
			})
			
		});
	});
