            e.preventDefault();
            $("[data-js=prescription_list]").append('<li><dl><dt><span class="label">руб. | коп. | Rp.</span></dt><dd><div class="input"><input type="text" value="" name="prescription_item[]"><span class="delete_item" data-js="prescription_delete"></span><span class="search_item" data-js="prescription_search"></span></div></dd></dl><div class="drug-list"></div></li>');
