        $(document).on("click", "[data-js=prescription_delete]", function(e) {
            e.preventDefault();
            if($("[data-js=prescription_list] li").length == 1)
                return;
            $(this).closest("li").remove();
        });
        
        $(document).on("click", "[data-js=prescription_list] .drug-item", function(e) {
            e.preventDefault();
            $(this).closest("li").find("input[name^=prescription_item]").val($(this).find(".name").html());
            $(this).closest(".drug-list").empty();
        });
        
        $(document).on("click", "[data-js=prescription_search]", function(e) {
            e.preventDefault();
            var request = $(this).siblings("input").val();
            var drug_list = $(this).closest("li").find(".drug-list");
            drug_list.empty();
            $.ajax({
                url: "/medicament/molecules",
                dataType: "json",
                data: {
                    term: request
                },
                success: function(data) {
                    $.each(data, function(i, item){
                        drug_list.append('<div class="drug-item"><div class="title"><span class="name">' + item.name + '</span></div><div class="description">' + item.description + '</div></div>');
                    });
                }
            });
        });
        
 
