        $.datepicker.setDefaults($.datepicker.regional["ru"]);
        $('#prescription_date').datepicker({
            changeYear: false,
            yearRange: '1950:2025',
            beforeShow: function(input) {
                $(input).css("background-color","#e9f5f6");
            },
            onSelect: function(dateText, inst) {
                $(this).css("background-color","");
            },
            onClose: function(dateText, inst) {
                $(this).css("background-color","");
            }
        });

        $(document).on("prescription_popup", function(e){
            var c = $('#prescription_form');
            $("*[data-js=prescription_list] li").each(function(){
                if ($(this).index() > 0)
                {
                    $(this).remove();
                }
                else
                {
                    $("input", $(this)).val("");
                }
            });
            $("*[data-ref_molecule]").each(function(){
                if ($("[data-js=prescription_list] li").length == 1 && $("input", $("[data-js=prescription_list] li")).val() == "")
                {
                    $("input", $("[data-js=prescription_list] li")).val($(this).html());
                }
                else
                {
                    $("[data-js=prescription_list]").append('<li><dl><dt><span class="label">руб. | коп. | Rp.</span></dt><dd><div class="input"><input type="text" value="'+$(this).html()+'" name="prescription_item[]"><span class="delete_item" data-js="prescription_delete"></span><span class="search_item" data-js="prescription_search"></span></div></dd></dl><div class="drug-list"></div></li>');
                }
            })
        });

