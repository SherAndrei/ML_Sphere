        <script type="text/template" id="service-tree">
    <% for (var i = 0; i < tree.length; ++i) { %>
        <% var root_node = tree[i]; var last_iteration = i == tree.length - 1; %>
        <% if (i == leftMenuHideAfter) { %>
            <div class="rubrics-scroll-block">
            <div class="rubrics-scroll-hidden-rubrics">
        <% } %>
        <% if (root_node[itemCountFieldName] > 0) { %>
            <li class="menu_item_level_1 offer_menu_item<% if (last_iteration) { %> border_bottom<% } %>"
                service_id="<%= root_node.service_type %>">

                <span class="item_main_menu<% if (root_node.selected) { %> active_menu_item<% } %>">
                    <%= root_node.name %>
                </span>

                <ul class="submenu_items_open"<% if (root_node.selected) { %> selected="selected"<% } %>
                    style="display: none;">
                    <% for (var j = 0; j < root_node.children.length; ++j) { %>
                        <% var item_level2 = root_node.children[j]; %>
                        <% if (item_level2[itemCountFieldName] > 0) { %>
                            <li class="menu_item_level_2" service_id="<%= item_level2.service_type %>">
                                <% if (isSpecialistsCatalog) { %>
                                    <a href="/tseny-<%= item_level2[serviceUrlFieldName] %>">
                                <% } else { %>
                                    <a href="/<%= item_level2[serviceUrlFieldName] %>">
                                <% } %>
                                    <span class="submenu_item_menu_left<% if (item_level2.selected) { %> active-submenu-item<% } %>">
                                        <%= item_level2.name %>
                                    </span>
                                    <span class="number_services">
                                        <%= item_level2[itemCountFieldName] %>
                                    </span>
                                </a>
                            </li>
                        <% } %>
                    <% } %>
                </ul>
            </li>
        <% } %>
        <% if (last_iteration && tree.length > 10) { %>
            </div>
                <div class="rubrics-scroll-header rubrics-scroll-show-action">
                    <span class="arrow down"></span>
                </div>
                <div class="rubrics-scroll-header rubrics-scroll-hide-action">
                    <span class="arrow up"></span>
                </div>
            </div>
        <% } %>
    <% } %>
</script>

<script type="text/template" id="offers-item">
    <div class="offer <% if (has_light) { %>backlight<% } %><% if (is_master) { %> master<% } %>"
         user_id="<%= author__id %>"
         is_profi_ru_specialist="<%= author__is_profi_ru_specialist %>">
        <div class="offer__header">
            <a href="<%= author__url %>">
                <img class="offer__avatar" src="<%= author__logo %>" width="70" height="70"/>
            </a>
            <div class="offer__right">
                <div class="offer__offer-order-button input button orange">Предложить заказ</div>
                <% if (claims_is_done > 0) { %>
                <div class="offer__claims_is_done">
                    Выполнено заказов: <%= claims_is_done %>
                </div>
                <% } %>
            </div>
            <div class="offer__info">
                <div class="offer__first-info-block">
                    <% if (is_master) { %><span class="ico icon_master"><span></span></span><% } %>
                    <% if (author__verify) { %>
                    <div class="b-verified">
                        <div class="b-verified__check"></div>
                        <span class="b-verified__text b-verified_margin_left-m3-right-13">Данные подтверждены</span>
                        <% if (author__individual) { %>
                        <div class="help-hint">ФИО и фото подтверждены документом (<%= author__verify %>)</div>
                        <% } else { %>
                        <div class="help-hint">Данные организации подтверждены документом (<%= author__verify %>)</div>
                        <% } %>
                    </div>
                    <% } %>
                </div>
                <div class="offer__second-info-block">
                    <a href="<%= author__url %>" class="offer__name"><%= author__name %></a>
                    <% if (author__last_visit != 'online') { %>
                    <div class="offer__last-visit">Был на сайте: <%= author__last_visit %></div>
                    <% } else { %>
                    <div class="offer__status icon-online" title="Сейчас на сайте"></div>
                    <% } %>
                </div>
                <div class="offer__third-info-block">
                    <% if (page_data.city_domain != 'novokuznetsk') { %>
                    <span class="offer__digit-rating">Рейтинг: <b><%= star_rating_count.toFixed(1) %></b></span>
                    <div class="b-stars-rating">
                        <div class="b-stars-rating__stars" style="width: <%= star_rating_percent %>%;"></div>
                    </div>
                    <% } %>
                    <% if (review_count_safe > 0) { %>
                    <div class="offer__reply-count">Отзывов: <%= review_count_safe %></div>
                    <% } %>
                </div>
                <% if (is_master) { %>
                <% if (author__phone_formatted) { %>
                <span class="info_user_phone">
                <span>Телефон: <%= author__phone_formatted['masked'].slice(0, 6) %></span>
                <span class="pseudolink info_user_phone__show-feedback-form"
                      data-touserid="<%= author__id %>">
                XXX XX XX
                </span>
                </span>
                <% } %>
                <% } %>
            </div>
            <div class="clear"></div>
        </div>
        <div class="offer__services-info">
            <p class="offer__description">
            <pre class="offer__description-text"><% print(description ? description.slice(0, 200) : ''); %></pre>
            </p>
            <ul class="offer__pricelist">
                <% for (var i = 0; i < offer_cost.length; ++i) { %>
                <% var cost = offer_cost[i]; %>
                <li class="offer__service">
                    <span class="offer__service-name"><%= cost.name %></span>
                    <% if (cost.cost) { %>
                    <span class="offer__service-price offer__service-price_type_specified">от <%= cost.cost %> р/<%= cost.measure %></span>
                    <% } else { %>
                    <span class="offer__service-price offer__service-price_type_contract">цена договорная</span>
                    <% } %>
                    <div class="offer__dots"></div>
                </li>
                <% } %>
            </ul>
            <div class="offer__footer-links">
                <a class="offer__link"
                   href="/users/<%= author__id %>/uslugi/<% if (service_type_translit_name) { %>#<%= service_type_translit_name %><% } %>">
                    Смотреть подробнее>>
                </a>
            </div>
        </div>

        <% if (works_exists) { %>
        <div class="offer__images">
            <% for (var i = 0; i < works_images.length; ++i) { %>
            <% var img = works_images[i]; %>
            <% if (i == works_images.length - 1 && works_images.length >= 5) { %>
            <a class="offer__images__item" href="/users/<%= author__id %>/portfolio/">
                <div class="offer__images__item__show-all">Смотреть все фото (<%= works_count %>)</div>
                <img class="offer__images__item__img--blur" src="<%= img.small %>">
            </a>
            <% } else { %>
            <a class="offer__images__item" title="<%= img.work_name %>">
                <img class="offer__images__item__img" src="<%= img.small %>" portfolio-id="<%= img.work__id %>" user-id="<%= author__id %>" thumb-id="<%= img.i %>" type="portfolio" />
            </a>
            <% } %>
            <% } %>
        </div>
        <% } %>
    </div>
</script>

