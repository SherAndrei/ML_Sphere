        <script>
      $(document).ready(function() {
        //$('#dc').prepend(trackstr);
        var mans=[];
        $(".mans").each(function(){
          man=$.trim($(this).text().toLowerCase());
          if(mans.indexOf(man)==-1)
            mans.push(man);
        });
        //лого к производителям
        var manslogos=[];
        $.ajax({
          type: "POST",
          url: "logosrequest.php",
          data: "mans="+mans,
          success: function(msg){
            eval(msg);
            $(".mans").each(function(){
              man=$.trim($(this).text().toLowerCase());
              if(manslogos.indexOf(man)!=-1)
                $(this).before('<img src="/logo/'+man+'.png" height=16> ');
            });
          }
        });
        //флаги к производителям
        var mansflags={};
        $.ajax({
          type: "POST",
          url: "flagsrequest.php",
          data: "mans="+mans,
          success: function(msg){
            eval(msg);
            console.log(msg);
            $(".mans").each(function(){
              man=$.trim($(this).text().toLowerCase());
              if(man in mansflags)
                $(this).before(mansflags[man]+' ');
            });
          }
        });

        $('span.jQtooltip').each(function() {
          var el = $(this);
          var title = el.attr('title');
          if (title && title != '') {
            el.attr('title', '').append('<div>' + title + '</div>');
            var width = el.find('div').width();
            var height = 200;
            el.hover(
              function() {el.find('div').clearQueue().delay(200).animate({width: width, height: height}, 200).show(200);},
              function() {el.find('div').animate({width: 'hide', height: 'hide'}, 150);}
            ).mouseleave(function() {
              if (el.children().is(':hidden')) el.find('div').clearQueue();
            });
          }
        });

        try { go(); } catch(e) {}
        $(".popups").fancybox({});
      });

      var checkbot;
      function isthisbot()
      {
        if(checkbot)
          return;
        checkbot=1;
        document.getElementById('hf').src='/hf.php?human=1';
      }
    </script>
