  $(".feedback :input").each(function(){
    if(this.type=='text' || this.type=='textarea'){
      if(!$(this).attr('title').length) return false;

      $(this).val($(this).attr('title'));
      $(this)
        .focus(function(){if($(this).val()==$(this).attr('title')) $(this).val('');})
        .blur(function(){if($(this).val()=='' || $(this).val()==$(this).attr('title')) $(this).val($(this).attr('title'));});
    }
  })
  function clear_form(form){
    $(form).find(':input').each(function(){
      switch(this.type){
        case 'password':
        case 'select-multiple':
        case 'select-one':
        case 'text':
        case 'textarea':
          $(this).val('');
          break;
        case 'checkbox':
        case 'radio':
          this.checked = false;
          break;
      }
    });
  }
  $(".feedback .form-submit .submit").live("click", function(e){
    var form=$(this).parent().parent();
    var to = form.find(".to").val();
    var name = form.find(".name").val();
    var email = form.find(".email").val();
    var message = form.find(".message").val();
    
    if(!name.length || !email.length || name==form.find(".name").attr('title') || email==form.find(".email").attr('title')){
      $(".feedback .alert").text("Please input your Name and Email first");
      return false;
    }
    
    var ajax_url = 'http://mimiton.ru/wp-admin/admin-ajax.php';
    var data = {
      action: 'of_ajax_feedback_action',
      to: to,
      name: name,
      email: email,
      message: message
    };
    $.post(ajax_url, data, function(response){
      $(".feedback .alert").text("Сообщение отправлено! Спасибо.");
      clear_form(form);
    });

    return false;
  })
