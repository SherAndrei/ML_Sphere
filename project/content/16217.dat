    <script>
            var map, geoResult, lastAddress;
      // Создает обработчик события window.onLoad
	YMaps.jQuery(function () {
	    // Создает экземпляр карты и привязывает его к созданному контейнеру
	    map = new YMaps.Map(YMaps.jQuery("#YMapsID")[0]);

	    // Выставляет центр карты в центр
	    map.setCenter(new YMaps.GeoPoint(49.122853,55.786764), 12);
	    map.enableScrollZoom();
	    map.addControl(new YMaps.TypeControl());
	    map.addControl(new YMaps.ToolBar());
	    map.addControl(new YMaps.Zoom());
	    map.addControl(new YMaps.ScaleLine());

	});

	$PJ(".show-address-on_ymap").live('click', function(e) {
		if ($PJ("#YMapsID").is(":hidden")) {
			var sleft = (e.pageX)+"px";
			var stop = (e.pageY+10)+"px";
			$PJ("#YMapsID").css("left",sleft);
			$PJ("#YMapsID").css("top",stop);
		};
	});
        // Функция для отображения результата геокодирования
        // Параметр value - адрес объекта для поиска
        function showAddress (value) {
            // Удаление предыдущего результата поиска
	    if (lastAddress!=value) {
		    lastAddress = value;
		    map.removeOverlay(geoResult);

		    // Запуск процесса геокодирования
		    var geocoder = new YMaps.Geocoder(value, { results: 1, boundedBy: map.getBounds()});
		    // Создание обработчика для успешного завершения геокодирования
		    YMaps.Events.observe(geocoder, geocoder.Events.Load, function () {
			// Если объект был найден, то добавляем его на карту
			// и центрируем карту по области обзора найденного объекта
			if (this.length()) {
			    geoResult = this.get(0);
			    map.addOverlay(geoResult);
			    map.setBounds(geoResult.getBounds());
			     $PJ("#YMapsID").show("normal");
			}else {
			    alert("Ничего не найдено")
			}
		    });

		    // Процесс геокодирования завершен неудачно
		    YMaps.Events.observe(geocoder, geocoder.Events.Fault, function (geocoder, error) {
			alert("Произошла ошибка: " + error);
		    });
	    }
	    else {
		if ($PJ("#YMapsID").is(":hidden")) {
			$PJ("#YMapsID").show("normal");
		}
		else {
			$PJ("#YMapsID").hide("normal");
			lastAddress = "";
		};
	    };
        };
    </script>

