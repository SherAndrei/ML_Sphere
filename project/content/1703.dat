            function showMessagesForParentId(id) {
                var selector = "div[data-parent-id='" + id + "']";
                $(selector).removeClass('readed');
                $(selector).parent().parent().show();
                $(selector).parent().parent().removeClass('padding0');
                $(selector).each(function () {
                    showMessagesForParentId($(this).data('message-id'));
                });
            }
            function showMessage(id) {
                var idSelector = '#message-' + id;
                if ($(idSelector).data('level') == '3') {
                    var parentId = $(idSelector).data('parent-id');
                    var parent = $('#message-' + parentId);
                    while (parent.data('level') != '2') {
                        parentId = parent.data('parent-id');
                        parent = $('#message-' + parentId);
                    }
                    showMessagesForParentId(parent.data('message-id'));
                } else {
                    $(idSelector).removeClass('readed');
                    $(idSelector).parent().parent().show();
                }
            }

            function applyActions() {
                $('.return-click').on('click', function (e) {
                    e.preventDefault();
                    var id = $(this).data('return-id');
                    $("html, body").animate({scrollTop: $('#message-' + id).offset().top - 50}, 500);
                    $(this).remove();
                });
            }
