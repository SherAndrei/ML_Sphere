                    if ($('#message-' + id + ' .card-action.clearfix a.return-click').length == 0) {
                        $('#message-' + id + ' .card-action.clearfix a').after('  <a class="btn red return-click" href="!#" data-return-id="' + currentId + '" class="btn"><i class="material-icons md-24">arrow_downward</i></a>');
