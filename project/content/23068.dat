				if(!cb.length){
					return;
				}
				e.bind('mouseenter', function(){
					cb.show();
				});
				e.bind('mouseleave', function(){
					if (!cb.attr('checked')) {
						cb.hide();
					}	
				});
				if(cb.attr('checked')){
					cb.show();
				}
			});
		}, 0);
	}

