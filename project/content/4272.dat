<script type="text/javascript">
    var rubricMenuPot = 0;

    $(document).ready(function(){
        var rubrMenu = $('.nav.main_top_menu .menu-item-categories .nav');
        rubrMenu.after('<ul class="nav"></ul>');
        var cUl = $('.nav.main_top_menu .menu-item-categories .nav:last');
        rubrMenu.find('li.lv2').each(function(ind){
            var subPot = 0;

            var cLi = $('.nav.main_top_menu .menu-item-categories .nav li.lv2').eq(ind);

            rubricMenuPot = rubricMenuPot + Math.ceil(parseInt(cLi.attr('rel'))/30); //считаем, что в одной строке примерно 30 символов

            if(cLi.hasClass('parent'))
            {
                cLi.find('.lv3').each(function(subInd){
                    subPot = subPot + Math.ceil(parseInt(cLi.find('.lv3').eq(subInd).attr('rel'))/30);
                });
                if(rubricMenuPot + subPot> 18)
                {//проверяем подпункты, если много -> перенос в новый столбец
                    cUl.attr('rel', rubricMenuPot).after('<ul class="nav"><li class="lv2 parent" rel="' + (rubricMenuPot + subPot) + ' ms">' + cLi.html() + '</li></ul>');
                    rubricMenuPot = subPot;
                    cUl = $('.nav.main_top_menu .menu-item-categories .nav:last');
                }
                else
                {//если немного -> добавляем в текущий столбец
                    rubricMenuPot = rubricMenuPot + subPot;
                    cUl.html(cUl.html() + '<li class="lv2" rel="' + rubricMenuPot + ' ms">' + cLi.html() + '</li>');
                }
            }

            if(rubricMenuPot > 0 && subPot == 0)
            {
                if(rubricMenuPot > 15)
                {//если строк набролось много -> переходим в новый столюец
                    cUl.attr('rel', rubricMenuPot).after('<ul class="nav"><li class="lv2">' + cLi.html() + '</li></ul>');
                    rubricMenuPot = 0;
                    cUl = $('.nav.main_top_menu .menu-item-categories .nav:last');
                }
                else
                {//иначе добавляем данные в текущий столбец
                    cUl.html(cUl.html() + '<li class="lv2" rel="' + rubricMenuPot + ' ms">' + cLi.html() + '</li>');
                }
            }
        });
        rubrMenu.remove();
    });
</script>
