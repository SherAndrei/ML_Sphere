<script type="text/ignore" id="kh_script" src="http://kinohod.ru/static/js/widget/v2.1/boxoffice.js?apikey=f50e81f7-6818-3529-b067-2404c97e7163&base_uri=http%3a%2f%2fkinohod.ru%2fapi%2fwidget"></script>
<script>
require(['jquery'], function($) {
	if(window['kinohod']) {
		return;
	}
	
	/*
	 * Calc iframe loading time
	 */
	
	function jsStat() {
		var now = new Date().getTime();
		var error = false;
		var timeoutId;
		
		timeoutId = setTimeout(function(){
			error = true;
			$.post('/js.php', { area: 'misc', action: 'jsPerformance', type: 'kinohod', value: 'error' });
		}, 60 * 1000);
		
		$('#khBoxOffice > iframe').on('load', function(e){
			if(error) {
				return;
			}
			
			clearTimeout(timeoutId);
			var loadingTime = (new Date().getTime()-now);
			$.post('/js.php', { area: 'misc', action: 'jsPerformance', type: 'kinohod', value: loadingTime/1000 });
		});
	}
	
	$(function(){
		requirejs.config({
			paths: {'kinohod' : 'http://kinohod.ru/static/js/widget/v2.1/boxoffice.js?apikey=f50e81f7-6818-3529-b067-2404c97e7163&base_uri=http%3a%2f%2fkinohod.ru%2fapi%2fwidget'},
			shim: {'kinohod' : {exports: 'kinohod'}}
		});
		require(['kinohod'], function(kinohod){
			kinohod.contentLoaded(window, function() {
				$('.service-items-medium').on('click', '.kh_boxoffice', function(e){
					e.preventDefault();
					$(window).trigger('track-event', [ 'cinema', 'catalog_seance_click' ]);
					var seance = $(this).data('seance');
					var href = kinohod.base_uri + 'seance/' + seance + '?no_back=1&ext=' + kinohod.external_request;
					if('') {
						create_layer('kinohod', {
							tpl: '<div style="background: url(http://kinohod.ru/static/i/feet.gif) #444 50% 50% no-repeat;"><iframe src="'+href+'" width="797" height="512" frameborder="0" style="vertical-align: top;" /></div><a href="#" class="layer-close button button-big mauto mt20 mlr20">Вернуться назад</a>'
						});
					} else {
						kinohod.buildIframe(href);
						jsStat();
					}
				});
			});
		});
	});
});

require(['jquery', 'js2/Carousel'], function($, Carousel) {
	docready(function(){
		var $box = $('.cinemaTags');
		var $ul = $('ul.cinema-films', $box);
		var $controls = $('.pager-next-prev', $box);

		var w = 50;
		$ul.find('li').each(function() {
			w+=$(this).width();
		});

					var $next = $('.next', $controls);
			var $prev = $('.prev', $controls);
			$ul.data('z-carousel', new Carousel($ul, $next, $prev));

			$(window).off('resize.promotags');
			$(window).on('resize.promotags',function() {
				if (w>=$box.width()) {
					$controls.show();
				} else {
					$controls.hide();
				}
			});
			$(window).trigger('resize.promotags');
				$ul.height($ul.height());
		
		
	});
});
