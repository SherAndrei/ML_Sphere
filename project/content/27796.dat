                    success: function (response) {
                        if (response.success) {
                            window.location.href = response.route;

                        } else {
                            $('.js-registration-form-wrapper').html(response.form);
                        }
                    }
                });

                return false;
            });
        </script>
        <script src="https://cdnjs.cloudflare.com/ajax/libs/select2/4.0.3/js/select2.min.js"></script>

    

    <script>
        jQuery(function(){

            $('.js-article-actions').tooltip({
                html: true,
                template: '<div class="tooltip tooltip_white2"><div class="tooltip-arrow"></div><div class="tooltip-inner"></div></div>'
            });

            $('.js-watch-article').on('click', function () {
                var checkbox = $(this);
                var article  = checkbox.val();

                var url;
                if (checkbox.is(':checked')) {
                    url = Routing.generate('application_user_watch_article', { article: article });

                } else {
                    url = Routing.generate('application_user_unwatch_article', { article: article });
                }

                $.post(url, function(response ) {
                    if (!response.success) {
                        bootbox.alert("Произошла ошибка.");

                        if (checkbox.is(':checked')) {
                            checkbox.prop('checked', false);
                        } else {
                            checkbox.prop('checked', true);
                        }
                    }
                });
            });
        });
    </script>




</body>
</html>
