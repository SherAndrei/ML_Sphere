							$('.new_mag_main.new_mag_main__closed').animate({ left: 360 }, 360, function() {
								$(this).removeClass('new_mag_main__closed');
								$(this).addClass('new_mag_main__open');
								$('.nav_icons').addClass('nav_icons_active');
								$('.tcon-menu--xcross').addClass('tcon-transform');
							});
							$('.new_mag_main.new_mag_main__open').removeClass('new_mag_main__open').animate({ left: 0 }, 300, function() {
								$(this).removeClass('new_mag_main__open');
								$(this).addClass('new_mag_main__closed');
								$('.tcon-menu--xcross').removeClass('tcon-transform');
								$('.nav_icons').removeClass('nav_icons_active');
								$('.click_blocked').hide();
							});
						}

						/*$('.mag_content_item').click(function() {
							var clicked = $(this).attr('data-clicked');
							var items_count = $('.mag_content_item').size();
							var this_index = $('.mag_content_item').index( this );

							console.log(clicked, items_count, this_index)

							if (clicked != 'yes') {
