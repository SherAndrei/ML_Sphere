					+ '&description=' + encodeURIComponent(options.text)
					+ '&imageurl=' + encodeURIComponent(options.image);
		},

		// Открыть окно шаринга
		popup: function (url) {
			return window.open(url, '', 'toolbar=0,status=0,scrollbars=1,width=626,height=436');
