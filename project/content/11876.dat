<script type="text/javascript">
	$(document).ready(function(){
		$('.feedback-show').click(function() {
			$('div.feedback-form').css('top', $(document).scrollTop() + 50).show();
			$('#overlay').show();
			return false;
		});

		$('span.feedback-hide').click(function() {
			$('div.feedback-form').hide();
			$('#overlay').hide();
		});

		$('input.feedback-button').click(function() {
		type = $(this).attr('data-type');

		if(type == 'send') {
			if ($('#feedback2-validate').validationEngine('validate'))
			{
				$('#waiter').show();

				$.post(
					cmsPathAjax,
					{
						'module': 'cmspl_feedback',
						'do': 'send',
						'type': 'Запись на прием',
						'name': $('#feedback2-name').val(),
						'phone': $('#feedback2-phone').val(),
						'msg': $('#feedback2-msg').val()
					},
					function(xml) {
						$('#waiter').hide();

						if ($('text', xml).text() == 'ok')
						{
							$('#overlay').hide();
							alertify.alert('Сообщение успешно отправлено.');
						}
						else
						{
							alertify.error($('text', xml).text());
						}
					},
					'xml'
				);
			}
		} else {
			if ($('#feedback-validate').validationEngine('validate'))
			{
				$.post(
					cmsPathAjax,
					{
						'module': 'cmspl_feedback',
						'do': 'send',
						'type': 'Заказ звонка с сайта',
						'name': $('#feedback-name').val(),
						'phone': $('#feedback-phone').val()
					},
					function(xml) {
						$('#waiter').hide();

						if ($('text', xml).text() == 'ok')
						{
							$('div.feedback-form').hide();
							$('#overlay').hide();
							alertify.alert('Сообщение успешно отправлено.');
						}
						else
						{
							alertify.error($('text', xml).text());
						}
					},
					'xml'
				);
			}
		}
	});
});
</script>

