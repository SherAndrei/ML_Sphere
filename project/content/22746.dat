                            <!-- CONTENT-->
                            <h1 class="widgettitle">Как сделать скриншот в Windows 10 и где он хранится?</h1>
                            <div class="row post_meta">
                                <time class="post_date">
                                    <i class="fa fa-clock-o"></i> 18 Янв, 2016 в 23:12                                </time>
                                <IDXSKIP><span style="margin-left: 10px;">
                                    <i class="fa fa-user" aria-hidden="true"></i>
                                    Автор: <a href="http://windowsten.ru/author/kristina/" title="Записи vova" rel="author">vova</a>                                </span></IDXSKIP>
                            </div>
<!--                            <div class="article_soc_buttons before_article_soc_buttons">-->
<!--                                <script type="text/javascript" src="//yastatic.net/es5-shims/0.0.2/es5-shims.min.js" charset="utf-8"></script>-->
<!--                                <script type="text/javascript" src="//yastatic.net/share2/share.js" charset="utf-8"></script>-->
