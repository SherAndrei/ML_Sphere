			callback = args[i];
			if (args.length > i + 1) {
				callback_object = args[i + 1];
			}
			break;
		} else {
			func_args[func_args.length] = args[i];
		}
	}
	var request = new AjaxRequest(method, script_name, func, func_args, callback, callback_object);
	ajax_requests.push(request);
	if (!request.send()) {
