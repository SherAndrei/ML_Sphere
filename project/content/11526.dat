<script type="text/javascript"> 
	$(function() {	 
		$(window).scroll(function() {		 
			if($(this).scrollTop() != 0) {			 
				$('#toTop').fadeIn();			 
			} else {			 
				$('#toTop').fadeOut();			 
			}		 
		});		 
		$('#toTop').click(function() {		 
			$('body,html').animate({scrollTop:0},800);		 
		});	 
	}); 
</script>

<script type="text/javascript">
	jQuery(document).ready(function(){
		
		setTimeout(function() {	
		
			var teaser = $(".flying-teaser");					
			var teaserTop = teaser.offset();
			var teaserTopValue = teaserTop.top - 30;
			
			$(window).scroll(function() {
				console.log($(this).scrollTop());
				if ($(this).scrollTop() >= teaserTopValue) {
					teaser.addClass("fly");
				} else {
					teaser.removeClass("fly");
				}
			});
		
		}, 2000);
		
	});
</script>

