            $("#count-" + id).attr('class', 'rate-high');
           }else{
               $("#count-" + id).attr('class', 'rate-low');
           }
           if(type == 'like'){
           $('#like-'+id).addClass('rate-own');
               $('#dislike-'+id).removeClass('rate-own'); 
           }else{
          $('#dislike-'+id).addClass('rate-own'); 
          $('#like-'+id).removeClass('rate-own'); 
           }
        }
     });
 });

