			this.parentTree.typesField.value = state[8];
		}
		
		this.state[0] = state = eval(state[1]);
		
		if (state.length > 0)
		{
			PowerUpTreeViewGenerate(this.container.nextSibling.firstChild, this.parentTree, this, state);
			
			this.populating = false;
		}
		else
