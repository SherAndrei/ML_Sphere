  function showNavigationPageContent(page, total, totalPages) {
     packToCollect = [];
     var startIdx = (page - 1) * perPage;
     var endIdx = page * perPage - 1;
     if (endIdx > (total - 1)) {
        endIdx = total - 1;
     }
     var posIdx = 0;
     var toLoad = [];

     for (var i=(commentInfo.length-1); i>=0; i--) {
         posIdxNew = posIdx + commentInfo[i][1] - 1;

         var getIt = false;
         if (startIdx >= posIdx && endIdx <= posIdxNew) {
            packToCollect.push([ commentInfo[i][0], 0, [], (posIdxNew-startIdx), (endIdx+1-startIdx) ]);
            toLoad.push(commentInfo[i][0]);
         } else if (startIdx >= posIdx && startIdx < posIdxNew && endIdx >= posIdxNew) {
            packToCollect.push([ commentInfo[i][0], 0, [], (commentInfo[i][1]-1-(startIdx-posIdx)), (commentInfo[i][1]-(startIdx-posIdx)) ]);
            toLoad.push(commentInfo[i][0]);
         } else if (startIdx <= posIdx && endIdx >= posIdxNew) {
            packToCollect.push([ commentInfo[i][0], 0, [], (commentInfo[i][1]-1), commentInfo[i][1] ]);
            toLoad.push(commentInfo[i][0]);
         } else if (startIdx <= posIdx && endIdx < posIdxNew) {
            packToCollect.push([ commentInfo[i][0], 0, [], (commentInfo[i][1]-1), (endIdx+1-posIdx) ]);
            toLoad.push(commentInfo[i][0]);
         }
         posIdx = posIdxNew + 1;
         if (posIdx >= endIdx) {
            break;
         }
     }
     for (var i=0; i<toLoad.length; i++) {
        try {
          loadJS('/js/comment1_'+object+'_'+toLoad[i]+'.js');
        } catch(e) {
          window.parent.loadJS('/js/comment1_'+object+'_'+toLoad[i]+'.js');
        }
     }
  }

