		$(function(){
			var subscribe = new tFormer('subscribe', {
				fields:{
					SUBSCRIBE_BLOG_NAME: '* l>1',
					SUBSCRIBE_BLOG_EMAIL: '* @',
					SUBSCRIBE_BLOG_CHECK: '*',
				}
			}).submit(function(){
				if($('#subscribe input[type="checkbox"]').hasClass('error') && $('#subscribe input[type=text]').hasClass('error') == false){
					$('#subscribe .rules.error p:first').html('Вы должны согласиться с условиями подписки.');
					$('#subscribe .rules.error').slideDown();
					return false;
				}
			
				//window.yaCounter10895779.reachGoal('RSUP_FORM');
				
				var SUBSCRIBE_BLOG_NAME = $('input[name="SUBSCRIBE_BLOG_NAME"]').val();
				var SUBSCRIBE_BLOG_EMAIL = $('input[name="SUBSCRIBE_BLOG_EMAIL"]').val();
				
				$('#subscribe .rules').slideUp();
				$('#subscribe .rules').slideUp();
				
				$.post(
					'/bitrix/tools/ajax/subscribe.php',
					{
						NAME: SUBSCRIBE_BLOG_NAME,
						EMAIL: SUBSCRIBE_BLOG_EMAIL
					},
					function(__obResult){
						__obResult = $.parseJSON(__obResult);
						if(__obResult['TYPE'] == 'ERROR'){
							if($('#subscribe input[type="text"]').eq(0).hasClass('error') && $('#subscribe input[type="text"]').eq(1).hasClass('error')){
								$('#subscribe .rules.error p:first').html('Заполните все поля!');
								$('#subscribe .rules.error').slideDown(200);
							} else if($('#subscribe input[type="text"]').eq(0).hasClass('error') && $('#subscribe input[type="text"]').eq(1).hasClass('error') == false){
								$('#subscribe .rules.error p:first').html('Введите верное имя!');
								$('#subscribe .rules.error').slideDown(200);
							} else if($('#subscribe input[type="text"]').eq(1).hasClass('error') && $('#subscribe input[type="text"]').eq(0).hasClass('error') == false){
								$('#subscribe .rules.error p:first').html('Введите верный email!');
								$('#subscribe .rules.error').slideDown(200);
							} else {
								$('#subscribe .rules.error p:first').html(__obResult['MESSAGE']);
								$('#subscribe .rules.error').slideDown(200);
							}
						}else if(__obResult['TYPE'] == 'OK'){
							$('#subscribe .rules.success').slideDown(200);
						}
					}
				);

				setTimeout(function(){
					$('#subscribe input[type="submit"]').removeClass('processing');
				}, 200)

			});
		});
	</script>

	
