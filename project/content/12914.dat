		$(document).ready(function() {
			$.widget( "custom.catcomplete", $.ui.autocomplete, {
				_renderMenu: function( ul, items ) {
					var that = this,
						currentCategory = "";
					$.each( items, function( index, item ) {
						if ( item.category != currentCategory ) {
							ul.append( "<li class='ui-autocomplete-category'>" + item.category + "</li>" );
							currentCategory = item.category;
						}
						that._renderItemData( ul, item );
					});
				},
				_renderItemData: function(ul, item) {
					return $( "<li />" )
					.data('ui-autocomplete-item', item)
			        .append(getItemTemplate.apply(this, [item]))
			        .appendTo( ul );
				}
			});
		});
		function getItemTemplate(item) {
			$template = $('<div>');
			// If image is available append it
			if (item.imageUrl) {
				$imageTemplate = '<img src="http://www.glgcdn.com/tim/w50/h/z/cmirbonus_ru/s--placeholder--" />';
				$template.prepend($imageTemplate.replace('--placeholder--', encodeURIComponent(encodeURIComponent(item.imageUrl))));
			}
			// If price is available append it
			if (item.price) {
				$template.prepend(
					$('<span>', {'class': 'price'}).text(item.price)
				);
			}

			// Get search terms
			$parts = this.term.match(/[^\s]+/g);
			$label = item.label;
			// Highlight each team insite the label
			for ($i in $parts) {
				$regexp = new RegExp('(' + $parts[$i] + ')', 'i');
				$label = $label.replace($regexp, '<b>$1</b>');
			}
			$template.append(
				$('<span>').html($label)
			);

			return $('<a>', {'href': item.url}).append($template);
		}

