				var link = $(el).attr('href');
				if( !_.isUndefined(link) && ( link.match(/watch\?v=([a-zA-Z0-9\-_]+)/) || link.match(/youtu.be\/([a-zA-Z0-9\-_]+)/ )  ) ){
				// if( !_.isUndefined(link) && link.match(/watch\?v=([a-zA-Z0-9\-_]+)/) ){
					var linl;
					link = link.replace('https://','http://');
					linl = link.replace(/(?:http:\/\/)?(?:www\.)?(?:youtube\.com|youtu\.be)\/(?:watch\?v=)?(.+)/g, '$1');
					$(el).replaceWith( '<iframe width="400" height="350" src="http://www.youtube.com/embed/'+linl+'" frameborder="0" allowfullscreen></iframe><br/><a href="'+link+'" target="_blank">ссылка на видео</a>' );
				}
			});
		}
