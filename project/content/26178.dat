				skip: count,
				limit: page_size,
			},
			beforeSend: function(){
				$loader.show();
			},
			success: function(R){
				if(!R.success) return;
				if(!R.items) return;
				
				$list.append(R.items);
				lazyload($list);

				count += page_size;
				remain -= page_size;

				if (remain > page_size) {
					$showMoreButton.show().text('Показать ещё ' + page_size + ' из ' + remain + ' ' + plural(remain, 'записей', 'записи', 'записей'));

				} else if (remain > 0) {
					$showMoreButton.show().text('Показать ещё ' + remain + ' ' + plural(remain, 'записей', 'запись', 'записи'));

				} else {
					$showMoreBox.remove();
				}
			},
			complete: function(){
				$loader.hide();
			}
		});
	});
	
	lazyload($list);

