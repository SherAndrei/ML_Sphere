<script>
filepicker.setKey('AbF7QOJYgQW2QDfMZJhIgz');
var up_files = []
function remove_file(i){
  up_files.splice(i, 1)
  recreate_file_list()
}
function recreate_file_list(){
  if(up_files.length > 0){
    var t = '<b>Файлы</b><ul>'
    for(var i in up_files){
      t += '<li>' + up_files[i].filename + ' <a class="red-colored delete-me" onclick="remove_file('+i+'); return false;">[x]</a></li>';
    }
    t+='</ul>'
    $('.upload-data').html(t)
  }
  $('#files_input').val(JSON.stringify(up_files))
}

$(function(){
  recreate_file_list()
  $('#upload-files').click(function(){
    //[
    //  {
    //   "url":"https://www.filepicker.io/api/file/06tMtVMhSQuLvtO0kVy0",
    //   "filename":"154288.jpg",
    //   "mimetype":"image/jpeg",
    //   "size":61180,
    //   "key":"1a3cIp1lRHKiFOjHTopw_154288.jpg",
    //   "isWriteable":true
    //   }
    //]
    filepicker.pickAndStore({},{},function(InkBlobs){
      for(var i in InkBlobs)
        up_files.push(InkBlobs[i]);
      recreate_file_list()
    });
  });
});
</script>
