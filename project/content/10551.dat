<script>
  $(function(){
    if(ab()){
      $('#ab').val('a');
    }else{
      $('#ab').val('b');
    }
    if(document.URL.match(/zakaz-ok/)){
      $('#form-all-good').fadeIn('slow');
    }
    if(document.URL.match(/zakaz-form/)){
      $('#zakaz-new-form input:first').focus();
    }
    if(getParameterByName('z-price')){
      $('#zakaz-new-form input[name=Summa]').val(getParameterByName('z-price'))
    }
    if(getParameterByName('z-time')){
      $('#zakaz-new-form input[name=Data]').val(getParameterByName('z-time'))
    }

    var discount = getParameterByName('z-discount')
    if(discount == 'repeated'){
      $('#ff18').click();
    }
    if(discount == 'second-dip'){
      $('#ff20').click();
    }

    if(typeof form_theme != 'undefined'){
      $('input[name=Tema]').val(form_theme)
    }

    $('.focus-email').click(function(){
      $('#ff7').focus();
      return false;
    });
    $('.focus-phone').click(function(){
      $('#ff8').focus();
      return false;
    });
    var ff7 = $('#ff7'), ntf_span = $('#need-to-fill span'),
        ff8 = $('#ff8'), ntf_input = $('#need-to-fill input');
    setInterval(function(){
      if(ff7.val() != ''){
        ntf_span.fadeOut('slow');
        ntf_input.removeAttr('disabled');
        return;
      }

      if(ff8.val() != ''){
        ntf_span.fadeOut('slow');
        ntf_input.removeAttr('disabled');
        return;
      }
      ntf_span.fadeIn('slow');
      ntf_input.attr('disabled', 'disabled');
    }, 300)

    function change_to_simple(){
      $('#zakaz-btn-simple').addClass('active');
      $('#form-more-info p').hide();
      $('#form-text-label').html('Ваши пожелания');
      $('#form-submit-text').val('   Обсудить   ');
      if($('input[name=Tema]').val() != '')
        $('#form-tema-block').show();
      $('#zakaz-btn-hard').removeClass('active');

    }
    $('#zakaz-btn-simple').click(function(){
      change_to_simple();
      $('#zakaz-new-form input:first').focus()
      return false;
    });
    function change_to_hard(){
      $('#zakaz-btn-hard').addClass('active');
      $('#zakaz-btn-simple').removeClass('active');
      $('#form-more-info p').show();
      $('#form-text-label').html('Особые требования');
      $('#form-submit-text').val('   Заказать   ');
    }
    $('#zakaz-btn-hard').click(function(){
      change_to_hard()
      $('#zakaz-new-form input:first').focus()
      return false;
    });
    change_to_hard();
  });
</script>
