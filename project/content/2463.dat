    // Показываем какой-то сезон
    function show_season(box, r, first) {
        var season = r.replace("#sn", "");
        $(".select_season").removeClass("select_season-active");
        $("#select_season_" + season).addClass("select_season-active");

        $(".episode_h").hide();

        $(box).show();
        $(r).attr("class", "season-name-ro");

        // Показываем первый роли сезона
        var first_episode = false;
        for (key in m_episodes) {
            if (key.startsWith(season + '__')) {
                first_episode = {
                    'tt': m_episodes[key][0]['tt'],
                    'movie_id': m_episodes[key][0]['id'],
                    'season': season,
                    'episode': key.replace(season + '__', '')
                }
            }
        }
        if (!first && first_episode) {
            show_episodes(first_episode['tt'], first_episode['movie_id'], first_episode["season"], first_episode["episode"]);
        }

    }

    function show_episodes(tt, movie_id, season, episode) {
        show_episode(tt, movie_id, season, episode);

        //<img src="/media/prof_odnog.png" alt="" style="float: left;margin: -5px 5px 0 0" /> Лостфильм (Вконтакте)</a>

        var ss = season + "__" + episode;

        var subs = '';

        if (ss in m_episodes) {
            for (var s in m_episodes[ss]) {
                var m_id = m_episodes[ss][s]['id'];
                var tt2 = m_episodes[ss][s]['tt'];
                if (!season || (season == "None")) season = "1";
                var func = 'javascript:show_sub(\'' + tt2 + '\', ' + m_id + ', ' + season + ', ' + episode + ');';
                subs += '<a id="subs_' + m_id + '" href="' + func + '" class="episode_h subs">';
                subs += '<span style="font-size: 14px;border-color: transparent;vertical-align: middle">';
                //subs += '<img src="/media/prof_odnog.png" alt="" style="float: left;margin: -5px 5px 0 0" />';

                
                    if (m_episodes[ss][s]['show_var']) {
                        subs += 'Вариант ' + (parseInt(s) + 1);
                        if (m_episodes[ss][s]['source_name']) {
                            subs += " (" + m_episodes[ss][s]['source_name'] + ")";
                        }
                    } else {
                        subs += m_episodes[ss][s]['name'];
                    }
                
                subs += '</span></a>';
            }
        }

        $("#sub_series").html(subs);

        $(".subs").removeClass("episode_h-active");
        $("#subs_" + movie_id).addClass("episode_h-active");
    }

    function show_sub(tt, movie_id, season, episode) {
        show_episode(tt, movie_id, season, episode, true);

        $(".subs").removeClass("episode_h-active");
        $("#subs_" + movie_id).addClass("episode_h-active");
    }

    function show_episode(tt, movie_id, season, episode, subs) {

       if (!subs) {
           $("#sub_series").html('');
       }

        $(".episode_h").removeClass("episode_h-active");
        $("#episode_h-" + season + "-" + episode).addClass("episode_h-active");

        $.ajax({
            url: "/get_movie_code/",
            type: 'get',
            dataType: "json",
            data: {
                "movie_id": movie_id,
                "tt": tt
            },
            error: function() {
                alert('Ошибка загрузки видео');
            },
            success: function(data) {
                if (data) {
                    MOVIE_TYPE = tt;
                    MOVIE_ID = movie_id;

                    var start = false; // Сделать false когда ролики устаканятся

                    if (start) {
                        var name = '';
                    } else {
                        var name = film_name;
                    }


                    

                    $("#complaint_source").val(tt);
                    $("#complaint_source_id").val(movie_id);
                    $("#complaint_source_name").val('');

                    

                    $("#movie-code").html(data["code"]);

                    // Дата
                    if (data["add_date"]) {
                        $("#movie_add_date").html('Дата добавления: ' + data["add_date"]);
                    } else {
                        $("#movie_add_date").html('');
                    }

                    // Просмотров
                    if (data["views_count"]) {
                        $("#movie_views_count").html('Просмотров: ' + data["views_count"]);
                    } else {
                        $("#movie_views_count").html('');
                    }

                    // Название фильма
                    if (data["movie_name"]) {
                        if (data["movie_name"] != film_name) {
                            name += "<br/>" + data["movie_name"];
                        }
                    }

                    $("#se-name").html(name);
                    SEASON = season;
                    SERIES = episode;

                    
                }
            }
        }); // $.ajax
    }

