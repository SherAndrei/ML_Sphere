$(function() {
  var f_link = window.location.pathname.replace(/topic/, 'forum').replace(/\/\d+\.html$/, '');
  var button = $('<a/>', {
    href:  f_link,
    class: 'button',
    title: 'В начало раздела',
    text:  'Выше в раздел'
  });
  $("#nav > a").first().after(button);
  $("div.msg").each(function(i){
    $(this).attr("id", 'p'+(i+1));
    var number = $('<a/>', {
      href:  '#p'+(i+1),
      class: 'anchor',
      title: 'Ссылка на сообщение №'+(i+1),
      text:  '#'+(i+1)
    });
    number.prependTo($(this));
  });
  $("img.thumbnail").css('display','inline-block').lazyload({
    effect : "fadeIn",
  });
  var wH = $(window).height();
  var bH = $("#footer").offset().top;
  var blocks = $("div.ya-text").get();
  if (wH >= bH) {
    $(blocks.reverse()).each(function(i){
      $("div.msg").last().after($(this));
    });
  } else {
    var post = $("div.msg").get();
    $(post).first().after($(blocks).first());
    $(post).last().after($(blocks).last());
    for (var i = 1, max = $(blocks).size() - 1; i < max; i++) {
      var bp = i * ($(post).size() - 2) / max;
      $(post).eq(Math.floor(bp)).after($(blocks).eq(i));
    }
  }
  if (($(window).width() - $("#sidebar").width()) < $("div.msg").width()) {
    $("#sidebar").css('position', 'static');
  }
});
</script>
