        config.slider = config.slider || {},
            config.slider.min = config.slider.min || 1,
            config.slider.max = config.slider.max || 1000,
            config.slider.values = config.slider.values || [config.slider.min, config.slider.max];

        $this.find('.prod').brands();
		
        (function () {

            var inputFrom = priceElement.find('.title .from input'),
                inputTo = priceElement.find('.title .to input'),
                element = priceElement.find(".slider");

            var updateSlider = function () {
                var from = inputFrom.val() - 0,
                    to = inputTo.val() - 0;
                element.slider("values", 0, [ (from > 0) ? from : config.slider.min]);
                element.slider("values", 1, [ (to > 0) ? to : config.slider.max]);
            }

            var handleInputToValueChange = function () {
                var from = inputFrom.val() - 0,
                    to = inputTo.val() - 0;

                if (isNaN(to)) {
                    to = element.slider("values")[1];
                    inputTo.val(to);
                } else if (to < from) {
                    to = from;
                    //					inputTo.val(to);
                } else if (to > config.slider.max) {
                    to = config.slider.max;
                    inputTo.val(to);
                }

                updateSlider();
            }

            var handleInputFromValueChange = function () {
                var from = inputFrom.val() - 0,
                    to = inputTo.val() - 0;

                if (isNaN(from)) {
                    from = element.slider("values")[0];
                    inputFrom.val(from);
                } else if (from > to) {
                    from = to;
                    //					inputFrom.val(from);
                } else if (from < config.slider.min) {
                    from = config.slider.min;
                    inputFrom.val(from);
                }

                updateSlider();
            }

            element.slider({
                range: true,
                min: config.slider.min,
                max: config.slider.max,
                values: config.slider.values,
                step : 1,
                slide: function(event, ui) {
                    var
                        min = element.slider( "option", "min" )-0, max = element.slider( "option", "max" )-0,
                        from = inputFrom.val() - 0, to = inputTo.val() - 0,
                        v0 = ui.values[0] - 0, v1 = ui.values[1] - 0;

                    if ((from=='' && v0 > min) || (from!='' && from != v0))
                    {
                        inputFrom.val(v0);
                    }

                    if (v0==min)
                    {
                        inputFrom.val('');
                    }

                    if ((to=='' && v1 < max) || (to!='' && to != v1))
                    {
                        inputTo.val(v1);
                    }

                    if (v1==max)
                    {
                        inputTo.val('');
                    }
                }
            });

            inputFrom
                .keypress(function (e) {
                if (e.which == 13) {
                    handleInputFromValueChange();
                }
            })
                .blur(function () {
                    handleInputFromValueChange();
                });
            inputTo
                .keypress(function (e) {
                if (e.which == 13) {
                    handleInputToValueChange();
                }
            })
                .blur(function () {
                    handleInputToValueChange();
                });

            updateSlider();
        })();


	};
</script>
