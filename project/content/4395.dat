<script>
    var arrSections = new Array();
            arrSections['3125'] = 'Холодильники';
    	
	function bindTabClick(tab){
		var vTab = $(tab).attr('data-tab');
		$('.footer_bookmark .widget_popup, .footer_bookmark .bookmark_tab').removeClass('active');
		$(tab).addClass('active');
		$('.widget_popup.pop_' + vTab).addClass('active');
	}
	
    jQuery(function(){
		$('body').on('click','.left_poiner, .right_poiner',function(){
            if(!$(this).hasClass('pointer_isActive')){
                return;
            }
            arr = this.id.split('_');
            jQuery.get('http://'+window.location.host+'/widgets/getbookmarks',
				{
					type: arr[0],
					section: arr[1],
					offset: arr[2],
					direction: arr[3]
				},
				function(data) {
					afterAjaxBookmark(data, arr[0]);
				}
            )
        });
    });
    jQuery(function(){$('body').on('change','.bookmarksSelect',function(){
             arr = this.id.split('_');
			 //alert('http://'+window.location.host+'/widgets/getbookmarks');
             jQuery.get('http://'+window.location.host+'/widgets/getbookmarks/?type=' + arr[2] + '&section=' + $(this).val()  + '&offset=0&direction=right' ,
                    function(data) {
                        afterAjaxBookmark(data, arr[2]);
                    }
            )
        });
    });
    jQuery(function(){$('body').on('click','.delBP',function()
    {
            arr = this.id.split('_');
            poiner = jQuery('.widget_'+arr[0]+' .left_poiner').attr('id').split('_');
            jQuery.get('http://'+window.location.host+'/widgets/getbookmarks',
                    {
                        type: arr[0],
                        section: poiner[1],
                        offset: Number(poiner[2])+6,
                        direction: 'right',
                        del: arr[1]
                    },
                    function(data) {
                        count = jQuery('.count-numbers_'+arr[0]).html()-1;
                        if(count == 0){
                            jQuery('.widget_item_'+arr[0]+' .widget_popup').hide();
                            jQuery('.widget_item_'+arr[0]).removeClass('widget_hasPopup');
                            jQuery('.count-numbers_'+arr[0]).html("");
                        } else {
                            jQuery('.count-numbers_'+arr[0]).html(count);
                        }
                        afterAjaxBookmark(data, arr[0]);
                        if(jQuery('#bookmarks_sections_'+arr[0]+' option:selected').attr('class')){
                            arrSectionCount = jQuery('#bookmarks_sections_'+arr[0]+' option:selected').attr('class').split('_');
                            countInSection = Number(arrSectionCount[1]) - 1;
                            jQuery('#bookmarks_sections_'+arr[0]+' option:selected').attr('class', arrSectionCount[0]+'_'+countInSection);
                            if(countInSection == 0){
                                jQuery('#bookmarks_sections_'+arr[0]+' option:selected').remove();
                            } else {
                                jQuery('#bookmarks_sections_'+arr[0]+' option:selected').html(arrSections[arrSectionCount[0]] + ' (' + countInSection +')');
                            }
                        }
                    }
            )
        });
    });
    function afterAjaxBookmark(data, type){
        data = jQuery.parseJSON(data);
        if(data.success == true){
            $("."+type+"_carousel").html(data.content);
            if(data.poiners){
                $('.widget_item_'+type+' .left_poiner').show();
                $('.widget_item_'+type+' .right_poiner').show();
            } else {
                $('.widget_item_'+type+' .left_poiner').hide();
                $('.widget_item_'+type+' .right_poiner').hide();
            }
            for(var a in data.active){
                if(data.active[a] == true){
                    jQuery('.widget_item_'+type+' .'+a).addClass('pointer_isActive');
                } else {
                    jQuery('.widget_item_'+type+' .'+a).removeClass('pointer_isActive');
                }
            }
            for (var k in data.newIDs){
                jQuery('.widget_item_'+type+' .'+k).attr('id', data.newIDs[k]);
            }
        }
    }
	

