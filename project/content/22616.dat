<script type='text/javascript'>
	jQuery(window).load(function() {
		if(jQuery.cookie('rating_347') == 'true') {
		rating_readOnly = true;
	} else {
		rating_readOnly = false;
	}
	jQuery('.user-rating').raty({
		half: true,
		size: 13,
		path: 'http://winsetting.ru/wp-content/themes/splash/images/stars/',
		starHalf: 'star-half.png',
		starOff: 'star-off.png',
		starOn: 'star-on.png',
		width: 90,
		readOnly: rating_readOnly,
		hintList: ['bad', 'poor', 'okay', 'good', 'excellent'],
		space: false,
		start: 0,
		click: function(s, e) {
			jQuery.cookie('rating_347', true);
			jQuery.ajax({
				url: 'http://winsetting.ru/wp-admin/admin-ajax.php',
				data: 'action=themesector_rating&rating='+s+'&ip=80.232.144.98&post_id=347',
				type: 'POST',
				dataType: 'json',
				success: function(data) {
					if(data.success == false) {
						alert('You have already rated this post.');
						jQuery('.user-rating').raty('readOnly', true);
						jQuery('.user-rating').raty('start', 0);
					}
				}
			});
		}
	});
		});
