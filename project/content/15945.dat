    jQuery(document).ready(function () {
        jQuery(".widget.city .widget-header, .widget-city .widget-header").live("click", function () {
            jQuery(".city-popup #current-city").text(getCurrentCity());
            jQuery(".city-popup").show();
            jQuery(".city-popup #cityTitle").focus();
        });

        jQuery(".city-popup #cityTitle").autocomplete({
            source: '/global/getcitieslist/',
            select: function (event, ui) {                                
                jQuery(".city-popup #cityId").val(ui.item.id);
            }
        });
    });

    jQuery(".city-popup #cityTitle").bind('keyup', function (e) {
        if (e.keyCode == 13) {
            $(".city-popup #cityTitle").autocomplete("close")
            saveCityChoice();
        }else{
            jQuery(".city-popup #cityId").val(jQuery(this).val());        
        } 
    });
   
   function returnWithoutCityChosen(){
     jQuery(".city-popup").hide();
   }

   function saveCityChoice() {
       if (typeof locSaveCityChoice == "function") {
           locSaveCityChoice();
           
       } else {
           
           jQuery(".city-popup").hide();
           var chosenCity = jQuery(".city-popup #cityId").val();

           jQuery.ajax({
               type: 'POST',
               url: '/global/savecity/',
               data: { city: chosenCity },
               success: function (data) {
                   bindData(data);
