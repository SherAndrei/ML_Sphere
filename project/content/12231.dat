<IDXSKIP><div id="button-move-up"><img src="/img/forum-move-up.png?2" /></div></IDXSKIP>
<script type="text/javascript">
$(document).ready(function()
{
	var button_move_up_flag     = false;
	var button_move_up_position = 0;

	/** Кнопка "Вверх" */
	$('#button-move-up').toggle(function()
	{
		//Запоминаем позицию
		button_move_up_position = $(document).scrollTop();

		//Скролим
		$('html, body').stop().scrollTop(0);

		//Переводим флаг
		button_move_up_flag = true;

	}, function()
	{
		//Скролим
		$('html, body').stop().scrollTop(button_move_up_position);

		//Переводим флаг
		button_move_up_flag = false;
	});

	/** Скрываем кнопку "Вверх" и показываем */
	$(document).scroll(function()
	{
		if($(this).scrollTop() > 0 || button_move_up_flag)
		{
			$('#button-move-up:hidden').fadeIn(0);

			if($(this).scrollTop() > button_move_up_position)
			{
				button_move_up_flag = false;
				button_move_up_position = 0;
			}
		}
		else $('#button-move-up:visible').fadeOut(0);
	});
});
</script>



