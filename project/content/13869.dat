            <script type="text/javascript">

                var about_href = "#" + $("#actual_about_url_id").val();
                var doctors_href = "#" + $("#actual_doctors_url_id").val();
                var services_href = "#" + $("#actual_services_url_id").val();
                var contacts_href = "#" + $("#actual_contacts_url_id").val();

//            var about_href = "#about";
//            var doctors_href = "#doctors";
//            var services_href = "#services";
//            var contacts_href = "#contacts";

//                alert("1 " + about_href + " 2 " + doctors_href + " 3 " + services_href + " 4 " + contacts_href);

            // Чтобы не менять по нескольку раз в разных местах
            $('#menu #about_h a').attr('href', about_href);
            $('#menu #text a').attr('href', doctors_href);
            $('#menu #gallery a').attr('href', services_href);
            $('#menu #info a').attr('href', contacts_href);

            function aboutOffset() {
                var ab = $(about_href).offset().top - 170;
//                    console.log("about: " + ab);
                return $(about_href).offset().top - 170;
            }

            function doctorsOffset() {
                var doc = $(doctors_href).offset().top - 200;
//                    console.log("doctors: " + doc);
                return $(doctors_href).offset().top - 200;
            }

            function servicesOffset() {
                var serv = $(services_href).offset().top - 160;
//                    console.log("services: " + serv);
                return $(services_href).offset().top - 160;
            }

            function contactsOffset() {
                var con = $(contacts_href).offset().top - 170;
//                    console.log("contacts: " + con);
                return $(contacts_href).offset().top - 170;
            }


            //if(! /Android|webOS|iPhone|iPod|BlackBerry/i.test(navigator.userAgent) ) {
            // Необходимые апдейты во время прокрутки сайта



            var isScrolled = false;
            $(window).scroll(function() {

                var scrolled = $(window).scrollTop();
                if (scrolled > $("#pom").offset().top + 100) {
                    //                    console.log($("#pom").offset().top);
                    if (!isScrolled) {
                        $("#menu").addClass('scrolled');
                        isScrolled = true;
                    }
                } else {
                    if (isScrolled) {
                        $("#menu").removeClass('scrolled');
                        isScrolled = false;
                    }
                }

//                    console.log("scrolled: " + scrolled);

                if (scrolled >= aboutOffset() && scrolled < doctorsOffset()) {
                    //window.location.hash = about_href;
                    //document.body.scrollTop = scrolled;
                    $('#menu #about_h a').removeAttr('href');
                    $('#menu #about_h a').addClass('disabledLink');
                } else {
                    $('#menu #about_h a').attr('href', about_href);
                    $('#menu #about_h a').removeClass('disabledLink');
                }

                if (scrolled >= doctorsOffset() && scrolled < servicesOffset()) {
                    //window.location.hash = text_href;
                    //document.body.scrollTop = scrolled;
                    $('#menu #text a').removeAttr('href');
                    $('#menu #text a').addClass('disabledLink');
                } else {
                    $('#menu #text a').attr('href', doctors_href);
                    $('#menu #text a').removeClass('disabledLink');
                }

                if (scrolled >= servicesOffset() && scrolled < contactsOffset()) {
                    //window.location.hash = gallery_href;
                    //document.body.scrollTop = scrolled;
                    $('#menu #gallery a').removeAttr('href');
                    $('#menu #gallery a').addClass('disabledLink');
                } else {
                    $('#menu #gallery a').attr('href', services_href);
                    $('#menu #gallery a').removeClass('disabledLink');
                }

                if (scrolled >= contactsOffset()) {
                    //window.location.hash = info_href;
                    //document.body.scrollTop = scrolled;
                    $('#menu #info a').removeAttr('href');
                    $('#menu #info a').addClass('disabledLink');
                } else {
                    $('#menu #info a').attr('href', contacts_href);
                    $('#menu #info a').removeClass('disabledLink');
                }
            });

            var jump = function(e) {
                e.preventDefault();
                var target = $(this).attr("href");
//                console.log($(target).offset().top - 20);
                $('html,body').animate({
                    scrollTop: $(target).offset().top - 150
                }, 400, function() {
                    location.hash = target;
                });
            };

            $('a[href*=' + about_href + ']').bind("click", jump);
            $('a[href*=' + doctors_href + ']').bind("click", jump);
            $('a[href*=' + services_href + ']').bind("click", jump);
            $('a[href*=' + contacts_href + ']').bind("click", jump);

            window.onload = function() {
//                    go(document.getElementById('about'));
                var res;
                var target;

                res = location.href.split('/');
//                target = '#' + res[4];
                    target = '#' + res[3];

                if (target == "#consult") {
                    console.log(target);
                    location.hash = target;
                } else {
                    $('html,body').animate({
                        scrollTop: $(target).offset().top - 150
                    }, 400, function() {
//                        location.hash = target;
                    });
                }
            }

            $(window).on('hashchange', function(e) {

                var addr = window.location.hash;
                var addr_href = window.location.href.split('/');
                console.log("addr_href: " + addr_href);
                var my_location;
//                my_location = addr_href[0] + '/' + addr_href[1] + '/' + addr_href[2] + '/' + addr_href[3] + '/';
                    my_location = addr_href[0] + '/' + addr_href[1] + '/' + addr_href[2] + '/';
                console.log("my_location: " + my_location);
                var hash = addr.replace("#", "");
                console.log("hash: " + hash);

                if (hash === "consult") {
                    click_call_event("on");
                }
                changeHash(hash, my_location);
            });
//
            function changeHash(id, my_location) {

                console.log("my_loc_id: " + my_location + '' + id);

                try {
                    history.replaceState(null, null, my_location + '' + id);
                }
                catch (e) {
                    location.hash = '#' + id;
                }

            }
