        function exchangeOnline () {
            $.ajax({
                type:'GET',
                url:'/ajax/ExchOnlineParam',
                data: {full_data:true},
                success:function(b) {
                    var data = JSON.parse(b);
                    var new_min_date = min_date(data.date);
                    var change_usd = (data.usd.change_curr>0?"up":"down");
                    var change_eur = (data.eur.change_curr>0?"up":"down");
                    var change_rub = (data.rub.change_curr>0?"up":"down");
                    $('.ex_ch, .ex.sup-cur').removeClass('up').removeClass('down');

                    $('.bl_usd_ex').html(data.usd.avg_curr);
                    $('.bl_eur_ex').html(data.eur.avg_curr);
                    $('.bl_rub_ex').html(data.rub.avg_curr);
                    $('.ch_usd_ex').html(data.usd.change_curr>0?"+"+data.usd.change_curr:data.usd.change_curr).addClass(change_usd);
                    $('.ch_eur_ex').html(data.eur.change_curr>0?"+"+data.eur.change_curr:data.eur.change_curr).addClass(change_eur);
                    $('.ch_rub_ex').html(data.rub.change_curr>0?"+"+data.rub.change_curr:data.rub.change_curr).addClass(change_rub);
                    $('.nbrb_torg_time').html(data.date);
                    $('.ex_date').html(new_min_date);

                }
            });
        }
        function min_date(full_date) {
            var min_date = full_date.replace(/\.[0-9]{4}/g,"");
            var date_time = min_date.split('<br />');
            return date_time[1] + ' ' + date_time[0];
        }

