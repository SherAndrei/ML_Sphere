                </div>
            </div><!-- .author-item -->

                                                                        
                                
                                                            <div class="branch-cont">
                                                                        
                                                    <div class="branch-item"><!-- сообщение -->
                <div class="branch-head"><!-- разворачивалка -->
                                            <div class="advanced-cont" data-karma="0">
                                                    </div>
                                                                                    
                    <span class="wrap-cont">
                        <span class="branch-begin">
                                                    </span>
                    </span>
