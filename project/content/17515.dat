        "datePublished": "2015-11-23T17:10:08+02:00",
            "dateCreated": "2015-11-23T17:10:08+02:00",
                "dateModified": "2015-12-30T15:40:07+02:00",
        
        "publisher": {
        "@type": "Organization",
        "name": "РБК-Украина",
        "logo": {
            "@type": "ImageObject",
