    function reinitMediaContent($objArea) {
        $('.content-slider', $objArea).each(function () {
            $(this).on('init', function (event, slick, currentSlide) {
                var nrCurrentSlide = slick.currentSlide + 1; // daca ai 5 thumb-uri pe pagina pui + 4
                $(this).parent().find('.controls').html(nrCurrentSlide + " / " + slick.slideCount);
            })
        });
        $('.content-slider', $objArea).slick({
            arrows: true,
            dots: false,
            speed: 300,
            prevArrow: '<div class="PrevArrow"></div>',
            nextArrow: '<div class="NextArrow"></div>'
        });

        window.stickyBlockMotion();

        var current = 0; // current slider dupa refresh
        $('.portfolio-thumb-slider .slick-slide:not(.slick-cloned)', $objArea).each(function () {
            $(this).eq(current).addClass('slick-current');
        });
        $('.content-slider', $objArea).each(function () {
            $(this).on('afterChange', function (event, slick, currentSlide, nextSlide) {
                current = currentSlide;
                $(this).parent().find('.portfolio-thumb-slider .slick-slide').removeClass('slick-current');
                $(this).parent().find('.portfolio-thumb-slider .slick-slide:not(.slick-cloned)').eq(current).addClass('slick-current');
                var nrCurrentSlide = slick.currentSlide + 1, // din cauza ca e array si incepe de la 0
                    totalSlidesPerPage = nrCurrentSlide + 3; // daca ai 5 thumb-uri pe pagina pui + 4

                if (totalSlidesPerPage > slick.slideCount) {
                    $(this).parent().find('.controls').html(nrCurrentSlide + " / " + slick.slideCount);
                } else {
                    $(this).parent().find('.controls').html(nrCurrentSlide + " / " + slick.slideCount);
                }
            });
        });
        $('a.last-news__heading', $objArea).on('click', function() {
            $('.last-news__heading', $(this).closest('.last-news')).removeClass('current');
            $('.last-news__heading', $(this).closest('.last-news')).addClass('inactive');
            $(this).removeClass('inactive');
            $(this).addClass('current');
            $('.row', $(this).closest('.last-news')).hide();
            $('.'+$(this).data('tab'), $(this).closest('.last-news')).show();
            sameheight('.last-news .last-news__article');
        });
        
        prepareLightBox();

    }

