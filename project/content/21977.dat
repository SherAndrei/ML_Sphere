<script>
  /**
   * Experiments class,
   * contain basic logic for all experiments.
   * Experiments enable if cookie value more than zero
   * @param options
   * @constructor
   */
  function Experiment(options) {
    if (!options) {
      console && console.error('no Experiment options');
      return;
    }

    this.options = options;
    this.expValue = this._getCookieValue();

    if (this.expValue === '') {
      this.expValue = this._generateExperimentValue();
      this._setCookie();
    }
  }

  Experiment.prototype = {

    /**
     * Return experiment`s cookie value
     * @returns {*}
     */
    getValue: function () {
      return this.expValue;
    },

    /**
     * Checks whether the passed value
     * in the value range of the experiment
     * @param expValue
     * @returns {boolean}
     */
    inRange: function (expValue) {
      var options = this.options;

      return expValue >= options.min && expValue <= options.max;
    },

    logs: function () {
      window.debug && console && console.log('exp value: %s', expValue);
    },

    /**
     * Set experiment`s cookie value
     * @private
     */
    _setCookie: function () {
      var name = this.options.name;

      if (!name && window.debug && window.console) {
        window.console.error('Experiment can`t set cookie, because options not is set the name');
      }

      cookie.set(name, this.expValue.toString(), 30, window._sf.cookieDomain);
    },

    /**
     * Return experiment`s cookie value
     * @returns {*}
     * @private
     */
    _getCookieValue: function () {
      var val = cookie.get(this.options.name);

      if (val === '')
        return val;

      val = parseInt(val);

      if (isNaN(val))
        return '';

      return val;
    },

    /**
     * Validate experiment`s cookie value
     * @param expValue
     * @returns {boolean}
     * @private
     */
    _isNotCookieValue: function (expValue) {
      return expValue < 0 || expValue === 0;
    },

    /**
     * Generate experiment`s value
     * If random value more than options proporsion
     * experiments not switch on.
     * @returns {*}
     * @private
     */
    _generateExperimentValue: function () {
      var options = this.options,
        chance = this._getRandom(1, 100);

      // skip experiments
      if (chance > options.proporsion)
        return 0;

      // enable experiments
      return this._getRandom();
    },

    /**
     * Get random value between min and max
     * If arguments min, max not passed,
     * get value from options
     * @param min - {Number} minimal value
     * @param max - {Number} maximum value
     * @returns {*}
     * @private
     */
    _getRandom: function (min, max) {

      if (!min) {
        min = this.options.min;
      }

      if (!max) {
        max = this.options.max;
      }

      return Math.floor(Math.random() * (max - min + 1)) + min;
    }
  };
</script>
<script>
  function helperPromoH10Experiment(continueFn){
    var bodyClass = 'h10-e', showForm = true;

    _sf.openxAutorun = false;

    var h10 = {
      ready: false,
      videoId: '',
      isHd: null,
      vid: '',
      gaProperty: '',
      gaTrackerName: 'h10',
      eventCategory: 'h10_test_group',
      ssyoutube: false,
      installBtnText1: '',
      installBtnText2: '',

      initExperiment: function(data) {
        var _this = this,
            $box = $('.ssyoutube-helper-promo-h10');

        if (this.ssyoutube) {
          $('#js-install-now').remove();
        }
        this.setImage();

        $box.addClass('lang-' + _sf.lang);
        if (_sf.lang == 'tr') {
          $('.h10_screenshot-box > img', $box).attr('src', '/img/ssyoutube-helper/screenshot_tr.png');
        }

        var $installBtn = $('.h10__install_btn', $box);
        if ($installBtn[0]) {
          $installBtn.addClass('ga_track_events').
            attr('data-ga-event', 'send;pageview;extension/button_os_windows.ext?vid=' + this.vid);

          if (_sf.lang == 'ru') {
            $installBtn.on('click', function(){
              window.ga(_this.gaTrackerName +  '.send', 'event', _this.eventCategory, 'install_click', 'true');
            });
          }

          if (data && data.download_url) {
            $installBtn.attr('href', replaceVidInUrl(data.download_url, this.vid));
          }

          if (this.installBtnText1) {
            $('.h10__install_btn_text_1', $installBtn).html(this.installBtnText1);
          }

          if (this.installBtnText2) {
            $('.h10__install_btn_text_2', $installBtn).html(this.installBtnText2);
          }
        }

        var $freeBtn = $('.h10__mq_btn', $box);
        if ($freeBtn[0]) {
          $freeBtn.on('click', function(event){
              event.preventDefault();

              if (!_this.ssyoutube || !$(this).hasClass('active')) {
                _this.showResult();
                $(this).addClass('active');
              }

              if (_sf.lang == 'ru') {
                window.ga(_this.gaTrackerName +  '.send', 'event', _this.eventCategory, 'download_click', 'true');
              }
          });
        }

        if (_sf.lang == 'ru' && this.ssyoutube) {
          setTimeout(function(){
            if (_this.gaProperty) {
              window.ga('create', _this.gaProperty, 'savefrom.net', {'name': _this.gaTrackerName});
              window.gaHandler.trackers[_this.gaTrackerName] = _this.gaTrackerName;
              window.ga(_this.gaTrackerName + '.send', 'pageview');
            }
          }, 250);
        }

        $box.show();
        this.ready = true;
      },

      showResult: function () {
        if (this.ssyoutube) {
          $('#link-form-new-cont, #link-form-old-cont form').hide();
          $('.open-link-form-all-block').show();
          $('input[name="sf_url"]').val(this.url);
          sf.autoSubmitForm('sf_submit');
        }
        else {
          $('html, body').animate({scrollTop: $('.ssyoutube-helper-promo-h10 .h10__mq_btn').offset().top - 50},
            function(){
              $('#sf_result').css('display', 'block');
            });
        }
      },

      setImage: function() {
        var _this = this,
          src = '//i.ytimg.com/vi/' + this.videoId + '/maxresdefault.jpg',
          $box = $('.ssyoutube-helper-promo-h10 .image-box');
          $box.removeClass('ready').find('img').remove();


        $img = $('<img alt="">').load(function(){
            var $this = $(this),
                width = this.width,
                height = this.height;

            if (width < 720 && height < 720) {
              $(this).remove();
              // new source
              var src = '//i.ytimg.com/vi/' + _this.videoId + '/hqdefault.jpg';
              $box.append($('<img alt="">').load(function(){
                $box.addClass('ready');
                _this.setImagePosition();
              }).attr('src', src));
              return false;
            }
            else {
              $box.addClass('ready');
              _this.setImagePosition();
            }
          }).
          attr('src', src);

        $box.append($img);
      },

      setImagePosition: function()
      {
        var $box = $('.ssyoutube-helper-promo-h10 .image-box'),
            $img = $('img', $box),
            top = Math.round(($box.height() - $img.height()) / 2);

        if (top) {
          $img.css('top', top + 'px');
        }
      },

      show: function(data, videoId)
      {
        $('#js-install-now').remove();

        if (videoId && videoId != this.videoId) {
          this.videoId = videoId;
        }
        else {
          videoId = '';
        }

        if (!this.ready) {
          return this.initExperiment(data);
        }

        if (videoId) {
          this.setImage();
        }
        $('.ssyoutube-helper-promo-h10').show();
      },

      hide: function()
      {
        $('.ssyoutube-helper-promo-h10').hide().removeClass('active');
      }
    };

    function replaceVidInUrl(url, vid)
    {
      return url = url.replace(/((?:\?|&)vid=)\d+/ig, '$1' + vid);
    }

    function runSsyoutube(data)
    {
      $('body').addClass('no-autosubmit');
      _sf.openxEnabled = false;

      h10.ssyoutube = true;
      h10.videoId = videoId;
      h10.url = _sf.hash.url;
      h10.vid = (_sf.lang == 'tr') ? 393 : 392;
      h10.gaProperty = 'UA-7055055-25';
      h10.initExperiment(data, true);
    }

    // Main page experiment H23
    function initH23ControlGroup(data)
    {
      var vid = 351;

      if (data.download_url) {
        data.download_url = replaceVidInUrl(data.download_url, vid);
      }

      if (data.download_box_tpl) {
        data.download_box_tpl = replaceVidInUrl(data.download_box_tpl, vid);
        data.download_box_tpl = data.download_box_tpl.
          replace(/(data-vid=[\"']?)\d+([\"']?)/i, '$1' + vid + '$2');
      }

      $('body')
        .addClass('h23-e')
        .removeClass('h23-e-test')
        .on('click', '#link-form-new-cont .promo__install a[data-action="download"]', function(){
          window.ga('h23_c.send', 'event', 'h23_control_group', 'install_click', 'true');
        });

      $(function(){
        if (sf.carouselBanner.data.helper) {
          sf.carouselBanner.data.helper.href = '/user.php?vid=351';
        }
        sf.carouselBanner.init(false, true);
        $('.partners').show();
      });
      $('#js-install-now').on('click', '.install-btn', function(){
        window.ga('h23_c.send', 'event', 'h23_control_group', 'install_click', 'true');
      });
      $('.r-tabs .r-tabs__title:first').click();

      setTimeout(function(){
        window.ga('create', 'UA-7055055-36', 'savefrom.net', {'name': 'h23_c'});
        window.gaHandler.trackers['h23_c'] = 'h23_c';
        window.ga('h23_c.send', 'pageview');
      }, 250);
    }

    function runH23(data)
    {
      var exp = new Experiment({
        name: 'h23-e',
        proporsion: 100,
        min: 1,
        max: 4
      });

      var expValue = exp.getValue();
      if (expValue % 2 == 1) {
        return initH23ControlGroup(data);
      }

      $('.site-info-tabs').remove();
      $('body').addClass('h23-e').removeClass('h23-e-test');

      $(function(){
        if (sf.carouselBanner.data.helper_new) {
          if (data.download_url) {
            sf.carouselBanner.data.helper_new.href = replaceVidInUrl(data.download_url, 348);            
          }
          else {
            sf.carouselBanner.data.helper_new.href = '/user.php?vid=348';
          }

          sf.carouselBanner.data.helper = sf.carouselBanner.data.helper_new;
        }

        sf.carouselBanner.init(false, true);
      });

      $('#js-install-now').on('click', '.install-btn', function(){
        var vid = this.href.match(/\.exe\?vid=(\d+)/i);
        if (vid) {
          window.ga('send', 'pageview', 'extension/button_os_windows.ext?vid=' + vid[1]);
        }
        window.ga('h23.send', 'event', 'h23_test_group', 'install_click', 'before');
      });

      h10.vid = 349;
      h10.gaProperty = '';
      h10.gaTrackerName = 'h23';
      h10.eventCategory = 'h23_test_group';

      $('.sf-form').on('submit', function(){
        $('.ssyoutube-helper-promo-h10').hide();
      });

      sf.result.addResultCallback(function (result, video, multi)
      {
        if (!result || !video || multi) {
          $('#sf_result').css('display', 'block');
          return;
        }

        if (result.hosting != 101 && result.hosting.indexOf('101') !== 0) {
          $('#sf_result').css('display', 'block');
          return;
        }

        if (!result.id) {
          $('#sf_result').css('display', 'block');
          return;
        }

         $('#sf_result').css('display', 'none');
         h10.show(data, result.id);
      });

      setTimeout(function(){
        window.ga('create', 'UA-7055055-37', 'savefrom.net', {'name': 'h23'});
        window.gaHandler.trackers['h23'] = 'h23';
        window.ga('h23.send', 'pageview');
      }, 250);

      return true;
    }

    function initH23()
    {
      if (_sf.lang != 'ru') {
        return;
      }
      $('body').addClass('h23-e-test');
      sf.popupAfterDownloadVideo.disabled = true;
      _sf.carouselBanner = _sf.carouselBanner || {};
      _sf.carouselBanner.disabled = true;
      showForm = false;

      newMainForm(
        false,
        function(data){ return runH23(data); },
        function(helperEnabled, newDesign){
          if (!helperEnabled) {
            $('body').removeClass('h23-e-test');
            sf.popupAfterDownloadVideo.disabled = false;
            _sf.carouselBanner.disabled = false;
            if (window._ox7) {
              window._ox7.run();
            }
          }
        }
      );
    }

    function isAvailable() {
      if ( (_sf.lang != 'ru' && _sf.lang != 'tr') || _sf.os.name != 'Windows' ) {
        return false;
      }

      if (!_sf.hash || !_sf.hash.utm_campaign || !_sf.hash.url) {
        initH23();
        return false;
      }

      if (_sf.hash.utm_campaign.indexOf('ssyoutube.com') == -1) {
        initH23();
        return false;
      }

      return true;
    }

    function getVideoId(url)
    {
      var url = _sf.hash.url;
      if (url.search(/^https?%3A%2F%2F/i) > -1) {
        url = decodeURIComponent(url);
      }

      var videoId = url.match(/\/\/(?:[a-z]+\.)?youtube\.com\/(?:#!?\/)?watch\?[^\s\"\'\<\>]*v=([\w\-]+)/i);
      if (!videoId) {
        videoId = url.match(/\/\/(?:[a-z0-9]+\.)?youtube\.com\/embed\/([\w\-]+)/i);
      }

      if (!videoId || videoId.length < 2) {
        return '';
      }

      return videoId[1];
    }

    function stop() {
      $('body').removeClass(bodyClass);

      if (showForm) {
        $(function(){ 
          window.newMainForm();
          if (window._ox7) {
            window._ox7.run();
          }
        });
      }

      if (continueFn) {
        continueFn.call(window);
      }
    }

    if (!isAvailable()) {
      return stop();
    }

    var videoId = getVideoId(_sf.hash.url);
    if (!videoId) {
      return stop();
    }

    // ssyoutube experiment H10
    newMainForm(
      false,
      function(data){ runSsyoutube(data); },
      function(helperEnabled, newDesign){
        if (!helperEnabled) {
          $('body').removeClass(bodyClass);
        }
      }
    );
  }
</script>
