                setForm();
              },
              function(){
               setForm();
              }
            );

            function runExperiment(template)
            {
              if (!window.Experiment) {
                setForm();
                return;
              }

              var boxId = 'link-form-new-cont';

              try {
                boxId = '#' + boxId;
                $(boxId).find('.js-download-box-container').replaceWith($(template));
                setNewLayout();
              }
              catch (err) {
                console.log(err);
                setForm();
              }

              setForm(true);
            }
          }
          else {
            setForm();
          }

          function setNewLayout()
          {
            sf.newMain = true;
          }

          function setForm(newDesign)
          {
            var $formsContainer = $(".open-link-form-all-block");

            var className = 'open-link-form-old',
                id = 'link-form-old-cont';

            if (newDesign == 1) {
              // show new form
              className = 'open-link-form-new';
              id = 'link-form-new-cont';
            }

            $formsContainer.addClass(className);
            sf.setFormIds({ container: '#' + id });

            $(document).trigger('sf_form:ready'); // оповещает, что форма готова к использованию
            $('.open-link-form-all-block .sk-wave').hide();

            if (callbackFn) {
              callbackFn(helperEnabled, newDesign);
            }
          }

          function getHelperDownloadBox (vid, success, fail)
          {
            var jqxhr = $.post('/tools/ext_helper.php', {
                v: 2,
                lang: _sf.lang,
                action: 'get_helper_download_box',
                tpl: 'new',
                no_uvd: 1,
                setup_instruction: 0,
                vid: vid,
                uid: _sf.stats.uid,
                video_id: getVideoId()
              },
              success,
             'json');

            if (fail) {
              jqxhr.fail(fail);
            }
          }

          function getVideoId()
          {
            if (!_sf.hash || !_sf.hash.url) {
              return '';
            }

            var url = _sf.hash.url;

            if (url.search(/^https?%3A%2F%2F/i) > -1) {
              url = decodeURIComponent(url);
            }

            var videoId = url.match(/\/\/(?:[a-z]+\.)?youtube\.com\/(?:#!?\/)?watch\?[^\s\"\'\<\>]*v=([\w\-]+)/i);
            if (!videoId) {
              videoId = url.match(/\/\/(?:[a-z0-9]+\.)?youtube\.com\/embed\/([\w\-]+)/i);
            }

            if (!videoId || videoId.length < 2) {
              return '';
            }

            return videoId[1];
          }
        }
