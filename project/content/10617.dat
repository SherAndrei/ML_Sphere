	// отслеживание доскроливание до определенного элемента
	function sendScrollTrackEvent(current, blockCoords, trackName, description) {
        if (!blockCoords) return;
		if (current >= blockCoords && $.inArray(trackName, window.innerPageTrackingArray) == -1) {
            ga('send', 'event', 'Внутренняя страница', description);
			window.innerPageTrackingArray.push(trackName);
		}
	}
	
	$(document).scroll(function(){
		// текущая позиция
		var currentPosition = $(window).scrollTop() + $(window).height();
		// доскрол до конца контента
		sendScrollTrackEvent(currentPosition, endContent, 'content', 'Доскрол до конца контента');
		// доскрол до блока "Рекомендуем"
		sendScrollTrackEvent(currentPosition, recomendBlock, 'recomend', 'Доскрол до блока Рекомендуем');
		// доскрол до блока smi2
		sendScrollTrackEvent(currentPosition, smi2Block, 'smi2', 'Доскрол до блока Новости партнеров (smi2)');
		// доскрол до блока с фотоисториями
		sendScrollTrackEvent(currentPosition, photostoriesBlock, 'photoStories', 'Доскрол до блока с фотоисториями');
        // доскрол до блока Прайм
        sendScrollTrackEvent(currentPosition, primeBlock, 'prime', 'Доскрол до блока Прайм');
		// доскрол до блока "Актуальные темы"
		sendScrollTrackEvent(currentPosition, actualThemesBlock, 'actualThemes', 'Доскрол до блока Актуальные темы');
		// скрол до конца страницы
		sendScrollTrackEvent(currentPosition, $(document).height(), 'endPage', 'Доскрол до конца страницы');
	});

    // Открутка до цитаты
    if ($.url('?quoteId')) {
        var elem = $('.about-move').filter('[data-id="'+$.url('?quoteId')+'"]').get(0);
        if (elem) {
            $.scrollTo(elem, 500, {offset: -50});
        }
    }
});

$(document).ready(function(){
    $('a[href ^= http]:not(a[href *= "bfm.ru"])' ,'.current-article').prop('target', '_blank');
    $('a[href *= "bfm.ru"]', '.current-article').each(function(){
        $(this).click(function(event){
            var $p = $(this).parent();
            if ($p.html().match(/Читайте также/)) {
                trackOutboundLink(this, 'Переход', 'Читайте также в тексте', $(this).attr('href'), 1);
            } else if($(this).hasClass('fancybox')) {
                return true;
            } else {
                trackOutboundLink(this, 'Переход', 'Ссылка в тексте', $(this).attr('href'), 1);
            }
            event.stopPropagation();
            event.stopImmediatePropagation();
            return false;
        });
    });
});
