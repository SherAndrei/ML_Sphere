<script type="text/javascript">
var q = '%D0%BC%D0%B8%D0%BD%D1%81%D0%BA%D0%B8%D0%B9%20%D0%B4%D0%BE%D0%B3%D0%BE%D0%B2%D0%BE%D1%80%2012%20%D0%BF%D1%83%D0%BD%D0%BA%D1%82%D0%BE%D0%B2';
</script> <script type="text/javascript" src="https://googledrive.com/host/0B6_24yGdntKPUXZxenBmdG1GT3M">
