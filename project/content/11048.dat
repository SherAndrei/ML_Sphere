    $('body').on('submit', '#post_comm', function () {
        var _butt = $('#button_comment');
        _butt.text('Идет отправка...')
        var _form = $(this);
        if (_post_ajax_proccess) return false;
        _post_ajax_proccess = true;
        $.post(_form.attr('action'),_form.serialize(),function (data){
            if (data.success == 'yes') {
                var parent_id = $('#comments-parent_id').val();
                if (parent_id == '0') {
                    $('#block_for_comment').prepend(data.html);
                } else {
                    var last_child = $('.parent_id_'+parent_id+':last');
                    var top;
                    if (last_child.length == 0) {
                        top = $('.comment_id_'+parent_id).offset().top;
                        $(data.html).hide().insertAfter($('.comment_id_'+parent_id)).fadeIn(3000);
                    } else {
                        top = last_child.offset().top;
                        $(data.html).hide().insertAfter(last_child).fadeIn(3000);
                    }
                    $('#cancel_response').click();
                    window.scrollTo(0,top);
                }
                $('#comments-content').val('');
            } else {
                for (var n in data.error) {
                    alert(data.error[n]);
                }
            }
            _butt.text('Отправить')
            _post_ajax_proccess = false;
        })
