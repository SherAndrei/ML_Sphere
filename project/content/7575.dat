    }

    function updateIncutDisplayStat(inIds) {
        if (inIds.length)
            $.post('https://regnum.ru/update/incutmultipledisplaystat', {incut_ids: inIds.join(','), incut_page: 1});
    }

    function updateIncutClickStat(inId) {
        $.post('https://regnum.ru/update/incutclickstat', {incut_id: inId, incut_page: 1});
