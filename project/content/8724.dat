        <div class="post_date"><a  class="post_link"  href="/wall-101076544_285"  onclick="return showWiki({w: 'wall-101076544_285'}, false, event);" ><span class="rel_date">25 окт 2015</span></a></div>
        <div class="ui_actions_menu_wrap _ui_menu_wrap " onmouseover="uiActionsMenu.show(this);" onmouseout="uiActionsMenu.hide(this);">
