    //now, that all the initial page's content is loaded...
    sizeContentBars();

	//keep aligning all bars' bottoms in case content gets ADDED (not taken out) to any of the bars (content-bar, narrow-sidebar1, and narrow-sidebar2, wide-sidebar) by means of AJAX or whatever
	setInterval(function(){
		if (viewport().width > 1215)
		{
			var neededSidebarsHeight =
                document.getElementById('bodyAndSidebars').offsetHeight -
                document.getElementById('wide-sidebar').offsetHeight -
                parseInt(document.getElementById('wide-sidebar').style.marginBottom);
			document.getElementById('narrow-sidebar1').style.height = neededSidebarsHeight + 'px';
			document.getElementById('narrow-sidebar2').style.height = neededSidebarsHeight + 'px';
		}
		else
		{
			document.getElementById('narrow-sidebar1').style.height = 'auto';
			document.getElementById('narrow-sidebar2').style.height = 'auto';

            //when scrolling page to the very bottom really fast
            stickSidebars();
		}
	}, 2000);
};

