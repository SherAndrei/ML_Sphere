<script type="text/javascript">

$(document).ready(function() {	
	//Show Banner
	$(".main_map_image .desc").show(); //Show Banner
	$(".main_map_image .block_map").animate({ opacity: 0.7 }, 1 ); //Set Opacity
	//Click and Hover events for thumbnail list
	$(".image_thumb_map ul li:first").addClass('active'); 
	$(".image_thumb_map ul li").click(function(){ 
		//Set Variables
		var imgAlt = $(this).find('img').attr("alt"); //Get Alt Tag of Image
		var imgTitle = $(this).find('a').attr("href"); //Get main_map Image URL
		var imgDesc = $(this).find('.block_map').html(); 	//Get HTML of block_map
		var imgDescHeight = $(".main_map_image").find('.block_map').height();	//Calculate height of block_map	
		if ($(this).is(".active")) {  //If it's already active, then...
			return false; // Don't click through
		} else {
			//Animate the Teaser				
			$(".main_map_image .block_map").animate({ opacity: 0, marginBottom: -imgDescHeight }, 250 , function() {
				$(".main_map_image .block_map").html(imgDesc).animate({ opacity: 0.85,	marginBottom: "0" }, 250 );
				$(".main_map_image img").attr({ src: imgTitle , alt: imgAlt});
			});
		}
		$(".image_thumb_map ul li").removeClass('active'); //Remove class of 'active' on all lists
		$(this).addClass('active');  //add class of 'active' on this list only
		return false;
	}) .hover(function(){
		$(this).addClass('hover');
		}, function() {
		$(this).removeClass('hover');
	});
	//Toggle Teaser
	$("a.collapse").click(function(){
		$(".main_map_image .block_map").slideToggle();
		$("a.collapse").toggleClass("show");
	});
});//Close Function
</script>

