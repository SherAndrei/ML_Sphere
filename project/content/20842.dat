    <script type="text/javascript">
    
    var editorObject;
    
    $(function() {
	editorObject = $("#comment_text").cleditor({
							width: 530,
							height: 150,
							controls: "bold italic underline strikethrough link unlink"
						    })[0];
	$('#add_comment_btn').on('click', subimNewComment);
	$('img.delete_comment')
	    .on('click', deleteComment)
	    .on('mouseover', function() { $(this).attr('src', 'i/cross.png'); })
	    .on('mouseout', function() { $(this).attr('src', 'i/s_cross.png'); });
    });
    
    
    function deleteComment(){
	var container = $(this).parents('div.photo_comment_div');
	var postId = $("#post_id").val();
	var commentId = container.attr('cid');
	
	$.post(
	    'blogajax.php?action=delcomment', 
	    {
		commentId: commentId, 
		postId : postId
	    }, 
	    function(data){
		if(data.code != '0'){
		    var msg;
		    switch(data.code){
			case '2':
			    msg = 'Операция требует авторизации';
			    break;
			case '3':
			    msg = 'Запись блога не найдена. Проверьте параметры запроса';
			    break;
			case '4':
			    msg = 'Доступ запрещен';
			    break;   
			case '1':
			    msg = 'Комментарий не найден';
			    break;   
			default:
			    msg = data.message;
			    break;
		    }
		    alert(msg);
		    return;
		}
		container.fadeOut(200, function(){container.remove();});
	    },
	    "json"
	    );
    }
    
    function subimNewComment(){
	
	var author = $("#new_author").val();
	var mail = $("#new_mail").val();
	var capcha = $("#new_capcha").val();
	var msg = $("#comment_text").val();
	var postId = $("#post_id").val();
	
	if(author == '' || capcha == '' || msg == ''){
	    alert('Не все поля заполнены!');
	    return;
	}
	if(mail != '' && !mailRegex.test(mail)){
	    alert('Почта указано неверно');
	    return;
	}
	
	$.post(
	    'blogajax.php?action=addcomment', 
	    {
		author: author, 
		mail : mail,
		capcha: capcha,
		msg: msg,
		postId: postId
	    },
	    function(data){
		var r = Math.random();
		$("#new_capcha").val("");
		$('#capcha_image').attr("src", "../image.php?s=postcomment&r=" + r);
		
		if(data.code != '0'){
		    var emsg;
		    switch(data.code){
			case '2':
			    emsg = 'Ошибка: проверьте текст комментария';
			    break;
			case '3':
			    emsg = 'Ошибка: Проверочный код не совпадает!';
			    break;
			case '4':
			    emsg = 'Запись не найдена';
			    break;   
			case '1':
			    emsg = 'Адрес почты указан неверно';
			    break;   
			default:
			    emsg = data.message;
			    break;
		    }
		    alert(emsg);
		    return;
		}
		
		try{
		    editorObject.clear();
		}
		catch(e){
		    
		}
		var commentTitle = $('<div></div>',{
				    'class': 'photo_comment_title'
				    });
		if(data.authorized == '1'){
		    commentTitle.append($('<img></img>', {
				    'class': 'delete_comment',
				    'src': 'i/s_cross.png',
				    'title': 'Удалить комментарий'
				    })
				    .on('click', deleteComment)
				    .on('mouseover', function() { $(this).attr('src', 'i/cross.png'); })
				    .on('mouseout', function() { $(this).attr('src', 'i/s_cross.png'); })
				);
		}
		commentTitle.append(author + ' (' + data.appendDate + ')');
		
		var comment = $('<div></div>',{
			    'class': 'photo_comment_div',
			    'style': 'display: none',
			    'cid': data.commentId
			})
			.append(commentTitle)
			.append($('<div></div>', {
				    'style': 'padding-left: 10px;',
				    'html': msg
				})
			);
		
		$('#photo_comments_collection').prepend(comment);
		comment.fadeIn(200);
	    },
	    "json"
	);
    }
