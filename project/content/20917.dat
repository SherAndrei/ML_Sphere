var opts = {"owner_id":20247898,"start":0,"offset":20,"per_page":20,"count":27,"url":"\/notes20247898","params":false,"href":"\/notes20247898?offset=","max_post_len":280,"post_hash":"daa514040aa3608cf9","media_types":false,"reply_names":[],"wall_type":"full_all","with_id":0,"rmedia_types":[["photo","Фотография"],["video","Видеозапись"],["audio","Аудиозапись"],["doc","Документ"],["note","Заметка"],["map","Карта"],["graffiti","Граффити"],["postpone","Таймер",{"lang":{"profile_choose_timer":"Время публикации","profile_wall_postpone_at":"в","profile_wall_postpone_btn":"В очередь","export_to_twitter":"экспорт в Twitter","export_to_facebook":"экспорт в Facebook","friends_only":"только для друзей"},"date":1480902641}]],"share":{"url":"https:\/\/pu.vk.com\/c01\/upload.php","hash":"7e1029f63552c336523c132b5504bf3b","rhash":"1c9188350482f5b69c867604f7bc16f7","timehash":"1480899041_063ecefc14cdf897f4","allow_custom_photo":true,"allow_edit_title":true,"max_title_len":100},"search":false,"upload":null}, preload = [27,40,"<div id=\"post20247898_289\" class=\"_post post page_block\" data-post-id=\"20247898_289\" onclick=\"wall.postClick('20247898_289', event);\" >\n  <div class=\"_post_content\">\n    \n    <div class=\"post_header\">\n      <a class=\"post_image\" href=\"\/eva.stixiya\">\n        <img src=\"https:\/\/pp.vk.me\/c837328\/v837328898\/8900\/2U2shdr44yQ.jpg\" data-post-id=\"20247898_289\" width=\"50\" height=\"50\" class=\"post_img\" \/>\n        <span class=\"blind_label\">.<\/span>\n      <\/a>\n      <div class=\"post_header_info\">\n        <h5 class=\"post_author\"><a class=\"author\" href=\"\/eva.stixiya\" data-from-id=\"20247898\" data-post-id=\"20247898_289\">Наташа Янпольская<\/a> <span class=\"explain\"><span class=\"wall_fixed_label\">&nbsp;запись закреплена<\/span><\/span><\/h5>\n        <div class=\"post_date\"><a  class=\"post_link\"  href=\"\/wall20247898_289\"  onclick=\"return showWiki({w: 'wall20247898_289'}, false, event);\" ><span class=\"rel_date\">16 фев 2011<\/span><\/a><\/div>\n        \n      <\/div>\n    <\/div>\n    <div class=\"post_content\">\n      <div class=\"post_info\">\n        <div class=\"wall_text\"><div id=\"wpt20247898_289\" class=\"_wall_post_cont\"><div class=\"wall_post_text\"><a class=\"wall_note_type\" href=\"\/note20247898_10700971\">Про ежиков&#33;&#33;&#33;<\/a><div class=\"wk_text\">Про ежиков.. <br\/><br\/>знаешь, что ёжики разумные существа??? Не веришь?? <br\/><br\/>Вот положи перед ёжиком 200г. Ветчины и кусок булки. Ёжик обязательно съест ветчину и не притронется к булке. Как разумное существо он понимает, что переедать вредно.. <br\/><br\/>Попробуй предложить ёжику работу с минимальной оплатой труда, ну хотя бы инженера..... и ёжик откажется от неё. Мало того, он настолько ловко и умно прикинется полным идиотом, ничего не понимающим в чертежах, что ты ничего не сможешь с ним поделать. <br\/><br\/>Понаблюдай, как ёжик ведёт себя при встрече с собакой......он шипит, выгибает спину огромным горбом. А знаешь почему? Да потому, что только ёжики знают, что собаки терпеть не могут верблюдов... <br\/><br\/>Видала ли ты женатого ёжика? А тем более ёжика, выплачивающего алименты хотя бы мышами? Это ли не высшее проявление разума... <br\/><br\/>Попробуй предложить ему сколько угодно денег.....и он их ни за что не возьмёт)))) Иметь дело с рэкетом, налоговой полицией только ради того, чтобы в конце концов всё равно лишиться своей шкуры, может только менее развитое существо - человек. А ёжику хорошо и на диване... <br\/><br\/>Вот так.. <\/div><\/div><\/div><\/div>\n        <div class=\"post_full_like_wrap clear_fix\">\n  <div class=\"post_full_like\">\n    <a href=\"#\" class=\"post_like _like_wrap \" onmouseover=\"Wall.likesShow(this, '20247898_289')\" onclick=\"return Wall.likeIt(this, '20247898_289', '159ec459efc14139ca', event);\">\n      <i class=\"post_like_icon _icon\"><\/i>\n      <span class=\"post_like_link _link\"><\/span>\n      <span class=\"post_like_count _count\">1<\/span>\n    <\/a>\n    <span class=\"blind_label\" tabindex=\"0\" role=\"link\" onclick=\"Wall.likesShowList(this, '20247898_289')\">Показать список оценивших<\/span>\n    <a href=\"#\" class=\"post_share _share_wrap no_shares\"  onmouseover=\"Wall.sharesShow(this, '20247898_289')\" onclick=\"return Wall.sharesOpen(event, '20247898_289');\">\n      <i class=\"post_share_icon _icon\"><\/i>\n      <span class=\"blind_label\"><\/span>\n      <span class=\"post_share_link _link\"><\/span>\n      <span class=\"post_share_count _count\"><\/span>\n    <\/a>\n    <span class=\"blind_label\" tabindex=\"0\" role=\"link\" onclick=\"Wall.sharesShowList(this, '20247898_289')\">Показать список поделившихся<\/span>\n  <\/div>\n  \n<\/div>\n        <div class=\"replies\"><div class=\"replies_wrap\" id=\"replies_wrap20247898_289\" style=\"display: none\">\n  <div class=\"replies_list _replies_list\" id=\"replies20247898_289\"><input type=\"hidden\" id=\"start_reply20247898_289\" value=\"\" \/><\/div>\n  \n<\/div><\/div>\n      <\/div>\n    <\/div>\n    \n  <\/div>\n  \n<\/div><div id=\"post20247898_note10673714\" class=\"_post post page_block\" data-post-id=\"20247898_note10673714\" onclick=\"wall.postClick('20247898_note10673714', event);\" >\n  <div class=\"_post_content\">\n    \n    <div class=\"post_header\">\n      <a class=\"post_image\" href=\"\/eva.stixiya\">\n        <img src=\"https:\/\/pp.vk.me\/c837328\/v837328898\/8900\/2U2shdr44yQ.jpg\" data-post-id=\"20247898_note10673714\" width=\"50\" height=\"50\" class=\"post_img\" \/>\n        <span class=\"blind_label\">.<\/span>\n      <\/a>\n      <div class=\"post_header_info\">\n        <h5 class=\"post_author\"><a class=\"author\" href=\"\/eva.stixiya\" data-from-id=\"20247898\" data-post-id=\"20247898_note10673714\">Наташа Янпольская<\/a> <span class=\"explain\"><span class=\"wall_fixed_label\">&nbsp;запись закреплена<\/span><\/span><\/h5>\n        <div class=\"post_date\"><a  class=\"post_link\"  href=\"\/note20247898_10673714\" ><span class=\"rel_date\">11 фев 2011<\/span><\/a><\/div>\n        \n      <\/div>\n    <\/div>\n    <div class=\"post_content\">\n      <div class=\"post_info\">\n        <div class=\"wall_text\"><div id=\"wpt20247898_note10673714\" class=\"_wall_post_cont\"><div class=\"wall_post_text\"><a class=\"wall_note_type\" href=\"\/note20247898_10673714\">Без названия<\/a><div class=\"wk_text\">МНОГО, но и много интересного нашла... <br\/>1. Забудь про недостатки. Думай только о своих достоинствах. Первая заповедь. <br\/>2. Не опускай подбородок. Девушки, которым нравится быть лучшими, всегда смотрят прямо перед собой. <br\/>3. Больше позитива. Будь активна и уверена в себе. Позволь энергии бить ключом. Старайся источать как можно больше положительных эмоций. Ассоциируйся у окружающих с солнышком. <br\/>4. Говори губами. Ты не представляешь, на что способна капля блеска для твоих губ, - такие губы хочется целовать! <br\/>5. Умей слушать. Уважай чужое мнение. Не навязывай свое. <br\/>6. Ухаживай за бровями. Глаза – зеркало души. Но не стоит забывать и про оправу! Корректировать форму бровей лучше у специалиста. <br\/>7. Не бойся меняться. Развивайся, двигайся вперед. Будь разносторонней. Не циклись на одном. Пробуй. Ищи себя. <br\/>8. Пахни собой. Найди &quot;свой&quot; запах и оставайся ему верна. Запахи ассоциируются с людьми. <br\/>9. Экономь на румянах. Больше времени проводи на воздухе. Барышню, нагулявшую на морозе румянец, немедленно хочется согреть. <br\/>10. Будь увлеченной. Например, научись фехтовать на самурайских мечах. Ведь любой парень мечтает встречаться с супердевушкой. <br\/>11. Не копируй других. Найди свой стиль. Только глупец будет гордиться тем, что его девушка похожа на Анджелину Джоли. <br\/>12. Запасись конфеткой. Все рекламы твердят, что свежее дыхание облегчает понимание. Так и есть! <br\/>13. Кушай. С аппетитом и красиво. Слово &quot;диета&quot; раздражает. <br\/>14. Следи за шагами. Пусть походка будет женственной. Не смотри под &#91;&#124;ноги&#93;, не спеши, не сутулься. <br\/>15. Уменьшай размеры. <br\/>Пересмотри размеры сумки. Зачем нужен баул, когда можно обойтись симпатичной маленькой сумочкой? <br\/>16. Больше читай. С интересным человеком хочется общаться. <br\/>17. Будь целеустремленной. Поставь перед собой цель и иди к ней. Верь внутреннему голосу. Цельные натуры притягивают. <br\/>18. Запасись специями. Научись готовить хотя бы одно блюдо и радуй им друзей и близких. Даже если это простой яблочный пирог. <br\/>19. Береги голову. Вычурная укладка – это хорошо. Для торжественных случаев. А волосы, к которым хочется прикоснуться, выглядят как у романтических барышень из фильмов про пиратов. <br\/>20. Умолчи о главном. Девушки, не надо на первом же свидании все о себе рассказывать! Оставляйте некоторую загадку, парням так нравится отгадывать загадки! <br\/>21. Не повышай голос. Держи себя в руках. <br\/>22. Играй! Снимай кино про себя, играй главную роль, примеряй различные образы. Будь разной и одинаково настоящей всегда. <br\/>23. Не умничай. Раздражает, когда девушка, желая продемонстрировать, какая она умная, сыплет цитаты через фразу. <br\/>24. Дари подарки. Друзьям и знакомым. Маленькие и даже самодельные. Просто так. Потому что у тебя хорошее настроение. <br\/>25. Возьми за руку. Прикасайся к человеку во время разговора. Психологи утверждают, что это помогает снять напряжение и расположить собеседника к себе. <br\/>26. Не дергайся. Не гоняй крошки по столу, не мни салфетки, не ковыряй корешок книжки. Эти жесты ужасны. Вместо того, чтобы нервно тискать волосы, сдуй прядь челки с лица. Многие считают это сексуальным. <br\/>27. Почувствуй разницу. Одевайся сексуально, но не вульгарно. Или короткая юбка, или смелая кофта, или яркая помада. Но вместе – никогда! <br\/>28. Делай ручки. Не забывай про &#91;&#124;маникюр&#93;. Ухоженная рука – это та, которую хочется держать в своей руке. <br\/>29. Чаще улыбайся. Пусть все видят: ты счастлива. <br\/>30. Меньше говори о себе. Пусть люди сами спрашивают. <br\/>31. Не плачь. Девочки полагают: этот трюк способен растрогать сильный пол. На самом деле – наоборот. <br\/>32. Не экономь на аксессуарах. Носи знак отличия. Пусть даже самый маленький. <br\/>33. Строй глазки. Большие глаза завораживают. Желательно, чтобы в черноте туши они были видны! <br\/>34. Смотри в оба. Когда разговаривают с человеком, который интересен, смотрят в глаза. Именно это называется умением строить глазки, а не жеманное закатывание орбит. <br\/>35. Хвали. Честно. Делай окружающим комплименты. Только искренне! <br\/>36. Ступай смело. Обувь должна быть удобной. Ковыляя на шпильках, парня не покоришь. А паря в классных «Адидасах» (если ему, конечно, нравится «Адидас»), очаруешь любого точно. <br\/>37. Помни про ЧЮ. Не бойся смеяться над собой. Это свидетельствует о твоем чувстве юмора. <br\/>38. На волне. Научись разговаривать с парнями на их языке, понимать их шутки. Это важно! <br\/>39. Будь проще. Не делай из мухи слона. <br\/>40. Меньше сиди дома. Вокруг куча интересного! Исследуй мир. И поделись открытиями с друзьями. <br\/>41. Взлетай! Научись кокетливо вскидывать бровь. Парней это сводит с ума! <br\/>42. Спокуха! Не думай постоянно о том, как выглядишь со стороны. Расслабься и наслаждайся. <br\/>43. Вспомни дни рождения всех – это святое. Поэтому составь список и своевременно по нему поздравляй. <br\/>44. Стилизация. Выдерживай индивидуальный стиль в одежде. Не сливайся с толпой, но и не чуди. <br\/>45. Не завидуй. Радуйся тому, что у тебя есть. <br\/>46. Признавайся в любви. Но тем, кого действительно любишь. <br\/>47. Не трепи. Лучше сделай. Верь делам, а не словам. Слова бывают пусты. <br\/>48. Будь другом. Не позволяй говорить плохо о дорогих тебе людях. Это ценят. <br\/>49. Разделяй интересы. Дорогого тебе человека. <\/div><\/div><\/div><\/div>\n        <div class=\"post_full_like_wrap clear_fix\">\n  <div class=\"post_like_wrap\">\n    <a href=\"#\" class=\"post_like _like_wrap no_likes\" onmouseover=\"Wall.likesShow(this, '20247898_note10673714')\" onclick=\"return Wall.likeIt(this, '20247898_note10673714', '851c84d64c258e3a73', event);\">\n      <i class=\"post_like_icon _icon\"><\/i>\n      <span class=\"post_like_link _link\"><\/span>\n      <span class=\"post_like_count _count\"><\/span>\n    <\/a>\n    <span class=\"blind_label\" tabindex=\"0\" role=\"link\" onclick=\"Wall.likesShowList(this, '20247898_note10673714')\">Показать список оценивших<\/span>\n  <\/div>\n  <span id=\"reply_link20247898_note10673714\" class=\"reply_link_wrap\"><a href=\"#\" class=\"reply_link\" onclick=\"return Wall.showEditReply('20247898_note10673714', event)\">Комментировать<\/a><\/span>\n<\/div>\n        <div class=\"replies\"><div class=\"replies_wrap\" id=\"replies_wrap20247898_note10673714\" style=\"display: none\">\n  <div class=\"replies_list _replies_list\" id=\"replies20247898_note10673714\"><input type=\"hidden\" id=\"start_reply20247898_note10673714\" value=\"\" \/><\/div>\n  <div id=\"reply_box_wrap20247898_note10673714\"><div class=\"reply_box_inner_wrap\">\n  <div class=\"reply_fakebox_wrap\" id=\"reply_fakebox20247898_note10673714\" onclick=\"return Wall.showEditReply('20247898_note10673714', event)\" data-owner-photo=\"\" data-owner-href=\"\">\n    <div class=\"post_field_user\">\n  <a class=\"post_field_user_link\" href=\"\/id\" onclick=\"return nav.go(this, event)\">\n    <img class=\"post_field_user_image\" src=\"\/images\/camera_50.png\" \/>\n  <\/a>\n<\/div>\n    <div class=\"reply_fakebox\">Написать комментарий...<\/div><\/div>\n<\/div><\/div>\n<\/div><\/div>\n      <\/div>\n    <\/div>\n    \n  <\/div>\n  \n<\/div><div id=\"post20247898_258\" class=\"_post post page_block\" data-post-id=\"20247898_258\" onclick=\"wall.postClick('20247898_258', event);\" >\n  <div class=\"_post_content\">\n    \n    <div class=\"post_header\">\n      <a class=\"post_image\" href=\"\/eva.stixiya\">\n        <img src=\"https:\/\/pp.vk.me\/c837328\/v837328898\/8900\/2U2shdr44yQ.jpg\" data-post-id=\"20247898_258\" width=\"50\" height=\"50\" class=\"post_img\" \/>\n        <span class=\"blind_label\">.<\/span>\n      <\/a>\n      <div class=\"post_header_info\">\n        <h5 class=\"post_author\"><a class=\"author\" href=\"\/eva.stixiya\" data-from-id=\"20247898\" data-post-id=\"20247898_258\">Наташа Янпольская<\/a> <span class=\"explain\"><span class=\"wall_fixed_label\">&nbsp;запись закреплена<\/span><\/span><\/h5>\n        <div class=\"post_date\"><a  class=\"post_link\"  href=\"\/wall20247898_258\"  onclick=\"return showWiki({w: 'wall20247898_258'}, false, event);\" ><span class=\"rel_date\">4 фев 2011<\/span><\/a><\/div>\n        \n      <\/div>\n    <\/div>\n    <div class=\"post_content\">\n      <div class=\"post_info\">\n        <div class=\"wall_text\"><div id=\"wpt20247898_258\" class=\"_wall_post_cont\"><div class=\"wall_post_text\"><a class=\"wall_note_type\" href=\"\/note20247898_10640306\">вселенная...мое стихотворение...<\/a><div class=\"wk_text\">Вселенная, как много в этом слове <br\/>Того, что не дано еще понять <br\/>Всем нам, которые, как в море, <br\/>Никак не могут сушу отыскать. <br\/><br\/>Живем, живем, не видя света, <br\/>Погрязши в безнадежности своей. <br\/>И ждем откуда – то ответа, <br\/>Когда наступит власть царей? <br\/><br\/>Которые за нас все смогут, <br\/>Свершат великие дела, <br\/>Которые, подобно Богу, <br\/>Вдохнут любовь в наши тела. <br\/><br\/>Не кажется ль вам это глупым? <br\/>Быть может стоит нам самим <br\/>Уж ото сна воспрянуть духом, <br\/>А мы лишь лень в себе творим. <br\/><br\/>Ведь мы – частички всей Вселенной, <br\/>И это вовсе не смешно, <br\/>А может стоит нам наверно, <br\/>Искоренить навеки зло. <br\/><br\/>Иначе жить в счастливом мире <br\/>Уж никогда не суждено. <br\/>Задумайтесь, иль вы забыли, <br\/>Что все мы с вами за одно. <br\/><br\/>А вам по нраву эта скука: <br\/>Лежать и не смотреть в окно, <br\/>И если сердце в груди бьется, <br\/>Нам не должно быть все равно. <br\/><br\/>Ну неужели мы не видим, <br\/>Что мир летит в тартарары, <br\/>Товарищи ну оглянитесь, <br\/>Как вы не видите вины? <br\/><br\/>Я не навязываю мненья, <br\/>Я лишь хочу вам уяснить, <br\/>Что с этим страхом и сомненьем <br\/>Никак нельзя на свете жить. <\/div><\/div><\/div><\/div>\n        <div class=\"post_full_like_wrap clear_fix\">\n  <div class=\"post_full_like\">\n    <a href=\"#\" class=\"post_like _like_wrap \" onmouseover=\"Wall.likesShow(this, '20247898_258')\" onclick=\"return Wall.likeIt(this, '20247898_258', '276ba58e7ed6df3337', event);\">\n      <i class=\"post_like_icon _icon\"><\/i>\n      <span class=\"post_like_link _link\"><\/span>\n      <span class=\"post_like_count _count\">2<\/span>\n    <\/a>\n    <span class=\"blind_label\" tabindex=\"0\" role=\"link\" onclick=\"Wall.likesShowList(this, '20247898_258')\">Показать список оценивших<\/span>\n    <a href=\"#\" class=\"post_share _share_wrap no_shares\"  onmouseover=\"Wall.sharesShow(this, '20247898_258')\" onclick=\"return Wall.sharesOpen(event, '20247898_258');\">\n      <i class=\"post_share_icon _icon\"><\/i>\n      <span class=\"blind_label\"><\/span>\n      <span class=\"post_share_link _link\"><\/span>\n      <span class=\"post_share_count _count\"><\/span>\n    <\/a>\n    <span class=\"blind_label\" tabindex=\"0\" role=\"link\" onclick=\"Wall.sharesShowList(this, '20247898_258')\">Показать список поделившихся<\/span>\n  <\/div>\n  \n<\/div>\n        <div class=\"replies\"><div class=\"replies_wrap\" id=\"replies_wrap20247898_258\" style=\"display: none\">\n  <div class=\"replies_list _replies_list\" id=\"replies20247898_258\"><input type=\"hidden\" id=\"start_reply20247898_258\" value=\"\" \/><\/div>\n  \n<\/div><\/div>\n      <\/div>\n    <\/div>\n    \n  <\/div>\n  \n<\/div><div id=\"post20247898_note10640265\" class=\"_post post page_block\" data-post-id=\"20247898_note10640265\" onclick=\"wall.postClick('20247898_note10640265', event);\" >\n  <div class=\"_post_content\">\n    \n    <div class=\"post_header\">\n      <a class=\"post_image\" href=\"\/eva.stixiya\">\n        <img src=\"https:\/\/pp.vk.me\/c837328\/v837328898\/8900\/2U2shdr44yQ.jpg\" data-post-id=\"20247898_note10640265\" width=\"50\" height=\"50\" class=\"post_img\" \/>\n        <span class=\"blind_label\">.<\/span>\n      <\/a>\n      <div class=\"post_header_info\">\n        <h5 class=\"post_author\"><a class=\"author\" href=\"\/eva.stixiya\" data-from-id=\"20247898\" data-post-id=\"20247898_note10640265\">Наташа Янпольская<\/a> <span class=\"explain\"><span class=\"wall_fixed_label\">&nbsp;запись закреплена<\/span><\/span><\/h5>\n        <div class=\"post_date\"><a  class=\"post_link\"  href=\"\/note20247898_10640265\" ><span class=\"rel_date\">4 фев 2011<\/span><\/a><\/div>\n        \n      <\/div>\n    <\/div>\n    <div class=\"post_content\">\n      <div class=\"post_info\">\n        <div class=\"wall_text\"><div id=\"wpt20247898_note10640265\" class=\"_wall_post_cont\"><div class=\"wall_post_text\"><a class=\"wall_note_type\" href=\"\/note20247898_10640265\">Мои стихи... продолжение...<\/a><div class=\"wk_text\">Ночная коробка <br\/><br\/>Пустая коробка стоит у стены. <br\/>Открыв, мы увидим там свои сны, <br\/>Увидим в ней небо, увидим луну, <br\/>Звонок будильника уже поутру. <br\/>Увидим в ней книги, пустой потолок, <br\/>Подушку, кровать и бурчащий живот. <br\/>Еще в ней пижаму сумеем найти, <br\/>Одеяло, светильник и фонари, <br\/>Серую улицу, темный подъезд, <br\/>Куриц, севших на насест. <br\/>Чтение сказок перед сном, <br\/>Крики кошек под окном. <br\/>Гудки машин и тишину, <br\/>Вой собаки на луну. <br\/>Биенье сердца и любовь, <br\/>Шептание еле слышных слов. <br\/>Увидим слезы, окон свет, <br\/>Пепел, дым от сигарет. <br\/>Огонь костра, туман и дождь, <br\/>В коробку это спрятала ночь. <\/div><\/div><\/div><\/div>\n        <div class=\"post_full_like_wrap clear_fix\">\n  <div class=\"post_like_wrap\">\n    <a href=\"#\" class=\"post_like _like_wrap no_likes\" onmouseover=\"Wall.likesShow(this, '20247898_note10640265')\" onclick=\"return Wall.likeIt(this, '20247898_note10640265', '5220bc570f0ab49d41', event);\">\n      <i class=\"post_like_icon _icon\"><\/i>\n      <span class=\"post_like_link _link\"><\/span>\n      <span class=\"post_like_count _count\"><\/span>\n    <\/a>\n    <span class=\"blind_label\" tabindex=\"0\" role=\"link\" onclick=\"Wall.likesShowList(this, '20247898_note10640265')\">Показать список оценивших<\/span>\n  <\/div>\n  <span id=\"reply_link20247898_note10640265\" class=\"reply_link_wrap\"><a href=\"#\" class=\"reply_link\" onclick=\"return Wall.showEditReply('20247898_note10640265', event)\">Комментировать<\/a><\/span>\n<\/div>\n        <div class=\"replies\"><div class=\"replies_wrap\" id=\"replies_wrap20247898_note10640265\" style=\"display: none\">\n  <div class=\"replies_list _replies_list\" id=\"replies20247898_note10640265\"><input type=\"hidden\" id=\"start_reply20247898_note10640265\" value=\"\" \/><\/div>\n  <div id=\"reply_box_wrap20247898_note10640265\"><div class=\"reply_box_inner_wrap\">\n  <div class=\"reply_fakebox_wrap\" id=\"reply_fakebox20247898_note10640265\" onclick=\"return Wall.showEditReply('20247898_note10640265', event)\" data-owner-photo=\"\" data-owner-href=\"\">\n    <div class=\"post_field_user\">\n  <a class=\"post_field_user_link\" href=\"\/id\" onclick=\"return nav.go(this, event)\">\n    <img class=\"post_field_user_image\" src=\"\/images\/camera_50.png\" \/>\n  <\/a>\n<\/div>\n    <div class=\"reply_fakebox\">Написать комментарий...<\/div><\/div>\n<\/div><\/div>\n<\/div><\/div>\n      <\/div>\n    <\/div>\n    \n  <\/div>\n  \n<\/div><div id=\"post20247898_145\" class=\"_post post page_block\" data-post-id=\"20247898_145\" onclick=\"wall.postClick('20247898_145', event);\" >\n  <div class=\"_post_content\">\n    \n    <div class=\"post_header\">\n      <a class=\"post_image\" href=\"\/eva.stixiya\">\n        <img src=\"https:\/\/pp.vk.me\/c837328\/v837328898\/8900\/2U2shdr44yQ.jpg\" data-post-id=\"20247898_145\" width=\"50\" height=\"50\" class=\"post_img\" \/>\n        <span class=\"blind_label\">.<\/span>\n      <\/a>\n      <div class=\"post_header_info\">\n        <h5 class=\"post_author\"><a class=\"author\" href=\"\/eva.stixiya\" data-from-id=\"20247898\" data-post-id=\"20247898_145\">Наташа Янпольская<\/a> <span class=\"explain\"><span class=\"wall_fixed_label\">&nbsp;запись закреплена<\/span><\/span><\/h5>\n        <div class=\"post_date\"><a  class=\"post_link\"  href=\"\/wall20247898_145\"  onclick=\"return showWiki({w: 'wall20247898_145'}, false, event);\" ><span class=\"rel_date\">15 ноя 2010<\/span><\/a><\/div>\n        \n      <\/div>\n    <\/div>\n    <div class=\"post_content\">\n      <div class=\"post_info\">\n        <div class=\"wall_text\"><div id=\"wpt20247898_145\" class=\"_wall_post_cont\"><div class=\"wall_post_text\"><a class=\"wall_note_type\" href=\"\/note20247898_10320188\">Без названия<\/a><div class=\"wk_text\"><b>Мысли... <\/b> <br\/>В тишине пустынь холодных, <br\/>Среди скал, возросших ввысь, <br\/>Ты живешь с тоской голодной, <br\/>Кинув призрачную жизнь. <br\/>Глухо манит побережье <br\/>Жидкой, сумрачной луны, <br\/>Тебя в куклу мертвой жизни <br\/>Превратили колдуны. <br\/><div class=\"wall_view_note\"><a onclick=\"FullWall.notePart(this, 'note10320188');\">Показать полностью…<\/a><\/div><span id=\"note10320188\" style=\"display: none;\">Ты не думаешь о смерти, <br\/>Ведь она уже пришла, <br\/>И навеки в твоих мыслях <br\/>Свое место заняла. <br\/>Тебе трудно примириться созвездой в своем пути, <br\/>И в глушь глубин пустыньхолодных <br\/>Тебя мысли увели. <br\/><br\/><center><\/center><center><b><i><u>Ночная коробка<\/u><\/i><\/b><\/center><center><\/center><center>Пустая коробка стоит устены.<\/center><center>Открыв, мы увидим там своисны,<\/center><center>Увидим в ней небо, увидимлуну,<\/center><center>Звонок будильника ужепоутру.<\/center><center>Увидим в ней книги, пустойпотолок,<\/center><center>Подушку, кровать ибурчащий живот.<\/center><center>Еще в ней пижаму сумеемнайти,<\/center><center>Одеяло, светильник и фонари,<\/center><center>Серую улицу, темныйподъезд,<\/center><center>Куриц, севших на насест.<\/center><center>Чтение сказок перед сном,<\/center><center>Крики кошек под окном.<\/center><center>Гудки машин и тишину,<\/center><center>Вой собаки на луну.<\/center><center>Биенье сердца и любовь,<\/center><center>Шептание еле слышных слов.<\/center><center>Увидим слезы, окон свет,<\/center><center>Пепел, дым от сигарет.<\/center><center>Огонь костра, туман идождь,<\/center>          В коробку это спряталаночь. <\/span><\/div><\/div><\/div><\/div>\n        <div class=\"post_full_like_wrap clear_fix\">\n  <div class=\"post_full_like\">\n    <a href=\"#\" class=\"post_like _like_wrap \" onmouseover=\"Wall.likesShow(this, '20247898_145')\" onclick=\"return Wall.likeIt(this, '20247898_145', '76903f4823fd5c2ffe', event);\">\n      <i class=\"post_like_icon _icon\"><\/i>\n      <span class=\"post_like_link _link\"><\/span>\n      <span class=\"post_like_count _count\">1<\/span>\n    <\/a>\n    <span class=\"blind_label\" tabindex=\"0\" role=\"link\" onclick=\"Wall.likesShowList(this, '20247898_145')\">Показать список оценивших<\/span>\n    <a href=\"#\" class=\"post_share _share_wrap no_shares\"  onmouseover=\"Wall.sharesShow(this, '20247898_145')\" onclick=\"return Wall.sharesOpen(event, '20247898_145');\">\n      <i class=\"post_share_icon _icon\"><\/i>\n      <span class=\"blind_label\"><\/span>\n      <span class=\"post_share_link _link\"><\/span>\n      <span class=\"post_share_count _count\"><\/span>\n    <\/a>\n    <span class=\"blind_label\" tabindex=\"0\" role=\"link\" onclick=\"Wall.sharesShowList(this, '20247898_145')\">Показать список поделившихся<\/span>\n  <\/div>\n  \n<\/div>\n        <div class=\"replies\"><div class=\"replies_wrap\" id=\"replies_wrap20247898_145\" style=\"display: none\">\n  <div class=\"replies_list _replies_list\" id=\"replies20247898_145\"><input type=\"hidden\" id=\"start_reply20247898_145\" value=\"\" \/><\/div>\n  \n<\/div><\/div>\n      <\/div>\n    <\/div>\n    \n  <\/div>\n  \n<\/div><div id=\"post20247898_144\" class=\"_post post page_block\" data-post-id=\"20247898_144\" onclick=\"wall.postClick('20247898_144', event);\" >\n  <div class=\"_post_content\">\n    \n    <div class=\"post_header\">\n      <a class=\"post_image\" href=\"\/eva.stixiya\">\n        <img src=\"https:\/\/pp.vk.me\/c837328\/v837328898\/8900\/2U2shdr44yQ.jpg\" data-post-id=\"20247898_144\" width=\"50\" height=\"50\" class=\"post_img\" \/>\n        <span class=\"blind_label\">.<\/span>\n      <\/a>\n      <div class=\"post_header_info\">\n        <h5 class=\"post_author\"><a class=\"author\" href=\"\/eva.stixiya\" data-from-id=\"20247898\" data-post-id=\"20247898_144\">Наташа Янпольская<\/a> <span class=\"explain\"><span class=\"wall_fixed_label\">&nbsp;запись закреплена<\/span><\/span><\/h5>\n        <div class=\"post_date\"><a  class=\"post_link\"  href=\"\/wall20247898_144\"  onclick=\"return showWiki({w: 'wall20247898_144'}, false, event);\" ><span class=\"rel_date\">15 ноя 2010<\/span><\/a><\/div>\n        \n      <\/div>\n    <\/div>\n    <div class=\"post_content\">\n      <div class=\"post_info\">\n        <div class=\"wall_text\"><div id=\"wpt20247898_144\" class=\"_wall_post_cont\"><div class=\"wall_post_text\"><a class=\"wall_note_type\" href=\"\/note20247898_10320184\">Без названия<\/a><div class=\"wk_text\"><center><b><i><u>Судьба<\/u><\/i><\/b><\/center><center><b><i><u><\/u><\/i><\/b><\/center><center><i>Прости меня судьба моя,<\/i><\/center><center><i>Что, может, я была глупа.<\/i><\/center><center><i>Что я не видела огня,<\/i><\/center><center><i>Горевшего внутри меня<\/i><\/center><center><i><\/i><\/center><center><i>За то, что я была слепа,<\/i><\/center><div class=\"wall_view_note\"><a onclick=\"FullWall.notePart(this, 'note10320184');\">Показать полностью…<\/a><\/div><span id=\"note10320184\" style=\"display: none;\"><center><i>Но были все в слезах глаза.<\/i><\/center><center><i>И было больно и темно.<\/i><\/center><center><i>А жизнь стучала мне в окно.<\/i><\/center><center><i><\/i><\/center><center><i>Увы, никто меня не слышал.<\/i><\/center><center><i>Пойми же ты меня хоть раз.<\/i><\/center><center><i>Я лишь хотела быть поближе<\/i><\/center><center><i>К смыслу твоих скучных фраз.<\/i><\/center><center><i><\/i><\/center><center><i>Мне хотелось тихо плакать.<\/i><\/center><center><i>Спрятав от тебя свой взгляд,<\/i><\/center><center><i>А ты достала снова жало<\/i><\/center><center><i>И пустило горький яд.<\/i><\/center><center><i><\/i><\/center><center><i>Нет, не вижу я дороги<\/i><\/center><center><i>Залила зрачки тоска.<\/i><\/center><center><i>Я упаду в конце дороги,<\/i><\/center><center><i>И придет опять зима.<\/i><\/center><center><i><\/i><\/center><center><i>Она скроет от прохожих<\/i><\/center><center><i>Мои мысли под замок,<\/i><\/center><center><i>А снег летит и кругом лужи,<\/i><\/center><center><i>И твой голос снова смолк…<\/i><\/center> <\/span><\/div><\/div><\/div><\/div>\n        <div class=\"post_full_like_wrap clear_fix\">\n  <div class=\"post_full_like\">\n    <a href=\"#\" class=\"post_like _like_wrap \" onmouseover=\"Wall.likesShow(this, '20247898_144')\" onclick=\"return Wall.likeIt(this, '20247898_144', 'f82e36f6a3011902cd', event);\">\n      <i class=\"post_like_icon _icon\"><\/i>\n      <span class=\"post_like_link _link\"><\/span>\n      <span class=\"post_like_count _count\">1<\/span>\n    <\/a>\n    <span class=\"blind_label\" tabindex=\"0\" role=\"link\" onclick=\"Wall.likesShowList(this, '20247898_144')\">Показать список оценивших<\/span>\n    <a href=\"#\" class=\"post_share _share_wrap no_shares\"  onmouseover=\"Wall.sharesShow(this, '20247898_144')\" onclick=\"return Wall.sharesOpen(event, '20247898_144');\">\n      <i class=\"post_share_icon _icon\"><\/i>\n      <span class=\"blind_label\"><\/span>\n      <span class=\"post_share_link _link\"><\/span>\n      <span class=\"post_share_count _count\"><\/span>\n    <\/a>\n    <span class=\"blind_label\" tabindex=\"0\" role=\"link\" onclick=\"Wall.sharesShowList(this, '20247898_144')\">Показать список поделившихся<\/span>\n  <\/div>\n  \n<\/div>\n        <div class=\"replies\"><div class=\"replies_wrap\" id=\"replies_wrap20247898_144\" style=\"display: none\">\n  <div class=\"replies_list _replies_list\" id=\"replies20247898_144\"><input type=\"hidden\" id=\"start_reply20247898_144\" value=\"\" \/><\/div>\n  \n<\/div><\/div>\n      <\/div>\n    <\/div>\n    \n  <\/div>\n  \n<\/div><div id=\"post20247898_note7622720\" class=\"_post post page_block\" data-post-id=\"20247898_note7622720\" onclick=\"wall.postClick('20247898_note7622720', event);\" >\n  <div class=\"_post_content\">\n    \n    <div class=\"post_header\">\n      <a class=\"post_image\" href=\"\/eva.stixiya\">\n        <img src=\"https:\/\/pp.vk.me\/c837328\/v837328898\/8900\/2U2shdr44yQ.jpg\" data-post-id=\"20247898_note7622720\" width=\"50\" height=\"50\" class=\"post_img\" \/>\n        <span class=\"blind_label\">.<\/span>\n      <\/a>\n      <div class=\"post_header_info\">\n        <h5 class=\"post_author\"><a class=\"author\" href=\"\/eva.stixiya\" data-from-id=\"20247898\" data-post-id=\"20247898_note7622720\">Наташа Янпольская<\/a> <span class=\"explain\"><span class=\"wall_fixed_label\">&nbsp;запись закреплена<\/span><\/span><\/h5>\n        <div class=\"post_date\"><a  class=\"post_link\"  href=\"\/note20247898_7622720\" ><span class=\"rel_date\">14 окт 2008<\/span><\/a><\/div>\n        \n      <\/div>\n    <\/div>\n    <div class=\"post_content\">\n      <div class=\"post_info\">\n        <div class=\"wall_text\"><div id=\"wpt20247898_note7622720\" class=\"_wall_post_cont\"><div class=\"wall_post_text\"><a class=\"wall_note_type\" href=\"\/note20247898_7622720\">Про последние события...<\/a>Как прошли выходные?<br\/>У меня... да уж у меня &quot;весело&quot;&#33; В субботу пошла гулять - замерзла, как 150 китайцев, не выдержала приехала на такси домой. Сижу значит я за компом, играю, смотрю дым повалил из-за компа. Ну думаю, мой компьютер горит&#33;&#33;&#33; Выключаю, а дым все идет,выхожу я из комнаты, открываю дверь... а там туман-туманище&#33; Кашляю, глаза слезятся. А мама с папой спят, прикинь. Бужу их, мол кричу: ГОРИМ&#33;&#33;&#33;&#33;&#33;&#33;&#33;&#33;&#33; Потом тушили ведрами. В подвале проводка загорелась что ли?&#33; Нижняя лестница начала гореть&#33; А прикинь не замерзла бы я, и не пришла бы пораньше домой? А еще загорелась то она прямо рядом с котлом, обогревательным, газовым&#33; Еще бы 15 минут и бу-бух&#33; И нет ни дома ни родителей, а если бы я приехала пораньше домой и заснула, то и меня тоже&#33;<br\/>Не выспалась&#33;<br\/>Приехала в воскресенье в Старый, а ко мне Яна приехала и не давала мне спать&#33;<br\/>Не выспалась&#33;<br\/>А вчера девчонки слишком буйно веселились на кухне всю ночь, им сегодня никуда не идти, высыпаются, а я ... опять не выспалась&#33;<br\/>на работе все нормально, привыкаю&#33;<br\/>Пишу статьи по чуть-чуть&#33; В завтрашнем номере 2 мои статьи сразу выйдут&#33; Вчера с одной из них запарилась&#33; Про ярмарку&#33; Пришлось за один день писать, фотки делать, все редактировать и сдавать в печать&#33;<br\/>А сейчас в &quot;одноклассниках&quot; сижу...<\/div><\/div><\/div>\n        <div class=\"post_full_like_wrap clear_fix\">\n  <div class=\"post_like_wrap\">\n    <a href=\"#\" class=\"post_like _like_wrap no_likes\" onmouseover=\"Wall.likesShow(this, '20247898_note7622720')\" onclick=\"return Wall.likeIt(this, '20247898_note7622720', '189117ef40ea90e1ba', event);\">\n      <i class=\"post_like_icon _icon\"><\/i>\n      <span class=\"post_like_link _link\"><\/span>\n      <span class=\"post_like_count _count\"><\/span>\n    <\/a>\n    <span class=\"blind_label\" tabindex=\"0\" role=\"link\" onclick=\"Wall.likesShowList(this, '20247898_note7622720')\">Показать список оценивших<\/span>\n  <\/div>\n  \n<\/div>\n        <div class=\"replies\"><div class=\"replies_wrap\" id=\"replies_wrap20247898_note7622720\" style=\"\">\n  <div class=\"replies_list _replies_list\" id=\"replies20247898_note7622720\"><input type=\"hidden\" id=\"start_reply20247898_note7622720\" value=\"19241553\" \/><div id=\"post20247898note_19241553\" class=\"reply reply_dived clear  _post\" data-post-id=\"20247898note_19241553\" >\n  <div class=\"reply_wrap _reply_content _post_content clear_fix\">\n    \n    <a class=\"reply_image\" href=\"\/id3854672\">\n      <img src=\"https:\/\/pp.vk.me\/c637119\/v637119672\/20275\/OLskXxdiwtc.jpg\" width=\"40\" height=\"40\" class=\"reply_img\" alt=\"Александр Осмалов\" \/>\n    <\/a>\n    <div class=\"reply_content\" >\n      \n      <div class=\"reply_author\">\n        <a class=\"author\" href=\"\/id3854672\" data-from-id=\"3854672\">Александр Осмалов<\/a>\n      <\/div>\n      <div class=\"reply_text\" ><div id=\"wpt20247898note_19241553\"><div class=\"wall_reply_text\">СУПЕР-ЧЕЛОВЕК&#33;&#33;&#33;<\/div><\/div><\/div>\n      <div class=\"reply_footer clear_fix\" id=\"wpe_bottom20247898note_19241553\">\n        \n        <div class=\"reply_date\"><a class=\"wd_lnk\"  class=\"post_link\"  href=\"\/note20247898_7622720?reply=19241553\" ><span class=\"rel_date\">14 окт 2008<\/span><\/a><\/div>\n        \n      <\/div>\n    <\/div>\n  <\/div>\n<\/div><div id=\"post20247898note_19249377\" class=\"reply reply_dived clear  _post\" data-post-id=\"20247898note_19249377\" >\n  <div class=\"reply_wrap _reply_content _post_content clear_fix\">\n    \n    <a class=\"reply_image\" href=\"\/eva.stixiya\">\n      <img src=\"https:\/\/pp.vk.me\/c837328\/v837328898\/8900\/2U2shdr44yQ.jpg\" width=\"40\" height=\"40\" class=\"reply_img\" alt=\"Наташа Янпольская\" \/>\n    <\/a>\n    <div class=\"reply_content\" >\n      \n      <div class=\"reply_author\">\n        <a class=\"author\" href=\"\/eva.stixiya\" data-from-id=\"20247898\">Наташа Янпольская<\/a>\n      <\/div>\n      <div class=\"reply_text\" ><div id=\"wpt20247898note_19249377\"><div class=\"wall_reply_text\">Ой, спасибо&#33;<\/div><\/div><\/div>\n      <div class=\"reply_footer clear_fix\" id=\"wpe_bottom20247898note_19249377\">\n        \n        <div class=\"reply_date\"><a class=\"wd_lnk\"  class=\"post_link\"  href=\"\/note20247898_7622720?reply=19249377\" ><span class=\"rel_date\">15 окт 2008<\/span><\/a><\/div>\n        \n      <\/div>\n    <\/div>\n  <\/div>\n<\/div><\/div>\n  <div id=\"reply_box_wrap20247898_note7622720\"><div class=\"reply_box_inner_wrap\">\n  <div class=\"reply_fakebox_wrap\" id=\"reply_fakebox20247898_note7622720\" onclick=\"return Wall.showEditReply('20247898_note7622720', event)\" data-owner-photo=\"\" data-owner-href=\"\">\n    <div class=\"post_field_user\">\n  <a class=\"post_field_user_link\" href=\"\/id\" onclick=\"return nav.go(this, event)\">\n    <img class=\"post_field_user_image\" src=\"\/images\/camera_50.png\" \/>\n  <\/a>\n<\/div>\n    <div class=\"reply_fakebox\">Написать комментарий...<\/div><\/div>\n<\/div><\/div>\n<\/div><\/div>\n      <\/div>\n    <\/div>\n    \n  <\/div>\n  \n<\/div>",{"3854672":["Александру","Александр, "],"_":false,"20247898":["Наташе","Наташа, "]}];
