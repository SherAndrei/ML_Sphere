    $(document).ready(function () {
        setTimeout(function () {

                    // Eсть не адаптированные окна шириной 800+, показываем только на ПК
                    // А так же не показываем:
                    // для пользователей с отключенными куками, иначе для них окна будет открываться на каждоый странице
                    // если уже открыто какое-то окно
                    if (1024 >= App.detectWindowSize().width || !App.isEnabledCookie() || allReadyHasPopupShow()) {
                        return;
                    }

                    var inst = $.remodal.lookup[$('#popup').data('remodal')];
                    if (inst) {
                        inst.open();
                    }

                    $('#popup .js-on-remodal-open').click();

                    var cookieString = $.cookie('popup_cookie');
                    var cookie;
                    if (cookieString == undefined) {
                        cookie = {};
                    } else {
                        cookie = JSON.parse(cookieString);
                    }
                    if (cookie.ids == undefined) {
                        cookie.ids = [];
                    }
                    cookie.ids.push(12);
                    cookie.last_show = 1496134738; //серверное время

                    $.cookie('popup_cookie', JSON.stringify(cookie), {expires: 60, path: '/'})

                    if (inst) {
                        // с адблокером нет попапа и нет прокрутки, проверяем состояние и удаляем
                        setTimeout(function () {
                            if (inst.getState() != 'opened') {
                                inst.close();
                                inst.destroy();
                            }
                        }, 2000);
                    }

                }, 12000
        );


        // Проверяет есть ли открытие окна через remodal and fancybox
