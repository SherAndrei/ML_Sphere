<script>
   function build_support_form( place, callback ) {
          place = $( place );
          $.ajax( { url : '/' + language + '/node_product/contact/form/?site_id='+1+'&rand='+Math.random(),
                    success : function ( data ) {
                         $.getScript( '/static/js/jquery.form.js' );
                         $.getScript( '/static/faq/faq_site_1_ru.js', function () {
                                  place.find( "#faq-list" ).empty().append( html_faq );
                                  init_faq( place );
                         } );
                         place.empty().append( data );
                         place.find('* [placeholder]' ).placeholder();
                         user_email = "";
                         user_name = " ";
                         if ( user_email ) {
                             place.find( 'input#inputEmail' ).val( user_email );
                             place.find( 'input#name' ).val( user_name );
                         }
                         //place.find( '#contents' ).focus();
                         support_work_time( place );
                         // меняет бутстраповский на обычный файловый input
                         support_form_ie_fix( place );
                         // вешаем нужные обработчики
                         place.find( '#attach-file' ).click( function () {
                              place.find( 'input[id=attachment]' ).click();
                         });
                         place.find( '#faq-search' ).keyup( function () {
                              faq_search( place );
                         });
                         callback();
                        },
                   type : 'GET'
                   });
        }

   jQuery( document ).ready( function () {
        $support_dialog = makeDialog("#support-dialog", {
            width: 900,
            height: 'auto'
        });

        $support_dialog.open = function ( ) {
          var dialog = this;
          build_support_form( "#support-dialog", function () {
               dialog.dialog( "open" );
               $( '#support-dialog #faq-search' ).focus();
               $( '#support-dialog button#send-msg' ).click( function () {
                   sendMessage( $( "#support-dialog" ), function () { $( '#support-dialog' ).dialog( "close" ); } );
               });
          } );
        }

   } );
</script>

