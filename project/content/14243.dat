<script id="cartTmpl" type="text/x-jquery-tmpl">
    <div class="cart_table_box cart_${id}">
        {{if _itemsCnt}}
        <form class="cart-form" action="/checkout/csh_remember/" method="POST">
            <input type="hidden" name="csrfmiddlewaretoken" value="{{html csrftoken}}">
            <input type="hidden" name="cart_id" value="${id}" />
            <div class="cart_provider">
                <b><span>${cartI18N.provider}</span>
                    <a href="#category=${provider_id}&type=provider">${provider_name}</a></b></div>
            <div>
                <table class="cart_table" style="width:100%">
                    <tr class="cart_table_head">
                        <td style="width:2px"></td>
                        <td style="width:75px;">${cartI18N.in_stock}</td>
                        <td align="center" colspan="2">${cartI18N.product}</td>
                        <td style="width:120px;" align="center">${cartI18N.qty}</td>
                        {{if use_weight}}
                        <td align="center" class="weight_cl" style="width:75px;">${cartI18N.weight}</td>
                        {{/if}}
                        <td align="center" style="width:150px;">${cartI18N.price}</td>
                        <td style="width:70px;" align="center">${cartI18N.deleteB}</td>
                        <td style="width:10px;"></td>
                    </tr>
                    <tr class="tr_cart_summ">
                        <td></td>
                        <td></td>
                        <td></td>
                        <td></td>
                        <td align="right">${cartI18N.subtotal}</td>
                        {{if use_weight}}
                        <td align="center" class="weight_cl cart_weight_summ" style="font-weight: bold;"></td>
                        {{/if}}
                        <td align="center" class="cart_total" style="font-weight: bold;">${ cldr.currencyFormatter(currency)(total) }</td>
                        <td align="center"><a class="cart-clear" style="font-size: 11px;">${cartI18N.delete_all}</a></td>
                        <td></td>
                    </tr>
                </table>
            </div>

            <div class="clear"></div>

            {{if fstock}}
            <div style="margin-top: 5px">
                 <div class="outofstock">
                      <div  style="color: red; float: left; width: 10px; font-size: 20px;"><sup>*</sup></div>
                      <div>
                        ${cartI18N.minus}
                      </div>
                 </div>
            </div>
            {{/if}}

            <div class="timer_block">
                {{html cartI18N.ship_refersh}}
                <a class="start-check-now" href="#">${cartI18N.refresh}</a>
            </div>

            <div class="cart-ajax-loading" align="center">
                   ${cartI18N.wait}<br>
                   <img src="/static/images/ajax-loader.t1462131733t.gif"> <br>
                   <span class="gray-text fs0d9em">${cartI18N.min2_3}</span>
            </div>

            <div class="postage"></div>
        </form>
        {{/if}}
    </div>
</script>
