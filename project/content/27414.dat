function insertBlogImageFile(id)
{
	img = BX.findChild(BX('wd-doc'+id), {'tag': 'img'}, true, false);
	src = img.getAttribute('rel');
	imageId = id+'file';
	pLEditor = window.oBlogComLHE;
	if (!pLEditor.arBlogImages[imageId])
	{
		pLEditor.arBlogImages[imageId] = {
