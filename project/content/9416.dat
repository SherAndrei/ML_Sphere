        $.observer.add('set_user_community_status', function(e, s) {
            if (s == 0) {
                $('.hotel-profile-community-users .side-subscribe-btn.app-user-allocation-group').show();
            } else {
                $('.hotel-profile-community-users .side-subscribe-btn.app-user-allocation-group').hide();
            }
        });

        $.app.auth.check().done(function() {
            $.depends('app.userAllocationGroup').done(function() {
                $.app.userAllocationGroup.checkMembership(alId).always(function() {
                    $.observer.notify('set_user_community_status', [(this.response||{}).status||0]);
                });
            });
        }).fail(function() {
            $.observer.notify('set_user_community_status', [0]);
        });

        $.observer.add('hotel-group-membership-changed', function(e, al, userId, status) {
            if (status) {
                if (userId) {
                    $.observer.notify('set_user_community_status', [parseInt(status||0)]);
                }
            } else {
                $.observer.notify('set_user_community_status', [0]);
            }
        });
    });
</script>
    
    
