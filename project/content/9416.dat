    $(function(){
        //открытие скрытых блоков инфо на мобильных разрешениях
        $('.act-open-hide-block').on('click', function () {
            $(this).toggleClass('open');
            $(this).closest('.hotel-profile__ttl-mobile-display').next().toggleClass('close');
        });

        var $tooltips = $(".tooltip");

        if ($tooltips.length) {
            //показ картинок
            $tooltips.each(function () {
                var $this = $(this),
                    $content = $(document.createElement('span')).addClass('tooltip-content').html($this.attr('title')),
                    tooltipWidth = $this.data('tooltipWidth');

                if (tooltipWidth) {
                    $content.width(tooltipWidth);
                }

                $this.append($content).removeAttr('title');
            });

            var ua = window.navigator.userAgent;
            var msie = ua.indexOf("MSIE ");

            function msieversion() {

                var ua = window.navigator.userAgent;
                var msie = ua.indexOf("MSIE ");

                if (msie > 0 || !!navigator.userAgent.match(/Trident.*rv\:11\./)) { // If Internet Explorer, return version number
                    return parseInt(ua.substring(msie + 5, ua.indexOf(".", msie)));
                } else { // If another browser, return 0
                    return 0;
                }
            }

            if (msie && msieversion() < 7) {
                $('.tooltip').on('mouseover mouseout', function (aEvt) {
                    $(this).children('.tooltip-content').css({
                        opacity: 1,
                        visibility: (aEvt.type == 'mouseover') ? 'visible' : 'hidden'
                    });
                });
            }
        }
    });
</script>

    

