<script>
    $(function(){

        //установка min-height в зависимсти от высоты блока catalog-banner-holder-right
        if(window.innerWidth > 1279){
            $('.act-hotel-description-wrap').css('min-height', $('.act-banner-holder-right').outerHeight(true));
        }

        function fixDescriptionHeight() {
            //проверка высоты контента в блоке
            if(window.innerWidth <= 639){
                if($('.act-hotel-description-wrap').outerHeight(true) < 400) {
                    $('.act-hotel-description').addClass('hotel-profile-full-info_little-content');
                    $('.act-open-hotel-description').closest('.hotel-profile__more-wrap').addClass('hidden');
                }
            };
        }

        $(window).resize(function() {
            fixDescriptionHeight();
        });

        fixDescriptionHeight();

        //правим стили для описания идущего с FCKEditor'a чтобы соотвествовало версточным
//         $(".hotel-profile-full-info .ckeditor-block-wrap")
//             .find('p')
//             .addClass('hotel-profile-full-info-p');

        $('.act-open-hotel-description').on('click', function () {
            $('.act-hotel-description').addClass('open');
            $(this).closest('.hotel-profile__more-wrap').addClass('hidden');
        });

        var refreshCaptchaSrc = function() {
            $.ajax({url: '/new/actions/captcha/?math=1&refresh=1&name=submit_description_form&rnd=' + Math.random(), dataType: 'json', cache: false, success: function (data) {
                $("#submit_description_form").find("#captcha_confirm_value")
                    .attr("src", data.url + '&name=submit_description_form');
            }});
        };

        var resetDescriptionForm = function() {

            var $form = $("#submit_description_form");
            $form.find("input[name='username']")
                    .val('');
            $form.find("input[name='hotelurl']")
                    .val('');
            $form.find("input[name='captcha']")
                    .val('');
        };

        var resetHightlightDescriptionForm = function() {

            var $form = $("#submit_description_form");
            $form.find("input[name='username']")
                    .removeClass("form-hotel-description__inp_empty");
            $form.find("input[name='hotelurl']")
                    .removeClass("form-hotel-description__inp_empty");
            $form.find("input[name='captcha']")
                    .removeClass("form-hotel-description__inp_empty");
        };

        resetDescriptionForm();
        refreshCaptchaSrc();

        $("#submit_description_form")
            .submit(function(){

                var $this = $(this);

                if ($this.data("locked")) {
                    return false;
                }

                $this.data("locked", true);

                var username = $this.find("input[name='username']").val();
                var hotelurl = $this.find("input[name='hotelurl']").val();
                var captcha = $this.find("input[name='captcha']").val();
                var allocId = $this.find("input[name='allocId']").val();

                var formData = {
                    username: username,
                    hotelurl: hotelurl,
                    captcha: captcha,
                    allocId: allocId
                };

                $.request('/new/actions/hotel_link_form/' + '?rnd=' + Math.random(), formData)
                    .done(function(){

                        resetDescriptionForm();
                        resetHightlightDescriptionForm();
                        refreshCaptchaSrc();
                        $this.data("locked", false);

                        $.app.system.alert("Сохранено!", "Добавленная информация успешно сохранена <br/> и будет рассмотрена в ближайшее время.<br/>");
                    })
                    .fail(function(){

                        resetHightlightDescriptionForm();
                        var msg = "";
                        var ipBlocked = false;
                        if (this.errors.length) {
                            $.each(this.errors, function (k, v) {
                               if (v.code == 5) {
                                   ipBlocked = true;
                                   msg = v.message;
                               }
                            });


                            if (!ipBlocked) {
                                $.each(this.errors, function (k, v) {
                                    switch (v.code) {
                                        case 1:
                                            $this.find("input[name='username']").addClass("form-hotel-description__inp_empty");
                                            break;
                                        case 2:
                                            $this.find("input[name='hotelurl']").addClass("form-hotel-description__inp_empty");
                                            break;
                                        case 3:
                                            $this.find("input[name='captcha']").addClass("form-hotel-description__inp_empty");
                                            break;
                                        default:
                                            break;
                                    }

                                    msg += v.message + "<br/>";
                                });
                            }

                            $.app.system.errorAlert(msg);
                        }

                        refreshCaptchaSrc();
                        $this.data("locked", false);
                    });
                return false;
            })
            .delegate('.captcha_image_reload', 'click', function() {

                refreshCaptchaSrc();
                return false;
            });
    });
</script>
