var searchVal =
$(document).ready(function() {

	function searchSuggest (request, response) {
		$.ajax({
			url: "/search/",
			dataType: "json",
			type: "GET",
			data: {
				q: $('#global_search').val(),
				hint: 1,
				c: $('#search_category_id').val() || null
			}, success: function (jsonResponse) {
				setTimeout(function(){
					response(jsonResponse);
				},300);

			}
		});
	};

	$('#global_search').autocomplete({
		source: searchSuggest,
		select: function(event, ui) {
			event.preventDefault();
			$(this).val(ui.item.title);
			$(this).attr('value',ui.item.title);
			/*$(this).autocomplete( "destroy" );*/
			document.location.href = ui.item.link;
		},
		focus: function(event, ui) {
			event.preventDefault();
			$(this).val(ui.item.title);

		},
		open: function( event, ui ) {
			var currSearchVal = $(this).val()? $.trim($(this).val()):null;
			$('.search_suggest_item').each(function(){
				if($(this).text().indexOf(currSearchVal) != -1){
					var startValNum = $(this).text().indexOf(currSearchVal);
					var finishValNum = startValNum + (currSearchVal.length-1);
					var finishStringBegin = $(this).text().slice(0, startValNum);
					var finishStringPreTarget = $(this).text().slice(startValNum, finishValNum+1);
					var finishStringTarget = '<span class="bold">' + finishStringPreTarget + '</span>';
					var finishStringEnd = $(this).text().slice(finishValNum+1);

					$(this).html(finishStringBegin + finishStringTarget + finishStringEnd);

					if($(this).text() == "услуги"){
						$(this).html('<i class="icon-biglion-tags font_tags"></i>&nbsp;<span class="bold">'+$(this).text()+'</span>')
					}else if($(this).text() == "отели"){
						$(this).html('<i class="icon-biglion-hotel-1 font_hotel"></i>&nbsp;<span class="bold">'+$(this).text()+'</span>')
					}else if($(this).text() == "туры"){
						$(this).html('<i class="icon-biglion-flight font_flight"></i>&nbsp;<span class="bold">'+$(this).text()+'</span>')
					}else if($(this).text() == "магазины"){
						$(this).html('<i class="icon-biglion-bag font_bag"></i>&nbsp;<span class="bold">'+$(this).text()+'</span>')
					}else if($(this).text() == "товары по купонам"){
						$(this).html('<i class="icon-biglion-headphones font_headphones"></i>&nbsp;<span class="bold">'+$(this).text()+'</span>')
					}
				}
			});
		},
		delay: 300,
		position: { my : "left top", at: "left bottom" , of: ".search-header"},
		appendTo: '#menu-container'

	});



	$('#global_search').focus(function(){
		setTimeout(function() {
			if ( $('.searchbox').hasClass('active') ) {
				$('#global_search').data("autocomplete").search(' ');
			}
		},300);
	});

	$('#global_search').data("autocomplete")._renderItem = function (ul, item) {
		//console.log($('#global_search').val());
		return $("<li></li>")
				.data("item.autocomplete", item)
				.append(function(){
					if(item.link == '/services/') {
						return "<a class='search_suggest_item' href='" + item.link + "'>" + "<div class='search_suggest_menu'><i class='icon-biglion-tags font_tags'></i>" + item.title + "</div>" + "<em>" + (item.catid ? item.catid : '') + "</em>" + "<div class='search_suggest_subtext'>" + (item.subtext ? item.subtext : '') + "</div>" + "</a>";
