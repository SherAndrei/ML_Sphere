				$('#global_search').width( textboxWidth );*/
				/*$('#choose_city_btn').fadeIn('slow');
				$('.filter_block').fadeOut('slow');*/
			} else {
				if ( target.parents('.search_icon_block2').length != 0 && target.is('a') ) {
					var catName = target.html();
