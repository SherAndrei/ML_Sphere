<script>
	$(document).ready(function() {
		toggleClearBtn($('#city-input'));

        // Выбор стран
        $('.citylist-tabs li').each(function(index, element){

            $(element).live('click', function(){
                var country = $(this).attr('data-country');

                $('.citylist-tabs li').each(function(index, element){
                    $(element).removeClass('active');
                    if ($(element).attr('data-country') == country) {
                        $(element).addClass('active');
                    }
                });

                if ( country != 'ru') {
                    $('.citylist-search, .citylist-alphabet, .citylist-title').hide();
                } else {
                    $('.citylist-search, .citylist-alphabet, .citylist-title').show();
                }

                $('.citylist-cont div').hide();

                // В верстке сайта, в частности - на странице регистрации, присутствуют два блока
                // с выбором стран. Они имеют различные идентификаторы.
                var blocks_ids = ['#citylist-', '#p_citylist-'];

                for (var i in blocks_ids) {
                    $(blocks_ids[i] + country).each(function(index, element){
                        $(element).show();
                    })
                }
            });
        });
        
		$('#city-input').autocomplete({
			source: function (request, response) {
				$.ajax({
					url: "/general/ajax.php",
					dataType: "json",
					type: "GET",
					data: {
						cl: 'm_city',
						method: 'search_city',
						q: $('#city-input').val()
					},
					success: function (jsonResponse) {
						response(jsonResponse);
					}
				});
			},
			select: function(event, ui) {
				document.location.href = ui.item.link;
			},
			delay: 700,
			position: { my : "left top", at: "left+14 bottom", of: ".citylist-search-cont" },
			appendTo: '#city-container',
			autoFocus: true
		});

		$('#city-input').data("autocomplete")._renderItem = function (ul, item) {
			return $("<li></li>")
					.data("item.autocomplete", item)
					.append("<a href='" + item.link + "'>" + "<h4>" + item.title + "</h4>" + "</a>")
					.appendTo(ul);
		};
		$('#city-input').on( 'keyup', function() { toggleClearBtn($(this)); })
						.on( 'change', function() { toggleClearBtn($(this)); })
						.on( 'focus', function() {
							var currentCity = 'Москва',
								handler = $(this);
							handler.css( 'color', '#000' );
							window.setTimeout (function(){
							   handler.select();
							},100);
						})
						.on( 'blur', function() { $(this).css( 'color', '#8c8c8c' ); });
		$('.clear-search').on( 'click', function() { $(this).hide().prev().val(''); });

		$('.alphabet-block span').on( 'click', function() {
			var cityChar = $(this).attr('data-char');
			$('.alphabet-block span').removeClass('active');
			$(this).addClass('active');
			$('.citylist-title').html(cityChar);
			$.ajax({
				url: "/general/ajax.php",
				dataType: "html",
				type: "GET",
				data: {
					cl: 'm_city',
					method: 'search_city',
					start: '1',
					q: cityChar
				},
				success: function (htmlResponse) {
					$('#citylist-ru').html(htmlResponse);
				}
			});
		});
	});
	function toggleClearBtn(input) {
		if ( $(input).val() != '' ) {
			$(input).parent().find('.clear-search').show();
		} else {
			$(input).parent().find('.clear-search').hide();
		}
	}
</script>
