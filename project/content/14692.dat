<script type="text/javascript" src="http://maps.googleapis.com/maps/api/js?key=AIzaSyD8_WZmrlr0GtIKRICumOOGA7PFyqOX-6Y&sensor=true"></script>
<script type="text/javascript">
var Gmap = {
 
  DATA: {
  	marker_new: 0,
  	uid: 1,
  	zoom: 1,
  	coords: "1,1",
  	testo: 1
  },

  init: function(param) {
  	var Obj = this;
		Obj.setDATA("uid", param['uid']);
		Obj.setDATA("coords",param['coords']);
		if (param['zoom'] != '' && param['zoom']) Obj.setDATA("zoom",param['zoom']);
		cr = Obj.DATA.coords.match(/([0-9\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\.\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\-]+),([0-9\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\.\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\-]+)/);
    var latLng = new google.maps.LatLng(cr[1],cr[2]);
    Obj.map = new google.maps.Map(document.getElementById('map-container'+Obj.DATA.uid), {
      zoom: parseInt(Obj.DATA.zoom),
      center: latLng,
      mapTypeId: google.maps.MapTypeId.ROADMAP,
      scaleControl: true
    });
  
    var marker = new google.maps.Marker({
      position: latLng,
      map: Obj.map,
      title:""+latLng+""
    });

    google.maps.event.addListener(Obj.map, "rightclick",function(event){
     		Obj.NewMarker(event.latLng);
				Obj.setDATA("coords", event.latLng);
     		Obj.MakeBB();
    });
    
    google.maps.event.addListener(Obj.map, "click",function(event){
     		Obj.NewMarker(event.latLng);
     		Obj.setDATA("coords", event.latLng);
     		Obj.MakeBB();
    });
    
    google.maps.event.addListener(Obj.map, "zoom_changed",function(event){
     		Obj.setDATA("zoom", Obj.map.getZoom());
     		Obj.MakeBB();
    });

  },
  	
	NewMarker: function(LatLng) {
		var Obj = this;
		if (Obj.DATA.marker_new) {
			Obj.DATA.marker_new.setMap(null);
		}
		
		Obj.DATA.marker_new = new google.maps.Marker({
      position: LatLng,
      map: Obj.map,
      draggable:true,
      icon: '/custom/img/map/blue-dot.png',
      animation: google.maps.Animation.DROP,
      title:""+LatLng+""
    });
	
		google.maps.event.addListener(Obj.DATA.marker_new, "drag",function(event){
			Obj.setDATA("coords", event.latLng);
			Obj.MakeBB();
    });
		
	},
	
	MakeBB: function(LatLng) {
		dat = "c"+this.DATA.coords+"";
		if (this.DATA.zoom) dat += "|z"+this.DATA.zoom;
		var iname = 'input[name="coords-container'+this.DATA.uid+'"]';
		$(iname).val('[map '+dat+']');

	},
	
	setDATA: function(varname,latLng) {
		if(varname == "coords") {
			latLng += '';
			latLng = latLng.replace(/[\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\(\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\)]/g,"");
		}
		this.DATA[varname] = latLng;
	
	}

};

</script>

