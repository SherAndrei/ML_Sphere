<div class="row col-sm-12">
    <p>
        Формула объема цилиндра через высоту и площадь основания:
        <h3><b><var>V</var>=<var>S&middot;h</var></b></h3>
        <b>S</b> - площадь основания цилиндра; <IDXSKIP><b>h</b></IDXSKIP> - высота цилиндра.
    </p>
</div>

<script>
    var units_1 = [['мм', 'см', 'м'], [0.001, 0.01, 1]];
    var units_2 = [['мм&sup2;', 'см&sup2;', 'м&sup2;'], [0.000001, 0.0001, 1]];
    var units_3 = [['см&sup3;', 'м&sup3;', 'л.'], [1000000, 1, 1000]];
    function volume_cylinder_s(isCal) {
        var ac = $('#ac2').val();
        if (isNaN(ac) || (!jQuery.isNumeric(ac)) || (ac < 0) || (ac > 10)) {
            $("#vs").html("<p class='text-danger'>Точность от 0 до 10</p>");
            $("#ac2").parent().addClass("has-error");
            return;
        }
        $("#ac2").parent().removeClass("has-error");

        var height = parseFloat($("#hs").val().replace(',', '.'));
        if (isNaN(height) || (!jQuery.isNumeric(height)) || (height <= 0)) {
            $("#vs").html("<p class='text-danger'>Введите высоту</p>");
            $("#hs").parent().addClass("has-error");
            return;
        }
        $("#hs").parent().removeClass("has-error");
        var idH = $('#metric_hs').val();
        height = height * (units_1[1][idH]);

        var square = parseFloat($("#ss").val().replace(',', '.'));
        if (isNaN(square) || (!jQuery.isNumeric(square)) || (square <= 0)) {
            $("#vs").html("<p class='text-danger'>Введите площадь</p>");
            $("#ss").parent().addClass("has-error");
            return;
        }
        $("#ss").parent().removeClass("has-error");
        var idS = $('#metric_s').val();
        square = square * (units_2[1][idS]);

        var v = height * square;
        var idV = $('#metric_v2').val();
        v = v * (units_3[1][idV]);

        $("#vs").html("<p class='text-success'>V = " + parseFloat(v.toFixed(ac)) + "(" + units_3[0][idV] + ")</p>");
        if (isCal) {
            if ($("#results").html() == "") {
                $("#results").append("<p>Объем цилиндра через высоту и площадь основания.</p>");
            }
            $("#results").css("display", "block");
            $("#results").append("<hr /><p>h = " + parseFloat($("#hs").val().replace(',', '.')) + "(" + units_1[0][idH] + ")" +
                "; S = " + parseFloat($("#ss").val().replace(',', '.')) + "(" + units_2[0][idS] + ")" +
                "; V = " + parseFloat(v.toFixed(ac)) + "(" + units_3[0][idV] + ")</p>");
        }
    }
</script>

