	document.addEventListener('DOMContentLoaded', function() {
		if (isMobile()) {
			Array.prototype.forEach.call(document.querySelectorAll("#inguru_osago_banner .inguru_banner-select"), function(e){
				e.classList.add('mobile');	
			});
		}
		document.getElementById("inguru_osago_banner").classList.add(startType);
		document.getElementById('inguruType').setAttribute("value", startType);

		if (startType == 'KASKO') {
			document.querySelector("#inguruType ul .KASKO").classList.add('selected');
			document.querySelector("#inguruType .text").innerHTML = "КАЛЬКУЛЯТОР <span class='type'>КАСКО</span>";
		} else {
			document.querySelector("#inguruType ul .OSAGO").classList.add('selected');
			document.querySelector("#inguruType .text").innerHTML = "ЭЛЕКТРОННОЕ <span class='type'>ОСАГО</span>";
		}

		inguruChangeLink();

		vechiles.forEach(function(v) {			
			var select = document.querySelector("#inguruBrands ul"),
				opt = document.createElement('li');
			opt.setAttribute('value', v.id);
			opt.setAttribute('label', v.name);
			opt.innerHTML = v.name;
			select.appendChild(opt);
		});
		document.body.addEventListener('click', inguruHandleClick);
		Array.prototype.forEach.call(document.querySelectorAll("#inguru_osago_banner .inguru_banner-select li"), function(e){
			e.addEventListener('click', inguruSelect);
		});
		Array.prototype.forEach.call(document.querySelectorAll("#inguru_osago_banner .inguru_banner-select .inguru_banner-select-header"), function(e){
			e.addEventListener('click', inguruToggleSelect);
		});
		Array.prototype.forEach.call(document.querySelectorAll("#inguru_osago_banner .inguru_banner-select .inguru_banner-close"), function(e){
			e.addEventListener('click', inguruCloseSelect);
		});
	});

	function inguruToggleSelect() {
		if (this.parentNode.classList.contains('opened')) {
			this.parentNode.classList.remove('opened');
			lockMobileScroll(false);
		} else {
			this.parentNode.classList.add('opened');
			lockMobileScroll(true);
		}
	}

	function inguruCloseSelect() {
		this.parentNode.parentNode.parentNode.classList.remove('opened');
		lockMobileScroll(false);
	}

	function inguruSelect() {
		if (!!this.getAttribute('value')) {
			Array.prototype.forEach.call(this.parentNode.querySelectorAll("li"), function(e){
				e.classList.remove('selected');
			});
			this.parentNode.parentNode.parentNode.querySelector(".inguru_banner-select-header .text").innerHTML = this.getAttribute("label");
			this.classList.add('selected');
			this.parentNode.parentNode.parentNode.setAttribute("value", this.getAttribute("value"));
		}
		this.parentNode.parentNode.parentNode.classList.remove('opened');
		lockMobileScroll(false);
		if (this.parentNode.parentNode.parentNode.getAttribute("id") == "inguruType") changeType();
		if (this.parentNode.parentNode.parentNode.getAttribute("id") == "inguruBrands") changeModels();

		inguruChangeLink();
	}

	function changeType() {
		var type = document.getElementById('inguruType').getAttribute("value"),
			banner = document.getElementById('inguru_osago_banner');

		banner.classList.remove('KASKO');
		banner.classList.remove('OSAGO');
		banner.classList.add(type);
	}

	function changeModels() {
		var brand = document.getElementById('inguruBrands').getAttribute("value"),
			models = vechiles.filter(function(o){return o.id == brand;})[0].models,
			selectWrapper = document.querySelector("#inguruModels"),
			select = document.querySelector("#inguruModels ul"),
			selectLabel = document.querySelector("#inguruModels .inguru_banner-select-header .text");
			selectWrapper.removeAttribute('value');
			select.innerHTML = '';
			selectLabel.innerHTML = 'Модель авто<span class="large">мобиля</span>';

		models.forEach(function(v) {			
			var opt = document.createElement('li');
			opt.setAttribute('value', v.id);
			opt.setAttribute('label', v.name);
			opt.innerHTML = v.name;
			opt.addEventListener('click', inguruSelect);
			select.appendChild(opt);
		});
	}

	function inguruChangeLink() {
		var type = document.getElementById('inguruType').getAttribute("value"),
			vechile = document.getElementById('inguruVechile').getAttribute("value"),
			drivers = document.getElementById('inguruDrivers').getAttribute("value"),
			model = document.getElementById('inguruModels').getAttribute("value"),
			brand = document.getElementById('inguruBrands').getAttribute("value"),
			url;
		if (type == 'OSAGO') {
			url = "http://www.inguru.ru/kalkulyator_osago"
			if (!!vechile || !!drivers) {
				url += "#calculator=1";
				if (!!vechile) url += "&vechile=" + vechile;
				if (!!drivers) url += "&drivers=" + drivers;
				if (!!partnerId) url += "&adv_p_id=" + partnerId;
			} else if (!!partnerId) {
				url += "#adv_p_id=" + partnerId;
			}
		} else {
			url = "http://www.inguru.ru/kalkulyator_kasko"
			if (!!model && !!brand) {
				url += "#calculator=1&model=" + model;
				if (!!partnerId) url += "&adv_p_id=" + partnerId;
			} else if (!!partnerId) {
				url += "#adv_p_id=" + partnerId;
			}
		}
		document.getElementById("inguru_banner-button").setAttribute("href", url);
	}

	function inguruHandleClick (event) {
		Array.prototype.forEach.call(document.querySelectorAll(".inguru_banner-select.opened"), function(e){
			if (e && !e.contains(event.target)) {
				e.classList.remove('opened');
			}
		});
	}

	function isMobile() {
		if(typeof window == 'undefined') {
			return false;
		}

