					<noindex>
					<div class="user_avatar"><img src="http://cdn01.ru/files/users/avatars/9/5/6/956_42.jpg?1385793104"></img></div>
					<div class="user_info"><span l="/profile956/" id="answer564058_author" class="l user_name_link">Ташa
						</span><span l="/profile956/" class="l user_name_link premium_highlight" title="premium-аккаунт">[88.9K]</span></span>
						
						
						
						<div class="time_asked" title="2013-01-09 18:09:09">4 года назад
							
							
						</div>
					</div>
					<div class="clear"></div>
					</noindex>
