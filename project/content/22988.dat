  <IDXSKIP><div class="news-category"><div onmouseover="this.className='news-category-hover'" onmouseout="this.className=''"><a target="_blank" href="http://forums.goha.ru/forumdisplay.php?f=3852">WARGAMING.NET LEAGUE</a></div></div></IDXSKIP>
