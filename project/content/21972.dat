                    opts.step( delta(progress) );

                    if (progress == 1) {
                        clearInterval(timer);
                        opts.complete && opts.complete();
                    }
                }, opts.delay || 13);

                return timer;
            }
               
            function animateProp(opts) {
                var start = opts.start, end = opts.end, prop = opts.prop;
