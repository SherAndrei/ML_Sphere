  (function() {
    function setCookie(name, value, time, path) {
      var date = new Date();
      date.setTime(date.getTime() + (time * 1000));
      document.cookie = name + "=" + value + ";"
        + "expires=" + date.toUTCString() + ";"
        + "path=" + (path || "/");
