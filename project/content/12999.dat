	//обновляем погоду для города
	function weatherUpdater(cityId)
	{
		if(cityWeatherInt && !auto)
		{
			clearInterval(cityWeatherInt);
			$.cookie('gtrk_city', cityId, { expires: 365});
		}
		else if(!auto)
		{
			$.cookie('gtrk_city', cityId, { expires: 365});
		}
		
		//вычисляем время суток
		todCalculation(cityId);
	}
	
	function todCalculation(cityId)
	{
		var cur_time = arResult[cityId]["HOUR"];
		if(cur_time)
		{
			if(cur_time>=3 && cur_time<9)
			{var tod=1;}
			else if(cur_time>=9 && cur_time<15)
			{var tod=2;}
			else if(cur_time>=15 && cur_time<21)
			{var tod=3;}
			else if((cur_time>=21  &&  cur_time<23) || (cur_time>=0 && cur_time<3))
			{var tod=0;}
		}
		
		$('div.weather').html(arResult[cityId]['MINTEMP_'+tod]+'° С');
		auto = false;
		
		weatherIcoUpdater(cityId, tod);
		
		var time = $('#cityWeather option:selected').attr('class');
		if(time != 0)
		{
			$.get('/dev/includes/nowtimeregion.php', function(data){$('.cities #time').html(data);},'html');
		}
		else
		{
			$.get('/dev/includes/nowtime.php', function(data){$('.cities #time').html(data);},'html');
		}
		
	}
	
	$("#cityWeather").change(function(){
		weatherUpdater($('#cityWeather option:selected').attr("id"))
	});

	//выбираем иконку погоды
	function weatherIcoUpdater(cityId, tod)
	{		
		//informer.gismeteo.ru/getcode/xml.php
		var cloudiness = arResult[cityId]['CLOUDINESS_'+tod];
		//0 - ясно, 1- малооблачно, 2 - облачно, 3 - пасмурно
		var precipitation = arResult[cityId]['PRECIPITATION_'+tod];
		//4 - дождь, 5 - ливень, 6,7 – снег, 8 - гроза, 9 - нет данных, 10 - без осадков
		if(tod != 0){var tod = 1;}
		//tod 0 - ночь 1 - утро, 2 - день, 3 - вечер
		var weaserClass = 's-w-m_'+cloudiness+'_'+precipitation+'_'+tod;
		$('#weather_ico').removeClass().addClass(weaserClass);
	}
	
	//стилизация select погоды
	$(function(){
		$('#cityWeather').styler();
