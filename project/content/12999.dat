{
	var id = $(_this).attr('id');
	id = id.replace(/ex_news_nav_/g, "");
	
	if($("#exclusive_news_"+id))
	{
		var height = $("#exclusive_news_"+id).height();
		var html = $("#exclusive_news_"+id).html();
		
		$(".ex_see_wrapper").animate({height: height, opacity: 0}, 150, function()
		{
			$(".ex_see_wrapper .ex_see_content").css("display", "none");
			$(".ex_see_wrapper .ex_see_content").html(html);
			$(".ex_see_wrapper .ex_see_content").fadeIn(150);
			$(".ex_see_wrapper").animate({opacity: 100}, 150, function()
			{
				$(".ex_news_nav.current").removeClass("current");
