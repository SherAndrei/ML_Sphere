		function subscribeOnMaster(){
			MWindow.box.show({
				html: '<div align="center">\n' +
					i18n._('Будьте всегда в курсе новинок любимого магазина!') + "\n" +
					'<div class="clear sep-v-10"></div>\n' +
					'<input id="subscriberMasterObjectsEmail" type="text" name="email" style="width: 221px; height: 31px; border-radius: 4px; padding: 0 12px 0 12px;" placeholder="' + i18n._('укажите свой email') + '" />\n' + 
				'</div>', 
				btn1: {
					text: i18n._('Подписаться'),
					onClick: function() {
						proceedSubscribeOnMaster();
					}
				},
				closeBtn: true
			});
		}

		function proceedSubscribeOnMaster(){
			var windowText = '';
			var hideLink = false;
			jQuery.ajax({
				url: "/subscribes/subscribeto/masterobjects",
				type: "post",
				data: {guestEmail: jQuery('#subscriberMasterObjectsEmail').val(), muserID: 2139385},
				dataType: "json"
			}).done(function(response) {
				if (response.result && response.retCode === 0) {
					var confText = '';
					if (response.sbStatus === 3) {
						confText = '<br><br>' + i18n._('Мы отправили Вам письмо активации. Перейдите по указанной в нем<br>ссылке, чтобы подтвердить свой email и активировать подписку.');
					}
					windowText = i18n._('Поздравляем!<br>Теперь уведомления о новых товарах данного магазина<br>будут приходить к Вам на электронную почту.') + confText;
					hideLink = true;
				}
				if (!response.result && response.retCode === 2) {
					windowText = i18n._('Вы уже подписаны на обновления этого магазина');
					hideLink = true;
				}
				if (!response.result && response.retCode === 3) {
					windowText = i18n._('Введите верный email');
					hideLink = false;
				}
				if (!response.result && response.retCode === 4) {
					windowText = i18n._('Вы уже подписаны на обновления этого магазина');
					hideLink = true;
				}
				if (!response.result && response.retCode === 5) {
					loginModal();
					return false;
				}
				if (!response.result && response.retCode === 'limit') {
					windowText = i18n._('Вы оформили максимум подписок. Но для наших хороших знакомых у нас нет лимитов!<br>Войдите в свой аккаунт или зарегистрируйтесь,<br>чтобы подписываться на любое коли -<br>чество мастеров без ограничений.');
					hideLink = true;
				}
				MWindow.box.show({html: windowText, btn1: {text: 'OK', size: 'small'}});

				if (hideLink) {
					jQuery('#master-subscribe').fadeOut(function() {
													if (jQuery('#master-aftersubsribe').length) {
								jQuery('#master-aftersubsribe').show();
							}
											});
				}
			});
		}

		jQuery('#master-subscribe-link-icon, #master-subscribe-link, #master-unsubscribe-link').on('click', function(e) {
			e.preventDefault();

							subscribeOnMaster();
