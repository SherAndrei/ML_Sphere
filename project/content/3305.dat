function ChangeFoto(id, path, path2, path3, descript, photo_id, elemNumbStart) {
	photo_id = typeof photo_id == 'undefined' ? 0 : photo_id;
	if ($('mainZoom')) {
		$('mainZoom').style.display = "none";
        $('mainZoom').innerHTML = "<img src=\"/image/s/zoom-in.jpg\">&nbsp;<a class=\"gray link2\" href=\""+path2+"\" target=\"img"+id+"\">" + i18n._('Увеличить') + "</a>";
	}
	elemNumbStart = elemNumbStart.trim();
	if ($('mainFoto')) {
		$('mainFoto').innerHTML = '<a href="'+path2+'" target="_blank"><img class=" item_img link_img photo-large" id="main-foto-img" src="'+path+'" data-slider-start-index="' + elemNumbStart + '" alt="' + descript + '" title="' + descript + ' " data-share="'+path2+'"></a>';
	}
	
	if (descript) {
		if ($('mainComment')) {
			$('mainComment').innerHTML = "<a style=\"margin-top:5px; display:block; float:left; width: 340px;\" href=\""+path2+"\" target=\"_blank\" class=\"nolink fotocomment\" \">"+descript.replace(/([^>])\n/g, '$1<br/>')+"</a>";
			$('mainFoto').innerHTML = $('mainFoto').innerHTML+$('mainComment').innerHTML+'<div style="margin-top:5px; display:inline; font-size:11px; float:right">'+$('mainZoom').innerHTML+'</div>';
		}
	} else {
		if ($('mainComment')) {
			$('mainComment').style.diplay = "none";
			$('mainComment').innerHTML = "";
			$('mainFoto').innerHTML = $('mainFoto').innerHTML+"<div class=\"sep-v-5\"></div>"+$('mainComment').innerHTML;
		}
	}

	if ($('mainCode')) {
        $('mainCode').innerHTML = "<div class=\"sep-v-5\"></div><textarea class=\"codeblock\" onclick=\"this.select(); \" rows=\"3\"><a href=\"http://www.livemaster.ru/item/11316113\" title='" + descript + "'><img border=0 src=\""+path3+"\" title='" + descript + "'></a><br> <a href='http://www.livemaster.ru/item/11316113-dlya-doma-interera-adresnaya-tablichka-a-12'>Адресная табличка А-12</a> " + i18n._('на <a href="http://www.livemaster.ru">Ярмарке Мастеров</a></textarea><br>Код для форума (BBCode):') + "<br><textarea class=\"codeblock\" onclick=\"this.select(); \">[url=\"http://www.livemaster.ru/item/11316113\"][img]"+path3+"[/img][/url]\n [url='http://www.livemaster.ru/item/11316113-dlya-doma-interera-adresnaya-tablichka-a-12']Адресная табличка А-12[/url] " + i18n._('на [url="http://www.livemaster.ru"]Ярмарке Мастеров[/url]') + "</textarea><br><br><div class=\"codeblock gray\" ></div>";
	}

	if ($('wdpath')) {
		$('wdpath').value = path2;
	}
	
	// Добавляем кнопки шаринга на каждое большое фото работы
	jQuery('#mainFoto').shareImg({
		socialSet: i18n._('Работа'),
		title: '\u041A\u0443\u043F\u0438\u0442\u044C\x20\u0410\u0434\u0440\u0435\u0441\u043D\u0430\u044F\x20\u0442\u0430\u0431\u043B\u0438\u0447\u043A\u0430\x20\u0410\x2D12\x20\x2D\x20\u043A\u043E\u0440\u0438\u0447\u043D\u0435\u0432\u044B\u0439,\x20\u0410\u0434\u0440\u0435\u0441\u043D\u0430\u044F\x20\u0442\u0430\u0431\u043B\u0438\u0447\u043A\u0430,\x20\u0430\u0434\u0440\u0435\u0441\u043D\u044B\u0435\x20\u0442\u0430\u0431\u043B\u0438\u0447\u043A\u0438,\x20\u043D\u043E\u043C\u0435\u0440\x20\u043D\u0430\x20\u0434\u043E\u043C'
	});

	jQuery('.social-likes').socialLikes();
	jQuery('input[name="photo_id"]').val(photo_id);

	//Вешаем тень-обводку на текущую миниатюру 
