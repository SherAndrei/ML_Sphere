		function manageItems(actionManage){
            var comment = null;
			var masterID = 519037;
            if (actionManage == 'disable-all-items') {
				comment = prompt(i18n._('Укажите причину отключения работ:'),'');     
            } else if (actionManage == 'enable-all-items') {
                if (!confirm(i18n._('Вы действительно хотите включить работы этого мастера?'))) {
                    return false; 
                }
            } 
            if(comment == '') {
                alert(i18n._('Необходимо указать комментарий'));
                return false;
            }
            if ((actionManage == 'disable-all-items' && comment !== null) || actionManage == 'enable-all-items'){
                jQuery.ajax({
                    url: "item/moderation/",
                    type: "post",
                    data: {action: actionManage, comment: comment, masterID: masterID},
                    dataType: "json"
                    }).done(function(response) {
                        if (response.result) {
                            window.location.reload();  
                        } else {
                             alert(i18n._('Ошибка. Обратитесь к разработчикам.'));
                        }
                });
            } else {
                return false;
            }
        }
