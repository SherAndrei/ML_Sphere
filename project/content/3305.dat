function manageMethodDesc(methodID) {
	if (jQuery("#method-"+methodID+"-desc").is(":visible")) {
        jQuery("#show-method-"+methodID+"-arrow").removeClass('opened');
        jQuery("#method-"+methodID+"-desc").slideUp();        
    } else {        
        jQuery("#show-method-"+methodID+"-arrow").addClass('opened');
        jQuery("#method-"+methodID+"-desc").slideDown();
