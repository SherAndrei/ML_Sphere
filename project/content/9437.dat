    $(document).ready(function(){
        $( "#postWrapper img, #postContent img").each(function() {
            var imgEl = $(this);
            var imgWidth = imgEl.width();
            var isPoster = imgEl.hasClass( 'poster' );
            var imgFloat = imgEl.css('float');

            imgEl.wrap( "<div class='image--box'></div>" );

            var imgWrapper = imgEl.parent();

            if(imgWidth > 0) {
                imgWrapper.width(imgWidth + 'px');
            }

            if(imgFloat != '' && imgFloat != 'none') {
                imgWrapper.css('float', imgFloat);

                imgWrapper.css('margin', '0px 10px 10px 0px');
            }
        });

        $('.image--box').append($('.rollover-social-networks'));
        $('.image--box').hover(function() {
            $(this).find('.rollover-social-networks').css('opacity', 100);
        }, function() {
            $(this).find('.rollover-social-networks').css('opacity', 0);
        })

        $(".share--image--btn" ).hover(
                function() {
                    $(this).find('.social--share--button--hover').css('display', 'block');
                }, function() {
                    $(this).find('.social--share--button--hover').css('display', 'none');
                }
        );

        $('body').on('click', '.share--image--btn.s_vk', function() { shareImage(this, 's_vk'); });
        $('body').on('click', '.share--image--btn.s_facebook', function() { shareImage(this, 's_facebook'); });
        $('body').on('click', '.share--image--btn.s_pinterest', function() { shareImage(this, 's_pinterest'); });
        $('body').on('click', '.share--image--btn.s_tumblr', function() { shareImage(this, 's_tumblr'); });
	});

    function shareImage(that, networkStyle) {
        var imgUrl = $(that).closest('.image--box').find('img').prop('src');
        var title = $('header h1').html();

        $('body').append('<span class="temp-link ' + networkStyle + '"></span>');
        $('.temp-link').ShareLink({
            title: title,
            image: imgUrl
        });

        $('.temp-link').click();
        $('.temp-link').detach();
    }
