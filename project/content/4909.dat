<script>
 // TODO: refactor
 function wo(_wurl,_ww,_wh,_wscrl){
 /***************
 wurl: window URL
 wh: window height
 ww: window width
 wsr: scrollbars 1 - yes, 0 - no
 ***************/
 if(screen.width){
 var wpos_x = (screen.width-_ww)/2;
 if (wpos_x < 0){
 wpos_x = 0;
 }
 var wpos_y = (screen.height-_wh)/2;
 if (wpos_y < 0){
 wpos_y = 0;
 }
 } else {
 wpos_x = 0;
 wpos_y = 0;
 }
 // if(_wscrl==undefined){ _wscrl = 0;}
 var wind_desc = window.open(_wurl,"","alwaysRaised=0,top="+wpos_y+",left="+wpos_x+",height="+_wh+",width="+_ww+",menubar=0,resizable=1,scrollbars="+_wscrl+",status=1");
 wind_desc.focus();
 }
