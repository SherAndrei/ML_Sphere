			//Также управляет значением данной куки
			function showSubscriptionForm(){
				//Открывает всплыващее окно, содержащее форму подписки
				function openWindow(showAfterSecs){
					//console.info('showAfterMSecs',showAfterSecs);
					setTimeout(function(){
						var iframeUrl='http://www.umnitsa.ru/bitrix/tools/ofsys_form.php';
						//var iframeUrl='//localhost/d6sc/umnitsaclub/test.php';
					  $.colorbox({html:"<iframe src='"+iframeUrl+"' scrolling='no' style='width:615px; height:430px; border:0;'></iframe>"});
					},showAfterSecs);
				}

				var showTime=$.cookie('sform_showtime');
				var showAfterMSecs=3*60*1000; // показать через 3 мин нахождения на сайте
				if(!showTime){
					//первый раз на сайте, поэтому куки ещё нет
					showTime=new Date().getTime()+showAfterMSecs;
					$.cookie('sform_showtime',showTime);
	  			openWindow(showAfterMSecs);	
				}else{
					//вычислим, через какое время нужно показать окно
					showAfterMSecs=showTime-new Date().getTime();
					if(showAfterMSecs>=0){
						//еще не показывали окно - покажем
	  				openWindow(showAfterMSecs);	
					}else{
						//это только для тестов: по прошествии 7 дней после показа удалим куки и начнём всё сначала
						if(showAfterMSecs<-7*24*60*60*1000){
							$.cookie('sform_showtime',null);
							showSubscriptionForm();
						}
					}
					//console.log(showTime,showAfterMSecs);
				}
			}
			
		});
		</script>
				
