    <script type="text/javascript">

        $(document).ready(function() {
            $(".bb-mainmenu-item.bb-collapsed-content")
				.on("mouseenter", function(){
					$(this).addClass("bb-collapsed-content-expanded");
				}).on("mouseleave", function(){
					$(this).removeClass("bb-collapsed-content-expanded");
				});

            $('#headerMenu').find('a').filter(function(i, a){
                return a.getAttribute('href').match(/\//);
            });

            // верхнее меню пользователя
            var cloud_user_menu = BBAPI.dom().id("headerProjectMenu");
            var cloud_user_block = BBAPI.dom().id("headerUserMenu");
            var cloud_user_menu_over = 0, cloud_user_block_over = 0;

            if (cloud_user_block) {
                BBAPI.event.addEvent(cloud_user_block, "mouseover", function() {
                    BBAPI.css.addClass(cloud_user_block, "bb-collapsed-content-expanded");
                    cloud_user_block_over++;
                    setTimeout(function(){cloud_user_block_over++;},200);
                });
                BBAPI.event.addEvent(cloud_user_block, "mouseout", function() {
                    BBAPI.css.removeClass(cloud_user_block, "bb-collapsed-content-expanded");
                    cloud_user_block_over = 0;
                });

                BBAPI.event.addEvent(BBAPI.dom().id("headerUserMenuJournalLink"),'click', function(ev){
                    if(cloud_user_block.className.split(" ").indexOf("bb-collapsed-content-expanded") == -1 || cloud_user_block_over <= 1) {
                        cloud_user_block_over++;
                        return BBAPI.event.stopEvent(ev);
                    }
                });
            };


            if (cloud_user_menu) {
                BBAPI.event.addEvent(cloud_user_menu, "mouseover", function() {
                    cloud_user_menu_over++;
                    BBAPI.css.addClass(cloud_user_menu, "bb-collapsed-content-expanded");

                });
                BBAPI.event.addEvent(cloud_user_menu, "mouseout", function() {
                    BBAPI.css.removeClass(cloud_user_menu, "bb-collapsed-content-expanded");
                    cloud_user_menu_over = 0;
                });
                BBAPI.event.addEvent(BBAPI.dom().id("headerProjectMenuIcon"),'click',function(ev){
                    if(cloud_user_menu.className.split(" ").indexOf("bb-collapsed-content-expanded") != -1 && cloud_user_menu_over > 1) {
                        BBAPI.css.removeClass(cloud_user_menu, "bb-collapsed-content-expanded");
                        cloud_user_menu_over = 0;
                    } else if(cloud_user_menu.className.split(" ").indexOf("bb-collapsed-content-expanded") == -1) {
                        cloud_user_menu_over = 2;
                        BBAPI.css.addClass(cloud_user_menu, "bb-collapsed-content-expanded");
                    } else {
                        cloud_user_menu_over++;
                    }
                });
            }

            // кнопка "Наверх"
            var up_btn = BBAPI.dom().id("js__scroll-up");

            up_btn && BBAPI.event.addEvent(up_btn, "click", function(){
                var scrollTop = BBAPI.utils.scrollTop();

                BBAPI.animate(function(progress) {
                    window.scrollTo(0, scrollTop - progress * scrollTop);
                });

                return false;
            });


            // состояние блока fixed
            var is_fixed = false;

            // позиции меню пользователя
            var fromDown = -45, toDown = Math.abs(fromDown) + 5, fromUp = 5, toUp = 0;

            // скролл
            BBAPI.event.addEvent(window, "scroll", function() {
                var scrollTop = BBAPI.utils.scrollTop();
                if (scrollTop > 50) {
                    if (!is_fixed) {
                        // кнопка "Наверх"
                        if(up_btn){
                            BBAPI.css.show(up_btn);
                            BBAPI.animate(function(progress, progress_clean) {
                                up_btn.style.opacity = progress_clean;
                            });
                        }
                        is_fixed = true;
                    }
                } else {
                    if (is_fixed) {
                        // кнопка "Наверх"
                        if(up_btn){
                            BBAPI.animate(function(progress, progress_clean) {
                                up_btn.style.opacity = 1 - progress_clean;
                            }, false, function() {
                                !is_fixed && BBAPI.css.hide(up_btn);
                            });
                        };

                        is_fixed = false;
                    }
