    !function () {
        'use strict';

        function changeInit() {
            $(".currency-list .item").on("click", function(){
                var inp = $(this),
                    amount = inp.closest("form").find(".amount .form-input-sl");
                    if(amount.length == 1){
                        var val_inp = inp.closest("form").find("#curr_"+inp.html());
                        if(val_inp.length == 1){
                            var json = JSON.parse(val_inp.attr("data-val")),
                                slider = amount.get(0).noUiSlider;
                            slider.updateOptions(json);
                            slider.set(json.start);
                        }
                    }
                    $("#currInput").val(inp.attr("data-val"));
            })
        }

        $(window).load(function () {
            changeInit();
        });
    }();
