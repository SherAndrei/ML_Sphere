			if ($(el[key]).offset().left !== null && $(el[key]).offset().left > parseInt($(window).width()-$(el[key]).width()) || $(el[key]).offset().left < -2 || $(el[key]).offset().top !== null && $(el[key]).offset().top > parseInt($(document).height()-$(el[key]).height()) || $(el[key]).offset().top < -2 || key.indexOf('bottom') == -1 && $(el[key]).offset().top > parseInt($(window).height()-$(el[key]).height())){
				if (i==0){
					pcss = 'left:0 !important;top:0 !important;bottom:inherit !important;right:inherit !important';
					$(el[key]).css('cssText',pcss);
					$(el[key]).css({'left':0,'top':0,'bottom':'','right':''});
				} else ac++;
			}
		}
	}
	if (ac > 0)
		$('').appendTo('body');
});
}catch(e){}
</script>
</body>
</html>

