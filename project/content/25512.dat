<script type="text/javascript">
		function bufferInit() {
			// setup single ZeroClipboard object for all our elements
			var clip = new ZeroClipboard.Client();
			clip.setHandCursor( true );
			
			// assign a common mouseover function for all elements using jQuery
			$('#get-buffer').mouseover( function() {
				if (!clip.div)
					clip.glue(this);
				else
					clip.show();
			} );

			 // Нажали мышкой по флешке
			 clip.addEventListener( 'mouseDown', function(client) {
				// Копируем нужный текст
				var input = $('#code-content .code-container.active');
				clip.setText( input.val() );
			 });
			 
			 // текст скопировался
			 clip.addEventListener('complete', function(client, text) {
				 $('#code-content').addClass('copied');
				 setTimeout(function() { $('#code-content').removeClass('copied'); }, 2000);
				 clip.hide();
	       });
		}

		$(document).ready(function(){
			bufferInit();
		});
</script>
