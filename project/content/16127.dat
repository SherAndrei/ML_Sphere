        document.head.appendChild(script);
    }

    function insertGalleryBlock() {
        var $pGalleryLink = $('.news_detail_gallery_link');
        var pList = [];

        if (!$pGalleryLink.length) return;

        $('.news_body > p').each(function() {
            var $pItem = $(this);

            if ($pItem.text().trim().length < 75) return;
            if (($pItem.css('text-align') == 'right') || ($pItem.attr('align') == 'right')) return;
            if ($pItem.css('margin-left').replace('px', '') > 20) return;
            if ($pItem.find('strong > em').length) return;

            pList.push($pItem);
        });

        $(pList[0]).after('<p class="news_detail_gallery_link">'+ $pGalleryLink.html() + '</p>');
    }

    function displayIncutBlock(blockItem, positionName) {
        if (blockItem.length === 0) return;

        var pList = getListParagraphs();
        var $container = $('<div class="news_body_ad ads_click partner_block"></div>');
        var $itemTemplate = $(
            '<div class="news_body_ad_item">' +
                '<a href="" target="_blank" rel="nofollow" title="" class="news_body_ad_item_title"></a>' +
            '</div>'
        );

        var pTotalCount = pList.length;
        var pNumber = (positionName === 'top') ? 2 : 0;
        var displaedItemIds = [];

        if (!pNumber && pTotalCount >= 7 && positionName === 'middle') {
            pNumber = Math.floor( (pTotalCount - 2) / 2 + 2 );
        }

        if (!pNumber && pTotalCount >= 5 && positionName === 'bottom') {
            pNumber = pTotalCount - 1;
        }

        if (pNumber > pTotalCount || pNumber === 0) return;

        $(blockItem).each(function() {
            var newsItem = this;
            var $item = $itemTemplate.clone();

            $item.find('a').attr('href', newsItem.url).html(newsItem.header);
            if (newsItem.partner) {
                $item.find('a').attr('onclick', 'updateIncutClickStat(' + newsItem.id + ')');
                displaedItemIds.push(newsItem.id);
            }

            $container.append($item);
        });

        var partnerName = "Новости&ensp;партнеров";
        $container.append('<div class="news_body_ad_title"><span>'+partnerName+'</span></div>');

        updateIncutDisplayStat(displaedItemIds);

        $('.news_body > p').eq(pList[pNumber - 1]).before($container);
    }

