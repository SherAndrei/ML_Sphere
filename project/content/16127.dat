    function getListParagraphs() {
        var pList = [];

        $('.news_body > p').each(function(index) {
            var $pItem = $(this);

            if ($pItem.text().trim().length < 75) return;
            if (($pItem.css('text-align') == 'right') || ($pItem.attr('align') == 'right')) return;
            if ($pItem.css('margin-left').replace('px', '') > 20) return;
            if ($pItem.text().trim().length == 0) return;
            if ($pItem.find('table').length) return;
            if ($pItem.find('img').length) return;
            if ($pItem.find('iframe').length) return;

            pList.push(index);
        });

        return pList;
    }

    function insertMediaBlock() {
        var pList = [];

        $('.news_body > p').each(function() {
            var $pItem = $(this);

            if ($pItem.text().trim().length < 75) return;
            if ($pItem.find('.regnum_title').length) return;
            if (($pItem.css('text-align') == 'right') || ($pItem.attr('align') == 'right')) return;
            if ($pItem.text().trim().length == 0) return;
            if ($pItem.find('table').length) return;
            if ($pItem.find('img').length) return;
            if ($pItem.find('iframe').length) return;
            if ($pItem.find('strong > em').length) return;

            pList.push($pItem);
        });

        $(pList[1]).after('<div id="inpage_VI-183752-0" style="width: 623px;margin-left: 17px;"></div>');

