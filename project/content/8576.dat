<script>
$(document).ready(function(){
	var timeout;
	//TAG MENU
	$('.get_menu_open').mouseenter ( function () {
		var _this = $(this);
		
		clearTimeout(timeout);
		timeout = setTimeout(function() { 

			$('.menu_content').html('<center><img src="/img/preloader.gif" style="margin: 5px 0px 26px 0px;"></center>');
			
			
			if ($.browser.msie) {
				$('.menu_container').css('display','block');
			} else {
				$('.menu_container').stop(true,true).fadeIn(400); 
			}
			
			
			
			$('.menu_container').css('top',(_this.offset().top-30));
			$('.menu_arrow').css('top',28);
			var top_main = _this.offset().top;
			
			//IE CORD
			var correct = 0;
			if ($.browser.msie) { correct = 3; }
			
			$('.menu_container').css('left',(_this.offset().left-0+185+correct));
			$('.menu_content').load('/bend/menu.php', 
				{url: _this.attr('rel'), type: _this.attr('type')},
				function() {
					$('.menu_container').css('display','block');
					//$('.menu_container').css('opacity','1');
					//count
					if ( $('.menu_block_simple').length == 1 ) {
						$('.menu_container').css('width','400px');
					} else {
						$('.menu_container').css('width','780px');
					}
					//position
					var top = (top_main-($('.menu_container').height()/2)+20);
					if (top<25) top = 25; 
					var scroll = $(window).scrollTop();
					var correct = 0;
					var before = top;
					if(scroll > (top + 10)) {
						top = scroll + 10;
						correct = top - before;
					}
					//alert('main_top '+top_main+"\r\n"+'top '+top+ ' before top '+before+' scroll '+scroll+ ' correct '+correct+ ' --- container height'+$('.menu_container').height());

					$('.menu_container').css('top',top);
					
					$('.menu_arrow').css('position','absolute');
					$('.menu_arrow').css('top',(($('.menu_container').height()/2)-22-correct));		
					
					//correct 2
					var correct2 = $('.menu_arrow').offset().top - top_main;
					if(correct2 > 0) {
						$('.menu_arrow').css('top',(($('.menu_container').height()/2)-24-correct-correct2));
					}
					//main_top 306
					//top 238 before top 25 scroll 228 correct 213 --- container height722
					
				}
			);
			
		}, 300);
	});
	
	$('.get_menu_open').mouseleave ( function (e) {		
		if ($.browser.msie) {
			$('.menu_container').css('display','none');
		} else {
			$('.menu_container').stop(true,true).fadeOut(400);
		}
		clearTimeout(timeout);
	});
	
	
	//REGION MENU
	$('.get_menu_open_region').mouseenter ( function () {
		var _this = $(this);
		
		clearTimeout(timeout);
		timeout = setTimeout(function() { 

			$('.menu_content').html('<center><img src="/img/preloader.gif" style="margin: 5px 0px 26px 0px;"></center>');
			
			
			if ($.browser.msie) {
				$('.menu_container').css('display','block');
			} else {
				$('.menu_container').stop(true,true).fadeIn(400); 
			}
			
			
			
			$('.menu_container').css('top',(_this.offset().top-30));
			$('.menu_arrow').css('top',30);
			var top_main = _this.offset().top;
			
			//IE CORD
			var correct = 0;
			if ($.browser.msie) { correct = 3; }
			
			$('.menu_container').css('left',(_this.offset().left-0+185+correct));
			$('.menu_content').load('/bend/menu-region.php', 
				{url: _this.attr('rel')},
				function() {
					$('.menu_container').css('display','block');
					//$('.menu_container').css('opacity','1');
					//count
					if ( $('.menu_block_simple').length == 1 ) {
						$('.menu_container').css('width','400px');
					} else {
						$('.menu_container').css('width','780px');
					}
					//position
					var top = (top_main-($('.menu_container').height()/2)+20);
					if (top<25) top = 25; 
					var scroll = $(window).scrollTop();
					var correct = 0;
					var before = top;
					if(scroll > (top + 10)) {
						top = scroll + 10;
						correct = top - before;
					}
					//alert('main_top '+top_main+"\r\n"+'top '+top+ ' before top '+before+' scroll '+scroll+ ' correct '+correct+ ' --- container height'+$('.menu_container').height());

					$('.menu_container').css('top',top);
					
					$('.menu_arrow').css('position','absolute');
					$('.menu_arrow').css('top',(($('.menu_container').height()/2)-14-correct));		
					
					//correct 2
					var correct2 = $('.menu_arrow').offset().top - top_main;
					if(correct2 > 0) {
						$('.menu_arrow').css('top',(($('.menu_container').height()/2)-14-correct-correct2));
					}
					//main_top 306
					//top 238 before top 25 scroll 228 correct 213 --- container height722
					
				}
			);
			
		}, 300);
	});
	
	$('.get_menu_open_region').mouseleave ( function (e) {		
		if ($.browser.msie) {
			$('.menu_container').css('display','none');
		} else {
			$('.menu_container').stop(true,true).fadeOut(400);
		}
		clearTimeout(timeout);
	});
	
	
	//FOR ALL MENU
	$('.menu_container').mouseenter ( function (e) {
		if ($.browser.msie) {
			$('.menu_container').css('display','block');
		} else {
			$('.menu_container').stop(true,false).fadeIn(400);
		}
	});
	$('.menu_container').mouseleave ( function (e) {
		if ($.browser.msie) {
			$('.menu_container').css('display','none');
		} else {
			$('.menu_container').stop(true,true).fadeOut(400);
		}
		clearTimeout(timeout);
	});
	
	//Open menu if active
	if($('li').is('.menu_active')) {
		$('.menu_active .acitem').css('display','block');
		$('.menu_active_p').css('font-weight','bold');
	}
	
	//Switch between menu 
	$('#to_tag').click ( function () {
		$(this).addClass('active');
		$('#to_dir').removeClass('active');
		$('#menu_tag').css('display','block');
		$('#menu_dir').css('display','none');
		$.cookie("menu_type", "menu_tag", {expires: 31, path: "/"});
		
		$('#menu_tag li .active').parent().children('ul').css('display','none');
		$('#menu_tag li .active').removeClass('active');
	});
	$('#to_dir').click ( function () { 
		$(this).addClass('active');
		$('#to_tag').removeClass('active');
		$('#menu_tag').css('display','none');
		$('#menu_dir').css('display','block');
		$.cookie("menu_type", "menu_dir", {expires: 31, path: "/"});
	});
		
	
});
</script>




