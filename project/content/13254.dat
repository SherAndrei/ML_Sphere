}

function jumpto()
{
	var page = prompt('Введите номер страницы, на которую хотите перейти:', '1');
	var perpage = '';
	var base_url = '';

	if (page !== null && !isNaN(page) && page > 0)
	{
		document.location.href = base_url.replace(/&amp;/g, '&') + '&start=' + ((page - 1) * perpage);
	}
}

