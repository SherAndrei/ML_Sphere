		cmContainer.find('.level4 > ul > li > a').on('hover', function () {
				var image = $(this).attr('data-attr')
				if (image) {
					cmContainer.find('.level3-image > img').attr('src', image);
				} else {
					cmContainer.find('.level3-image > img').attr('src', main);
				};
			});

		cmContainer.find('.level4 > ul > li > a').on('mouseleave', function () {
				cmContainer.find('.level3-image > img').attr('src', main)
			});
	});

function show_second(){
	$('#cmenu.desktopview .second').removeClass('vis')
	$(second_hover_element).addClass('vis');
	$(second_hover_element).find('img').lazyload();
}

$('#cmenu.desktopview .second').on('hover', function (e) {

	if (mobile_viev) {
		e.preventDefault();
		return false;
	}

	clearTimeout(second_hover);
	clearTimeout(first_hover);
	second_hover_element = this;
	second_hover = setTimeout(function(){show_second()}, 100);
});


$('#cmenu.desktopview .cmenu_item > a').on('click', function (e) {
	
	if (mobile_viev) {
		goMobile($(this));
		e.preventDefault();
		return false;
	}

	$(this).parent().find(' a.second-title span img').lazyload();
	$(this).parent().find('.second:first-child .level3-image > img').lazyload();

	isExpanded = true;
	show_first();
	if(!$('#cmenu').hasClass('gogogo')) {
		$('#cmenu').toggleClass('gogogo');
		$('#cmenu').css('z-index', '1001');
		$('body').append('<div onclick="closeMenu();" id="mcover" style=" position: fixed;  top: 0;  left: 0;  width: 100%;  height: 100%;  background: rgba(128, 128, 128, 0.07); z-index: 1000;"><div>')
		$(this).parents('.cmenu_item').find('.subwrapper').css('overflow', 'visible');
		e.preventDefault();
	}
		return false;
	});


$('#mcover').on('click', function () {
	//if($(this).hasClass('gogogo')) {
	//	$(this).toggleClass('gogogo');

	//}
});

function show_first(){


  	$('#cmenu.desktopview .subwrapper').css('overflow', 'hidden');
		if(isExpanded){
			$(first_hover_element).find('.subwrapper').css('overflow', 'visible');
	}

	$('.cmenu_item').removeClass('vis');
	$(first_hover_element).addClass('vis');

	$(first_hover_element).find('a.second-title span img').lazyload();

	$(first_hover_element).find('.second').removeClass('vis');
	el = $(first_hover_element).find('.second:first-child');
	el.addClass('vis');

	$(el).parent().find('.second:first-child .level3-image > img').lazyload();

	resizeLevel2(el);
}

$('#cmenu.desktopview .cmenu_item').on('hover', function (e) {


	if (mobile_viev) {
		e.preventDefault();
		return false;
	}


  if(!$(this).hasClass('vis')) {
		clearTimeout(first_hover);
		clearTimeout(second_hover);
		first_hover_element = this;
		first_hover = setTimeout(function(){show_first()}, 300);
	};

}).on('mouseleave', function(){
	clearTimeout(first_hover);
	clearTimeout(second_hover);
	if(!isExpanded){
		$(this).find('.subwrapper').css('overflow', 'hidden');
		$(this).removeClass('vis');
	}
}).on('click', function(event){

}).on('dblclick', function(event){});

});

function closeMenu() {
	isExpanded = false;
	$('#cmenu .subwrapper').css('overflow', 'hidden');
	$('#cmenu .cmenu_item').removeClass('vis');
	$('#cmenu').toggleClass('gogogo');
	$('#cmenu').css('z-index', '1');
	$('#mcover').remove();
};

//mobile

$('#cmenu .navcat').on('click', function (e) {
	$('#cmenu').toggleClass('desktopview mobileview');
	mobile_viev = $('#cmenu').hasClass('mobileview');
});

function goMobile(el) {
	var href = '';



	if(!$(el).parent().hasClass('collapsed')) {
		$('.cmenu_item').removeClass('collapsed');
		//href = $(el).attr('href');
	} else {

	}

	$(el).parent().toggleClass('collapsed');


	return false;

