        <div class="post_date"><a  class="post_link"  href="/wall-31981319_23"  onclick="return showWiki({w: 'wall-31981319_23'}, false, event);" ><span class="rel_date">23 сен 2014</span></a></div>
        <div class="ui_actions_menu_wrap _ui_menu_wrap " onmouseover="uiActionsMenu.show(this);" onmouseout="uiActionsMenu.hide(this);">
