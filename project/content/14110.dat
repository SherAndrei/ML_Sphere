	<script>
		mambo.use(function($) {
			if (!$('.alpha-login').length) return;

			// VARS
			var $container = $('#social_links_container');
			var $restContainer = $('#social_links_rest_container');
			var $restButton = $('#social_links_rest_button');
			var $units, $unitsRest;
			var unitCount;
			var UNIT_FIXED = 3;
			var UNIT_WIDTH = 50;

			// FUNCTIONS
			function resize() {
				var count = Math.floor($container.width() / UNIT_WIDTH);

				for (var i = UNIT_FIXED; i < unitCount ; i++) {
					if (i < count - 1 || count == unitCount) {
						$units.eq(i).show();
						$unitsRest.eq(i - UNIT_FIXED).hide();
					} else {
						$units.eq(i).hide();
						$unitsRest.eq(i - UNIT_FIXED).show();
					}
				}

				if (count < unitCount) {
					$restButton.show();
				} else {
					$restButton.hide();
				}
			};

			function init() {
				$restButton.before($restContainer.find('li').clone());

				$units = $container.find('>li').not('.lastLink');
				$unitsRest = $restContainer.find('>li');
				unitCount = $units.length;
			};

			// EVENTS
		    $(window).resize(resize);

			// INITS
			init();
			resize();
		});
	</script>

    

