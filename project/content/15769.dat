<script type="text/javascript">
    function login() {
        $.ajax({
            async: false,
            type: 'POST',
            cache: false,
            url: '/login.htm',
            data: $("#loginForm").serialize(),
            success: function (data) {
                var d = prepareJson(data);
                if (d.result) {
                    document.location.href = '/m/fbl.htm';
                }
                else {
                    alert('Проверьте правильность логина и\\или пароля.');
                }
            },
            error: function (err) {
                alert(err);
            },
            dataType: 'text'
        });
        return false;
    }
</script>

