  Drupal.behaviors.ticker = function(context){
  var reilHoverTimer = null;
  var elReil = null;
  var speedPerChar = 30;

  $('.ticker-wrapper:not(.ticker-processed)').addClass('ticker-processed').hover(function(){
  var maxWidth = $(this).width();
  var elWidth = $('.ticker-content', this).width();
  var textLenght = ($('.ticker-content', this).text()).length;
  var speed_forvard = 2 * speedPerChar * textLenght;  
  var delay_forvard = 300;  
  elReil = this;

  if ( elWidth > maxWidth ){
  reilHoverTimer = setTimeout(function(){
  $('.ticker-content', elReil).animate({
  left: '-' + ((elWidth - maxWidth) + 20) + 'px'
  }, speed_forvard);
  }, delay_forvard);
  }
  }, function(){
  var textLenght = ($('.ticker-content', this).text()).length;
  var speed_back = speedPerChar * textLenght;

  clearTimeout(reilHoverTimer);
  $('.ticker-content', this).stop(true).animate({
  left: '0px'
  }, speed_back);
  });
  };
  
