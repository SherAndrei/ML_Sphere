<div id="oneclick-dialog" class="hide adaptive-dialog" title="Перезвоните мне">
    <div id="oneclick-product" class="oneclick-product"></div>
    <IDXSKIP><div id="oneclick_phone" class="oneclick_phone no-scroll">
        &nbsp;
        <table class="phone-code-table">
            <tr>
                <td class="nobr">
                    <div style="position: relative;" class="b-phone-code__inputs_portal">
                        <input class="cabinet-text-input cabinet-text-input-240 i-form__input-phone" placeholder="Номер" autofocus="autofocus" style="margin-bottom:1px;" id="ocNumber" name="ocNumber" value="" type="text" />
                        <input type="hidden" name="ocPhone" id="ocPhone" value="" />
                    </div>
                </td>
                <td class="oneclick_phone-table-td">
