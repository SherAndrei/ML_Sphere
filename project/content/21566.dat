                $(function () {
                    var additionalCarousel = function (element, quantity) {
                        if (!window.additionalCarousel) {
                            var $this = element;
                            var $thisClassName = element.attr('class');
                            var itemWidth = $this.find('li').outerWidth(true);
                            var itemsWidth = itemWidth * quantity + 3;

                            $this.css("width", itemsWidth);
                            $this.css("left", 0);

                            if ($this.find('li').length > quantity) {
                                $this
                                    .wrap('<div class="' + $thisClassName + '__wrap-carousel" />')
                                    .parent()
                                    .append(
                                        '<a href="#" class="b-product__additional-pics__wrap-carousel-left"><i></i></a>' +
                                        '<a href="#" class="b-product__additional-pics__wrap-carousel-right"><i></i></a>'
                                    );

                                var scroll = $this.position().left - itemWidth;

                                var nextItem = function () {
                                    if (!$this.is(':animated')) {
                                        $this.animate({left : scroll}, 250, function () {
                                            $this
                                                .find('li:first')
                                                .appendTo($this)
                                                .parent()
                                                .css({'left' : 0});
                                        });
                                    }
                                    return false;
                                };

                                $('.b-product__additional-pics__wrap-carousel-right').click(nextItem);

                                $('.b-product__additional-pics__wrap-carousel-left').click(function () {
                                    if (!$this.is(':animated')) {
                                        $this
                                            .css({'left' : scroll})
                                            .find('li:last')
                                            .prependTo($this)
