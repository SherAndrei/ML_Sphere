<script type="text/javascript">
var hashTestimonials = window.location.hash;
    $(function () {
        var updateByHash = function(hash, callbackF) {
            if (typeof(hash) == 'undefined') {
                hash = hashTestimonials;
            }

            if (!hash) {
                return;
            }
            var tab = hash.replace('#', '');

            setTimeout(function(){
                var tabObj = $('li[tab=' + tab + '] a');
                if (tabObj.length) {
                    tabObj.trigger('click');
                    if (typeof(callbackF) != 'undefined') {
                        callbackF();
                    } else {
                        $('html, body').animate({
                            scrollTop: tabObj.offset().top - 10
                        }, 1000);
                    }
                }
            }, 100);
        };
        $("#testimonials_tabs").ZKTabs({
            selected    : ':first',
            classSuffix : 'admin',
            useHash     : true,
            onRender : function () {
                if (hashTestimonials == '#customers_rating' || hashTestimonials == '#statistics_rating') {
                    if (hashTestimonials == '#statistics_rating') {
                        updateByHash('#firm_testimonials', function(){
                            updateByHash(hashTestimonials);
                        });
                    } else {
                        updateByHash('#firm_testimonials');
                    }
                } else {
                    updateByHash(hashTestimonials);
                }
                if (typeof(history) == 'object' && typeof(history.pushState) == 'function') {
                    history.pushState("", document.title, window.location.pathname + window.location.search);
                }
                window.location.hash = hashTestimonials;
            }
        });

        $(document).on("click", "#show_product_testimonials, #show_firm_testimonials", function () {
            var tab = $(this).data('tab');
            updateByHash(tab);
            window.location.hash = hashTestimonials;
            return false;
        });

        $('.b-product__comments li').attr({
            'data-category' : 'tab',
            'data-objdata' : '1'
        })
            .addClass('event_ga');

        var company_testimonials = false;

        $(document).on("click", "#zk-tabs-menu-0-num-1 a", function () {
            if (!company_testimonials) {
                $.ajax({
                    url      : "/ajax/PublicFirmTestimonials/GetCompanyTestimonialsRatings/html/",
                    type     : "post",
                    dataType : "html",
                    data     : ({
                        idcompany : 28800                    }),
                    success  : function (data) {
                        $("#zk-tabs-0-content-1").html(data);
                        company_testimonials = true;
                    }
                });
            }
            return true;
        });

        $("#add-testimonial").click(function () {
            $("#testimonial-dialog").remove();
            $(document.body).append('<div id="testimonial-dialog" class="adaptive-dialog"></div>');
            $("#testimonial-dialog")
                .dialog({
                    width    : 750,
                    height   : 580,
                    modal    : true,
                    autoOpen : false
                });
            $.ajax({
                url      : "/ajax/PublicFirmTestimonials/GetForm/",
                type     : "post",
                dataType : "json",
                data     : ({
                    idcompany : 28800,
                    type      : 1,
                    item_id   : 5964823                }),
                success  : function (data) {
                    $("#testimonial-dialog")
                        .html(data.html)
                        .dialog("open")
                        .dialog({height : 'auto'})
                        .parent().find(".ui-dialog-title").text(data.title)
                    ;
                }
            });

            $(this).trigger('fake_click');
            return false;
        });

        $(document).on("click", ".vote_testominial_useful, .vote_testominial_useless", function () {
            var $this = $(this);
            $.ajax({
                url      : "/ajax/PublicFirmTestimonials/VoteTestimonial/",
                type     : "post",
                dataType : "json",
                data     : ({
                    testimonial_id : $(this).parents(".one_testimonial").data("testiminial_id"),
                    vote           : $(this).hasClass("vote_testominial_useful")
                }),
                success  : function (data) {
                    $this.parents("div").eq(1).html(data.message);
                }
            });
            return false;
        });
    });
</script>
    
    
