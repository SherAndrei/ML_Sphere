			try { return new ActiveXObject('Microsoft.XMLHTTP'); }
			  catch(e) {}
			throw new Error('This browser does not support XMLHttpRequest.');
		  };
		}
		return null;
	}

	function cache_search(inputString)
	{
		var found=false;
		var k;
		for(var i=0; (i<suggestionscacheinput.length)&&!found; i++)
		{
			if(suggestionscacheoutput[i]=='empty') //если ответ пустой, сравниваем начало
				found=inputString.substr(0, suggestionscacheinput[i].length)==suggestionscacheinput[i];
			else
				found=inputString==suggestionscacheinput[i];
			if(found)
				k=i;
		}
		if(found)
			return suggestionscacheoutput[k];
		else
			return false;
	}

	function cache_add(inputString, outputString)
	{
		suggestionscacheinput.push(inputString);
		suggestionscacheoutput.push(outputString);
		if(suggestionscacheinput.length>suggestionscachelength) //нужно удалить первый элемент
		{
			suggestionscacheinput.shift();
			suggestionscacheoutput.shift();
		}
	}

	function lookup_timeout(inputString)
	{
		clearTimeout(lookup_timeout_id);
		lookup_timeout_id=setTimeout(function (){lookup(inputString);}, 500);
	}

	// вызывается при изменении поля ввода
	function lookup(inputString)
	{
		if(inputString.length == 0)
		{
			suggestion_show(empty_input_tags);
		} else
		{
			inputString=inputString.toUpperCase();
			inputString=inputString.replace(/[^А-ЯІЇЄA-Z]/g, '');
			var cached=cache_search(inputString);
			if(cached)
				suggestion_show(cached);
			else if(req)
			{
				var url='/stags.php?category_ID=0&tags='+tags_str+'&letters='+encodeURIComponent(inputString);
				req.open('GET', url, true);
				req.onreadystatechange = function()
				{
					if((req.readyState == 4)&&(req.status == 200)&&(req.responseText.length>0))
					{
						var tags=string_to_tags(req.responseText);
						cache_add(inputString, tags);
						suggestion_show(tags);
					}
				};
				req.send(null);
			}
		}
	} // lookup

	//преобразовывает полученную от сервера строку в массив extra_tags - есть ли еще теги, tags- теги, каждая ячейка массива - массив 0-номер тега, 1 - размер шрифта, 2 - тег
	function string_to_tags(s)
	{
		var tags=new Array();
		if(s!='empty')
		{
			var tags_str=s.split(','); //в каждой ячейке тройка номер-размер шрифта-тег разделенные пробелом
			for(i=0; i<tags_str.length; i++)
				tags.push(tags_str[i].split(' '));
		}
		return tags;
	}

	function suggestion_show(tags)
	{
		var tags_list=document.getElementById('tags_list');
		if(tags.length==0)
		{
			var suggestionhtml='нет тегов по вашему запросу';
		} else
		{
			var suggestionhtml='';
			for(i=0; i<tags.length; i++)
			{
				tag=tags[i];url='/tagsc0t'+tags_str;
if(tags_str!='')
					url+='-';
				url+=tag[0]+'.html';
				if(i>0)
					suggestionhtml+=' ';
				suggestionhtml+='<'+'a href="'+url+'" style="font-size:'+tag[1]+';">'+tag[2]+'</a>';
			}
		}
		tags_list.innerHTML=suggestionhtml;
	}

	createRequestObject();
	var req=new XMLHttpRequest();
	var lookup_timeout_id;
	var suggestionscachelength=20; //количество элементов кеша
	var suggestionscacheinput=new Array();
	var suggestionscacheoutput=new Array();
	var tags_str='';
	var empty_input_tags=string_to_tags('15605 14 органический,1582 14 влияние,86076 14 безрассадный,38153 14 томат,21037 14 урожайность,86075 14 безрассадных,47 14 развитие,8488 14 удобрение');
	suggestion_show(empty_input_tags);
	</script></TD>
