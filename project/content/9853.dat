}
function comm_paginator(c_arr, page, c_p_limit) {
	pagination = 1;
	var limiter = c_p_limit;
	$('.navigation_com_pages').html('');
	var navi_page = '';
	if(page != 1)
		navi_page += "<ins class=\"thumbnail\"><div class=\"r\"><a class=\"comm_pagination\" onclick=\"comm_write_page(o.children, "+(page-1)+", "+c_p_limit+");\" href=\"#!cpage="+(page-1)+"\">‹</a></div></ins>";
	else
		navi_page += "<ins class=\"thumbnail\"><div class=\"r\"><span>‹</span></div></ins>";
	c_arr.forEach(function(item, i, arr) {
		limiter -= item.c_count;
		if(limiter > 0) {
		} else {
			limiter = c_p_limit;
			if(pagination == page)
				navi_page += '<ins class="thumbnail"><div class="r"><span>'+pagination+'</span></div></ins>';
			else
				navi_page += '<ins class="thumbnail"><div class="r"><a class="comm_pagination" onclick="comm_write_page(o.children, '+pagination+', '+c_p_limit+');" href="#!cpage='+pagination+'">'+pagination+'</a></div></ins>';
			pagination++;
		}
	});	
	if(limiter != c_p_limit) {
			if(pagination == page)
				navi_page += '<ins class="thumbnail"><div class="r"><span>'+pagination+'</span></div></ins>';
			else
				navi_page += '<ins class="thumbnail"><div class="r"><a class="comm_pagination" onclick="comm_write_page(o.children, '+pagination+', '+c_p_limit+');" href="#!cpage='+pagination+'">'+pagination+'</a></div></ins>';
	}
	if(pagination > 2){
		if(page != pagination)
			navi_page += "<ins class=\"thumbnail\"><div class=\"r\"><a class=\"comm_pagination\" onclick=\"comm_write_page(o.children, "+(parseInt(page)+1)+", "+c_p_limit+");\" href=\"#!cpage="+(parseInt(page)+1)+"\">›</a></div></ins>";
		else
			navi_page += "<ins class=\"thumbnail\"><div class=\"r\"><span>›</span></div></ins>";
	}

	if(pagination > 1)
		$('.navigation_com_pages').append(navi_page);

}		

function comm_write_page(c_arr, page, c_p_limit) {
	c_counter = 0;
	c_start = (page - 1) * c_p_limit;
	c_page_encounter = 0;
	comm_paginator(c_arr, page, c_p_limit);
	$('#comments_block').html('');
	if(c_start < 0) c_start = 0;
	c_arr.forEach(function(item, i, arr) {
		if(c_start > 0) {
			if(item.c_count > c_p_limit) {
				c_start -= (c_p_limit - c_page_encounter);
				c_page_encounter = 0;
			} else {
				c_start -= item.c_count;
				c_page_encounter += item.c_count;
			}
			return;
		}
		if(c_p_limit > 0) {
			comm_writer(item, 0);
			c_p_limit -= item.c_count;
		}
	});
	
	//var target_offset = $('#comments_block').offset();var target_top = target_offset.top;$('html, body').animate({scrollTop:target_top}, 300);
	var target_offset = $('.comments_header').offset();var target_top = target_offset.top;$('html, body').animate({scrollTop:target_top}, 300);
}
