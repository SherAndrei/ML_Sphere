$(function() {
	/*function paddy(n, p, c) {
		var pad_char = typeof c !== 'undefined' ? c : '0';
		var pad = new Array(1 + p).join(pad_char);
		return (pad + n).slice(-pad.length);
	}*/
	var pad = "000000";
	var news_id = '4561295';
	var cache_folder = (pad+news_id.substr(0,news_id.length-3)).slice(-pad.length);
	
	$.getJSON( "/uncleaned_cache/comm_cache/"+ cache_folder + "/" + news_id + ".json", function( data ) {
		nodes = data;
		var map = {}, node, roots = [];
		for (var i = 0; i < nodes.length; i += 1) {
			node = nodes[i];
			node.children = [];
			map[node.id] = i; // use map to look-up the parents
			if(node.parent_id == null || node.parent_id === "undefined")
				node.parent_id = "0";
			if (node.parent_id !== "0" && node.parent_id != 0 && node.approve == 1) {
				if(nodes[map[node.parent_id]]) {
					if(!nodes[map[node.parent_id]].hasOwnProperty('children')) {
						nodes[map[node.parent_id]].children = [];
					}
					nodes[map[node.parent_id]].children.push(node);
				}
			} else {
				if(node.approve == 1) {
					roots.push(node);
				}
			}
		}
		o = {};
		o.children = roots;
		var c_index = 0;
		function counter(node) {
			if(node.children === 'undefined'){
				return 1;
			}
			node.c_index = c_index++;
			var total = 1;
			node.children.forEach(function(item, i, arr) {
				total += counter(item);
			});
			node.c_count = total;
			node.children.sort(function(a, b) {
				return a.id - b.id;
			});
			return total;
		}
		counter(o);
		
		comm_write_page2(o.children, comm_page, 50);

	});
});
