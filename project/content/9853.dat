function MDTFormat(text) {
	var a=text.split(" ");
	var b=a[0].split("-");
	var t=a[1].split(":");
	var d = new Date(b[0],(b[1]-1),b[2],t[0],t[1],t[2]);
//    d = new Date(d);
	d2 = new Date(d);
	d2.setHours(0, 0, 0, 0);

    var months = ["января", "февраля", "марта", "апреля", "мая", "июня", "июля", "августа", "сентября", "октября", "ноября", "декабря"];
    var strDate = "";

    var today = new Date();
    today.setHours(0, 0, 0, 0);

    var yesterday = new Date();
    yesterday.setHours(0, 0, 0, 0);
    yesterday.setDate(yesterday.getDate() - 1);

    var tomorrow = new Date();
    tomorrow.setHours(0, 0, 0, 0);
    tomorrow.setDate(tomorrow.getDate() + 1);

    if (today.getTime() == d2.getTime()) {
        strDate = "Сегодня";
    } else if (yesterday.getTime() == d2.getTime()) {
        strDate = "Вчера";
    } else if (tomorrow.getTime() == d2.getTime()) {
        strDate = "Завтра";
    } else {
        strDate = d.getDate() + " " + months[d.getMonth()];
    }

	strDate += " " + d.getHours() + ":" + (d.getMinutes()<10?'0':'') + d.getMinutes();

    return strDate;
}

function comm_writer(node, depth) {
