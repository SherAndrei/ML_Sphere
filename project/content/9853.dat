	comm_block += '<div class=comm_reply>';
	//comm_block += '<a class="tahoma font12" title="Ответить" onmouseover="dle_copy_quote(\''+encodeURI(item.gast_name)+'\');" href="#" onClick="dle_ins2(\''+encodeURI(item.gast_name)+'\', \''+encodeURI(item.text)+'\', \''+item.primary_id+'\',\''+item.id+'\'); $(\'#editor_block\').show(); return false;">Ответить</a>';
	comm_block += '<a class="tahoma font12" title="Ответить" onmouseover="dle_copy_quote(\''+node.gast_name+'\');" href="#" onClick="dle_ins2(\''+node.gast_name+'\', \'\', \''+node.primary_id+'\',\''+node.id+'\'); $(\'#editor_block\').show(); return false;">Ответить</a>';
	//recheck div block close!
	comm_block += '</div>';
