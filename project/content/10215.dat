		}
		var link = '<a';
		link += (parameters.href) ? ' href="' + parameters.href + '"' : ' href="#"';
		if (parameters.id) {
			link += ' id="' + parameters.id + '"';
		}
		if (parameters.className) {
			link += ' class="' + parameters.className + '"';
		}
		if (parameters.title) {
