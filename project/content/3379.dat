                var curses=[[1, 1.988, 2.187, 3.015], [1,1.994, 2.195, 3.03]];
                function getVal(input)
                {
                  return parseFloat(input.value.replace(/^0/,"").replace(/,/g,".").replace(/[^\d\.]/g,"")).toFixed(2);
                }
                function iNumFormat(a)
                { 
                  if(isNaN(a)||a==Infinity) return "0";
                  return parseInt(a).toString().replace(/(\d{1,3})(?=(?:\d{3})+$)/g, "$1 ");
                }

                function fNumFormat(a)
                {
                  if(isNaN(a)||a==Infinity) return "0.00";
                  return parseFloat(a).toFixed(2).replace(/(\d{1,3})(?=(?:\d{3})+\.)/g, "$1 ");
                }
                function inputNumFormat(id)
                {
                  var value=parseInt(document.getElementById(id).value.replace(/^0/,"").replace(/[^\d]/g,""));
                  if(isNaN(value)) return;
                  document.getElementById(id).value=iNumFormat(value);
                  return true;
                }
                function calc(){
                        var sumIn=getVal(document.getElementById('sumIn'));
                        if(isNaN(sumIn))sumIn=0;
                        var curIn=document.getElementById('curIn').selectedIndex;
                        var curOut=document.getElementById('curOut').selectedIndex;

                        console.log(curOut) 
                        console.log(curIn)
                        var res=sumIn*curses[0][curIn]/curses[1][curOut];
                        if (curIn == 3) {
                            res = res/100;
                        }
                         if (curOut == 3) {
                            res = res*100;
                        }
                        document.getElementById('result').innerHTML=fNumFormat(res);
                }
                function sel(){
                        var curIn=document.getElementById('curIn');
                        var curOut=document.getElementById('curOut');
                        if (curIn.selectedIndex==curOut.selectedIndex)
                                if(curIn.selectedIndex==0)
                                        curOut.selectedIndex=1;
                                else
                                        curOut.selectedIndex=0;
                        for(var i=0;i<curIn.options.length;i++)
                                if(i==curIn.selectedIndex)
                                        curOut.options[i].disabled=true;
                                else
                                        curOut.options[i].disabled=false;

                        calc();

                }
                function changeCur(){
                        var curIn=document.getElementById('curIn');
                        var curOut=document.getElementById('curOut');
                        var curInSI=curIn.selectedIndex;
                        var curOutSI=curOut.selectedIndex;
                        curIn.selectedIndex=curOutSI;
                        curOut.selectedIndex=curInSI;
                        sel();
                }
                window.onload = function () { // Init
        calc(1); // From Dollars (number 1 in kurs array)
}
                </script>
                
                
