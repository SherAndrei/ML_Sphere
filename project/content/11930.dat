					customEvents.onAfterItemsMove($('#blog_form_content').serializeArray());
				}
				$('window').trigger('scroll')	
			}
		});
	}
	
	is_items = function(){
		/** Event handler */
		if(typeof customEvents.onBeforeItemsSortable == 'function') {
			var container = customEvents.onBeforeItemsSortable();
		} else {
			var container = '#items';
		}
