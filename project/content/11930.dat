	stick_action = function(id){
		var url = 'http://loadzoneden.fo.ru/ajax?service=blog&stick=' + id;
		waitDiv_show();		
		$.ajax({
			url: url,
			complete: function(transport, textStatus){
				waitDiv_hide();
				var stick = $('#stick_' + id);
				if(stick.length){
					if(stick.css('display') == 'none'){
						stick.show();
						$('#items').prepend($('#item_'+id));
					} else {
						var moved = false;
						$('.ic_stick_static').each(function(){
							var elem = $(this)
							if(elem.css('display') == 'none'){
								if(moved){
									return;
								}	
								elem.closest('div.item').before($('item_'+id));
								moved = true;
							}
						});
						if(!moved){
							$('#items').append($('#item_'+id));
						}
						stick.hide();
					}
					
				}

				if($('#item_'+id+'_edit').length){
					$('#item_'+id+'_edit').hide();
				}
				eval(transport.responseText);				
			}
		});
	}
	
