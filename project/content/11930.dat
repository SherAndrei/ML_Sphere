			return;
		}
		
		waitDiv_show();
		
		$.ajax({
			url: 'http://loadzoneden.fo.ru/ajax?message_multi_delete_action=blog',
			type: 'post',
			data: params.join('&'),
			complete: function(transport, textStatus){
				waitDiv_hide();
				eval(transport.responseText);
				if(transport.getResponseHeader('X-SUCCESS') == 1){
					inputs.each(function(){
						var e = $(this)
					    if(e.attr('checked')){
					        e.closest('.item').remove();
					    }
					});
				}	
			}
		});
	}
	
