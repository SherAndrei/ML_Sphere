	init_cb = function(){
		setTimeout(function(){
			
			$('.item').each(function(){
				var e = $(this);
				var cb = e.find('input.cb:first');
				if(!cb.length){
					return;
				}
				e.bind('mouseenter', function(){
					cb.show();
				});
				e.bind('mouseleave', function(){
					if (!cb.attr('checked')) {
						cb.hide();
					}	
				});
				if(cb.attr('checked')){
					cb.show();
				}
			});
		}, 0);
	}

	delete_item = function(id, redirect_url){
		redirect_url = redirect_url || false;
		if(!confirm('Удалить?')){
			return;
		}
		
		waitDiv_show();
		$.ajax({
			url: 'http://loadzoneden.fo.ru/blog?delete_action=' + id + '&sk=85f04647617b8c2a869af3168507446e', 
			complete: function(transport, textStatus){
				waitDiv_hide();
				eval(transport.responseText);
				if (redirect_url) {
					setTimeout(function(){window.location.href = 'http://loadzoneden.fo.ru/blog/' + redirect_url}, 2000);
					return;
				}
				if(transport.getResponseHeader('X-SUCCESS') == 1){
					$('#item_' + id).remove();
				}
				$(window).trigger('scroll');
				
				/** Event handler */
				if(typeof customEvents.onAfterSingleItemDelete == 'function') {
					customEvents.onAfterSingleItemDelete(id);
				}
			}
		});
	}
	
