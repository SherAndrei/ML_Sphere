					$('.ic_disable').each(function(){
						var e = $(this) 
						e.removeClass('ic_disable');
						e.addClass('ic_enable');
						e.attr('title', 'Разрешить комментарии')
					});
				}
				
				eval(transport.responseText);	
			}
		});
	}
	
	setTimeout(init_cb, 0);
