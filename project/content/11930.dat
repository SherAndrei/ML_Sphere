				$(this).unbind();		
			});
				
			$('a.tnode.empty,a.tnode.records').each(function(){
				var elem = $(this)
				elem.attr('title', '');
				elem.css({'color' : '#333333'});
				elem.bind('click', function(evn){
					evn.stopPropagation()
					var id = elem.attr('rel');
					multi_move_action(id);
					$('#folder_tree').hide();
					return false
				});
				
			});
