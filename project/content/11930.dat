<script type='text/javascript'>
	$('#main_content').mouseenter(function(){
		$('#ct').stop(true, true);
		$('#ct').fadeIn('slow');
	})
	
	$('#main_content').mouseleave(function(){
		$('#ct').stop(true, true);
		$('#ct').fadeOut('slow');
