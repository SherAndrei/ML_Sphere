<script type="text/javascript">
var hashTestimonials = window.location.hash;
    $(function () {
        var updateByHash = function(hash, callbackF) {
            if (typeof(hash) == 'undefined') {
                hash = hashTestimonials;
            }

            if (!hash) {
                return;
            }
            var tab = hash.replace('#', '');

            setTimeout(function(){
                var tabObj = $('li[tab=' + tab + '] a');
                if (tabObj.length) {
                    tabObj.trigger('click');
                    if (typeof(callbackF) != 'undefined') {
                        callbackF();
                    } else {
                        $('html, body').animate({
                            scrollTop: tabObj.offset().top - 10
                        }, 1000);
                    }
                }
            }, 100);
        };
        $("#testimonials_tabs").ZKTabs({
            selected    : ':first',
            classSuffix : 'admin',
            useHash     : true,
            onRender : function () {
                if (hashTestimonials == '#customers_rating' || hashTestimonials == '#statistics_rating') {
                    if (hashTestimonials == '#statistics_rating') {
                        updateByHash('#firm_testimonials', function(){
                            updateByHash(hashTestimonials);
                        });
                    } else {
                        updateByHash('#firm_testimonials');
                    }
                } else {
                    updateByHash(hashTestimonials);
                }
                if (typeof(history) == 'object' && typeof(history.pushState) == 'function') {
                    history.pushState("", document.title, window.location.pathname + window.location.search);
                }
                window.location.hash = hashTestimonials;
            }
        });

        $(document).on("click", "#show_product_testimonials, #show_firm_testimonials", function () {
            var tab = $(this).data('tab');
            updateByHash(tab);
            window.location.hash = hashTestimonials;
            return false;
        });

        $('.b-product__comments li').attr({
