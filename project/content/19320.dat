    <script type="text/javascript">
        $(function () {
            /** sidebar firminfo **/
            function move_sidebar() {
                // delete all SCRIPT nodes, because code will be executed on jquery DOM operations
                $(".sidebar-company-info-block").find("script").each(function () {
                    $(this).remove();
                });
                if ($(window).width() > COLLAPSE_WIDTH) {
                    $("#sideRight").prepend($(".sidebar-company-info-block"));
                    $(".show_big_map").parent("li").show();
                } else {
                    if ($(".b-sorting").length > 0) {
                        $(".b-sorting").before($(".sidebar-company-info-block"));
                    } else {
                        if ($(".firm_page_title").length > 0) {
                            $(".firm_page_title").after($(".sidebar-company-info-block"));
                        } else {
                            $(".pp-header-info").append($(".sidebar-company-info-block"));
                        }
                    }
                    $(".show_big_map").parent("li").hide();
                }
            }

            $(window).on('resize orientationchange', move_sidebar);
            move_sidebar();
            /****/

            /** catalog sidebar **/
            function close_bfilters() {
                var btn = $(".filters_switcher_btn");
                if (btn.is(":visible")) {
                    btn.removeClass("b-filter_active");
                    $(".b-filter").hide();
                }
            }

            $(document).on("click touch", ".filters_switcher_btn", function () {
                if ($(this).hasClass("b-filter_active")) {
                    close_bfilters()
                } else {
                    $(this).addClass("b-filter_active");
                    $(".b-filter").show();
                    return false;
                }
            });

            function move_catalog_sidebar() {
                var bfilter = $(".b-filter");
                var btn = $(".adaptive-filter-button");
                var sidebar_with_main_categories_menu = $(".sidebar_with_main_categories_menu");
                //var sidebar_testimonial = $(".sidebar_testimonial");
                var customers_action_banner = $(".customers_action_banner");
                var vertical_informer = $(".informer-vertical-marker");
                var $recommendCategories = $(".recommend-categories");

                if ($(window).width() > COLLAPSE_WIDTH) {
                    $(".sidebar").prepend(bfilter);
                    $("#container").after(sidebar_with_main_categories_menu);
                    //$(".stay_up_to_date").before(sidebar_testimonial);

                    $("#content_t").prepend(customers_action_banner);
                    //$("#content").prepend(customers_action_banner);
                    bfilter.show();
                    // horizontal informer => vertical informer
                    if (vertical_informer.length) {
                        vertical_informer.addClass("informer-block__vertical").removeClass("informer-block__horizontal");
                        vertical_informer.trigger("rebuild-informers", "vertical");
                    }

                    // вернем рекомендуемым обычный вид
                    if ($recommendCategories.length) {
                        $recommendCategories.removeClass("adaptive");
                        $recommendCategories.find("li a").removeClass("categories_menu_item_final categories_menu_item");
                        $recommendCategories.find("li").find("a:last").each(function (k, v) {
                            var self = $(this);
                            self.html(self.text());
                        });
                    }
                } else {
                    $("#container").before(sidebar_with_main_categories_menu);
                    //$(".homepage_text").after(sidebar_testimonial);
                    $("#content_t").before(customers_action_banner);
                    //$(sidebar_with_main_categories_menu).before(customers_action_banner);
                    // если есть сортировка, то не нужно адаптивное меню
                    var $sortingBlock = $(".b-sorting");
                    if ($sortingBlock.length) {
                        $sortingBlock.after(bfilter);
                        btn.remove();
                    } else {
                        $("#content").prepend(bfilter);
                        $("#content").prepend(btn);
                    }
                    bfilter.hide();
                    // vertical informer => horizontal informer
                    if (vertical_informer.length) {
                        vertical_informer.addClass("informer-block__horizontal").removeClass("informer-block__vertical");
                        vertical_informer.trigger("rebuild-informers", "horizontal");
                    }
                    // стилизуем свернутые рекомендуемое меню под обычное

                    if ($recommendCategories.length) {
                        $recommendCategories.addClass("adaptive");
                        $recommendCategories.find("li a").addClass("categories_menu_item_final categories_menu_item");
                        $recommendCategories.find("li").find("a:last").each(function (k, v) {
                            var self = $(this);
                            self.html('<span class="categories_menu_item__title">' + self.text() + '</span>');
                        });
                    }
                }
            }

            // mark vertical slider
            if ($(".informer-block__vertical").length) {
                $(".informer-block__vertical").addClass("informer-vertical-marker");
            }

            $(window).on('resize orientationchange', move_catalog_sidebar);
            move_catalog_sidebar();
            /****/

            /** goods list **/
            function rebuild_goods_list() {
                if ($(window).width() > 1000) {
                    $(".one_pp_wrap").each(function () {
                        $(this).prepend($(this).find(".pp_info2"));
                        $(this).find(".pp_info").prepend($(this).find(".title_in"));
                    });
                } else {
                    $(".one_pp_wrap").each(function () {
                        $(this).prepend($(this).find(".title_in"));
                        $(this).find(".pp_info").prepend($(this).find(".pp_info2"));
                    });
                }
            }

            $(window).on('resize orientationchange', rebuild_goods_list);
            rebuild_goods_list();
            /****/
        });
    </script>

