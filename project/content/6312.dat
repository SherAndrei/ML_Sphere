       var callback_result = $.ajax({url: "/index.php?do=callback", type: "POST", data: { customer_tovar: encodeURIComponent(customer_tovar), customer_comment: encodeURIComponent(customer_comment), customer_name: encodeURIComponent(customer_name), customer_phone: encodeURIComponent(customer_phone) }, cache:false, async:false }).responseText;
