  $.cookie('is_hide', null, {expires: 7, path: '/' });
}
toggleRulesUp = function() {
  $('#sidebar2').hide();      
  $('#arrowdown').show();
  $('#arrowup').hide();
  hidden = true;      
  $.cookie('is_hide', true, {expires: 7, path: '/' });
}   
if ($.cookie('is_hide')) {
  $('#sidebar2').hide(); 
  $('#arrowdown').show();
  $('#arrowup').hide();
  hidden = true;
} 
$('.arrows').click( function() {
  if (hidden == true) {
    toggleRulesDown();
  } else {
    toggleRulesUp();
  }
});
});
</script>


