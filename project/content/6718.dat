			success  : function(data, ans) {
				if (data && data.status == 'success') {
				
					if (!data.rows) {
						return true;
					}
					
					var html = '';
					
					for (i = 0; i < data.rows.length; i++) {
						html += '<li class="default"><a href="/bankovskie-uslugi-platezhnye-sistemy-otzyvy/' + ( (data.rows[i].query) ? '?' + data.rows[i].query : '' ) + '" title="' + htmlspecialchars(data.rows[i].title) + '">' + htmlspecialchars(data.rows[i].name) + '</a></li>';
					}
					
					$('#bind-list-' + bind_id + ' .list').empty().html(html);
					
					to = to - (limit - data.rows.length);
					
					$('#bind-list-' + bind_id + ' .note .from').text(from+1);
					$('#bind-list-' + bind_id + ' .note .to').text(to);
				
				}
				
				element_disable('bind-list-' + bind_id);
			}
		});
		
		return true;
		
	}
	
