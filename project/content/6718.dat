<script type="text/javascript">
	function catalog_bind_listing_ajax(bind_id, dir, page) {
	
		var go_page = 1;
		var limit = 15;
		var curr_page = $('#bind-list-' + bind_id).data('currpage');
		var overall = $('#bind-list-' + bind_id).data('overall');
		var overall_pages = parseInt(overall / limit) + ((overall % limit) ? 1 : 0);
		var from = 0;
		var to = 15;
		
		if (dir == 'forward') {
			go_page = curr_page + 1;
			if (page) {
				go_page = page;
			}
			if (go_page > overall_pages) return false;
			$('#bind-list-' + bind_id + ' .nav .backward .disabled').removeClass('disabled');
		} else if (dir == 'backward') {
			if (curr_page == 1) return false;
			go_page = curr_page - 1;
			$('#bind-list-' + bind_id + ' .nav .forward .disabled').removeClass('disabled');
		} else {
			return false;
		}
		
		element_disable('bind-list-' + bind_id);
		
		$('#bind-list-' + bind_id).data('currpage', go_page);
		
		if (go_page == 1) {
			$('#bind-list-' + bind_id + ' .nav .backward a').addClass('disabled');
		}
		if (go_page == overall_pages) {
			$('#bind-list-' + bind_id + ' .nav .forward a').addClass('disabled');
		}
		
		from = go_page * limit - limit;
		to = from + limit;
		
		$.ajax({
			type     : 'post',
			url      : '/catalog/',
			data     : 'do=binds_listing&section_id=1176&bind_id=' + bind_id + '&page=' + go_page + '&limit=' + limit + '&get=' + encodeURIComponent(location.search.replace(/^\?/, '')),
