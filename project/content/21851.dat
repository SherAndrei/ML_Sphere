<script type="text/javascript">

$(document).ready(function(){
    // свернуть/развернуть фильм
    $('div.title > div > p > u').click(function () {
        var new_class = $(this).attr('class') == 'plus' ? 'minus' : 'plus';
        $(this)
            .removeClass()
            .addClass(new_class);
        var id_film = $(this).attr('id').substr(4);
        $('#table_'+id_film).css('display', new_class == 'plus' ? 'none' : 'block');
    });

    // свернуть/развернуть все фильмы
    $('div.showing_links > p').click(function () {
        var to_state = null;
        // свернуть
        if ($(this).attr('class') == 'minus') {
            $(this)
                .removeClass()
                .addClass('minus_act');
            $('div.showing_links > p.plus_act')
                .removeClass()
                .addClass('plus');
            to_state = 'plus';
        }
        // развернуть
        if ($(this).attr('class') == 'plus') {
            $(this)
                .removeClass()
                .addClass('plus_act');
            $('div.showing_links > p.minus_act')
                .removeClass()
                .addClass('minus');
            to_state = 'minus';
        }

        // действие валидно
        if (to_state) {
            // кука
            var expDate = new Date();
            expDate.setTime(expDate.getTime() + 365*24*60*60*1000);
            document.cookie = 'afisha_showing='+to_state+'; expires='+expDate.toGMTString()+'; path=/';

            // обновляем строки всех фильмов
            $('div.title > div > p > u').each(function (i) {
                this.className = to_state;
                var id_film = this.id.substr(4);
                $('#table_'+id_film).css('display', to_state == 'plus' ? 'none' : 'block');
            });
        }
    });

    var myCookie = ' ' + document.cookie + ';';
    if (myCookie.indexOf('afisha_showing=plus;') > 0) {
        $('div.showing_links > p.minus').click();
    }
});

</script>

