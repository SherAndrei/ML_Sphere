			<a href="/club/users/{{author.id}}"><img src="{{ buildAvatarUrl author.file_host author.avatar_id '160x160' }}" alt="" />{{ author.name }}</a>
		</div>
		
		<div class="CommentBody">
			<p>{{formatText text}}</p>
		</div>
		<span class="Date">{{ formatDate created }}</span>
		<div class="CommentAfterBody">
			<div class="CommentService">
			<p><a name="comment_like_btn_{{_id}}" class="{{#if liked}}unlikeBtn{{^}}likeBtn{{/if}}" data-id="{{_id}}" data-type="comment"><i></i><span>{{#if liked}}Больше не нравится{{^}}Мне нравится{{/if}}</span></a></p>
			<p><u name="comment_vote_count_{{_id}}" data-count="{{vote_count}}">{{#if vote_count}}{{showCount vote_count}} это нравится{{/if}}</u></p>
			{{#can_modify author.id}}
				<p><a class="DeleteLink" data-id="{{_id}}" data-type="comment">Удалить</a></p>
			{{/can_modify}}
			</div>
		</div>

		<div name="comment_restore_{{_id}}" class="item_restore">
			<a class="RestoreLink" data-id="{{_id}}" data-type="comment">Восстановить</a>
		</div>

	</div>
{{! ]]> }}
</script>

