<!-- Base post template: user blog, post's items in feeds -->
<script id="postsTmpl" type="text/x-handlebars-template">
{{! <![CDATA[ }}
{{#if posts}}
    {{#posts}}
    <div name="post_{{_id}}" class="Post">
        <div class="PostBody">
        
        {{#if parent}} <!-- СОДЕРЖИТ родительский пост -->
            {{#if action_header}} <!-- если показывается в ленте (с заголовком действия) -->
                {{{action_header}}}
            {{^}} <!-- иначе показываем просто автора поста [Имя Фамилия] -->
                <div class="Author">
                    <a href="/club/users/{{author.id}}">
                        <img src="{{ buildAvatarUrl author.file_host author.avatar_id '160x160' }}" alt="" /><span>{{ author.name }}</span>
                    </a>
                </div>
            {{/if}}

            {{#is_blog this}}
                {{include "attachmentsTmpl" _photos=photos _videos=videos}}
            {{/is_blog}}

            {{#if text}}
                <p class="PostText">{{formatText text}}</p>
            {{/if}}

            {{include "relationsTmpl" _movies=movies _persons=persons}}

            <div class="LikePost">
                {{#unless action_header}} <!-- action_header в приоритете; если он есть, то показывать автора лайкнутого поста не нужно -->
                
                <div class="User">
                    <a href="/club/users/{{parent.author.id}}">
                        <img src="{{ buildAvatarUrl parent.author.file_host parent.author.avatar_id '160x160' }}" alt="" />{{ parent.author.name }}
                    </a>
                    <a class="Date" href="/club/posts/{{parent._id}}"><span>{{ formatDate parent.created }}</span></a>
                </div>
                
                {{^}} <!-- либо если лайкнули запись из блога/рецензия/мнение, то показывать даже при наличии action_header -->
                    
                {{#is_blog parent}} 
                    <div class="User">
                        <a href="/club/users/{{parent.author.id}}">
                            <img src="{{ buildAvatarUrl parent.author.file_host parent.author.avatar_id '160x160' }}" alt="" />{{ parent.author.name }}
                        </a>
                        <a class="Date" href="/club/posts/{{parent._id}}"><span>{{ formatDate parent.created }}</span></a>
                    </div>
                {{/is_blog}}
                    
                {{#is_opinion parent}} 
                    <div class="User">
                        <a href="/club/users/{{parent.author.id}}">
                            <img src="{{ buildAvatarUrl parent.author.file_host parent.author.avatar_id '160x160' }}" alt="" />{{ parent.author.name }}
                        </a>
                        <a class="Date" href="/club/posts/{{parent._id}}"><span>{{ formatDate parent.created }}</span></a>
                    </div>
                {{/is_opinion}}

                {{#is_review parent}} 
                    <div class="User">
                        <a href="/club/users/{{parent.author.id}}">
                            <img src="{{ buildAvatarUrl parent.author.file_host parent.author.avatar_id '160x160' }}" alt="" />{{ parent.author.name }}
                        </a>
                        <a class="Date" href="/club/posts/{{parent._id}}"><span>{{ formatDate parent.created }}</span></a>
                    </div>
                {{/is_review}}

                {{/unless}}

                {{#if parent.data}}
                    {{#is_article parent}}
                {{include "articleBodyTmpl" article=parent.data _photos=parent.photos}}
                    {{/is_article}}

                    {{#is_gallery parent}}
                {{include "articleBodyTmpl" article=parent.data _photos=parent.photos}}
                    {{/is_gallery}}

                    {{#is_trailer parent}}
                {{include "trailerBodyTmpl" trailer=parent.data}}
                    {{/is_trailer}}

                    {{#is_video parent}}
                {{include "trailerBodyTmpl" trailer=parent.data}}
                    {{/is_video}}
                    
                    {{#is_industry parent}}
                {{include "trailerBodyTmpl" article=parent.data}}
                    {{/is_industry}}

                    {{#is_filmproview parent}}
                {{include "filmproviewBodyTmpl" filmproview=parent.data}}
                    {{/is_filmproview}}

                {{^}}

                {{#is_blog parent.this}}

                    {{include "attachmentsTmpl" _photos=parent.photos _videos=parent.videos}}

                    {{#if parent.text}}
                    <p class="PostText">{{formatText parent.text}}</p>
                    {{/if}}

                {{/is_blog}}

                {{#is_opinion parent.this}}

                    {{#if parent.text}}
                    <p class="PostText">{{formatText parent.text}}</p>
                    {{/if}}
                
                {{/is_opinion}}

                {{#is_review parent.this}}

                    {{#if parent.rate}}
                    <p class="PostRate">Оценка: <span class="PostRatePercent PostRatePercent_{{parent.rate}}">{{parent.rate}} из 5</span></p>
                    {{/if}}

                    {{#if parent.title}}
                    <p class="PostTitle">{{parent.title}}</p>
                    {{/if}}

                    {{include "attachmentsTmpl" _photos=parent.photos}}

                    {{#if parent.text}}
                    <p class="PostText">{{formatBBText parent.text}}</p>
                    {{/if}}

                {{/is_review}}
                
                {{/if}}

                {{include "relationsTmpl" _movies=parent.movies _persons=parent.persons}}

            </div>
        {{^}} <!-- НЕ СОДЕРЖИТ родительский пост -->
            {{#if action_header}} <!-- если показывается в ленте -->
                {{{action_header}}}
            {{^}}
                <div class="Author">
                    <a href="/club/users/{{author.id}}">
                        <img src="{{ buildAvatarUrl author.file_host author.avatar_id '160x160' }}" alt="" /><span>{{ author.name }}</span>
                    </a>
                </div>
            {{/if}}

                {{#if data}}
                    {{#is_article this}}
                {{include "articleBodyTmpl" article=data _photos=photos}}
                    {{/is_article}}

                    {{#is_gallery this}}
                {{include "articleBodyTmpl" article=data _photos=photos}}
                    {{/is_gallery}}

                    {{#is_trailer this}}
                {{include "trailerBodyTmpl" trailer=data}}
                    {{/is_trailer}}
                    
                    {{#is_video this}}
                {{include "trailerBodyTmpl" trailer=data}}
                    {{/is_video}}
                    
                    {{#is_industry this}}
                {{include "trailerBodyTmpl" trailer=data}}
                    {{/is_industry}}

                    {{#is_filmproview this}}
                {{include "filmproviewBodyTmpl" filmproview=data}}
                    {{/is_filmproview}}

                {{/if}}

            {{#is_blog this}}
                {{include "attachmentsTmpl" _photos=photos _videos=videos}}

                {{#if text}}
                    <p class="PostText">{{formatText text}}</p>
                {{/if}}
            {{/is_blog}}

            {{#is_opinion this}}

                {{#if text}}
                <p class="PostText">{{formatText text}}</p>
                {{/if}}

            {{/is_opinion}}

            {{#is_review this}}

                {{#if rate}}
                <p class="PostRate">Оценка: <span class="PostRatePercent PostRatePercent_{{rate}}">{{rate}} из 5</span></p>
                {{/if}}

                {{#if title}}
                <p class="PostTitle">{{title}}</p>
                {{/if}}

                {{include "attachmentsTmpl" _photos=photos}}

                {{#if text}}
                <p class="PostText">{{formatBBText text}}</p>
                {{/if}}

            {{/is_review}}

            {{include "relationsTmpl" _movies=movies _persons=persons}}
            
        {{/if}}
        
        </div>

        {{#if deleted}}
            <div name="post_restore_{{_id}}" data-id="{{_id}}" class="item_restore" style="display:block">
                <a class="RestoreLink" data-id="{{_id}}" data-type="post">Восстановить</a>
            </div>
        {{^}}
            <div class="PostAfterBody">
                <div class="ComPostService">
                    <a class="Date" href="/club/posts/{{_id}}">{{ formatDate created }}</a>
                    <a name="post_like_btn_{{_id}}" class="LikeButton {{#if liked}}unlikeBtn{{^}}likeBtn{{/if}}" data-id="{{_id}}" data-type="post"><i></i>{{#if liked}}Больше не нравится{{^}}Мне нравится{{/if}}</a>
                    <a name="post_share_btn_{{_id}}" class="ShareLink" data-id="{{_id}}" data-type="post"><i></i>Поделиться</a>
                    {{#can_modify author.id}}
                    <a class="DeleteLink" data-id="{{_id}}" data-type="post"><i></i>Удалить</a>
                    {{/can_modify}}
                    &nbsp;
                </div>

                <div class="LikeAndShare">
                    <p><span name="post_vote_count_{{_id}}" data-count="{{vote_count}}">{{showCount vote_count}} понравилось, </span></p>
                    <p><span name="post_share_count_{{_id}}" data-count="{{share_count}}">{{showCount share_count}} поделился</span></p>
                </div>
            </div>

            <div class="CommentsBullet"></div>

<div class="CommentsWrapper">

            {{#greaterThan comments_count 5 }}
                <div class="ShowComments"><a data-id="{{_id}}">Показать все {{comments_count}}</a></div>
                {{^}}
            {{/greaterThan}}
        
            <div class="Comments">
                <div class="CommentsList">
                {{include "commentsTmplV2" post_id=_id comments=comments comments_count=comments_count }}
                </div>
            </div>
        
            <div class="CommentForm create_msg">
                <form name="createCommentFrm_{{_id}}" class="createCommentFrm" name="createCommentFrm" method="post">
                    <fieldset>
                        <legend>Форма добавления комментария</legend>
                        
                        <a class="CommentFormUser"><img src="{{ buildAvatarUrl_mine '160x160' }}" alt="" /></a>
                        
                        <div class="FormData1">
                            
                            <div class="Textarea1Wrapper">              
                                <input type="hidden" name="post_id" value="{{_id}}" />
                                <textarea data-bvalidator="required,maxlength[4096]" max-length="4096" id="comment_text_{{_id}}" name="text" class="Textarea1" placeholder="Написать комментарий..."></textarea>
                            </div>
                            <p class="charsRemaining">Символов осталось: 4096</p>
                        </div>
                    </fieldset>
                    
                    <div class="Button">
                        <p><a class="submitBtn createCommentBtn" data-id="{{_id}}"><span>Добавить</span></a></p>
                    </div>
                </form>
            </div>
        
            <div name="post_restore_{{_id}}" data-id="{{_id}}" class="item_restore">
                <a class="RestoreLink" data-id="{{_id}}" data-type="post">Восстановить</a>
            </div>

</div>

        {{/if}}

    </div>
    {{/posts}}
{{/if}}
{{! ]]> }}
</script>

