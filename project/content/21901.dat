$(document).ready(function(){
    $('.b-right-premiere .b-right-premiere__controlright').on('click', function(){
        current = $(this).parent().find('.b-right-premiere__item:visible');
        next = current.next();
        if (!next.length) {
            next = $(this).parent().find('.b-right-premiere__item').first();
        }
        current.hide();
        next.show();
    });

    $('.b-right-premiere .b-right-premiere__controlleft').on('click', function(){
        current = $(this).parent().find('.b-right-premiere__item:visible');
        prev = current.prev();
        if (!prev.length) {
            prev = $(this).parent().find('.b-right-premiere__item').last();
        }
        current.hide();
        prev.show();
    });
});
</script>


