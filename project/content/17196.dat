<script type="text/javascript">
function isGuest() { return true; }
function getEmailFrom() {return '';}
function getLoginFrom() {return '';}

function noAccess() { return false; }
function showErrors() { return false; }

/**
 *  Оценка фильма
 */
function vote(id_film, vote)
{
    if (isGuest()) {
        $.app.notice($.app.notices.authRequired);
        return false;
    }

    if (noAccess()) {
        alert('Для голосования необходимо заполнить дату рождения и пол в <a href="/level/78/edit_main/" class="all">профиле</a> пользователя.');
        return false;
    }

    $('#starbar'+id_film).addClass('user').css('width', vote * 22);

    ajax_star = $('#ajax_star'+id_film);
    ajax_star.css('display', 'inline');

    curr_user_vote = $('#curr_user_vote'+id_film);
    curr_user_vote.css('display', 'none');

    $.get(
        '/handler_vote.php',
        {id_film: id_film, vote: vote},
        function(data) {
            ajax_star.css('display', 'none');
            if(data == 'auth_error')
            {
                $.app.notice($.app.notices.authRequired);
            }
            if (data == 'Invalid ID') {
                // кривая идешка фильма
            } else if (data == 'Is guest') {
                // гость
            } else if (data == 'No access') {
                // не указан пол или дата рождения
            } else if (data == 'Impossible vote') {
                // фильм не вышел, голосовать нельзя
            } else if (data == 'Ok') {
                // оценка принята
                if (ajax_star.hasClass('empty')) {
                    ajax_star.removeClass('empty').addClass('vote');
                }
                $('#film_row'+id_film).addClass('act');
                curr_user_vote.css('display', 'block').text(vote);
            } else {
                if (showErrors()) {
                    alert(data);
                }
            }
        }
    ).fail(function(){
        ajax_star.css('display', 'none');
        alert("Произошла ошибка. Попробуйте позже.");
    });

    return false;
}


/**
 *  Добавить в мои фильмы
 */
function addMyMoviesFilm(id_film)
{
    if (isGuest()) {
        $.app.notice($.app.notices.authRequired, {'action':'использования сервиса &laquo;Мои фильмы&raquo;'});
        return false;
    }

    var now = new Date();

    $.get('/handler_mustsee.php', {add_film: id_film, rnd: now.getTime()}, function(data){
        if(data == 'auth_error')
        {
            $.app.notice($.app.notices.authRequired);
        }
        if (data.indexOf('<!-- impossibleAlert:') > 1) {
            reg = /<!-- impossibleAlert:(\d+) -->/;
            res = data.match(reg);
            to_folder = parseInt(res[1]);

            alert(
                'Вы добавляете фильм в папку, в которой не могут находится оцененные фильмы.<br />'+
                'Свойства папки можно изменить в разделе <a href="/level/78/movies/list/type/'+to_folder+'/" class="all">Мой КиноПоиск</a>.'
            );
        } else {
            $('#add_fav'+id_film).css('display', 'none');
            $('#my_fav'+id_film).css('display', 'block');
        }
    }).fail(function(){
        alert("Произошла ошибка. Попробуйте позже.");
    });
}
</script>

