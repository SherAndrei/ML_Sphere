	_popupHeights['google'] = '410';

	_popupHeights['odnoklassniki'] = '433';

	_popupHeights['mailru'] = '410';



	var left = (screen.width/2)-(_popupWidths[provider]/2);

	var top = (screen.height/2)-(_popupHeights[provider]/2);



        signinWin = window.open(oAuthURL, "AccountLinkingWindow", "width=" + _popupWidths[provider] + ",height=" + _popupHeights[provider] + ",resizable=1,top=" + top + ",left="+left);

        setTimeout(CheckLoginStatus, 2000);

        signinWin.focus();

        return false;



}



function CheckLoginStatus() {

    if (signinWin.closed) {

		$(".popup").hide();

		if (savelist != 0) { 
