        var shareTitle = encodeURIComponent($('meta[property="og:title"]').length > 0 ? $('meta[property="og:title"]:first').attr('content') : document.title);
        var shareText = encodeURIComponent($('meta[property="og:description"]').length > 0 ? $('meta[property="og:description"]:first').attr('content') : $('meta[name="description"]').attr('content'));
