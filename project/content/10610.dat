header .bottom>ul>li{display:block}
header .bottom form{width:calc(100% - 75px);margin:0;float:left}
.mobile_menu{display:block;width:45px;height:25px;position:relative;float:right;margin-right:10px;border-top:5px solid #fff;margin-top:10px;border-bottom:5px solid #fff;cursor:pointer}
header .bottom{position:relative}
.mobile_menu.active+ul{max-height:1100px}
.mobile_menu:after{content:'';display:block;width:100%;position:absolute;height:5px;background:#fff;top:0;bottom:0;margin:auto}
.sidebar{display:none}
header .bottom>ul>li>ul{max-height:none;position:relative;top:0;left:0}
header .bottom>ul>li>ul>li>a{text-align:right;transition:all .2s ease;padding:0 20px;display:block;color:#d5d6f2;text-decoration:underline;text-transform:uppercase;font-size:15px}
header .bottom>ul>li:hover>a,header .bottom>ul>li.current-menu-item>a{background:inherit}
header .bottom>ul>li>a:hover{background:#5a55a5}
header .bottom>ul>li:hover>ul{max-height:none;padding:0}
header .bottom>ul>li{height:auto}
header .bottom>ul>li>ul>li>a{line-height:inherit;background:#434372}
header .bottom>ul>li>ul>li{height:45px;line-height:45px;list-style:none;position:relative;margin-bottom:0}
header .bottom>ul>li>ul>li>a:hover{background:#5a55a5}
}
@media (max-width: 640px) {
html,body{min-width:0}
.comments .comment-form-author,.comments .comment-form-email{position:relative;margin-bottom:15px;left:auto;top:auto;bottom:auto;right:auto;max-width:100%}
.comments input[type=text],.comments input[type=submit]{width:100%}
div[id^=widget-3] .item{width:100%}
footer .wrap{height:auto}
footer .right p{text-align:center;padding-bottom:30px;width:auto;padding-right:0}
footer .left{width:auto;float:none}
footer .logo{margin-left:auto;margin-bottom:20px}
.section1 .item{width:auto;float:none!important}
.section1 .right_cont{width:calc(100% - 115px)}
.section2 .right,.section2 .left{float:none;width:auto}
.section3 .header_left{float:none;width:auto}
.kc__wrap{width:auto}
.section3 iframe{height:250px}
.nav_butt a.page:hover,.nav_butt .current,.nav_butt a.page{margin:0 3px}
.section3 .header_left .thumb{height:200px;width:auto}
}
.ddsg-wrapper li{display:block!important}
