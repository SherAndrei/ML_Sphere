<style type="text/css">
a.answer_link {font-weight: bold; /*text-transform: uppercase*/}
 div.answers_block {padding: 10px 0px 10px 20px}
 div.answers_item {padding: 4px 6px; border: solid 1px #e9e9e9}
 div.answers_link {padding-top: 10px}
 div.answers_title {font-weight: bold; text-transform: uppercase}
 div.answers_question {padding-left: 6px; padding-bottom: 4px; font-style: italic; color: #202020}
