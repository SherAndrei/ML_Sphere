    var insertTextareaTag = function(tag, attr) {

      var field = $(fieldId);
      var scrollTop = field.scrollTop();
      var selection = field.getSelection();

      if (selection.text) {

        attr = attr || {};

        var replaceText = '<' + tag, key = 0;

        for(key in attr) {
          replaceText += ' ' + key + '="' + attr[key] +  '"';
        }

        replaceText += '>' + selection.text + '</' + tag + '>';

        field.replaceSelection(replaceText);
      }

      field.scrollTop(scrollTop);
      return false;
    };

    var closeUploadForm = function(e) {
      e.find('.upload_box').animate({ "height": "0px" }, "fast", function() {
        $(this).remove();
      });
    };

    var openUploadForm = function() {

      var formHeight = 115;
      var toolbar = $(toolbarId);

      toolbar.append(
        '<div class="upload_box">'+
        '<iframe src="http://img.playground.ru/upload/form.php?field=text" height="'+formHeight+'" frameborder="0" scrolling="no"></iframe>'+
        '</div>'
      );

      //анимация
      toolbar.find('.upload_box').animate({height: formHeight+"px"}, "fast");
    };

    var toolbar = $(toolbarId);
    var field = $(fieldId);

    toolbar.find('.iButton').on('click', function() { insertTextareaTag('i'); });
    toolbar.find('.bButton').on('click', function() { insertTextareaTag('b'); });
    toolbar.find('.uButton').on('click', function() { insertTextareaTag('u'); });
    toolbar.find('.hButton').on('click', function() { insertTextareaTag('hide', {'title': ''}); });

    //нажатие на кнопку прикрепления картинки
    toolbar.find('.upload_btn').on('click', function(){

      if (!toolbar.find('.upload_box').length) {
        openUploadForm();
      }
      else {
        closeUploadForm(toolbar);
      }

      return false;
    });

    //колбэк-функция которая вызывается, при загрузке файла
    if (isImgUpload && typeof(window.attachFileCallback) == 'undefined') {

      window.attachFileCallback = function(response) {

        var data    = $.parseJSON(response);
        var error   = data['error']  || false;
        var file    = data['file']   || false;
        var width   = data['width']  || false;
        var height  = data['height'] || false;
        var field   = data['field']  || false;

        var jqText = $('#text_field_' + field);

        if (error) {
          alert(error);
        }
        else if (file) {

          var text = jqText.val();

          //перевод строки, для того, чтобы картинки не вставали в ряд
          if (text) {
            text += String.fromCharCode(13) + String.fromCharCode(10) + String.fromCharCode(13) + String.fromCharCode(10);
          }

          //всё ок, выводим
          if (width && height) {
            jqText.val(text + '<img src="' + file + '" width="' + width + '" height="' + height + '" />');
          } else {
            jqText.val(text + '<img src="' + file + '" />');
          }
        }

        closeUploadForm($('#text_toolbar_' + field));
      }
    }
  });
