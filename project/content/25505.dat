<script type="text/javascript">
	var AfterPaginationScroll = function(to_element, y_offset) {
		this.to_element = to_element;
		this.can_scroll = false;
		this.y_offset = y_offset;

		this.to_element = $(to_element);

		var referrer_page_number = this.get_page_number(this.get_referrer_full_path());
		var referrer_wo_page_number = this.get_referrer_full_path();
		if (referrer_page_number) {
			referrer_wo_page_number = this.get_referrer_full_path().replace(/^([^/]+[/][^/]+[/][^/]+)[/]\d+/, '$1');
		}

		// console.log(referrer_wo_page_number)

		if (referrer_wo_page_number && this.get_current_full_path().indexOf(referrer_wo_page_number)!=-1 && this.get_page_number(this.get_current_full_path()) != null) {
			// console.log('url')
			this.can_scroll = true;
		}

		referrer_wo_page_number = encodeURI(referrer_wo_page_number);
		if (referrer_wo_page_number && this.get_current_full_path().indexOf(referrer_wo_page_number)!=-1 && this.get_page_number(this.get_current_full_path()) != null) {
			// console.log('url')
			this.can_scroll = true;
		}

		if (this.can_scroll) {
			this.scrollTo(this.to_element);
		}
	};

	AfterPaginationScroll.prototype.remove_protocol_from_url = function(v){
		if (v.indexOf('http://')==0)
			v = v.replace('http://', '');

		if (v.indexOf('https://')==0)
			v = v.replace('https://', '');

		return v;
	};

	AfterPaginationScroll.prototype.get_current_full_path = function() {
		return this.remove_protocol_from_url(window.location.href.replace(window.location.search, ''));
	};

	AfterPaginationScroll.prototype.get_page_number = function(url) {
		var page_no = null;

		if (url==null)
			return page_no;

		if (/^[^/]+[/][^/]+[/][^/]+[/]\d+/.test(url)) {
			page_no = url.replace(/^[^/]+[/][^/]+[/][^/]+[/](\d+).*/, '$1');
		}

		try {
			page_no = parseInt(page_no);
		} catch(e) {}

		if (isNaN(page_no))
			page_no = null;

		return page_no;
	};

	AfterPaginationScroll.prototype.get_referrer_full_path = function() {
		var referrer_full_path = null;
		if ('referrer' in document) {
			referrer_full_path = document.referrer.indexOf('?')==-1 ? document.referrer : document.referrer.substring(0, document.referrer.indexOf('?')-1);
			referrer_full_path = this.remove_protocol_from_url(referrer_full_path);
		}

		return referrer_full_path;
	};

	AfterPaginationScroll.prototype.scrollTo = function(el) {
		var offset = 0;
		if ($('#sticked_menu').length) {
			offset = - $('#sticked_menu').outerHeight();
		}
		try{ offset = offset + this.y_offset } catch(e){};
		try{ console.log('offset: ' + offset)} catch(e){};
		$.scrollTo(
			$(this.to_element),
			{axis:'y', offset: offset}
		);
	};

	// init
	$(document).ready(function() {
		
			new AfterPaginationScroll('.content_head', -10)
		
	})

</script>

				
				
				
				
						
				
