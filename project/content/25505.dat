			'shipping': '0',					// Shipping.
			'tax': '0',							// Tax.
			'currency': 'RUB'
		});

		ga('ecommerce:addItem', {
			'id': transaction_id,		// Transaction ID. Required.
			'name': good_name,			// Product name. Required.
			'sku': goodid,				// SKU/code.
			'category': 'Click',		// Category or variation.
			'price': price,				// Unit price.
			'quantity': '1',			// Quantity.
			'currency': 'RUB'
		});

		ga('ecommerce:send');
	}
	function ya_sent_click_event (transaction_id, evt_type, link_type, good_name, store_name, price, goodid, yatarget) {
