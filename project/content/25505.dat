	<script type="text/javascript">
		window.chosen_time='';
	
		function show_chosen_baloon (sender) 
		{
			if ($(sender).parent().find('.chosen_baloon').length > 0) 
				return;
			
			var top_off = 0; //offset from top
			var left_off = 0; //offset from left
			
			var newelt = $('<img class="chosen_baloon" src="/static/images/editors_choice.png" style="position: fixed; width: 151px; height: 105px; z-index: 11;" />');
			
			newelt.insertAfter($(sender));
			
			if ($('body').scrollTop() > $(sender).offset().top) {
				top_off = $('body').scrollTop() - $(sender).offset().top;
			} else {
				top_off = $(sender).offset().top - $('body').scrollTop();				
			}
			if ($('body').scrollLeft() > $(sender).offset().left) {
				left_off = $('body').scrollLeft() - $(sender).offset().left;
			} else {
				left_off = $(sender).offset().left - $('body').scrollLeft();				
			}
	
			$('.chosen_baloon').css('top', top_off -81 + 'px');
			$('.chosen_baloon').css('left', left_off -81 + 'px');
			
		}
		
		function hide_chosen_baloon ()
		{
			window.chosen_time = setTimeout(function()
			{
	
				$('.chosen_baloon').remove();
			}, 500);			 
		}
		
		function continue_chosen_baloon () {
		  	clearTimeout(window.chosen_time);
		}
		
		$(document).scroll(function(){
	  		$('.chosen_baloon').remove();
		});
	</script>


	
	
		
		
		
			
				
	
		
	
	
	
		
		
	
	
	
		
	
