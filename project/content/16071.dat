<script>
  $(function() {
              var bpurl = "http://www.stranamam.ru/post/5304210/",
          purl,
          ptitle = "Как избавиться от муравьев в огороде?",
          pimg,
          text = 'Сейчас начинается сезон огорода и наверно каждый встречается с такой проблемой как муравьи в огороде.',

          $bbimg,
          ctype;

      $('.b-icon').click(function(){
        ctype = $(this).attr('data-type');
        $bbimg = $(this).parent().parent().parent().find('.bbimg');
        pimg = $bbimg.attr('src');
        purl = bpurl + '?image=' + ($('.usertext img').index($bbimg) + 1);
        console.log(purl);
        //$("meta[property='og:image']").attr('content', pimg);

        switch (ctype) {
          case 'vk':
            Share.vk(purl, ptitle, text, pimg);
            break;
          case 'ok':
            Share.ok(purl);
            break;
          case 'facebook':
            Share.facebook(purl, ptitle, text, pimg);
            break;
          case 'twitter':
            Share.twitter(purl, ptitle);
            break;
          case 'googleplus':
            Share.googleplus(purl);
            break;
          case 'pinit':
            Share.pinit(purl, pimg, text);
            break;

        }
      });

      Share = {
        vk: function(purl, ptitle, text, pimg) {
          url  = 'http://vk.com/share.php?';
          url += 'url='          + encodeURIComponent(purl);
          url += '&title='       + encodeURIComponent(ptitle);
          url += '&description=' + encodeURIComponent(text);
          url += '&image='       + pimg;
          url += '&noparse=true';
          Share.popup(url);
        },
        ok: function(purl) {
          url  = 'https://connect.ok.ru/dk?st.cmd=WidgetSharePreview';
          url += '&st.shareUrl=' + encodeURIComponent(purl);
          Share.popup(url);
        },
        facebook: function(purl, ptitle, text, pimg) {
          url  = 'http://www.facebook.com/sharer.php?s=100';
          url += '&p[title]='     + encodeURIComponent(ptitle);
          url += '&p[summary]='   + encodeURIComponent(text);
          url += '&p[url]='       + encodeURIComponent(purl);
          url += '&p[images][0]=' + encodeURIComponent(pimg);
          Share.popup(url);
        },
        twitter: function(purl, ptitle) {
          url  = 'http://twitter.com/share?';
          url += 'text='      + encodeURIComponent(ptitle);
          url += '&url='      + encodeURIComponent(purl);
          url += '&counturl=' + encodeURIComponent(purl);
          Share.popup(url);
        },
        googleplus: function(purl) {
          url  = 'https://plus.google.com/share?';
          url += 'url='          + encodeURIComponent(purl);
          Share.popup(url)
        },
        pinit: function(purl, pimg, text) {
          url  = 'http://pinterest.com/pin/create/button/?';
          url += 'url='          + encodeURIComponent(purl);
          url += '&media='       + encodeURIComponent(pimg);
          url += '&description=' + encodeURIComponent(text);
          Share.popup(url)
        },

        popup: function(url) {
          window.open(url,'','toolbar=0,status=0,width=626,height=436');
        }
      };
      });
</script>








