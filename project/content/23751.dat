<script type="text/javascript">
function context_click(id) {
  $.get('/context_click.php?id='+id);
}
$(document).ready(function() {
$('a.lc').click(function(){
  $.get('/clickcount.php', { name: $(this).text() });
});
$(".ajaxSubmit").live('click', function(){
		var formA = $(this).parent();
		$.ajax({
		  url: "/ajax.php",
		  dataType: "json",
		  type: "POST",
		  data: "action=popup/block&id="+ formA.attr('id') +"_&" +formA.serialize(),
		  timeout: 5000,
		  success: function(e) {
			if(e.status == 1) {
				$("#popblock .popcont").html(e.message);
			} else {
				$("#popblock .popcont .error_block").remove();
				$("#popblock .popcont").prepend(e.message);
			}
		  }
		});
	});
	$(".ajaxBlock").click(function () {
		var aBid = $(this).attr('id');
		$.ajax({
			type: "POST",
			url: "/ajax.php",
			cache: true,
			data: "action=popup/block&id=" + aBid,
			dataType: "json",
			timeout: 5000,
			success: function (data) {
				if(data.status == 1) {
					$("#popblock .popcont").html(data.message);
					$("#popblock").slideDown("slow");
				}
			}
		});
		$(".vbmenu_popup").hide();
		return false;
	});
$(".openblock").click(function () {
  $("#popblock .popcont").html($("#pophide").html());
  $("#popblock").slideDown("slow");
  return false;
});
$(".openblock3").click(function () {
  $("#popblock .popcont").html($("#pophide3").html());
  $("#popblock").slideDown("slow");
  return false;
});
$("#popblock .close").click(function () {
  $("#popblock").slideUp("slow");
});
$("#popblock .op").click(function () {
  $("#popblock .op").removeClass("sel");
  $(this).addClass("sel");
});
$("#topMenu li").mouseenter(function(){
	$(this).find("li").stop().slideDown("fast");
}).mouseleave(function(){
	$(this).find("li").slideUp("fast");
});

});
function openInfo() {
  $("#popblock .openinfo").hide();
  $("#popblock .info").slideDown("slow");
}
</script>

