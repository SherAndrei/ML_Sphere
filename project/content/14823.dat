<script id="template-friendsList" type="text/x-handlebars-template">

    <div class="b-friends__choice-info">
        <span class="js-friends__choice-count">{{{ count }}}</span>
    </div>
    <div class="b-friends__choice-content" id="js-friends__choice-content">
        {{#if count}}
        <div class="b-friends__choice-scrollable" id="js-friends__choice-scrollable">
            <ul class="b-friends__choice-list">
                {{#each users}}
                <li class="b-friends__choice-item">
                    <div class="b-friends__choice-avatar">
                        {{#if box }}<a href="//games.mail.ru/profile{{ box }}" target="_blank" class="js-userinfo__item" data-email="{{ email }}">{{/if }}
                            <img src="{{ avatar }}" alt="{{ show_name }}">
                        {{#if box }}</a>{{/if }}
                    </div>
                    <div class="b-friends__choice-name">
                        {{#if box }}<a href="//games.mail.ru/profile{{ box }}" target="_blank" class="js-userinfo__item" data-email="{{ email }}">{{/if }}
                            {{ show_name }}
                        {{#if box }}</a>{{/if }}
                    </div>
                    <div class="b-friends__choice-status">
                        {{#if has_profile }}
                            Профиль найден
                        {{/if}}
                    </div>
                    <div class="b-friends__choice-action">
                        {{#if has_profile }}
                            {{#unless is_friend }}
                            <button type="button"
                                    class="b-friends__btn-request b-friends__choice__btn-request js-friends__btn-request
                                        {{#if request_sent }}b-friends__btn-request_wait{{else}}b-friends__btn-request_add{{/if }}"
                                    data-url="//games.mail.ru/profile/ajax/friend_request/"
                                    data-uid="{{ email }}"
                                    data-action="{{#unless request_sent }}add{{/unless }}"
                                    data-name="{{ show_name }}"
                                    {{#if request_sent }}disabled{{/if }}>
                                {{#if request_sent }}Заявка отправлена{{else}}Подружиться{{/if }}
                            </button>
                            {{/unless }}
                            {{#if is_friend }}
                            <button type="button"
                                    class="b-friends__btn-request b-friends__choice__btn-request js-friends__btn-request b-friends__btn-request_done"
                                    data-url="//games.mail.ru/profile/ajax/friend_request/"
                                    data-uid="{{ email }}"
                                    data-action="{{#unless request_sent }}delete{{/unless }}"
                                    data-name="{{ show_name }}"
                                    {{#if request_sent }}disabled{{/if }}>
                                Вы друзья
                            </button>
                            {{/if }}
                        {{ else }}
                        <button type="button" class="b-friends__btn-request_invite b-friends__choice__btn-request js-invite-user" data-email="{{ user.email }}">
                            Пригласить
                        </button>
                        {{/if }}
                    </div>
                </li>
                {{/each}}
            </ul>
        </div>
        <div class="b-scrollable__track" id="js-friends__choice-scrollable__track">
            <div class="b-scrollable__bar" id="js-friends__choice-scrollable__bar"></div>
        </div>
        {{else}}
            <div class="b-friends__choice-empty">
                Возможные друзья не найдены, некому отправить запросы на добавление.
            </div>
        {{/if}}
    </div>

</script>

    <!-- /Handlebars templates -->

    <!-- Notifier popup -->
    

    <!-- /Notifier popup -->

    <!-- Subscribe popup -->
    <div class="b-subscribe-popup b-simple-popup" id="js-b-subscribe-popup">
    <div class="b-simple-popup__close-btn js-b-simple-popup__close-btn"></div>
    <IDXSKIP><div class="b-simple-popup__title">
        Рассылка новостей
    </div></IDXSKIP>
    <div class="b-simple-popup__main">
        <div class="b-simple-popup__content b-subscribe-popup__content">
            <div><IDXSKIP>Для подписки на новости укажите вашу почту</IDXSKIP></div>
            <input class="b-subscribe-popup__field js-b-subscribe__field" id="js-b-subscribe-popup__field" type="text" placeholder="E-mail" />
            <IDXSKIP><div class="b-subscribe-popup__submit js-b-subscribe__submit" id="js-b-subscribe-popup__submit">
                <span class="js-button__text">Подписаться</span>
            </div></IDXSKIP>
        </div>
    </div>
</div>

    <!-- /Subscribe popup -->

    
    

</body>
</html>



