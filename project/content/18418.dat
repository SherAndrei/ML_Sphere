                        $recommendCategories.addClass("adaptive");
                        $recommendCategories.find("li a").addClass("categories_menu_item_final categories_menu_item");
                        $recommendCategories.find("li").find("a:last").each(function (k, v) {
                            var self = $(this);
                            self.html('<span class="categories_menu_item__title">' + self.text() + '</span>');
                        });
                    }
                }
            }

            // mark vertical slider
            if ($(".informer-block__vertical").length) {
                $(".informer-block__vertical").addClass("informer-vertical-marker");
            }

            $(window).on('resize orientationchange', move_catalog_sidebar);
            move_catalog_sidebar();
