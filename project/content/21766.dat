<script type="text/javascript">(function () {
    $('blockquote.slammable').each( function() {
        var childs = this.childNodes;

        for (var i = 0; i < childs.length; i++) {
            var child = childs[i];
            if ( child.tagName == undefined ) continue;

            if ( child.tagName.match( /H\d/ ) && child.className != 'fragment_alias' ) {
                var header_html = child.innerHTML;
                child.parentNode.removeChild( child );

                var fragment_alias_el = $( '.fragment_alias', this ).get(0);
                var fragment_alias;

                // TODO
                if ( fragment_alias_el ) {
                    fragment_alias = fragment_alias_el.innerHTML.replace(/<[^>]+>/g, '');
                    fragment_alias_el.parentNode.removeChild( fragment_alias_el );
                }

                var block_html = this.innerHTML;

                var dl = document.createElement('dl');
                $( dl ).addClass('extbl').html(
                    ( fragment_alias
                        ? '<dt class="ext-' + fragment_alias + '"><a class="ico" name="ext_' + fragment_alias + '" href="#ext_' + fragment_alias + '"><span>' + header_html + '</span></a></dt>\n'
                        : '<dt><a class="ico" href="#"><span>' + header_html + '</span></a></dt>\n'
                    )
                    + '<dd>' + block_html + '</dd>' );

                this.parentNode.insertBefore( dl, this );
                this.parentNode.removeChild( this );

                //dl.remove( '.fragment_alias' );

                break;
            }
        }
    } )
})()

$('dl.extbl dt a').click( function() {
    $( this ).closest('dl').toggleClass('open-extbl');
//    $( this ).closest('dl').find("iframe").attr( 'src', function ( i, val ) { return val; });   // reload
    return false;
} )

var fragment_ext = document.location.hash.match( /^#ext_([^#]+)/ );
if ( fragment_ext && fragment_ext[1] ) {
    $('dl.extbl dt.ext-' + fragment_ext[1]).closest('dl.extbl').addClass( 'open-extbl' );
}
</script>





