<script type="text/javascript">
    var allow_user_to_vote = 0;
    var entity_id = 247;
    var site_category = 'article';


    var all_right_answer_id = 0;

    var poll_wrapper = $('#feedback-usefullness-poll-wrapper');

    if (allow_user_to_vote)
        $('.first-answer', poll_wrapper).show();

    function feedback_poll_vote( params ) {
        params += '&site_category=' + site_category + '&entity_id=' + entity_id;

        $('div.why-so-bad', poll_wrapper).hide();
        $('div.first-answer', poll_wrapper).hide();

        $.ajax({
            url: '/ext/feedback_poll_vote/?' + params,
            dataType: 'text',
            success: function( data ) {
                if (data == '1')
                    $('div.thank-you', poll_wrapper).show();
                else
                    $('div.are-you-kidding', poll_wrapper).show();
            }
        });
    }

    $('div.first-answer input.yes-button', poll_wrapper).click( function() {
        feedback_poll_vote( 'answer_id=' + all_right_answer_id );
    } )

    $('div.first-answer input.no-button', poll_wrapper).click( function() {
        $('div.first-answer', poll_wrapper).hide();
        $('div.why-so-bad', poll_wrapper).show();
    } )

    $('.why-so-bad input.submit', poll_wrapper).click( function() {
        feedback_poll_vote( $( this.form ).serialize() );
        return false;
    } );


</script>


    

    

    
