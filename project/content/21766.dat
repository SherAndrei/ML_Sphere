        var search_field_default_text = 'Поиск в помощи';

        function search_form_submit_handler() {
            if ( this.q.value == search_field_default_text ) {
                //this.q.value = ''
                this.q.focus();
                return false;
            }
        }

        $('input.user-search').each( function() {
            $( this ).closest( 'form' ).submit( search_form_submit_handler );

            if (this.value == '') {
                this.value = search_field_default_text;
                $( this ).addClass('unfocused-user-search');
            }
        } )

        function search_field_on_quasi_focus_handler() {
            if (this.value == search_field_default_text) {
                $( this ).removeClass('unfocused-user-search');
                this.value = '';
            }
        }

        $('input.user-search').click( search_field_on_quasi_focus_handler );
        $('input.user-search').focus( search_field_on_quasi_focus_handler );

        $('input.user-search').blur( function() {
            if (this.value == '') {
                $( this ).addClass('unfocused-user-search');
                this.value = search_field_default_text;
            }
        } )
