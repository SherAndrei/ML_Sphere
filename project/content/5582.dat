    <script type="text/javascript">
        $(function () {
            CabinetNav.init();
            setTimeout(hideSocial, 2000);
            $(document).on('click', function (e) { CabinetNav.click(e.target); });
            $('#fill-expense__phone').mask('(999) 999-9999');
        });
        function hideSocial() {
            if (typeof (ODKL) == 'undefined' && document.getElementById("odnTd"))
                document.getElementById("odnTd").style.display = "none";
            if (typeof (VK) == 'undefined' && document.getElementById("vkTd"))
                document.getElementById("vkTd").style.display = "none";
        }
        CabinetNav = {
            sNav: null,
            sLink: null,

            init: function () {
                CabinetNav.sNav = 'cabinet-sub-nav',
                CabinetNav.sLink = 'js-show-sub-nav';
            },
            click: function (elem) {
                if ($('#' + CabinetNav.sNav).is(':visible') && $(elem).closest('#' + CabinetNav.sNav).length == 0) {
                    $('#' + CabinetNav.sNav).hide()
                } else {
                    if (elem.id == CabinetNav.sLink) {
                        if ($('#' + CabinetNav.sNav).is(':hidden'))
                            $('#' + CabinetNav.sNav).show()
                    }
                }
            }
        }
		</script>

    
    
        
    
