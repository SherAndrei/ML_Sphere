<script type="text/javascript">
    var currentPage = 1;
    var totalPages = 2;
    var lastPostId = 16062712;

    $(document).ready(function(){
        $('.post-select').each(function() {
            $(this).prop('checked', false);
        });
        $('.bbcode-container').each(function(){
            $(this).html($(this).html().trim())
        });
    });

    $(document).mousedown(function(e){
        var container = $('#reply-tooltip');

        if (!container.is(e.target) && container.has(e.target).length === 0) {
            container.hide();
        }
    });

    function loadPollResults(container, showModal, toggleBlock) {
        if (container === undefined)
            container = $('#poll-results-container');
        else
            container = $(container);

        if (showModal === undefined)
            showModal = true;

        if (toggleBlock === undefined)
            toggleBlock = false;

        console.log('toggleBlock: ' + toggleBlock);

        requestHandler.ajaxRequest('/api/forum/getPollResults', {
            tid: '1026490'
        }, function(response) {

            container.empty();

            $(response).each(function(){
                total = this.total === undefined ? 0 : this.total;
                percentage = this.percentage === undefined ? 0 : this.percentage;
                var str = '<li class="poll-result">';

                str += '<div class="voted-icon-cell"></div>';

                str += '<h3 class="option-text">';
                str += this.name;
                str += '</h3>';

                str += '<div class="bar-cell">';
                str += '<span class="bar-container">';
                str += '<span class="bar" style="width: ' + percentage + '%"></span>';
                str += '</span>';
                str += '</div>';
                str += '<div class="count">';
                str += total + ' голосов';
                str += '</div>';
                str += '<div class="percentage">';
                str += percentage + '%';
                str += '</div>';
                str += '</li>';

                container.append(str);
            });

            if (toggleBlock != '') {
                if (toggleBlock == 'revote') {
                    toggleRevoteBlock();
                }
                if (toggleBlock == 'results') {
                    $('#poll-variants-block').hide();
                    $('#poll-vote-btn').hide();
                    $('#poll-results-btn').hide();
                    $('#poll-revote-btn-ajax').show();
                    $('#poll-revote-btn').show();
                    $('#poll-results').show();
                }
            }

            if (showModal)
                $('#poll-resultsModal').modal('show');
        });
    }

    function initPostLinks() {
        $(document).on('click', '.item-post-link' , function(e) {
            //console.log('[post] Item post link click!');
            //$('.share-comment').remove();

            //var parent = $(this).parent();

            e.preventDefault();

            //parent.append('<div style="display: none" class="share-comment"><input readonly style="cursor:text" value="http://dota2.ru/forum/posts/' + $(this).data('id') + '/"></div>');

            //$('.share-comment').show();
            //$('.share-comment > input').focus().select();
        });

        var clipboard = new Clipboard('.item-post-link', {
            text: function(trigger) {
                link = 'http://dota2.ru/forum/posts/' + $(trigger).data('id') + '/';
                return link;
            }
        });

        clipboard.on('success', function(e) {
            console.log('[clipboard] ' + e.action + ' (' + e.text + ')');

            $('.share-comment > input').hide();

            e.clearSelection();
            //$('.share-comment > input').focus().select();
            Utils.notify('Ссылка скопирована');
        });

        clipboard.on('error', function(e) {
            console.warn('[clipboard] can\'t copy text');

            $('.share-comment > input').focus().select();
            Utils.notify('Что бы скопировать ссылку<br>Нажмите CTRL+C');
        });
    }

    initPostLinks();
</script>
