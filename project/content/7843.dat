<script type="text/javascript">
	var $ = jQuery.noConflict();
	$(document).ready(function(){
		var items = $('.calcOne');
		for(var i=0;i<items.length;i++){
			$(items[i]).submit(function(){
				var result = '';
				var weigth = $(this).find('input[name="calc_weigth"]').val();
				var activity = $(this).find('select[name="calc_activity"]').val();
				if(weigth.length>0 && activity.length>0){
					result = weigth * activity;
					result = Math.ceil(result);
					result = result + ' ккал';
				}else{
					result = 'Пожалуйста, заполните все поля.';
				}

				$(this).find('.result').html(result);
				return false;
			});
		}	

		var items = $('.calcOne .reset');
		for(var i=0;i<items.length;i++){
			$(items[i]).click(function(){
				$(this).closest('form').find("input[type=text]").val("");
				$(this).closest('form').find("select").val($(this).closest('form').find("select option:first").val());
				$(this).closest('form').find('.result').html('');
				return false;
			});
		}




		var items = $('.calcTwo');
		for(var i=0;i<items.length;i++){
			$(items[i]).submit(function(){
				var result_norm = '';
				var result_lack = '';
				var sex = $(this).find('input[name="calc_sex"]:checked').val();
				var weigth = $(this).find('input[name="calc_weigth"]').val();
				var heigth_m = $(this).find('input[name="calc_heigth"]').val();
				var heigth_sm = $(this).find('input[name="calc_heigth_sm"]').val();
				if(heigth_m.length>0 && heigth_sm.length>0){
					var height_m2 = heigth_m * 100;
					var height = parseFloat(height_m2) + parseFloat(heigth_sm);
				}
				var age = $(this).find('input[name="calc_age"]').val();
				var lack = $(this).find('select[name="calc_lack"]').val();
				var activity = $(this).find('select[name="calc_activity"]').val();

				if(sex && weigth.length>0 && activity.length>0 && height>0 && age.length>0 && lack.length>0){

					if(sex=='female'){
						result_norm = 447.593 + (9.247 * weigth) + (3.098 * height) - (4.330 * age);
						result_norm = result_norm * activity;
						result_norm = Math.ceil(result_norm);
					}else if(sex=='male'){
						result_norm = 88.362 + (13.397 * weigth) + (4.799 * height) - (5.677 * age);
						result_norm = result_norm * activity;
						result_norm = Math.ceil(result_norm);
					}
					if(lack==500){
						result_lack = result_norm - lack;
						result_lack = Math.ceil(result_lack);
					}else{
						var koef = (100 - lack) / 100;
						result_lack = result_norm - (result_norm * koef);
						result_lack = result_norm - result_lack;
						result_lack = Math.ceil(result_lack);
					}
				}else{
					$(this).find('.result').html('Пожалуйста, заполните все поля.');
					$(this).find('.result_norm').html('Пожалуйста, заполните все поля.');
					return false;
				}

				$(this).find('.result').html(result_lack);
				$(this).find('.result_norm').html(result_norm);
				return false;
			});
		}	

		var items = $('.calcTwo .reset');
		for(var i=0;i<items.length;i++){
			$(items[i]).click(function(){
				$(this).closest('form').find("input[type=text]").val("");
				$(this).closest('form').find("input[type=radio]").attr('checked', false);

				var subitems = $(this).closest('form').find('select');
				for(var k=0;k<subitems.length;k++){
					$(subitems[k]).val($(subitems[k]).find("option:first").val());
				}

				$(this).closest('form').find("#sex_female").prop('checked', true);

				$(this).closest('form').find('.result_norm').html('');
				$(this).closest('form').find('.result').html('');
				return false;
			});
		}

	});
