        var isNotFirst = /\/\d+\/\d+\/(\?.*)?(#.*)?$/
        if (isNotFirst.test(window.location.href)) {
          opts.label = 'loc_not_first_page';
        } else {
          opts.label = 'loc_first_page';
        }
      }
    });

    mABC.addTarget('#add-comment-button, footer.comments__footer .comments__add__submit', {
      trigger: 'click',
      category: 'content_creation',
      action: 'add_comment',
      label: '',
      oneShot: true,
      mapper: function (opts) {
        if (window.location.href.indexOf('/comments/') > -1) {
          opts.label = 'loc_comments';
        } else {
          opts.label = 'loc_article';
        }
      }
    });
  });

</script><script>
