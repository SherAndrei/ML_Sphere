		function setCity( city )
		{
            jQuery.ajax({
                url:"/region.php?city="+city,

                success: function( d )
                {
                    if( parseInt( d ) > 0 )
                    {
                        $("[name=selectregion]").attr( "disabled", "disabled" );
                        $("[name=selectregion] option").removeAttr( "selected" );
                        $("[name=city_group] option").removeAttr( "selected" );

                        var city = $("[name=selectregion] option[value=" + d +"]");

                        $("[name=city_group] option[value=" + city.parent().attr( "data-group" ) + "]").parent().val( city.parent().attr( "data-group" ) );

                        city.attr( "selected", "selected" ).parent().removeAttr( "disabled" ).change();

                        submitCity();
                    }
                }
            });
