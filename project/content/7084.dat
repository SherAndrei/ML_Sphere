							$('#calc.nauch input').change(function(){
								var InputClass = $(this).attr("class");
								$("select#"+InputClass).val($(this).val());
								RenderPriceDetails_nauch();
							});
							$('#calc.nauch select').change(function(){
								var SelectClass = $(this).attr("id");
								$("input."+SelectClass).val($(this).val());
								RenderPriceDetails_nauch();
							});
							function RenderPriceDetails_nauch() {
								var preffix = "";
								if($(".calc-form:visible").hasClass("mob-block")) preffix = "mob-";
								var DataPages = parseInt($("#calc.nauch .calc-form:visible .DataPages").val());
								var DataCerts = parseInt($("#calc.nauch .calc-form:visible .DataCerts").val());
								var DataCertsEl = parseInt($("#calc.nauch .calc-form:visible ."+preffix+"DataCertsEl:checked").val());
								if(DataCerts > 0){
									DataCertsEl = 0;
								}
								var DataDigest = parseInt($("#calc.nauch .calc-form:visible .DataDigest").val());
								var DataPrint = parseInt($("#calc.nauch .calc-form:visible .DataPrint").val());
								var regprice = 0;
								var RegionVal = $("#calc.nauch .calc-form:visible input."+preffix+"DataRegion:checked").val();
								if(DataCerts > 0 || DataDigest > 0 || DataPrint > 0){
									if(DataDigest > 0){
										if(RegionVal == "SNG") {regprice = 300;};
										if(RegionVal == "Abroad") {regprice = 500;};
									} else{
										if(RegionVal == "SNG") {regprice = 150;};
										if(RegionVal == "Abroad") {regprice = 300;};
									}
								}
								total_order_price = 129 * DataPages + 120 * DataCerts + 350 * DataDigest + 170 * DataPrint + regprice + 50 * DataCertsEl;
								ResultText = total_order_price.toFixed(2) + " руб.";
								$("#calc.nauch .itogo span").text(ResultText);
							};
						</script>
