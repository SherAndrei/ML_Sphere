      <td style="padding: 0.00pt 2.16pt;background-color:#ffffff;color:#000000;border:0.50pt solid #000000;border-left-width:0.00pt;border-top-width:0.00pt">
       <p dir="ltr" style="text-align:right"><span style="font-size:12pt;font-family:'Times New Roman';color:#000000" xml:lang="ru-RU" lang="ru-RU">0,705</span></p>
