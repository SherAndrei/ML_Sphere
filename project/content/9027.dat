                var request_method = $(this).attr("method");
                var form_data = $(this).serialize();
                $("#repliesList").append("<div id=\"ajax-loader\" class=\"col-xs-12 col-md-12 col-lg-12 col-pad-bottom-10\" style=\"text-align: center\"><img src=\"https://cdn2.cdnstatic.ru/img/ajax-loader.gif\" /></div>");
                $.ajax({
                    url : post_url,
                    type: request_method,
                    data : form_data
                }).done(function(response){
                    console.log(response);
                    var checkExist = setInterval(function() {
                        if ($("#reply-"+response.replyid).length) {
                           $('html, body').animate({
                            scrollTop: $("#reply-"+response.replyid).offset().top - 100
                            }, 1000);
                           $("#replyForm").show();
                           clearInterval(checkExist);
                        }
                     }, 100);
                    document.getElementById("ajax-loader").remove();
                    $("#repliesList").append("<div class=\"col-xs-12 col-md-12 col-lg-12\"><div class=\"alert alert-success\" role=\"alert\">" + response.message + "</div></div>");
                    
                    $.ajax({
                    url: "/question/appendOtherQuestion/24383/76705",
                    type: 'GET',
                    data: '',
                    success: function (result) {
                    $("#repliesList").append("<div class=\"col-xs-12 col-md-12 col-lg-12\">" + result + "</div>");
                    },
                    error: function (result) {
                    var errors = $.parseJSON(result.responseText);
                    console.log(errors);
                    $.each(errors, function (index, value) {
                    });
                    }
                    });
                    
                    
                    if (typeof response.submission != 'undefined'){
                        if(response.submission == 2){
                            $('#submissionAjaxModal').modal('show');
                            $("#submission").val(response.submission);
                            $("#submissionid").val(response.submissionid);
                            console.log($("#submission").val());
                            console.log($("#submissionid").val());
                        }
                    }
                }).fail(function(response){ 
                    console.log(response);
                    document.getElementById("ajax-loader").remove();
                    $("#replyForm").show();
                    $("#replyError").html("<div class=\"alert alert-danger\" role=\"alert\">" + response.responseJSON.message + "</div>");
                });
            });

</script>
            <script>

                $(function () {
                    var box = $('#stickydiv'); // float-fixed block

                    var top = box.offset().top - parseFloat(box.css('marginTop').replace(/auto/, 0));
                    $(window).scroll(function () {
                        var windowpos = $(window).scrollTop() - 100;
                        if (windowpos < top) {
                            box.css('position', 'static');
                        } else {
                            box.css('position', 'fixed');
                            box.css('top', 0);
                        }
                    });
                });
            </script>
            <script>
                $("[data-toggle=popover]").popover({
                    html: true,
                    content: function () {
                        return $('#popover-content').html();
                    }
                });
            </script>
        </footer>
