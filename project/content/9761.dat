<script>
jQuery(document).ready(function($) {
	if ($('.commentlist').length) {
		var stringToColor = function stringToColor(str) {
		    var hash = 0;
		    var color = '#';
		    var i;
		    var value;
		    var strLength;

		    if(!str) {
		        return color + '333333';
		    }
		    strLength = str.length;

		    for (i = 0; i < strLength; i++) {
		        hash = str.charCodeAt(i) + ((hash << 5) - hash);
		    }
		    for (i = 0; i < 3; i++) {
		        value = (hash >> (i * 8)) & 0xFF;
		        color += ('00' + value.toString(16)).substr(-2);
		    }
		    return color;
		};
        $('.commentlist').find('.fn').each(function(index, el) {
         	var name = $(el).text();
         	var letter = name.substr(0, 1);
         	var backgroundColor = stringToColor(name);
         	var $elementAvatar=$('<div class="avatar"></div>')
            $elementAvatar.text(letter);
            $elementAvatar.css({
            	borderColor: backgroundColor,
            	color: backgroundColor
            });
            $(el).parent('.comment-author').prepend($elementAvatar);

        });
		//************************************8
	}
});
</script>

