    <script>
    	curl(['domReady!'])
			.next(['simpleDD'], function (simpleDD) {
				simpleDD.init(".b-simple-dd");
				

				var showHeaderBtn = $('#showHeaderBtn')
				showHeaderBtn.on('click', function () {
					$(this).fadeOut(200).closest('.b-services').css('margin-top', 0);
					return false;
				});

				//Смотрю, в какую сторону пользователь скроллит колесом мыши
				var scrolledEl = $(window).get(0);
				var headerOpen = false;
				if (scrolledEl.addEventListener) {
					if ('onwheel' in document) {
						// IE9+, FF17+
						scrolledEl.addEventListener("wheel", onWheel, false);
					} else if ('onmousewheel' in document) {
						// устаревший вариант события
						scrolledEl.addEventListener("mousewheel", onWheel, false);
					} else {
						// 3.5 <= Firefox < 17, более старое событие DOMMouseScroll пропустим
						scrolledEl.addEventListener("MozMousePixelScroll", onWheel, false);
					}
				} else { // IE<9
					scrolledEl.attachEvent("onmousewheel", onWheel);
				}
				function onWheel(e) {
					if (!headerOpen) {
						e = e || window.event;

						// wheelDelta не дает возможность узнать количество пикселей
						var delta = e.deltaY || e.detail || e.wheelDelta;

						var ua = navigator.userAgent;

						//IE считает скроллы наоборот
						if (ua.search(/MSIE/) > -1 || ua.search(/Trident/) > -1)
							delta = -delta;

						//Если пользователь находится вверху страницы и пытается проскроллить ещё выше, то раскрываем шапку
						if (delta < 0 && $(scrolledEl).scrollTop() == 0) {
							showHeaderBtn.trigger('click');
							headerOpen = true;
						}
					}
				}

				//Если пользователь находится вверху страницы и пытается проскроллить ещё выше, нажимая стрелочку "вверх" то раскрываем шапку
				scrolledEl.onkeydown = function (e) {
					e = e || event;

					if (e.keyCode == 38 && $(scrolledEl).scrollTop() == 0) {
						showHeaderBtn.trigger('click');
					}
				};
            })
