<script>
    $(document).ready(function(){
        var $autocomplete = function(){
            var val = $(this).val();
            if(val.length > 2){
                $.ajax({
                    type: "POST",
                    url: "/ajax/cityAC",
                    data: 'str='+val,
                    success: function(data){
                        $("#inputSearchDropdown").removeClass("fade").html(data);
                    }
                });
            } else {
                $("#inputSearchDropdown").addClass("fade").html("");
            }
        };
        $("#inputSearchListTown")
            .change($autocomplete)
            .keydown($autocomplete)
            .focus($autocomplete)
            .blur(function(e){
                $("#inputSearchDropdown").addClass("fade");
                e.preventDefault();
            });

        $("#searchSubmit").click(function(){
            return false;
        });

        $(document).on("click","a.set_city",function(){
            var selector = $(".city_selector"),
                modal = $("#modal-list-menu");

                var params = {
                    route: modal.find("input.route").val(),
                    params: modal.find("input.params").val(),
                    city: $(this).attr("data-val")
                };
                $.ajax({
                    type:"POST",
                    url:"/site/changeCity",
                    data: params,
                    success:function(url){
                        document.location.href = url;
                    }
                });

        });

    });
