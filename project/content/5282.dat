<script>
	$(function() {
		$("input:submit").button();
		$( "#dialog:ui-dialog" ).dialog( "destroy" );
		
		var login = $("#login"),
			password = $("#password"),
			allFields = $( [] ).add( login ).add( password ),
			tips = $( ".validateTips" );

		function updateTips( t ) {
			tips
				.text( t )
				.addClass( "ui-state-highlight" );
			setTimeout(function() {
				tips.removeClass( "ui-state-highlight", 1500 );
			}, 500 );
		}

		function checkLength( o, n, min, max ) {
			if ( o.val().length > max || o.val().length < min ) {
				o.addClass( "ui-state-error" );
				updateTips( "Длина " + n + " должна быть от " +
					min + " до " + max + " символов." );
				return false;
			} else {
				return true;
			}
		}

		function checkRegexp( o, regexp, n ) {
			if ( !( regexp.test( o.val() ) ) ) {
				o.addClass( "ui-state-error" );
				updateTips( n );
				return false;
			} else {
				return true;
			}
		}

		
		$( "#login-form" ).dialog({
          				autoOpen: false,
          				height: 270,
			width: 354,
			modal: true,
			closeOnEscape: false,
			resizable: false ,
			create: function() {
					var $this = $('#login-form');

					// focus first button and bind enter to it
						$this.keypress(function(e) {
						if( e.keyCode == 13 ) {
							$this.parent().find('.ui-dialog-buttonpane button:first').click();
							return false;
						}
					});
				},
			buttons: {
				"Войти": function() {
					var bValid = true;
					allFields.removeClass( "ui-state-error" );

					bValid = bValid && checkLength( login, "login", 3, 100 );
					

					bValid = bValid && checkRegexp( login, /[0-9a-z_.]+@[0-9a-z_-]+\.[a-z]{2,5}/i, "Используйте: A-Z, a-z, А-Я а-я, 0-9, @ пробел и тире" );
					
					if ( bValid ) {
						$(":button:contains('Войти')").prop("disabled", true).addClass("ui-state-disabled");
						$(":button:contains('Отмена')").prop("disabled", true).addClass("ui-state-disabled");
						$.ajax({
						   type: "POST",
						   url: "/index_login.php",
						   data: "login=" + login.val() + "&password=" + password.val() +"&partition=632&devices=1730&model=4209&description=5285&bonus=0&cowork_prov=0&path=/632/1730/4209/5285/&mykey=1e873a66ea94e686957b4e28e7307275",
						   						   success: function(){
							location.reload();
						   },
						   error : function() {
								allFields.addClass( "ui-state-error" );
								updateTips( "Неверный логин или пароль" );
								$(":button:contains('Войти')").prop("disabled", false).removeClass("ui-state-disabled");
								$(":button:contains('Отмена')").prop("disabled", false).removeClass("ui-state-disabled");
							},
						   
						   
						});
					}
				},
				"Отмена": function() {
								$(":button:contains('Войти')").prop("disabled", false).removeClass("ui-state-disabled");
								$(":button:contains('Отмена')").prop("disabled", false).removeClass("ui-state-disabled");
								$('#login-form').dialog( "close" );
				}
			},
			close: function() {
				$(":button:contains('Войти')").prop("disabled", false).removeClass("ui-state-disabled");
				$(":button:contains('Отмена')").prop("disabled", false).removeClass("ui-state-disabled");
				allFields.val( "" ).removeClass( "ui-state-error" );

			}
		});
		
		
		
			
		$( "#login_f" ).click(function() {
				$( "#login-form" ).dialog( "open" );
		});
	});
	</script>
