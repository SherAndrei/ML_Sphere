<script type="text/javascript">

	function close_subscribe(){
		$('#subscribe_form').slideUp(function(){
			$('#subscribe_form').remove();
		});
	}

	// if (location.pathname.match(/\/ru\/offer\b/)) {
	if (location.pathname.match(/\/ru\/offer\/(index\.html)?$/)) {
		var floatBanner = $(".inner_float");
		var watch4el = $("#content").eq(0);
		try {
			watch4el.waypoint(function (direction) {
				floatBanner.fadeToggle();
			}, {
				offset: -20
			});
		} catch (ex) { console.log(ex); }

		var $form = $('#subscribe_form').find('form');
		$form.submit(function (e) {
			var email = $('[name=Email]', $form).val();
			if (!email.match(/^(([^<>()[\]\\.,;:\s@\"]+(\.[^<>()[\]\\.,;:\s@\"]+)*)|(\".+\"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/)) {
				alert('Некорректный E-mail адрес!');
				return false;
			}

			var form_data = {
				SubscribeCode: 'SSN1',
				Timeoffset: -(new Date()).getTimezoneOffset() / 60,
				Email: email
			};

			$.post('https://ssl.samsung.ru/crmb2c/api/subscribe/Create', form_data, function (response_data) {
				if (response_data && Number(response_data['Status']) > 0) {
					alert("Ваша подписка оформлена!");
					$('#subscribe_form').slideUp();
				} else {
					console.log("*** $.post -- response_data: %o", response_data);
				}
			});
			e.preventDefault();
			return false;
		});

	}

</script>

