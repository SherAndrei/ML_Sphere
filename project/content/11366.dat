<script>
	(function () {
		var top_menu_items = $('.main-top nav ul li');
		if (!top_menu_items.length)
			return;

		var w = iface.viewport().width;
		var row_limit = 5;
		if (w > 500)
			row_limit = 2;

		var pos_last = 0;
		var row_now = 0;
		var hidden_items = 0;

		top_menu_items.each(function () {
			if (hidden_items <= 0) {
				var y = $(this).offset().top;
				// console.log('now offset: ',y,' prev offset: ', pos_last);
				if (pos_last != y) {
					pos_last = y;
					row_now++;
				}
			}
			if (hidden_items > 0 || row_now > row_limit) {
				$(this).closest('ul').addClass('is_hidden');
				$(this).addClass('hidden hidable');
				hidden_items++;
			}
			$(this).data('index', $(this).index());
		});

		if (hidden_items == 0)
			return;

		var ul_opener_text = {closed: 'Смотреть еще (' + hidden_items + ')', opened: 'Спрятать открывшиеся (' + hidden_items + ')', sort_az_title: 'Сортировать категории выше по алфавиту', sort_az: 'А-Я', sort_top: 'ТОП', sort_top_title: 'Сортировать категории выше по самым популярным'};

		var ul_opener = $('<li class="more clf"><a href="#" class="ul_opener"><span class="isp nije mr"></span><span class="text">' + ul_opener_text.closed + '</span></a><a href="#" class="ul_sort" title="' + ul_opener_text.sort_az_title + '">' + ul_opener_text.sort_az + '</a></li>');

		var fn_open = function (e, t, force) {
			e.preventDefault();
			var p = $(t).closest('ul');
			if (force || p.hasClass('is_hidden')) {
				p.removeClass('is_hidden');
				p.find('li').removeClass('hidden');
				$(t).find('span.isp').removeClass('nije').addClass('vyshe');
				$(t).find('span.text').text(ul_opener_text.opened);
			} else {
				p.addClass('is_hidden');
				p.find('li.hidable').addClass('hidden');
				$(t).find('span.isp').removeClass('vyshe').addClass('nije');
				$(t).find('span.text').text(ul_opener_text.closed);
			}
		};

		var fn_sort = function (e, t) {
			e.preventDefault();
			var p = $(t).closest('ul');
			fn_open(e, p.find('.ul_opener'), true);
			var items = p.find('li').not('.more').get();
			if (p.hasClass('is_sorted_az')) {
				items.sort(function (a, b) {
					var keyA = $(a).data('index');
					var keyB = $(b).data('index');

					if (keyA < keyB)
						return -1;
					if (keyA > keyB)
						return 1;
					return 0;
				});
				p.removeClass('is_sorted_az');
				$(t).text(ul_opener_text.sort_az).attr('title', ul_opener_text.sort_az_title);
			} else {
				items.sort(function (a, b) {
					var keyA = $(a).text().toLowerCase();
					var keyB = $(b).text().toLowerCase();

					if (keyA < keyB)
						return -1;
					if (keyA > keyB)
						return 1;
					return 0;
				});
				p.addClass('is_sorted_az');
				$(t).text(ul_opener_text.sort_top).attr('title', ul_opener_text.sort_top_title);
			}
			items.push(p.find('li.more').get(0));
			$.each(items, function (i, li) {
				p.append(li);
			});
		};

		ul_opener.on('click', '.ul_opener', function (e) {
			fn_open(e, this);
		});
		ul_opener.on('click', '.ul_sort', function (e) {
			fn_sort(e, this);
		});
		top_menu_items.parent().append(ul_opener);
	})();
</script></div></main>
