<div class=" c-user-card c-user-card--h c-user-card--nano"  itemscope itemtype="http://schema.org/Person">
    <div class="c-user-card__pic-container">
        <div class="c-user-card__pic u-vignette-tiny"><a href="/users/deathcar/"><img src="https://a.d-cd.net/3261dv/100.jpg" width="25" height="25" itemprop="image" alt=""></a></div>
