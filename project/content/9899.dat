			 $('#region_id3 option').each(function() {
			    $(this).attr('selected',false);
			 });
			$("#region_id3").dropdownchecklist( { 
				  textFormatFunction: function(options) {
		                var selectedOptions = options.filter(":selected");
		                var countOfSelected = selectedOptions.size();
		                var size = options.size();
		                if(size==0){ var countOfSelected = -1}
		                
		                switch(countOfSelected) {
		                	case -1: return "<i class='no_foc'>Выберите город</i>";
		                    case 0: return "<i>Выберите город</i>";
		                    case 1: return selectedOptions.text();
		                    case options.size(): return "<span></span>";
		                    default: return "Выбрано " + countOfSelected;					
		                }				
	      	},
				 maxDropHeight: 150,
				 width: 200
				 });
			
			$('#country_id3').dropdownchecklist('destroy');
			 $('#country_id3 option').each(function() {
			    $(this).attr('selected',false);
			 });
			  $("#country_id3").dropdownchecklist( { 
				  textFormatFunction: function(options) {
		                var selectedOptions = options.filter(":selected");
		                var countOfSelected = selectedOptions.size();
