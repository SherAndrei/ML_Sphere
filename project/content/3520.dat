<script>
function AjaxFormQuestion(result_id,formMain,url) {
    if(jQuery("#"+formMain+" #f_p_name").val()==="")
        {
            $("#"+formMain+" .error").fadeIn(200).html('Введите ваше имя!');
			jQuery("#"+formMain+" #f_p_name").focus();
            return false;
        }
    if(jQuery("#"+formMain+" #f_p_text").val()==="")
        {
            $("#"+formMain+" .error").fadeIn(200).html('Введите ваш вопрос!');
			jQuery("#"+formMain+" #f_p_text").focus();
            return false;
        }
     if(jQuery("#"+formMain+" #f_p_email").val()!=""){
			var email = jQuery("#"+formMain+" #f_p_email").val();
			var regV = /[a-z0-9-_]{2,1000}\@[a-z0-9\-\_]{2,100}\.[a-z0-9]{2,4}/gi;
			var result = email.match(regV);
			if(!result){
				$("#"+formMain+" .error").fadeIn(200).html('Введите корректный email');
				jQuery("#"+formMain+" #f_p_email").focus();
				return false;
			}
		} else if(jQuery("#"+formMain+" #f_p_email").val()===""){
            $("#"+formMain+" .error").fadeIn(200).html('Введите ваш email!');
			jQuery("#"+formMain+" #f_p_email").focus();
            return false;
        }
        
   jQuery.ajax({
        url:     '/netcat_template/template/saintdent/php/vopros_captcha.php',
        type:     "POST",
        dataType: "html",
        data: jQuery("#"+formMain).serialize(), 
        success: function(response) {
            if(response == 1){
                jQuery("#"+formMain+" .btn").hide().next().css('opacity',1);
				var that = $('#'+formMain)[0];
				formData = new FormData(that);
                jQuery.ajax({
                    url:     url,
                    type:     "POST",
                    contentType: false,
					processData: false,
					data: formData, 
					dataType: "html",
                    success: function(response) {
                        if(response == 1){
                            jQuery("#"+formMain).html('<div id="response_formIn"><div class="alert alert-info animated"><div class="alert-heading"><h4><i class="fa fa-check"></i><span>Выполнено</span></h4></div><div class="alert-content"><p>Ваш вопрос получен и будет опубликован после ответа специалиста.<br/>Вы также получите ответ на вопрос на указанный адрес электронной почты.</p></div></div></div>');
                        }
                    },
                    error: function(response) {
                        document.getElementById(result_id).innerHTML = "<p>Возникла ошибка при отправке формы. Попробуйте еще раз</p>";
                    }
                });
            }
            else{
                $("#"+formMain+" .error").fadeIn(200).html('Не верно введен код с картинки!');
			    $("#"+formMain+" #captcha_otzyv").focus();
                jQuery("#"+formMain+" .btn").show().next().css('opacity',0);
            }
        },
        error: function(response) {
            document.getElementById(result_id).innerHTML = "<p>Возникла ошибка при отправке формы. Попробуйте еще раз</p>";
        }
    });
}
