<script>
    function AjaxFormRequestLogin(ErrorLogin,formMain,url) {
                jQuery.ajax({
                    url:     url,
                    type:     "POST",
                    dataType: "html",
                    data: jQuery("#"+formMain).serialize(),
                    success: function(response) {
                    if(response.match('^[0-9]+$')){
                        location.reload();
                    } else{
                        document.getElementById('ErrorLogin').innerHTML = '<center><p style="color:#ff0f0f;">Неверный email или пароль!</p></center>';
                    }
                },
                error: function(response) {
                document.getElementById('ErrorLogin').innerHTML = "<p>Возникла ошибка при отправке формы. Попробуйте еще раз</p>";
                }
             });

             $(':input','#formMain')
 				.not(':button, :submit, :reset, :hidden')
 				.val('')
 				.removeAttr('checked')
 				.removeAttr('selected');
    }



