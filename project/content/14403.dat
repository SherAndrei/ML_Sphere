                    for (first in result.errors[name]) break;
                    $error.html(result.errors[name][first]);
                    $error.addClass('visible');
                }
            }
        });
        $form.submit(function () {
            $gmodal.addClass('active');
            $form.find('.error').text('').removeClass('visible');
        });

        $form.find('input[name="sex"]').change(function (e) {
            var $this = $(this);
            $form.find('input[name="sex"][value!="' + $this.val() + '"]').removeAttr('checked').parent('label').removeClass('checked');
        })

        $form.find('input[name="subscribe_interest"]').change(function (e) {
            var $this = $(this)
            if ($this.is(':checked')) {
                $form.find('.subscription-block').fadeIn().find('input[type="checkbox"]').attr('checked', 'checked').parent('label').addClass('checked');

            } else {
                $form.find('.subscription-block').fadeOut().find('input[type="checkbox"]').removeAttr('checked').parent('label').removeClass('checked');
            }
        })

        // клик на ОК
        $('#modal-register a.button').click(function (e) {
            e.preventDefault();
            $form.submit();
        });

        // обработка Enter'а
        $form.keydown(function (e) {
            if (e.keyCode == 13) {
                $form.submit();
            }
        })


    });
</script>

