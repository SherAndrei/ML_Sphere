    window.onload=function(){

        var offset = $("#aside-advertising-fixed").offset();
        var offsetTop = offset.top;
        var avatarTop = $("#right_colum").height();
        var avatarBottom = $(".off-canvas-wrap").height() - 1600;
        var avatarBody = document.getElementById('aside-advertising-fixed');
        
        window.onscroll = function() {
            
            //var widthBr = $(window).width();
            //if (widthBr > 980) {
            if (Foundation.utils.is_large_up()) {
                
                var pageY = window.pageYOffset || document.documentElement.scrollTop;

                if (pageY >= offsetTop) {
                    avatarBody.className = "aside-advertising-fixed";
                    $("#right_colum").css("height", avatarTop + "px");

                    if (pageY >= avatarBottom) {
                        avatarBody.className = "aside-advertising-absolute";
                    }
                } else {
                    avatarBody.className = "aside-advertising";
                }

            }else {
                avatarBody.className = "aside-advertising";
            }
        };
    }


</script>


<!-- quick reply -->
<script>
	$(document).ready(function(){

		jQuery(function($) {

			var id_topic = '';
			var name = '';

			$('.topic > div.row').mousedown(function(){
				id_topic = $(this).attr("id").substring(4);
				name = $(this).find("p.topic-info > a").text();
			});

			var txt = '';
			var txt_res = '';

			$('.topic-body > p').bind("mouseup", function(e){
				if (window.getSelection){
					txt = window.getSelection();
				}
				else if (document.getSelection){
					txt = document.getSelection();
				}
				else if (document.selection){
					txt = document.selection.createRange().text;
				}
				else return;
				if    (txt!=''){
					$('#popUpBox').css({'display':'block', 'left':e.pageX-19+'px', 'top':e.pageY+20+'px'});
				}
				txt_res = '[QUOTE=' + name +';' + id_topic + ']' + txt + '[/QUOTE]';
			});

			$(document).bind("mousedown", function(){
				$('#popUpBox').css({'display':'none'});
			});

			$('#popUpBox').bind("mousedown", function(){
				$('#vB_Editor_QR_textarea').val(txt_res);
				window.location='#vB_Editor_QR_textarea';
			});

			$('.quick_insert_btn').bind("mousedown", function(){
				if(txt==0){
					alert("Пожалуйста, выделите цитируемый текст в сообщении!")
				}else {
					$('#vB_Editor_QR_textarea').val(txt_res);
					window.location='#vB_Editor_QR_textarea';
				}
			});

		});

		<!-- Переноc имени кого цитировали -->
		$('.topic-cite-author strong').empty();
		var iAutor = 1;
		$('.topic-cite-author strong').each(function(){
			var phrase = $(this).closest("phrase").attr('1');
			$(this).append(phrase);
			iAutor++;
		});

	});
</script>
