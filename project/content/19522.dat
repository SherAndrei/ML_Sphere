        <div class="post_date"><a  class="post_link"  href="/wall-31027736_506"  onclick="return showWiki({w: 'wall-31027736_506'}, false, event);" ><span class="rel_date">5 мар 2012</span></a></div>
        <div class="ui_actions_menu_wrap _ui_menu_wrap " onmouseover="uiActionsMenu.show(this);" onmouseout="uiActionsMenu.hide(this);">
