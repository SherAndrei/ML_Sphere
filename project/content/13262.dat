function postComment(mode,text,parent_id,item_id, img, video, cn_name, cn_email,token) {
	var send;
	//$.ajaxSetup({ async: false });
		$.post("https://webspoon.ru/main/post_comment", { "mode": mode,"text": text,"parent_id": parent_id,"item_id": item_id,"type": "R", "image": img, "video": video, "name": cn_name, "email": cn_email, "token": token }, function(res) {
			if (res=='e403') {
				show_message("Можно писать не более 1 комментария в 20 секунд");
				send = false;
			}
			else if (mode=="") {
				var nc_block = $(res);
				if (parent_id==0)
					$("#comments_cont").append(nc_block);
				else {
					$("#an"+parent_id).append(nc_block);
				}
				if (!("R"=="Q" && parent_id!=0))
					$("#comm_count").text(parseInt($("#comm_count").text())+1);
				$("#comment_title").show();

				/*redactor = nc_block.find(".comment_textarea").redactor({ toolbar: false, removeStyles: true, removeClasses:true });
				nc_block.find(".textarea_block").data("redactor",redactor);

				SetUpload(nc_block.find(".image"))*/

                if($.browser.webkit)
                nc_block.find( "div.comment_text" ).each(function( index ) {
                    $( this ).html(emojione.toImage($( this ).html()))
                });
                else {
                    nc_block.find( "div.comment_text" ).each(function( index ) {
                        $( this ).html(emojione.shortnameToUnicode($( this ).html()))
                    });
                }

				send = true;
                redactor.setCodeEditor("<p>&nbsp;</p>");
				redactor.syncCodeToTextarea();
			}
			else if (mode=="edit_comment") {
					//$("#comm"+parent_id).html(res);
					$("#"+parent_id).parent().replaceWith(res);

					redactor = $("#"+parent_id).find(".comment_textarea").redactor({ toolbar: false, removeStyles: true, removeClasses:true });
					$("#"+parent_id).find(".textarea_block").data("redactor",redactor);

					SetUpload($("#"+parent_id).find(".image"));

					send = true;
			}
		});
//	$.ajaxSetup({ async: false });


	return send;
}

