function load_sessions(film, date, city) {
	$.ajax({
		type: "POST",
		url: '/ajax/film_sessions_load',
		data: {
			'film' : film,
			'date' : date,
			'city' : city
		},
		success: function(data) {
			if (data.error === undefined) {
				clear_sessions();

  				var source   = $("#session-item-template").html();
