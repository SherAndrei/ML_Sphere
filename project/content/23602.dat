				//$(template(data)).insertAfter('div.sell-line');
				$(template(data)).insertAfter('div.no-sessions');
				var count = data.result.length;

				if (count > 0) {
					$('div.no-sessions').hide();
				} else {
					$('div.no-sessions').show();
				}
    			$("td.cinema-room .dotted a").fancybox();
				clearMarkers();
				setMarkers(data.result);
			} else { console.log('Ошибка сервера'); }
		}
	});
}


function fancyoncomplete(){
	$('.shots .item').click(function(){

		var idx =  $('.shots .item').index($(this) );  //alert(idx);

		if ( (idx) ==  ($('.shots .item:visible').length - 1) ) {
			loadphotos();
		}

		$('#fancybox-right').click(function(){

			if ( idx++ ==  ($('.shots .item:visible').length - 2) ) {
				loadphotos();
				return false;
			}

			return false;
		});

		$('#fancybox-left').click(function(){
			idx--;
			return false;
		});

		return false;
	});

	$("#fancybox-wrap").unbind('mousewheel.fb');

}

function load_photos(film, limit, offset, animate) {
	$.ajax({
		type: "POST",
		url: '/ajax/film_photos_load',
		data: {
			'film' : film,
			'limit' : limit,
			'offset' : offset
		},
		success: function(data) {
			if (data.error === undefined) {
                var shotheight = $('.shots-holder').height();

				var count = photos_offset;
				$.each(data.result, function(index, value){

					var i = ($('.shots .shots-holder a').length ) +1;

					$('<a tabindex="1" class="item" alt="'+(i++)+'" rel="photoset" href="'+value.foto_url+value.foto+'">\
						<img src="'+value.foto_url+(value.is_old?'small/':'medium/')+value.foto+'" alt="'+value.foto+'" />\
						<span class="shot-ico"></span>\
					</a>').insertBefore( $('div.shots div.no-photos') );

					count++;

                    if (shotheight < $('.shots-holder').height() ) {
                        var shotheight2 = $('.shots-holder').height();
                        $('.shots-holder').css('height', shotheight);

						if (animate) {
							$('.shots-holder').animate({height: shotheight2}, function(){
                        	    shotheight = shotheight2;  $('.shots-holder').css('height', 'auto');
                        	});
						} else {
							$('.shots-holder').css('height', 'auto');
						}

                    }

				});
				photos_offset += photos_limit;
				if (data.count > count) {
					$('div.shots div.for-button').show()
				} else {
					$('div.shots div.for-button').hide()
				}



			} else { console.log('Ошибка сервера'); }
		}
	});
}

function loadphotos(){

	var shotheight = $('.shots-holder').height();

	for (i = 1; i<=6; i++) {
		$('.shots .item:hidden:eq(0)').show();
	}

	if (shotheight < $('.shots-holder').height() ) {
		var shotheight2 = $('.shots-holder').height();
		$('.shots-holder').css('height', shotheight);
		$('.shots-holder').animate({height: shotheight2}, function(){
				shotheight = shotheight2;  $('.shots-holder').css('height', 'auto');
		});
	}

	( $('.shots .item:hidden').length ==  0 ) ? $('.shots .for-button').hide() : "";

	return false;
}

