      /* Handle [data-label="baseline"] */
      mABC.addTarget('.ga__readability[data-label="baseline"]', {
        trigger: 'view:100',
        category: 'readability',
        ni: true,
        mapper: function(options) {
          options.action = this.getAttribute('data-action').split('-').join('_');
          options.label = this.getAttribute('data-label');

          /* Handle [data-label="content_end"] in fifteen seconds */
          setTimeout(function() {
            mABC.addTarget('.ga__readability[data-label="content_end"]', {
              trigger: 'view:100',
              category: 'readability',
              ni: false,
              mapper: function(options) {
                options.action = this.getAttribute('data-action').split('-').join('_');
                options.label = this.getAttribute('data-label');
              }
            });
          }, 15 * 1000) // Fifteen seconds later.

          /* Handle all events except [data-label="baseline"] and [data-label="content_end"] */
          mABC.addTarget('.ga__readability:not([data-label="baseline"]):not([data-label="content_end"])', {
            trigger: 'view:100',
            category: 'readability',
            ni: true,
            mapper: function(options) {
              options.action = this.getAttribute('data-action').split('-').join('_');
              options.label = this.getAttribute('data-label');
            }
          });
        }
      });
    });
  })()
