    function SetBanners(){
        $(".banners-place embed").each(function(){
            var bb_db=1;
            if ($(this).closest('.banners-place').hasClass('banner_items-4')) bb_db=4;
            if ($(this).closest('.banners-place').hasClass('banner_items-2')) bb_db=2;

            var wi1=$(this).width();
            var wi2=$(this).closest('.banners-place').parent().width()/bb_db;



            console.log(wi1+' '+wi2);
            if (wi2<wi1)
            {
                $(this).width(wi2);
                $(this).closest('.banners-place').data("width",wi1);
                console.log("sett");
            }
            else
            {
                if ($(this).closest('.banners-place').data("width")>0 && $(this).closest('.banners-place').data("width")<wi2)
                {
                    $(this).width($(this).closest('.banners-place').data("width"));
                }
            }

            if (bb_db>1)
            {
                var hi1=$(this).height();
                var hi2=$(this).closest('.banners-place').parent().height();
                if (hi2!=hi1)
                {
                    $(this).height(hi2);
                    console.log("h:"+hi2);
                }

            }


        })
    }

