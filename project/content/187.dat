                var currentAttrValue = $(this).attr('href');

                close_pharma_section()

                $('.pharma-form-list').hide();

                $(currentAttrValue).addClass('open');

                $('.pharma-form-list-2').show();

                var bcr = $(currentAttrValue)[0].getBoundingClientRect();
                if (bcr) {
                    if (bcr.top < 0) {
                        $(currentAttrValue)[0].scrollIntoView();
