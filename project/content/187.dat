                $('.pharma-form-list').show();
                $('.pharma-form-list-2').hide();

                e.preventDefault();
            });


            $('#forum-form-add').on('submit', {form_send: false}, function (e) {
                var $self_form = $(this);
                $self_form.find('input[name="repassword"]').val('')
                if (e.data.form_send) return true;
                var error = []
                var mail = $self_form.find('input[name="guestEmail"]').length ? $self_form.find('input[name="guestEmail"]').val() : false
                var name = $self_form.find('input[name="guestName"]').length ? $self_form.find('input[name="guestName"]').val() : false
                var text = $self_form.find('textarea[name="text"]').length ? $self_form.find('textarea[name="text"]').val() : false

                if (mail || name) {
                    if (mail && name && mail.length == 0 && name.length == 0) {
                        error.push('Вы не указали имя и e-mail.')
                    } else if (mail && mail.length == 0) {
                        error.push('Вы не указали e-mail.')
                    } else if (name && name.length == 0) {
                        error.push('Вы не указали имя.')
                    }
                }

                if (text && text.length == 0) {
                    error.push('Вы не написали сообщение.')
                }

                if (error.length > 0) {
                    alert(error.join("\n"))
                    return false
                }

                $.post("/ajax/forum/newmsg", $self_form.serializeArray(), function (response) {
                    console.log(response);
                    var message = [],
                            callback = function () {
                                if (response.msg) {
                                    var $list = $('.js-comment-list');
                                    if ($list.attr('data-reverse') == 'on') {
                                        $list.prepend(response.msg)
                                        var top = $list.find(".ms-comment:first-child").offset().top
                                    } else {
                                        $list.append(response.msg)
                                        var top = $list.find(".ms-comment:last-child").offset().top
                                    }
                                    close_popup_section();
                                    if (top) $("html, body").animate({scrollTop: top}, 1000)
                                }
                            }

                    if (response.error) {
                        if (response.error == 'empty_email') {
                            alert('Данный email зарегистрирован и не может быть использован! Авторизуйтесь или восстановите пароль!');
                        } else if (response == 'mail') {
                            alert('Введите правильный email!')
                        } else {
                            alert('Что-то пошло не так :(')
                        }
                    } else {
                        if (response.мoderation) {
                            alert('Тема в режиме премодерации. Ваше сообщение появится после проверки модератором.')
                        }

                        if (message.length) {
                            /*
                             $.magnificPopup.open({
                             items: [
