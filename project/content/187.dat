                             src: $('<div class="ms-f-popup-alert"><div class="ms-f-popup-alert-content">'+message.join('<br><br>')+'</div></div>'),
                             type: 'inline'
                             }
                             ],
                             closeOnBgClick: false,
                             callbacks: {
                             close: function() {
                             callback()
                             }
                             }
                             })
                             */
                        } else {
                            callback()
                        }
                    }
                })

                return false
            })


            $('.js-comment-list-more').click(function (e) {
                $(this).hide()
                $(this).parents('.js-comment').find('.js-comment-list > .ms-comment').show()
                e.preventDefault();
            });

            $("div.ms-header-search input.search").keyup(function (e) {
                search_val = $(this).val();
                if (search_val.length > 2) {
                    clearTimeout(searchTimer);
                    searchTimer = setTimeout(function () {
                        $.post("/herb_list/ajax/searchherb", {
                            search: search_val
                        }, function (data) {
                            $("div.ms-herb-search-result a").remove();
                            $.each(data, function (key, val) {
                                html = "<a href='/herb/" + val.id + "'>" + val.name + "<i>" + herb_types[val.type] + "</i></a>";
                                $("div.ms-herb-search-result").append(html);
                            })
                        })
                    }, 100)

                } else {
                    $("div.ms-herb-search-result a").remove();
                }
            });
            $.get("/herb_list/ajax/get-warning-conditions",{},function(data){$("div.ajax-warning-conditions").html(data);})
        });
        
    </script>

