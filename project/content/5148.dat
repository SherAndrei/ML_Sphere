</v:shape><![if !vml]><img width=29 height=24
src="stroika_10270.files/image155.png" v:shapes="_x0000_i1110"><![endif]><span
style='mso-spacerun:yes'> </span>и минимальное <v:shape
 id="_x0000_i1111" type="#_x0000_t75" style='width:20.25pt;height:18pt'>
 <v:imagedata src="stroika_10270.files/image156.wmz" o:title=""/>
