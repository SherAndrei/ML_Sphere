        $('.search-another-region-link').click(function () {
            var sr = $('#header');
            var widgetPosition = sr.position().top;
            var scrollTop = $(document).scrollTop();
            if (!scrollTop) {
                scrollTop = $(document.body).scrollTop();
            }
            if (scrollTop >= widgetPosition) {
                $.scrollTo('#header', 350);
            }
            sr.find(".region-selector-link").click();
            return false;
        });
    });
</script>


