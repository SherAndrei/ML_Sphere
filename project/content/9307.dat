<script type="text/javascript">
    $(function () {
        $('#oneclick-dialog').dialog({
            autoOpen : false,
            width    : 550,
            modal    : true
        });

        $('.oneclick-buy,.oneclick-order').click(function () {
            var mode = ($(this).hasClass('oneclick-buy')) ? 'buy' : 'order';
            var productId = $(this).attr("name");
            var id = addIndicator(this);

            $.post(
                '/ajax/Product/GetInfo/',
                {
                    id : productId
                },
                function (response) {
                    $('#ui-dialog-title-oneclick-dialog').text('Перезвоните мне');
                    $('#oneclick-submit-btn').text('Отправить');
                    $('.privacy-policy-link').attr('href', response.privacyPolicyLink);
                    $('#oneclick-product')
                        .attr("idcompany", response.idcompany)
                        .attr("product_id", response.id)
                        .attr("mode", mode);

                    if (!window.tracing) {
                        // Действие трекера на событие oneclick-preaction-goal
                        trackVisit(response.idcompany, 'oneclick-preaction-goal');
                        window.tracing = true;
                    }

                    var table = "" +
                        "<table>" +
                        "   <tr>";

                    if (response.img != '') {
                        table += "<td valign='middle'><img src='" + response.img + "' align='absmiddle' alt='' /></td>";
                    }

                    table += "" +
                    "       <td width='100%'><a href='" + response.url + "'>" + response.pp + "</a></td>" +
                    "       <td class='nobr'><strong>" + (response.price ? response.price : '') + "</strong></td>" +
                    "   </tr>" +
                    "</table>";

                    $('#oneclick-product').html(table);
                    $('#oneclick-dialog').dialog("open");
                    removeIndicator(id);
                    $('#error_ocPhone').html("").hide();
                },
                'json'
            );
            return false;
        });

        $('#ocNumber').on('keyup', function (e) {
            if (e.keyCode == 86) {
                // Чистим код
                var code_length = $("#ocCCode").val().length;
                var phone_number = $('#ocNumber').val().replace(/[^0-9]+/gi, "");
                var phone_code = phone_number.substr(0, code_length);
                if (phone_code == $("#ocCCode").val()) {
                    phone_number = phone_number.substr(code_length, phone_number.length);
                }
                $(this).val(phone_number);
            }
        });
        $('#ocNumber').on('click', function () {
            $('#error_ocNumber').hide();
        });
        $('#oneclick-submit-btn').click(function () {
            var error = 0;
            var phone_number = $('#ocNumber').val().replace(/[\D]+/gi,"");
            var selectedCode = $('.b-phone-code__item_selected');

            $('#ocNumber').val(phone_number);
            if (!check_field_is_empty('ocNumber', error)) {
                error = 1;
            } else {
                var phone_min_length = selectedCode.attr('data-min-length');
                var phone_max_length = selectedCode.attr('data-max-length');
                var iso_current = selectedCode.attr('data-iso');
                var code = selectedCode.attr('data-code');
                if (!check_field_is_wrong_phone3('ocNumber', 'ocNumber', phone_min_length, phone_max_length, error) || !check_is_only_digits('ocNumber', error)) {
                    error = 1;
                }
            }

            if (error == 0) {
                var idcompany = $('#oneclick-product').attr("idcompany");
                $.post(
                    '/ajax/Product/CreateOrder/',
                    {
                        id        : $('#oneclick-product').attr("product_id"),
                        idcompany : idcompany,
                        mode      : $('#oneclick-product').attr("mode"),
                        env       : window.environment,
                        phone     : code + phone_number,
                        phone_code: code,
                        phone_iso : iso_current
                    },
                    function (response) {
                        $('#oneclick-dialog').dialog("close");
                        $(document).cabinetReport(response);
                        trackMarketGidOrder();
                        trackVisit(idcompany, 'oneclick-goal');
                        callback_ga();
                    },
                    'json'
                );
            }
            return false;
        });
    });
</script>
