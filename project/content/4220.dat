                    $("#up").fadeIn();
                    $("#up").css({"width" : "15", "height" : "30px"});
                    $("#up").addClass('narrow');
                } else if (width < 200) {
                    $("#up").fadeIn();
                    $("#up").css({"width" : "15", "height" : "1000%"});
                    $("#up").addClass('narrow');
                } else {
                    $("#up").fadeIn();
                    $("#up").css("width", $(".up-container").offset().left - 110);
                    $("#up").removeClass('narrow');
                }
            } else {
                $("#up").fadeOut();
            }
        }

        $("#up").click(function () {
            $.scrollTo($("body"), 50);
        });

        set_up_block_param();
        window.onresize = function (event) {
            set_up_block_param();
        };

        $(window).scroll(function () {
