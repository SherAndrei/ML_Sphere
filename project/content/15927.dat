//                'validate': true,
                'location': locObject,
                'enableHaschange': true,
                'onMark': function () {
                    if (locObject.masha) {
                        locObject.masha = null;
                        locObject.hashChange ? locObject.hashChange() : null;
                    }
                },
                'onUnmark': function () {
                    if (locObject.masha) {
                        locObject.masha = null;
                        locObject.hashChange ? locObject.hashChange() : null;
                    }
                }
            });
        }

        if (window.addEventListener) {
            window.addEventListener('load', function () {
                init_masha();
            }, false);
        } else {
            window.attachEvent('onload', function () {
                init_masha();
            });
        }

            </script>
