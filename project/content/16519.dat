                function submitData(text, context, $btn) {
                    if (typeof text === 'undefined') {
                        return;
                    }
                    text = text.trim();
                    if (text.length <= 0) {
                        return;
                    }

                    $.ajax({
                        type: 'POST',
                        url: 'https://auto.drom.ru/ajax/',
                        xhrFields: { withCredentials: true },
                        data: {
                            mode: 'user_error_report',
                            url: window.location.href,
                            text: text,
                            context: context,
                            note: $('[data-text-error-note]:last').val() || '',
                            section_id: 4,                                                                                                                                            pub_id: 16988,                            crossdomain_ajax_request: 3
                        },
                        beforeSend: function (jqXHR, settings) {
                            $btn.addClass('b-button_state_loading');
                        },
                        success: function (data, textStatus, jqXHR) {
                            $content.hide();
                            $message.show();
                            setTimeout(function() {
                                modal.close();
                            }, 5000);
                        },
                        error: function (jqXHR, textStatus, errorThrown) {
                            $btn.removeClass('b-button_state_loading');
                            console.log('ajax error: ' + textStatus);
                        }
                    });
                }
            }
        });
    });

