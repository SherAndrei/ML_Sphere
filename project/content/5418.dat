    $(function () {
        var stopShow = $(".b-two-cols").position().top + $(".b-two-cols").height() - 500;
        var startShow = $(".b-two-cols .c-statistics").position().top + $(".b-two-cols .c-statistics").height()+150;

        if($(window).scrollTop() > startShow && stopShow > $(window).scrollTop()){
            $('#back-to-top').show();
            $('#back-to-top').css('position', 'fixed');
        }

        $(window).scroll(function () {
            if ($(this).scrollTop() > startShow && $(this).scrollTop() < stopShow){
                $('#back-to-top').fadeIn(0, function(){
                    $('#back-to-top').css('position', 'fixed');
                });

            } else {
                $('#back-to-top').fadeOut(0, function(){
                    $('#back-to-top').css('position', 'static');
                });
            }
        });

        $('#back-to-top a').click(function () {
            $('body,html').animate({
                scrollTop: 0
            }, 20);
            return false;
        });
    });
</script>
