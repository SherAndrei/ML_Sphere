                    p_request = new XMLHttpRequest();
                }
                catch (e) {}
            }
            else if (window.ActiveXObject)
            {
                try
                {
                    p_request = new ActiveXObject("Msxml2.XMLHTTP");
                }
                catch (e)
                {
                    try
                    {
                        p_request = new AciveXObject("Microsoft.XMLHTTP");
                    }
                    catch (e) {}
                }
            }

            if (p_request)
            {

            	// Get token
				new Ajax.Request('/journal_proc.php?action=token-json', {
