		success: function(html){
			$(".calendarDays").html(html);
			$(".calendarMonth").html(arr_month[cur_m-1]+" "+cur_y);
		}
	});		
}

function nextMonth(){
	if(cur_m == 12){
		cur_y++;
		cur_m=1;
	}else{
		cur_m++;
	}
	$.ajax({
		url: ".calendar.php?m="+cur_m+"&y="+cur_y,
