<script type="text/javascript">
    function escapeHtml(text) {
        return text
            .replace(/&/g, "&amp;")
            .replace(/</g, "&lt;")
            .replace(/>/g, "&gt;")
            .replace(/"/g, "&quot;")
            .replace(/'/g, "&#039;");
    }

    $('.comments-block')
        .on('click', 'span.likes', function () {
            if (confirm('Вы уверены, что хотите лайкнуть этот комментарий?')) {
                var $comment = $(this).parent().parent();
                $.ajax({
                    url: '/comment/like/' + $comment.data('id'),
                    type: 'POST',
                    dataType: 'json',
                    success: function (data) {
                        if (data.success) {
                            $comment.find('span.likes').text(data.votes);
                            showNotice('Ваш голос учтен!');
                        } else {
                            showNotice(data.error);
                        }
                    }
                });
            }
        })
        .on('click', 'span.dislikes', function () {
            if (confirm('Вы уверены, что хотите дизлайкнуть этот комментарий?')) {
                var $comment = $(this).parent().parent();
                $.ajax({
                    url: '/comment/dislike/' + $comment.data('id'),
                    type: 'POST',
                    dataType: 'json',
                    success: function (data) {
                        if (data.success) {
                            $comment.find('span.dislikes').text(data.votes);
                            showNotice('Ваш голос учтен!');
                        } else {
                            showNotice(data.error);
                        }
                    }
                });
            }
        })
        .on('click', '.send-claim', function () {
            if (confirm('Вы уверены, что хотите пожаловаться на этот комментарий?')) {
                var $comment = $(this).parent().parent();
                $.ajax({
                    url: '/comment/claim/' + $comment.data('id'),
                    type: 'POST',
                    dataType: 'json',
                    success: function (data) {
                        if (data.success) {
                            showNotice('Спасибо, ваша жалоба принята');
                        } else {
                            showNotice(data.error);
                        }
                    }
                });
            }
        }).on('click', '.remove-icon', function () {
        if (confirm('Вы действительно хотите удалить этот комментарий?')) {
            var that = $(this);
            var id = that.parents('.comment').data('id');
            $.ajax({
                type: 'post',
                url: '/profile/remove-comment',
                data: 'id=' + id,
                success: function (data) {
                    if (data.success) {
                        that.parents('.comment').fadeOut();
                    } else {
                        showNotice(data.error);
                    }
                }
            });
        }
    }).on('click', '.edit-icon', function () {
        var that = $(this);
        var id = that.parents('.comment').data('id');
        var text = that.parents('.comment').find('.comment-text').text();
        $.ajax({
            type: 'post',
            url: '/profile/edit-comment',
            data: 'id=' + id,
            success: function (data) {
                if (data.success) {
                    var html = '';

                    if (~~data.spoiler == 1) {
                        html += '<textarea class="edit-message">' + that.parents('.comment').find('.comment-hidden').text().replace(/<br>/g, '\n') + '</textarea>';
                        html += '<button class="main-button send-edit col">сохранить</button>';
                        html += '<div class="spoiler-block"><label for="spoiler-' + id + '">';
                        html += '<input type="checkbox" id="spoiler-' + id + '" checked name="spoiler">';
                    } else {
                        html += '<textarea class="edit-message">' + text.replace(/<br>/g, '\n') + '</textarea>';
                        html += '<button class="main-button send-edit col">сохранить</button>';
                        html += '<div class="spoiler-block"><label for="spoiler-' + id + '">';
                        html += '<input type="checkbox" id="spoiler-' + id + '" name="spoiler">';
                    }

                    html += '<span>спойлер</span>';
                    html += '</label></div>';
                    that.parents('.comment').find('.comment-text').html(html);
                    that.remove();
                } else {
                    showNotice(data.error);
                }
            }
        });
    }).on('click', '.send-edit', function () {
        var that = $(this);
        var id = that.parents('.comment').data('id');
        var text = $.trim(that.parents('.comment').find('.edit-message').val());

        var send_data = 'id=' + id + '&text=' + encodeURIComponent(text);

        var spoiler = that.parents('.comment').find('input[name="spoiler"]:checked').length;
        if (spoiler > 0) {
            send_data += '&spoiler=1';
        } else {
            send_data += '&spoiler=0';
        }

        $.ajax({
            type: 'post',
            url: '/profile/update-comment',
            data: send_data,
            success: function (data) {
                if (data.success) {
                    that.parents('.comment').find('.comment-karma').append('<span class="edit-icon fa fa-pencil"></span>');
                    if (spoiler > 0) {
                        that.parents('.comment').find('.comment-text').html('<div class="comment-hidden-action">Комментарий содержит спойлер. <a>Показать</a></div><div class="comment-hidden">' + escapeHtml(text).replace(/\n/g, '<br />') + '</div>');
                    } else {
                        that.parents('.comment').find('.comment-text').html(escapeHtml(text).replace(/\n/g, '<br />'));
                    }
                } else {
                    showNotice(data.error);
                }
            }
        });
    });

    function replyTo(comment_id, user_name) {
        $('.leave-comment .reply').remove();
        $('.comments-author').append('<div class="reply" data-id="' + comment_id +
            '">Вы отвечаете на сообщение <b>' + user_name + '</b> <i class="close-icon remove"></i></div>');
        $('.leave-comment textarea[name="comment"]').val(user_name + ', ' + $('.leave-comment textarea[name="comment"]').val()).focus();
    }

    function convertTime(UNIX_timestamp) {
        var a = new Date(UNIX_timestamp * 1000);
        var months = ['Янв', 'Фев', 'Мар', 'Апр', 'Май', 'Июн', 'Июл', 'Авг', 'Сен', 'Окт', 'Ноя', 'Дек'];
        var year = a.getFullYear();
        var month = months[a.getMonth()];
        var date = a.getDate() < 10 ? '0' + a.getDate() : a.getDate();
        var hour = a.getHours() < 10 ? '0' + a.getHours() : a.getHours();
        var min = a.getMinutes() < 10 ? '0' + a.getMinutes() : a.getMinutes();
        var sec = a.getSeconds() < 10 ? '0' + a.getSeconds() : a.getSeconds();
        return date + ' ' + month + ' ' + year + ' ' + hour + ':' + min + ':' + sec;
    }

    $('.leave-comment').on('click', 'i.remove', function () {
        $(this).parent().remove();
    });

</script>

