  <div class="_post_content">
    
    <div class="post_header">
      <a class="post_image" href="/vieles_krug">
        <img src="https://pp.userapi.com/c409419/v409419754/42e6/ZgkgnvbCgD8.jpg" data-post-id="-51682933_176709" data-post-click-type="post_owner_img" width="50" height="50" class="post_img" />
