              <a target="_blank" href="<%= _.last(company.onedays).url %>" title="Фоторепортаж"><i class="icon-pic-story"></i></a>
              <% } %>
              <% if (company.interviews && company.interviews.length) { %>
              <a target="_blank" href="<%= _.last(company.interviews).url %>" title="Интервью с представителем компании"><i class="icon-mic"></i></a>
              <% } %>
              <% if (company.video && company.video.length) { %>
              <a target="_blank" href="<%= _.last(company.video).url %>" title="Видеорепортаж"><i class="icon-video"></i></a>
              <% } %>
              <% if (company.photos && company.photos.length) { %>
              <a target="_blank" href="/company/view/<%= company.id %>/info" title="Фотографии в компании"><i class="icon-photo"></i></a>
              <% } %>
            </div>
            <div class="ra-element-reviews">
              <% if (ratingCompany) { %>
              <div class="ra-element-stars">
                <div class="ra-element-stars__background-def">
                  <ul>
                    <li class="ra-element-stars__big-star"></li>
                    <li class="ra-element-stars__big-star"></li>
                    <li class="ra-element-stars__big-star"></li>
                    <li class="ra-element-stars__big-star"></li>
                    <li class="ra-element-stars__big-star"></li>
                  </ul>
                </div>
                <div class="ra-element-stars__background-active">
                  <ul>
                    <li class="ra-element-stars__big-star"></li>
                    <li class="ra-element-stars__big-star"></li>
                    <li class="ra-element-stars__big-star"></li>
                    <li class="ra-element-stars__big-star"></li>
                    <li class="ra-element-stars__big-star"></li>
                  </ul>
                </div>
                <ul class="ra-element-stars__list" >
                  <li class="ra-element-stars__big-star-item"></li>
                  <li class="ra-element-stars__big-star-item"></li>
                  <li class="ra-element-stars__big-star-item"></li>
                  <li class="ra-element-stars__big-star-item"></li>
                  <li class="ra-element-stars__big-star-item"></li>
                </ul>
              </div>
              <% } %>
              <% if (reviewCompany) { %>
              <a target="_blank" href="/company/view/<%= company.id %>/reviews?city_id[]=<%= contact.city.id %>" title="Отзывы"><%= reviewCompany %></a>
              <% } %>
            </div>
          <% } %>

          <% if (is_archived) { %>
            <span class="ra-elements-list__card__title-warning">[вакансия в архиве]</span>
          <% } %>

          <% if (is_deleted) { %>
            <span class="ra-elements-list__card__title-warning">[вакансия удалена]</span>
          <% } %>
        </div>

        <div class="ra-elements-list__brief">
          <% if(contact.address) { %>
            <%=contact.address%><% if(contact.subway) { %>, метро &laquo;<%= contact.subway.title %>&raquo; <% } %>
            <% if (!is_iframe) { %>
              <%= mapLink %>
            <% } %>
          <% } %>
          <div class="ra-elements-list__block-favorites">
            <ul class="ra-elements-list__control-favorites">
              <li class="ra-elements-list__item-favorites ra-fav-handler">
                <a href="#" data-analytic="clickFavouriteFromVacancyList" href="#" class="<% if (!AUTH_TOKEN) { %> ra-modal__open-to-register_fav <% } else { %> ra-elements-list__item-favhandler <% } %>
                  <% if (favorite) { %> active <% } %>">
                  <% if (!favorite) { %><i class="icon-favtool-vacancy-blue"></i><% } else { %><i class="icon-favtool-vacancy"></i><% } %>
                  <span class="ra-elements__link-dotted"><% if (favorite) { %>Из избранного<% } else { %>В избранное<% } %></span>
                </a>
              </li>
              <li class="ra-elements-list__item-hide ra-hide-handler">
                <a href="#" data-analytic="click<% if (hided) { %>Dis<% } else{ %>HideFrom<% } %>Vacancy" href="#" class="<% if (!AUTH_TOKEN) { %> ra-modal__open-to-register_hide <% } else { %> ra-elements-list__item-hidehandler <% } %>
                  <% if (hided) { %> active <% } %>">
                  <% if (hided) { %><i class="icon-rotate"></i><% } else { %><i class="icon-cross"></i><% } %>
                  <span class="ra-elements__link-dotted"><% if (hided) { %>Восстановить<% } else { %>Вычеркнуть<% } %></span>
                </a>
              </li>
            </ul>
          </div>
        </div>
      </div>
    </div>
    <div class="ra-elements-list__section ra-elements-list__section_pay">
      <div class="ra-elements-list__pay <% if(salary_min == 0 && salary_max == 0) { %>ra-elements-list__pay_not-specified<% } %>">
        <%= c_salary %>
      </div>
    </div>
    <div class="ra-elements-list__section ra-elements-list__section_actions ra-elements-list__section_content-container-vacancy">
      <% if(PATH.action === 'favorites') { %>
      <a class="icon-delete ra-favorites-link-delete ra-fav-handler" title="Убрать из избранного" href="#"></a>
      <% } else { %>
        <% if(company) {
          if(logo) { %>
            <div class="ra-elements-list__logo">
              <img src="<%= logo %>" alt=""/>
            </div>
        <% } } %>

        <% if (score) { %><div class="ra-elements-list__score" title="score"><%= score %></div><% } %>
      <% } %>
    </div>
  </div>
  <noindex>
    <div class="ra-elements-list__content-container">
      <div class="ra-elements-list__content <% if (PATH.action === 'favorites') { %>ra-elements-list__content-fav-edit<% } %>">
        <% if(company) { %>
          <div class="ra-rep-holder ra-in-vac">
            <% if(company.onedays && company.onedays.length) { %>
              <a target="_blank" href="<%= _.last(company.onedays).url %>" title="Фоторепортаж">
                <i class="icon-pic-story"></i>
                <span>Фоторепортаж</span>
              </a>
            <% } %>
            <% if(company.interviews && company.interviews.length) { %>
              <a target="_blank" href="<%= _.last(company.interviews).url %>" title="Интервью с представителем компании">
                <i class="icon-mic"></i>
                <span>Интервью</span>
              </a>
            <% } %>
          </div>
        <% } %>
        <div class="ra-elements-list__extra-placeholder"></div>
      </div>
      <div class="ra-elements-list__toolbar <% if (PATH.action === 'favorites') { %>ra-elements-list__toolbar_fav<% } %>"></div>
    </div>
  </noindex>
  
</script>
    <script type="text/template" id="VacancyItemFull">
  
  <div class="ra-elements-list__info">
    <% if(!_.isEmpty(requirements_short)){ %>
    <ul class="ra-elements-list__list">
      <li><%= requirements_short %></li>
    </ul>
    <% } %>

    <% if(schedule || working_type) { %>
    <ul class="ra-elements-list__list">
      <% if (schedule) { %>
      <li class="schedule"><%= schedule.title%></li>
      <% } %>
      <% if (working_type) { %>
      <li class="schedule"><%= working_type.title%></li>
      <% } %>
    </ul>
    <% } %>

    <% if(!_.isEmpty(description)) { %>
    <div class="ra-elements-list__description ugc">
      <%= description %>
    </div>
    <% } %>
  </div>
  <% if (sync_articles) { %>
  <div class="ra-elements-list__articles <% if (!sync_articles.video) { %>ra-elements-list__articles_mod<% } %>">
    <% if(sync_articles.onedays || sync_articles.interviews) { %>
    <div class="ra-elements-list__articles-col">

      <% if(sync_articles.onedays) { %>
      <div class="ra-elements-list__article ra-elements-list__article_onedays">
        <a href="<%= sync_articles.onedays.url %>"
           class="ra-elements-list__article-link"
           title="Фоторепортаж"
           target="_blank">

          <i class="icon-pic-story"></i>

          <div class="ra-elements-list__articles-title">
            <span>Фоторепортаж: один день в компании</span>
          </div>

          <% if (sync_articles.onedays.photos) { %>
          <div class="ra-elements-list__articles-content">
            <div class="ra-elements-list__articles-content-photos">
              <% _.each(sync_articles.onedays.photos, function(elem, i) {
              if (i > 2) { return; }
              %>
              <div class="ra-elements-list__articles-content-photo _bottom-layer<%=i %>">
                <img class="ra-elements-list__articles-content-photo-img" src="<%= elem.preview_resize %>" alt=""/>
              </div>
              <% }); %>
            </div>

            <div class="ra-elements-list__articles-content-comment">
              <%= sync_articles.onedays.photo_comment_prune %>
            </div>
          </div>
          <%  } %>

        </a>
      </div>
      <% } %>

      <% if (sync_articles.interviews) { %>
      <div class="ra-elements-list__article ra-elements-list__article_interview">
        <a href="<%= sync_articles.interviews.url %>"
           class="ra-elements-list__article-link"
           target="_blank"
           title="Интервью с представителем компании">

          <i class="icon-mic"></i>

          <div class="ra-elements-list__articles-title">
            <span>Интервью с представителем компании</span>
          </div>


          <div class="ra-elements-list__articles-content">

            <% if (sync_articles.interviews.photos) { %>
            <div class="ra-elements-list__articles-content-photos">
              <div class="ra-elements-list__articles-content-photo">
                <img class="ra-elements-list__articles-content-photo-img" src="<%= _.first(sync_articles.interviews.photos).preview_resize %>" alt=""/>
              </div>
            </div>
            <%  } %>

            <div class="ra-elements-list__articles-content-comment">
              <%= sync_articles.interviews.lead_prune %>
            </div>
          </div>

        </a>
      </div>
      <% } %>

    </div>
    <% } %>

    <% if(sync_articles.photo || sync_articles.video || sync_articles.oneday) { %>
    <div class="ra-elements-list__articles-col">

      <% if(sync_articles.photo) { %>
      <div class="ra-elements-list__article ra-elements-list__article_photo">
        <a href="/company/view/<%= company.id %>/info"
           class="ra-elements-list__article-link"
           title="Фоторепортаж"
           target="_blank">

          <i class="icon-photo"></i>

          <div class="ra-elements-list__articles-title">
            <span>Фотографии в компании</span>
          </div>

        </a>

        <% if (sync_articles.photo.photos) { %>
        <div class="ra-elements-list__articles-content">
          <div class="ra-elements-list__articles-content-photos photos-length-<%= sync_articles.photo.photos.length %>">

            <% _.each(sync_articles.photo.photos, function(elem, i) {
              if (i > 3) { return; } %>
              <a href="#" class="ra-elements-list__articles-content-photo ra-elements-list__articles-content-photo-open" data-id="<%=i %>">
                <img class="ra-elements-list__articles-content-photo-img" src="<%= elem.preview_resize %>" alt=""/>

              <% if (i === 3 && sync_articles.photo.more_photo) { %>
                <div class="ra-elements-list__articles-content-photo-img-overflow">Еще <%= sync_articles.photo.more_photo %> фото</div>
              <% } %>

              </a>
            <% }); %>
          </div>
        </div>
        <%  } %>
      </div>
      <% } %>

      <% if (sync_articles.video) { %>
      <div class="ra-elements-list__article">
        <div class="ra-elements-list__articles-title">
          <i class="icon-video"></i>
          <span>Видеоматериал о вакансии</span>
        </div>
        <div class="ra-elements-list__articles-content">
        </div>
      </div>
      <% } %>

    </div>
    <% } %>

  </div>
  <% } %>

  
</script>
    <script type="text/template" id="VacancyContacts">
  
      <div class="ra-contacts" itemscope itemtype="http://schema.org/Organization">
        <% if (!_.isEmpty(contact)) { %>
            <div class="ra-contacts__company">
              <% if (company) { %>
              <a href="/company/view/<%= company.id %>/?city_id[]=<%= contact.city.id %>"
                 target="_blank"><%= company.title %></a>
              <% } %>
            </div>

            <% if (has_contacts) { %>
              <% if (sync_contacts.preloader) { %>
                <div class="ra-contacts__preloader">
                  <img src="https://cdn.zp.ru/www.zarplata.ru/static/img/public/preloader2.GIF" alt="" />
                </div>
              <% } else if (sync_contacts.loaded) { %>
                <% if(!_.isEmpty(sync_contacts.name)) { %>
                  <div class="ra-contacts__name" itemprop="contactPoint">
                    <span class="ra-contacts__name-text"><%= sync_contacts.name %></span>
                  </div>
                <% } %>
                  <div class="ra-contacts__units">
                    <div class="ra-contacts__unit">
                      <% if(!_.isEmpty(sync_contacts.phones)) { %>
                        <% _.each(sync_contacts.phones, function(elem, i) {
                          if (sync_contacts.phones[i].phone) { %>
                            <div class="ra-contacts__phone ra-contacts__unit">
                              <i class="icon-phone"></i>
                                <span class="ra-contacts__phone-context" itemprop="telephone">
                                  <%= sync_contacts.phones[i].phone %>
                                </span>
                                <% if( sync_contacts.phones[i].comment ) { %>
                                <span class="ra-contacts__comments">
                                  (<%= sync_contacts.phones[i].comment %>)
                                </span>
                                <% } %>
                            </div>
                         <% }
                        }); %>
                      <% } %>
                    </div>
                    <% if (!_.isEmpty(sync_contacts.skype)) { %>
                    <div class="ra-contacts__unit ra-contacts__skype">
                      <i class="icon-skype"></i>
                      <a href="skype: <%= sync_contacts.skype %>" class="ra-contacts__skype-link">
                        <%= sync_contacts.skype %>
                      </a>
                    </div>
                    <% } %>
                    <% if (show_email && sync_contacts.email) { %>
                    <div class="ra-contacts__unit">
                      <i class="icon-email-blue"></i>
                      <span class="ra-contacts__mail" data-id="<%= id %>" data-metric="vacancy_show_email">
                        <a class='ra-contacts__mail-link' href='mailto:<%= sync_contacts.email %>?subject=Отклик на вакансию "<%=c_headerMail%>" на сайте <%=site_name%>' data-analytic="showEmail<%=analytic%>">
                          <%= sync_contacts.email %>
                        </a>
                      </span>
                    </div>
                    <% } %>
                    <% if (sync_contacts.url) { %>
                    <div class="ra-contacts__unit">
                      <i class="icon-site"></i>
                      <span class="ra-contacts__site-link" data-metric="vacancy_click_link" data-id="<%= id %>">
                        <a itemprope="url" href="<%= sync_contacts.url %>" target="_blank">
                          <%= sync_contacts.url %>
                        </a>
                      </span>
                    </div>
                    <% } %>
                  </div>
              <% } else { %>
                <div class="ra-contacts__show-contacts">
                  <a href="" class="ra-contacts__show-contacts-link" data-id="<%= id %>" data-metric="vacancy_show_phone">
                    Показать контакты
                  </a>
                </div>
              <% } %>
            <% } %>
        <% } %>
      </div>
    <% if (can_accept_replies && !is_iframe) { %>
      
        <div class="ra-contacts__unit-response">
          
          <a href="<%= url %>#reply" class="button-green button-medium ra-contacts__response" data-analytic="send<%=analytic%>1" data-metric="vacancy_reply" data-id="<%= id %>">
            Отправить резюме
          </a>
          <% if (reply.length) { %>
          <span class="ra-contacts__unit-reply">Вы уже отправляли отклик на эту вакансию <%= reply[0].mod_date%></span>
          <% } %>
          
        </div>
      
    <% } %>

  <div class="ra-elements-list__meta">
    <ul class="ra-elements-list__meta-list">
      <% if(id) { %>
        <li class="ra-elements-list__meta-item">
          <span class="ra-elements-list__meta-text">
            Вакансия № <%= id%>
          </span>
        </li>
      <% } %>
      <% if (window.IS_USER_ADMIN === true) { %>
        <li class="ra-elements-list__meta-item">
          <span class="ra-elements-list__meta-text">
            Статус: <%= state_pretty %>, <%= validate_state_pretty %>
          </span>
        </li>
        <% if (company) { %>
          <li class="ra-elements-list__meta-item">
            <a class="ra-elements-list__meta-admin-link" target="_blank" href="/newadmin/companies/view/<%= company.id %>/info">Компания</a>
          </li>
        <% } else { %>
          <li class="ra-elements-list__meta-item">
            <a class="ra-elements-list__meta-admin-link" target="_blank" href="/admin/users/?q=<%= owner_id%>&search_by=id">Частное лицо</a>
          </li>
        <% } %>
        <li class="ra-elements-list__meta-item">
          <a class="ra-elements-list__meta-admin-link" target="_blank" href="/admin/vacancy/edit?id=<%= id%>">Редактировать</a>
        </li>
        <li class="ra-elements-list__meta-item">
          <a class="ra-elements-list__meta-admin-link" target="_blank" href="/admin/logs?object_type_alias=vacancy&search=<%= id%>">Лог изменений</a>
        </li>
        <li class="ra-elements-list__meta-item">
          <a class="ra-elements-list__meta-admin-link" target="_blank" href="/admin/asUser/<%= owner_id%>">Войти под пользователем</a>
        </li>
      <% } %>
    </ul>
  </div>
  
</script>
