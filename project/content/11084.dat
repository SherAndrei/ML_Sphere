            <li class="ra-toolbar__item">
                <a data-analytic="clickPrintFrom<%= analytic %>" class="stop-propagation" href="/<%=altType%>/print?id[]=<%= id %>&city_id=<%= geo_city_ids %>" target="_blank">
                    <i class="icon-print"></i>
                    <span class="ra-toolbar__link-regular">Распечатать</span>
                </a>
            </li>
        <% } %>

        <li class="ra-toolbar__item ra-fav-handler">
            <a data-analytic="clickFavouriteFrom<%= analytic %>" href="#" class="<% if (!AUTH_TOKEN) { %> ra-modal__open-to-register_fav <% } else { %> ra-fav-handler__toolbar <% } %>
