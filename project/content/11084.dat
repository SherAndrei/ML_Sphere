                <% if (!favorite) { %><i class="icon-favtool"></i><% } else { %><i class="icon-favtool-active"></i><% } %>
                <span class="ra-toolbar__link-dotted">
                  <% if (favorite) { %>
                    Из избранного
                  <% } else { %>
                    В избранное
                  <% } %>
                </span>
            </a>
        </li>

        <li class="ra-toolbar__item ra-toolbar__item-abuse" data-id="<%= id %>" data-form="entityComplaint">
            <a data-analytic="clickAbuseFrom<%= analytic %>" class="" href="#" data-id="<%= id %>">
                <i class="icon-moderator"></i>
                <span class="ra-toolbar__link-dotted">Пожаловаться</span>
            </a>
        </li>

        <li class="ra-toolbar__item ra-toolbar__item-link">
            <a data-analytic="clickLinkFrom<%=analytic%>" class="ra-toolbar__item-dropdown" data-target=".get-link-<%= id %>" data-toggle="dropdown" href="#">
                <i class="icon-anchor"></i>
                <span class="ra-toolbar__link-dotted">Ссылка</span>
            </a>

            <div class="dropdown stop-propagation get-link-<%= id %>">
                <textarea class="ra-textarea-normal" cols="10" rows="4"><%= window.location.origin + url %></textarea>
            </div>
        </li>

        <li class="ra-toolbar__item ra-toolbar__item-share stop-propagation"></li>

        <% if (_.isEmpty(access_status) || access_status === 'access') { %>
            <li class="ra-toolbar__item ra-toolbar__item-save">
                <a data-target=".save-to-file-<%= id %>" data-toggle="dropdown" class="ra-toolbar__item-dropdown">
                    <i class="icon-save"></i>
                    <span class="ra-toolbar__link-dotted">Сохранить в файл</span>
                </a>

                <ul class="dropdown save-to-file-<%= id %>">
                    <li>
                        <a data-analytic="clickDownloadDocFrom<%=analytic%>" class="stop-propagation"
                           href="/<%=altType%>/export/<%= beauty_name %>.doc?id=<%= id %>&city_id=<%= geo_city_ids %>"
                           title="Сохранить в Word">
                            <i class="icon-word"></i>
                            <span>Word</span>
                        </a>
                    </li>

                    <li>
                        <a data-analytic="clickDownloadPdfFrom<%=analytic%>" class="stop-propagation" target="_blank"
                           href="/<%=altType%>/export/<%= beauty_name %>.pdf?id=<%= id %>&city_id=<%= geo_city_ids %>"
                        title="Сохранить в PDF">
                            <i class="icon-pdf"></i>
                            <span>PDF</span>
                        </a>
                    </li>
                </ul>
            </li>
        <% } %>
    </ul>
  
</script>
