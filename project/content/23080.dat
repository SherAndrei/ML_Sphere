<script>
    var slide = 0;
    var slideDirection;
    jQuery(document).ready(function () {
        var events_control = jQuery('#events');
        var left_control = events_control.find('a.left');
        var right_control = events_control.find('a.right');
        var events_container = jQuery('#events > div > div');
        events_container.css('box-sizing', 'content-box');
        events_container.on('transitionend', function () {
            var paddingLeft = parseInt(events_container.css('padding-left'));
            if ( slideDirection == 'right' ) {
                events_container.css('padding-left', parseInt(paddingLeft + 600) + 'px');
                var first_event_link = jQuery('#events > div > div > a:nth-child(-n+3)');
                first_event_link.detach().appendTo('#events > div > div');
            } else {
                var last_event_link = jQuery('#events > div > div > a:nth-last-child(-n+3)');
                last_event_link.detach().prependTo('#events > div > div');
            }
        });
        right_control.click(function () {
            slide++;
            slideDirection = 'right';
            var marginLeft = parseInt(events_container.css('margin-left'));
            events_container.css('margin-left', parseInt(marginLeft - 600) + 'px');
            return false;
        });
        left_control.click(function () {
            if ( slide <= 0 ) return false;
            slide--;
            slideDirection = 'left';
            var marginLeft = parseInt(events_container.css('margin-left'));
            events_container.css('margin-left', parseInt(marginLeft + 600) + 'px');
            var paddingLeft = parseInt(events_container.css('padding-left'));
            events_container.css('padding-left', parseInt(paddingLeft - 600) + 'px');
            return false;
        });
    });
</script>


