    this.init = function(id, count, fixed){	    
		var sliderArr = this._getSliderData(fixed,count);
	    var sliderContainer = new Element('ul', { 'class': 'show'});
		$(id).insert({
		  top: sliderContainer
		});       
		sliderArr.forEach(function(element, index, array){
			var slide = this._getSlide(element);
			sliderContainer.insert(slide);
		},this);
		this._addSlider(id);
	},
    this._getSliderData = function(fixed,count){
		var slidesData = this.sliderData;       
        var size = count != undefined ? count : 3;
		var slides = [];

		if(fixed != undefined){
			slides[0] = slidesData[fixed];
			slidesData.splice(fixed, 1);
			size = size - 1;
		}

		for(var i = size; i > 0; i--){
            if(slidesData.length) {
                var index = Math.round(Math.random() * (slidesData.length - 1));
                slides[i] = slidesData[index];
                slidesData.splice(index, 1);
            }
		}
		return slides;
	},
    this._addSlider=function(id){
		$(id) && $(id).protoShow({
				interval	:	8000,
				navigation	: false,
				controls	:	false,
				pauseOnHover:	true,
				progressTimer: false,
                transitionType: 'slide'
		});	
	},
    this._getSlide = function(element){
		var html;		
		var width= 960;
		if(this.adaptive){
		    var el = $('post_body_b');		
			var elWidth = el.getWidth();
			if(elWidth < width){
				width = elWidth;
				var sliderBox = $('slider_theme_advt');
				$(sliderBox).setStyle({
				  width: width+'px',			  
				});
			}	
		}
		html = '<li style="width:'+width+'px;" class="slide">';
		if(element.url)		
			html = html + '<a href="'+element.url+'"><img width="'+width+'" src="'+element.image+'" /></a>';		
		else
			html = html + '<img width="'+width+'" src="'+element.image+'" />';
		html = html + '</li>';
		return html;
	},
	this.adaptive = false	
};
