$(document).ready(function(){
    // Если нет поискового запроса, пишем слово "Поиск"
    if ($('#search-input').val() == '') {
        $('#search-input').val('Поиск');
        $('#search-input').addClass('search-input-label');
    }
    $("#search-input").focus(function() {
        if ($('#search-input').val() == 'Поиск') {
            $('#search-input').val('');
            $('#search-input').removeClass('search-input-label');
        }
    });
    // Смена типа поиска
    search_type = $.cookie('search_type');    
    //alert(search_type);
    if (search_type != null) {
        set_search_type(search_type, true);
    }  
    $('#search-type-select select').change(function(){
        type = $(this).val();
        if (type != null) {
            set_search_type(type, false);
        }
        $.cookie('search_type', type, { path: '/' });
    });    
    window_width = parseInt($(window).width());
    head_width = parseInt($('#head').width());
    //alert(window_width);
    
    // Уменьшение размеров всех картинок
    $('.article-body img').each(function(){
        image_width = parseInt($(this).width());
        image_height = parseInt($(this).height());
        k = (window_width - 40) / image_width;
        //alert(window_width+', '+image_width+', '+k);
        if (k < 1) {        
            $(this).attr('width', image_width*k);
            $(this).attr('height', image_height*k);
        }
    });
    
});
$(window).resize(function(){
    create_menu_top_icon_shadows();
});
</script>

