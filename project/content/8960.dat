                e.preventDefault();
                if($('.add_comment_form').css('display') == 'none'){
                    $('.add_comment_form').css('display', 'block');
                } else {
                    $('.add_comment_form').css('display', 'none');
                }
            });
            var addLike = function(){
                var like = $(this);
                $.ajax({
                    'url': '/rating/setmark',
                    'type': 'GET',
                    'data': {'good_id':5562, 'rate':'1', 'modelname':'Articles'},
                    'success': function(data){
                        if(data == 'success'){
                            like.removeClass('like').addClass('dislike');
                            like.html('Не нравится');
                            like.unbind('click',addLike);
                            like.click(removeLike);
                            stars = stars + 1;
                            $('.fa-star:first').next().html(stars);
                        }
                    }
                })
            }
            var removeLike = function(){
                var like = $(this);
                $.ajax({
                    'url': '/rating/setmark',
                    'type': 'GET',
                    'data': {'good_id':5562, 'rate':'1', 'modelname':'Articles'},
                    'success': function(data){
                        if(data == 'success'){
                            like.removeClass('dislike').addClass('like');
                            like.html('Мне нравится');
                            like.unbind('click', removeLike);
                            like.click(addLike);
                            stars = stars - 1;
                            $('.fa-star:first').next().html(stars);
                        }
                    }
                })
            }

            if($('.like'))
                $('.like').click(addLike);

            if($('.dislike'))
                $('.dislike').click(removeLike);
            $('.more-comments').click(function(e){

                var id= $('.comment_block .comment_box:last').data('id');

                $.ajax({
                    url: '/comment/comment/comment',
                    data: {'model': 'Articles','model_id': '5562', 'id': id},
                    dataType: 'json',
                    success: function(data) {
                        $(data.comments).insertAfter('.comment_block .comment_box:last');

                        if(!data.next)
                            $('.get_more').remove();

                    }
                })
                return false;
            });
        });
    


