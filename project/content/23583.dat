 QuarantineFile('C:\Users\user\Start Menu\Programs\Startup\start1.exe','');
 DeleteFile('C:\Users\user\Start Menu\Programs\Startup\start1.exe','32');   
BC_ImportAll;
ExecuteSysClean;
ExecuteWizard('SCU', 2, 2, true);
BC_Activate;
ExecuteRepair(12);
RebootWindows(false);
end.</pre>
