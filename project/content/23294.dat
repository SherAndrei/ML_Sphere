<script type="text/javascript">
    function closeFullGift(){
        document.getElementById("giftHolder").style.display = "none";  
    };

    function showGiftCard(title, imgBase, smallImg, fullImg){
        document.getElementById("giftTitle").innerHTML = title;
        document.getElementById("giftFrom").innerHTML = "";
        document.getElementById("giftDate").innerHTML = "";
        document.getElementById("giftNote").innerHTML = "";
        document.getElementById("giftFromLabel").style.display = "none";
        document.getElementById("giftGibenLabel").style.display = "none";
        document.getElementById("giftNoteLabel").style.display = "none";

        var img = fullImg === "" ? smallImg : fullImg;
        document.getElementById("giftFullImg").setAttribute("src", imgBase + img);

        document.getElementById("giftHolder").style.display = "block";        
    };

    function showFullGift(title, from, fromHref, date, note, imgBase, smallImg, fullImg){
        var d = new Date(parseInt(date) * 1000);
        var formattedDate = (d.getDate() >= 10 ? d.getDate() : "0" + d.getDate()) + "." +
                            (d.getMonth() >= 10 ? (d.getMonth() + 1) : "0" + (d.getMonth() + 1)) + "." +
                            (d.getFullYear());

        document.getElementById("giftTitle").innerHTML = title;
        document.getElementById("giftFrom").innerHTML = "<a href='" + fromHref + "'>" + from + "</a>";
        document.getElementById("giftDate").innerHTML = formattedDate ;
        document.getElementById("giftNote").innerHTML = note;

        document.getElementById("giftFromLabel").style.display = "inline";
        document.getElementById("giftGibenLabel").style.display = "inline";
        document.getElementById("giftNoteLabel").style.display = "inline";

        var img = fullImg === "" ? smallImg : fullImg;
        document.getElementById("giftFullImg").setAttribute("src", imgBase + img);

        document.getElementById("giftHolder").style.display = "block";        
    };
</script>

		
		
